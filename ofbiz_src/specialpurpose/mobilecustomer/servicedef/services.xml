<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>MobileCustomer Services</description>
    <vendor></vendor>
    <version>1.0</version>

    <service name="noSyntaxError" engine="interface">
        <description>
            Dummy service to prevent empty files and syntax error - Remove when the 1st real service will be added here 
        </description>
    </service> 
    <service name="getProductStoreCustomer" engine="java" location="org.ofbiz.mobilecustomer.MobileCustomer" auth="true" invoke="getProductStoreCustomer">
	    <attribute name="listProductStore" mode="OUT" type="List"/>
	</service>
	<service name="mRegisterCustomerAccount" engine="java" location="org.ofbiz.mobilecustomer.MobileCustomer" auth="false" invoke="registerCustomerAccount">
		<attribute name="username" mode="IN" type="String" optional="false"/>
		<attribute name="password" mode="IN" type="String" optional="false"/>
		<attribute name="passwordRepeat" mode="IN" type="String" optional="false"/>
		<attribute name="fullName" mode="IN" type="String" optional="false"/>
		<attribute name="birthday" mode="IN" type="Long" optional="true"/>
		<attribute name="gender" mode="IN" type="String" optional="true"/>
		<attribute name="phoneNumber" mode="IN" type="String" optional="false"/>
		<attribute name="email" mode="IN" type="String" optional="true"/>
		<attribute name="address" mode="IN" type="String" optional="false"/>
		<attribute name="description" mode="IN" type="String" optional="true"/>
		<attribute name="customerId" mode="OUT" type="String"></attribute>
	</service>
	<service name="mGetAssociatedStateWithCountry" engine="java" invoke="mGetAssociatedStateWithCountry" location="org.ofbiz.mobilecustomer.MobileCustomer" auth="false">
		<attribute name="listIterator" mode="OUT" type="java.util.List" optional="true" />
	</service>
	<service name="mGetAssociatedStateOtherListGeo" engine="java" invoke="mGetAssociatedStateOtherListGeo" location="org.ofbiz.mobilecustomer.MobileCustomer" auth="false">
   		<attribute name="geoId" mode="IN" type="String" optional="true"/>
   		<attribute name="listIterator" mode="OUT" type="java.util.List" optional="true"/>
	</service>
</services>