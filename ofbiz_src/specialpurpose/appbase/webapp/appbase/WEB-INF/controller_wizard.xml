<?xml version="1.0" encoding="UTF-8"?>
<site-conf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/site-conf.xsd">
    <!-- The controller elements that are common to all OFBiz components
         can be found in the following xml file. A component can override the
         elements found in the common-controller.xml file. -->
    
    <!-- Request Mappings -->
    <request-map uri="jqGetAssociatedStateOtherListGeo">
        <event type="java" invoke="jqGetAssociatedStateOtherListGeo" path="com.olbius.appbase.events.OlbWizardEvents" global-transaction="false"/>
        <response name="success" type="none" />
        <response name="error" type="none" />
    </request-map>
    <request-map uri="wizardSetupOrganization">
    	<event type="service" invoke="wizardSetupOrganization"/>
    	<response name="success" type="request" value="json"/>
    	<response name="error" type="request" value="json"/>
    </request-map>
    <request-map uri="wizardSetupSubsidiary">
    	<event type="service" invoke="wizardSetupSubsidiary"/>
    	<response name="success" type="request" value="json"/>
    	<response name="error" type="request" value="json"/>
    </request-map>
    <request-map uri="wizardGetListPartyOrg">
    	<event type="service" invoke="wizardGetListPartyOrg"/>
    	<response name="success" type="request" value="json"/>
    	<response name="error" type="request" value="json"/>
    </request-map>
    <request-map uri="wizardSetupHrManager">
    	<event type="service" invoke="wizardSetupHrManager"/>
    	<response name="success" type="request" value="json"/>
    	<response name="error" type="request" value="json"/>
    </request-map>
    <request-map uri="wizardSetupOlbiusAdmin">
    	<event type="service" invoke="wizardSetupOlbiusAdmin"/>
    	<response name="success" type="request" value="json"/>
    	<response name="error" type="request" value="json"/>
    </request-map>
    <request-map uri="checkSetupComplete">
    	<event type="service" invoke="checkSetupComplete"/>
    	<response name="success" type="request" value="json"/>
    	<response name="error" type="request" value="json"/>
    </request-map>
    
    <!-- View Mappings -->
    
</site-conf>