<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>AppBaseMtl HR Process Services</description>
	<service name="checkPermissionApprRecruitmentAnticipate" engine="java" invoke="checkPermissionApprRecruitmentAnticipate" auth="true"
		location="com.olbius.appbase.hr.HRWorkFlowActionServices">
		<attribute name="recruitAnticipateId" mode="IN" type="String"></attribute>
		<attribute name="hasPermission" mode="OUT" type="Boolean"/>
	</service>
	<service name="checkPermissionApprRecruitmentRequire" engine="java" invoke="checkPermissionApprRecruitmentRequire" auth="true"
		location="com.olbius.appbase.hr.HRWorkFlowActionServices">
		<attribute name="recruitmentRequireId" mode="IN" type="String"></attribute>
		<attribute name="hasPermission" mode="OUT" type="Boolean"/>
	</service>
	<service name="getRecruitmentAnticipateStatusAfterAppr" engine="java" invoke="getRecruitmentAnticipateStatusAfterAppr" auth="true"
		location="com.olbius.appbase.hr.HRWorkFlowActionServices" default-entity-name="RecruitmentAnticipate">
		<auto-attributes mode="IN" include="pk" optional="false"></auto-attributes>	
		<attribute name="newStatusId" mode="OUT" type="String" optional="true"></attribute>
	</service>
	<service name="sendNotifyApprRecruitmentAnticipate" engine="java" invoke="sendNotifyApprRecruitmentAnticipate" auth="true"
		location="com.olbius.appbase.hr.HRWorkFlowActionServices">
		<attribute name="recruitAnticipateId" mode="IN" type="String"></attribute>	
		<attribute name="statusId" mode="IN" type="String" optional="true"></attribute>	
	</service>
	<service name="getRecruitAnticipateItemListCanEdit" engine="java" invoke="getRecruitAnticipateItemListCanEdit" auth="true"
		location="com.olbius.appbase.hr.HRWorkFlowActionServices">
		<attribute name="recruitAnticipateId" mode="IN" type="String"></attribute>	
		<attribute name="listReturn" mode="OUT" type="List" optional="true"></attribute>
	</service>
	<service name="getStatusListApprRecruitmentAnticipate" engine="java" invoke="getStatusListApprRecruitmentAnticipate" auth="true"
		location="com.olbius.appbase.hr.HRWorkFlowActionServices">
		<required-permissions join-type="AND">
			<check-permission permission="RECRUITOFFICEPLAN" action="_APPROVE"/>
		</required-permissions>	
		<attribute name="recruitAnticipateId" mode="IN" type="String"></attribute>
		<attribute name="statusList" mode="OUT" type="List"></attribute>
		<attribute name="statusIdNeedAppr" mode="OUT" type="String" optional="true"></attribute>
	</service>
	<service name="editRecruitmentAnticipate" engine="java" invoke="editRecruitmentAnticipate" auth="true"
		location="com.olbius.appbase.hr.HRWorkFlowActionServices">
		<attribute name="editRecruitAnticipateItemList" mode="IN" type="String"></attribute>	
		<attribute name="recruitAnticipateId" mode="IN" type="String"></attribute>	
	</service>
	<service name="updateRecruitmentRequireStatus" engine="java" invoke="updateRecruitmentRequireStatus" auth="true"
		location="com.olbius.appbase.hr.HRWorkFlowActionServices" default-entity-name="RecruitmentRequire">
		<auto-attributes mode="IN" include="pk" optional="false"></auto-attributes>
	</service>
	<service name="sendNotifyApprRecruitmentRequire" engine="java" invoke="sendNotifyApprRecruitmentRequire" auth="true"
		location="com.olbius.appbase.hr.HRWorkFlowActionServices" default-entity-name="RecruitmentRequire">
		<auto-attributes mode="IN" include="pk" optional="false"></auto-attributes>
		<attribute name="statusId" mode="IN" type="String" optional="true"></attribute>
	</service>
	<service name="getNextStatusRecruitmentRequire" engine="java" invoke="getNextStatusRecruitmentRequire" auth="true"
		location="com.olbius.appbase.hr.HRWorkFlowActionServices">
		<attribute name="recruitmentRequireId" mode="IN" type="String"></attribute>
		<attribute name="statusCode" mode="IN" type="String"></attribute>
		<attribute name="newStatusId" mode="OUT" type="String"></attribute>	
	</service>
	<service name="checkRecruitmentRequireEditable" engine="java" invoke="checkRecruitmentRequireEditable" auth="true"
		location="com.olbius.appbase.hr.HRWorkFlowActionServices" default-entity-name="RecruitmentRequire">
		<auto-attributes mode="IN" include="pk"></auto-attributes>
		<attribute name="isEditable" mode="OUT" type="Boolean"></attribute>	
	</service>
	<service name="getNextStatusRecruitmentSalesEmpl" engine="java" invoke="getNextStatusRecruitmentSalesEmpl" auth="true"
		location="com.olbius.appbase.hr.HRWorkFlowActionServices" default-entity-name="RecruitmentSalesEmpl">
		<auto-attributes mode="IN" include="pk"></auto-attributes>
		<attribute name="approvalType" mode="IN" type="String" optional="true"></attribute>
		<attribute name="newStatusId" mode="OUT" type="String"></attribute>	
	</service>
	<service name="getListViewableStatusRecruitmentSalesEmpl" engine="java" invoke="getListViewableStatusRecruitmentSalesEmpl" auth="true"
		location="com.olbius.appbase.hr.HRWorkFlowActionServices" >
		<attribute name="statusIdList" mode="OUT" type="List"></attribute>	
	</service>
	<service name="getListRecruitSalesEmplNotApprStatus" engine="java" invoke="getListRecruitSalesEmplNotApprStatus" auth="true"
		location="com.olbius.appbase.hr.HRWorkFlowActionServices" >
		<attribute name="statusIdList" mode="OUT" type="List"></attribute>	
	</service>
	<service name="getStatusSalesEmplProposable" engine="java" invoke="getStatusSalesEmplProposable" auth="true"
		location="com.olbius.appbase.hr.HRWorkFlowActionServices" >
		<attribute name="statusId" mode="OUT" type="String"></attribute>	
	</service>
	<service name="getPartyIdOfferByStatus" engine="java" invoke="getPartyIdOfferByStatus" auth="true"
		location="com.olbius.appbase.hr.HRWorkFlowActionServices">
		<attribute name="statusId" mode="IN" type="String"></attribute>	
		<attribute name="partyGroupId" mode="IN" type="String"></attribute>
		<attribute name="partyIdOffer" mode="OUT" type="String" optional="true"></attribute>	
	</service>
</services>        