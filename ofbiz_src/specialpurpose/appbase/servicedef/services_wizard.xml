<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>AppBase Services Wizard</description>
    <vendor></vendor>
    <version>1.0</version>
    
    <service name="wizardSetupOrganization" engine="java" 
    		invoke="wizardSetupOrganization" location="com.olbius.appbase.common.OlbWizardServices">
		<description>Setup organization in wizard setup</description>
   		<attribute name="partyName" mode="IN" type="String" optional="true"/>
   		<attribute name="phoneNumber" mode="IN" type="String" optional="true"/>
   		<attribute name="emailAddress" mode="IN" type="String" optional="true"/>
   		<attribute name="officeSiteName" mode="IN" type="String" optional="true"/>
   		<attribute name="taxAuthInfos" mode="IN" type="String" optional="true"/>
   		<attribute name="currencyUomId" mode="IN" type="String" optional="true"/>
   		<attribute name="countryGeoId" mode="IN" type="String" optional="true"/>
   		<attribute name="stateProvinceGeoId" mode="IN" type="String" optional="true"/>
		<attribute name="districtGeoId" mode="IN" type="String" optional="true"/>
   		<attribute name="wardGeoId" mode="IN" type="String" optional="true"/>
   		<attribute name="address1" mode="IN" type="String" optional="true"/>
   		
   		<attribute name="partyId" mode="INOUT" type="String" optional="true"/>
	</service>
    <service name="wizardSetupSubsidiary" engine="java" 
    		invoke="wizardSetupSubsidiary" location="com.olbius.appbase.common.OlbWizardServices">
		<description>Setup organization in wizard setup</description>
   		<attribute name="partySubsidiary" mode="IN" type="String" optional="true"/>
   		<attribute name="noHaveSubsidiary" mode="IN" type="Boolean" optional="false"/>
	</service>
	<service name="wizardGetListPartyOrg" engine="java" 
			invoke="wizardGetListPartyOrg" location="com.olbius.appbase.common.OlbWizardServices">
		<attribute name="results" mode="OUT" type="java.util.List" optional="true"/>
	</service>
	<service name="wizardSetupHrManager" engine="java" 
    		invoke="wizardSetupHrManager" location="com.olbius.appbase.common.OlbWizardServices">
		<description>Setup HR manager in wizard setup</description>
   		<attribute name="partyCode" mode="IN" type="String" optional="true"/>
   		<attribute name="partyName" mode="IN" type="String" optional="true"/>
   		<attribute name="phoneNumber" mode="IN" type="String" optional="true"/>
   		<attribute name="emailAddress" mode="IN" type="String" optional="true"/>
   		<attribute name="birthDate" mode="IN" type="Long" optional="true"/>
   		<attribute name="gender" mode="IN" type="String" optional="true"/>
   		<attribute name="organizationIds[]" mode="IN" type="java.util.List" optional="true"/>
   		<attribute name="countryGeoId" mode="IN" type="String" optional="true"/>
   		<attribute name="stateProvinceGeoId" mode="IN" type="String" optional="true"/>
		<attribute name="districtGeoId" mode="IN" type="String" optional="true"/>
   		<attribute name="wardGeoId" mode="IN" type="String" optional="true"/>
   		<attribute name="address1" mode="IN" type="String" optional="true"/>
   		
   		<attribute name="partyId" mode="INOUT" type="String" optional="true"/>
	</service>
	<service name="wizardSetupOlbiusAdmin" engine="java" 
    		invoke="wizardSetupOlbiusAdmin" location="com.olbius.appbase.common.OlbWizardServices">
		<description>Setup HR manager in wizard setup</description>
   		<attribute name="partyCode" mode="IN" type="String" optional="true"/>
   		<attribute name="partyName" mode="IN" type="String" optional="true"/>
   		<attribute name="phoneNumber" mode="IN" type="String" optional="true"/>
   		<attribute name="emailAddress" mode="IN" type="String" optional="true"/>
   		<attribute name="birthDate" mode="IN" type="Long" optional="true"/>
   		<attribute name="gender" mode="IN" type="String" optional="true"/>
   		<attribute name="countryGeoId" mode="IN" type="String" optional="true"/>
   		<attribute name="stateProvinceGeoId" mode="IN" type="String" optional="true"/>
		<attribute name="districtGeoId" mode="IN" type="String" optional="true"/>
   		<attribute name="wardGeoId" mode="IN" type="String" optional="true"/>
   		<attribute name="address1" mode="IN" type="String" optional="true"/>
   		
   		<attribute name="partyId" mode="INOUT" type="String" optional="true"/>
   		
   		<attribute name="useHrManager" mode="IN" type="Boolean" optional="false"/>
	</service>
	<service name="checkSetupComplete" engine="java" 
    		invoke="checkSetupComplete" location="com.olbius.appbase.common.OlbWizardServices">
   		<attribute name="isComplete" mode="OUT" type="Boolean" optional="false"/>
	</service>
	
</services>