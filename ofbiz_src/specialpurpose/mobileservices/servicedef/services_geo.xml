<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
	<description>Geo Services</description>
	<vendor>OFBiz</vendor>
	<version>1.0</version>

	<service name="salesmanCheckIn" engine="java" invoke="salesmanCheckIn" location="org.ofbiz.mobileservices.geo.GeoServices" auth="true">
		<attribute name="latitude" mode="IN" type="String" />
		<attribute name="longitude" mode="IN" type="String" />
		<attribute name="customerId" mode="IN" type="String" />
		<attribute name="message" mode="OUT" type="String" optional="true" />
		<attribute name="checkInOk" mode="OUT" type="java.lang.Boolean" />
	</service>
	<service name="salesmanCheckOut" engine="java" invoke="salesmanCheckOut" location="org.ofbiz.mobileservices.geo.GeoServices" auth="true">
		<attribute name="latitude" mode="IN" type="String" />
		<attribute name="longitude" mode="IN" type="String" />
		<attribute name="customerId" mode="IN" type="String" />
		<attribute name="message" mode="OUT" type="String" optional="true" />
		<attribute name="checkOutOk" mode="OUT" type="java.lang.Boolean" />
	</service>

	<service name="getTodayCheckedInCustomers" engine="java" invoke="getTodayCheckedInCustomers" location="org.ofbiz.mobileservices.geo.GeoServices" auth="true">
		<attribute name="partyId" mode="IN" type="String" />
		<attribute name="customers" mode="OUT" type="java.util.List" />
	</service>

</services>
