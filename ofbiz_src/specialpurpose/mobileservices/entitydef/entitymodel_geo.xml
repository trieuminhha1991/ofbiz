<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
	<!-- ========================================================= -->
	<!-- ======================== Defaults ======================= -->
	<!-- ========================================================= -->
	<title>Entity of an Open For Business Project Component</title>
	<description>None</description>
	<version>1.0</version>
	<default-resource-name>MobileServicesEntityLabels</default-resource-name>

	<entity entity-name="CheckInHistory" package-name="org.ofbiz.mobileservices"
		title="Product Quotation">
		<field name="checkInId" type="id-ne"></field>

		<field name="partyId" type="id-ne"></field>
		<field name="latitude" type="floating-point"></field>
		<field name="longitude" type="floating-point"></field>

		<field name="customerId" type="id-ne"></field>
		<field name="customerLatitude" type="floating-point"></field>
		<field name="customerLongitude" type="floating-point"></field>

		<field name="distance" type="floating-point"></field>
		<field name="checkInDate" type="date-time"></field>
		<field name="checkInOk" type="boolean"></field>
        <field name="checkOutDate" type="date-time"></field>
		<prim-key field="checkInId" />

		<relation type="one" fk-name="PART_REL" rel-entity-name="Party" title="party">
			<key-map field-name="partyId" />
		</relation>
		<relation type="one" fk-name="PART_REL2" rel-entity-name="Party" title="customer">
			<key-map field-name="customerId" rel-field-name="partyId" />
		</relation>
	</entity>
</entitymodel>