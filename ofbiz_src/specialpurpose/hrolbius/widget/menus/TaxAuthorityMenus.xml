<?xml version="1.0" encoding="UTF-8"?>
<menus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../framework/widget/dtd/widget-menu.xsd">

    <menu name="TaxAuthorityTabBar" modelMenu="olbiustab" extends="CommonTabBarMenu" extends-resource="component://common/widget/CommonMenus.xml"
          default-menu-item-name="EditTaxAuthority">
        <menu-item name="EditTaxAuthority" title="${uiLabelMap.AccountingTaxAuthority}">
            <link target="editTaxAuthority" style="icon-briefcase blue">
                <parameter param-name="taxAuthPartyId"/>
                <parameter param-name="taxAuthGeoId"/>
            </link>
        </menu-item>
        <menu-item name="EditTaxAuthorityAssocs" title="${uiLabelMap.CommonAssocs}">
            <link target="EditTaxAuthorityAssocs" style="icon-exchange blue">
                <parameter param-name="taxAuthPartyId"/>
                <parameter param-name="taxAuthGeoId"/>
            </link>
        </menu-item>
        <menu-item name="EditTaxAuthorityGlAccounts" title="${uiLabelMap.AccountingGlAccs}">
            <link target="EditTaxAuthorityGlAccounts" style="icon-pushpin blue">
                <parameter param-name="taxAuthPartyId"/>
                <parameter param-name="taxAuthGeoId"/>
            </link>
        </menu-item>
        <menu-item name="ListTaxAuthorityParties" title="${uiLabelMap.PartyParties}">
            <link target="ListTaxAuthorityParties" style="icon-group blue">
                <parameter param-name="taxAuthPartyId"/>
                <parameter param-name="taxAuthGeoId"/>
            </link>
        </menu-item>
        <menu-item name="ListTaxAuthorityRate" title="${uiLabelMap.PayrollTAR}">
            <link target="ListTaxAuthorityRate" style="icon-screenshot blue">
                <parameter param-name="taxAuthPartyId"/>
                <parameter param-name="taxAuthGeoId"/>
            </link>
        </menu-item>
    </menu>
</menus>
