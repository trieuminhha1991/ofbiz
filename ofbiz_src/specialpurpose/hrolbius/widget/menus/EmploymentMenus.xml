<?xml version="1.0" encoding="UTF-8"?>
<menus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../../framework/widget/dtd/widget-menu.xsd">
	
	<menu name="EmploymentBar" modelMenu="olbiustab"
		default-menu-item-name="EmployeeProfile" default-selected-style="selected"
		type="simple" menu-container-style="button-bar tab-bar"
		selected-menuitem-context-field-name="tabButtonItem">
		<menu-item name="EmployeeProfile" title="${uiLabelMap.PartyProfile}">
			<link target="EmpProfile" style="icon-user blue">
			    <parameter param-name="partyId" from-field="parameters.partyIdTo"/>
				<parameter param-name="roleTypeIdFrom"/>
				<parameter param-name="roleTypeIdTo" />
				<parameter param-name="partyIdFrom" />
				<parameter param-name="partyIdTo" />
				<parameter param-name="fromDate" />
			</link>
		</menu-item>
	    
		<menu-item name="EditEmployment" title="${uiLabelMap.HumanResEmployment}">
			<link target="EditEmployment" style="icon-edit blue">
				<parameter param-name="roleTypeIdFrom" />
				<parameter param-name="roleTypeIdTo" />
				<parameter param-name="partyIdFrom" />
				<parameter param-name="partyIdTo" />
				<parameter param-name="fromDate" />
			</link>
		</menu-item>
		<menu-item name="EditPartyBenefit" title="${uiLabelMap.EditPartyBenefit}">
			<link target="EditPartyBenefits" style="icon-plus-sign-alt blue">
				<parameter param-name="roleTypeIdFrom" />
				<parameter param-name="roleTypeIdTo" />
				<parameter param-name="partyIdFrom" />
				<parameter param-name="partyIdTo" />
				<parameter param-name="fromDate" />
			</link>
		</menu-item>
		<menu-item name="EditPayrollPreference"
			title="${uiLabelMap.HumanResEditPayrollPreference}">
			<link target="EditPayrollPreferences" style="icon-cogs blue">
				<parameter param-name="roleTypeIdFrom" />
				<parameter param-name="roleTypeIdTo" />
				<parameter param-name="partyIdFrom" />
				<parameter param-name="partyIdTo" />
				<parameter param-name="fromDate" />
			</link>
		</menu-item>
		<menu-item name="EditPayHistory" title="${uiLabelMap.HumanResEditPayHistory}">
			<link target="ListPayHistories" style="icon-time blue">
				<parameter param-name="roleTypeIdFrom" />
				<parameter param-name="roleTypeIdTo" />
				<parameter param-name="partyIdFrom" />
				<parameter param-name="partyIdTo" />
				<parameter param-name="fromDate" />
			</link>
		</menu-item>
		<menu-item name="EditUnemploymentClaims"
			title="${uiLabelMap.HumanResEditUnemploymentClaim}">
			<link target="EditUnemploymentClaims" style="icon-remove blue">
				<parameter param-name="roleTypeIdFrom" />
				<parameter param-name="roleTypeIdTo" />
				<parameter param-name="partyIdFrom" />
				<parameter param-name="partyIdTo" />
				<parameter param-name="fromDate" />
			</link>
		</menu-item>
		
	</menu>
	<menu name="EmplTimeManagerSubMenu" modelMenu="businessSubMenu" selected-menuitem-context-field-name="selectedSubMenuItem">
		<menu-item name="EmplLeaveAppl" title="${uiLabelMap.ApplicationLeave}">
			<link target="EditEmplLeave">
				<parameter param-name="partyId" from-field="userLogin.partyId"/>
			</link>
		</menu-item>
		<menu-item name="HolidaySetting" title="${uiLabelMap.SettingHoliday}">
			<link target="ListHolidaySetting"></link>
		</menu-item>
		<menu-item name="SettingWorkingShift" title="${uiLabelMap.SettingWorkingShift}">
			<link target="ListWorkingShift"></link>
		</menu-item>
		<menu-item name="AssignWorkingShift" title="${uiLabelMap.AssignWorkingShift}">
			<link target="AssignWorkingShift"></link>
		</menu-item>
	</menu>
</menus>
	