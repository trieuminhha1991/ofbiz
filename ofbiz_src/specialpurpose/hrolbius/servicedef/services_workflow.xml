<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
	<service name="createWorkFlowProcess" engine="java" auth="true" invoke="createWorkFlowProcess"
		location="com.olbius.workflow.WorkFlowService" default-entity-name="WorkFlowProcess">
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
		<auto-attributes mode="OUT" include="pk" optional="false"></auto-attributes>	
	</service>	        
	<service name="createWorkFlowRequest" engine="java" auth="true" invoke="createWorkFlowRequest"
		location="com.olbius.workflow.WorkFlowService" default-entity-name="WorkFlowRequest">
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
		<auto-attributes mode="OUT" include="pk" optional="false"></auto-attributes>	
	</service>
	<service name="approvalWorkFlowRequest" engine="java" invoke="approvalWorkFlowRequest" auth="true"
		location="com.olbius.workflow.WorkFlowService">
		<attribute name="requestId" mode="IN" type="String"></attribute>	
		<attribute name="partyId" mode="IN" type="String" optional="true"></attribute>	
		<attribute name="actionTypeId" mode="IN" type="String"></attribute>	
	</service>	        
	<service name="executeActivityAction" engine="java" invoke="executeActivityAction" auth="true"
		location="com.olbius.workflow.WorkFlowService">
		<attribute name="actionId" mode="IN" type="String"></attribute>
		<attribute name="requestId" mode="IN" type="String" optional="true"></attribute>	
	</service>
	<service name="updateWorkFlowRequest" engine="java" invoke="updateWorkFlowRequest" auth="true"
		location="com.olbius.workflow.WorkFlowService" default-entity-name="WorkFlowRequest">
		<auto-attributes mode="IN" include="pk" optional="false"></auto-attributes>
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
	</service>
</services>        