<?xml version="1.0" encoding="UTF-8"?>
<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
    <!-- ========================================================= -->
    <!-- ======================== Defaults ======================= -->
    <!-- ========================================================= -->
    <title>Entity of sf Component</title>
    <description>None</description>
    <copyright></copyright>
    <version></version>
    <entity entity-name="SellerFriend"
            package-name="com.olbius.sf"
            title="Seller Friend Entity">
      <field name="partyId" type="id-ne"></field>
      <field name="salutation" type="name"></field>
      <field name="firstName" type="name"></field>
      <field name="middleName" type="name"></field>
      <field name="lastName" type="name"></field>
      <field name="facebookId" type="name"></field>
      <field name="gender" type="indicator"></field>
      <field name="birthDate" type="date"></field>
      <prim-key field="partyId"/>
      <relation type="one" fk-name="SF_PARTY" rel-entity-name="Party">
        <key-map field-name="partyId"/>
      </relation>
    </entity>
    <entity entity-name="ProductSellerFriend"
            package-name="com.olbius.sf"
            title="Product Seller Friend Entity">
      <field name="productId" type="id-ne"></field>
      <field name="partyId" type="id-ne"></field>
      <field name="productPartyLinkTypeId" type="id-ne"></field>
      <field name="fromDate" type="date-time"></field>
      <field name="thruDate" type="date-time"></field>
      <prim-key field="productId"/>
      <prim-key field="partyId"/>
      <prim-key field="fromDate"/>
      <relation type="one" fk-name="PROD_SF_PROD" rel-entity-name="Product">
        <key-map field-name="productId"/>
      </relation>
      <relation type="one" fk-name="PROD_SF_SF" rel-entity-name="SellerFriend">
        <key-map field-name="partyId"/>
      </relation>
    </entity>
</entitymodel>