<?xml version="1.0" encoding="UTF-8"?>
<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
	<extend-entity entity-name="RequirementItem" >
		<field name="reason" type="long-varchar"></field>
		<field name="estimatedPrice" type="currency-amount"></field>
		<field name="estimatedReceiveDate" type="date-time"></field>
	</extend-entity>
	<view-entity entity-name="RequirementItemAndProduct" package-name="org.ofbiz.delys" 
		title="RequirementItem And Product">
		<member-entity entity-alias="RT" entity-name="RequirementItem"/>
		<member-entity entity-alias="PR" entity-name="Product"/>
		<alias-all entity-alias="RT"></alias-all>
		<alias-all entity-alias="PR"></alias-all>
		<view-link rel-entity-alias="PR" entity-alias="RT">
			<key-map field-name="productId" rel-field-name="productId"/>
		</view-link>
	</view-entity>
	<view-entity entity-name="PartyGroupAndPartyRoleStatus" package-name="com.olbius" 
		title="Party group  party role has status">
		<member-entity entity-alias="PT" entity-name="Party"/>
		<member-entity entity-alias="PG" entity-name="PartyGroup"/>
		<member-entity entity-alias="PR" entity-name="PartyRole"/>
		<alias-all entity-alias="PT"/>
		<alias-all entity-alias="PG"/>
		<alias-all entity-alias="PR"/>
		<view-link rel-entity-alias="PG" entity-alias="PT">
			<key-map field-name="partyId"/>
		</view-link>
		<view-link rel-entity-alias="PR" entity-alias="PT">
			<key-map field-name="partyId"/>
		</view-link>
	</view-entity>
	<entity entity-name="AgreementReview" package-name="com.olbius">
	    <field name="agreementId" type="id-ne"/>
	    <field name="reviewId" type="id-ne"/>
	    <field name="partyId" type="id-ne"/>
	    <field name="content" type="very-long"/>
	    <prim-key field="agreementId"/>
	    <prim-key field="reviewId"/>
	    <relation rel-entity-name="Agreement" type="many" fk-name="AGREVIEW">
	        <key-map field-name="agreementId"/>
	    </relation>
	    <relation rel-entity-name="PartyGroup" type="many" fk-name="AGRPARTY">
	        <key-map field-name="partyId"/>
	    </relation>
	</entity>
