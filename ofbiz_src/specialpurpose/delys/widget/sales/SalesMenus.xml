    		</link>
    	</menu-item>
    	<menu-item name="approve" title="${uiLabelMap.DAActionApprove}" widget-style="icon-ok open-sans">
    		<condition>
    			<and>
    				<if-has-permission permission="SALESMGR_INOUT" action="_CREATE"></if-has-permission>
    				<if-compare field="parameters.statusId" operator="equals" value="SALES_SM_CREATED"></if-compare>
    			</and>
    		</condition>
    		<link url-mode="plain" target="javascript:approveTotalStatement();"/>
    	</menu-item>
    	<menu-item name="cancel" title="${uiLabelMap.DACancelStatus}" widget-style="icon-remove open-sans">
    		<condition>
    			<and>
    				<if-has-permission permission="SALESMGR_INOUT" action="_CREATE"></if-has-permission>
    				<if-compare field="parameters.statusId" operator="equals" value="SALES_SM_CREATED"></if-compare>
    			</and>
    		</condition>
    		<link url-mode="plain" target="javascript:cancelTotalStatement();"/>
    	</menu-item>
    	<menu-item name="runStatement" title="${uiLabelMap.DARun}" widget-style="fa fa-bolt open-sans">
    		<condition>
    			<and>
    				<if-has-permission permission="SALESMGR_INOUT" action="_CREATE"></if-has-permission>
    				<if-compare field="parameters.statusId" operator="equals" value="SALES_SM_ACCEPTED"></if-compare>
    			</and>
    		</condition>
    		<link target="runSalesStatementReportGroup">
    			<parameter param-name="salesTypeId" from-field="parameters.salesTypeId"/>
    			<parameter param-name="customTimePeriodId" from-field="parameters.customTimePeriodId"/>
    			<parameter param-name="statusId" from-field="SALES_SM_ACCEPTED"/>
    		</link>
    	</menu-item>
    	<menu-item name="newSalesStatement" title="${uiLabelMap.DACreateNew}" widget-style="icon-plus open-sans">
    		<link target="newSalesStatement"></link>
    	</menu-item>
    </menu>
    <menu name="SalesPartyRoleTabBar" modelMenu="olbiustab" extends="CommonTabBarMenu" extends-resource="component://common/widget/CommonMenus.xml">
    	<menu-item name="listSalesman" title="${uiLabelMap.DAlistSalesmanOfSup}" >
    		<link target="listSalesman"></link>
    	</menu-item>
    	<menu-item name="salesmanDetails" title="${uiLabelMap.DASalesmanDetails}">
    		<link target="salesmanDetails"></link>
    	</menu-item>
    </menu>
</menus>