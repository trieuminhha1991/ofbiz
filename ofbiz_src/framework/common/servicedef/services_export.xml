<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../service/dtd/services.xsd">
	<description>Export excel Services</description>
	<vendor></vendor>
	<version>1.0</version>

	<service name="exportExcelBigData" engine="java" invoke="exportExcelBigData" location="com.olbius.common.export.ExportExcelServices" 
		max-retry="0" transaction-timeout="43200">
		<attribute name="eventPath" mode="IN" type="String"/>
		<attribute name="isCheckTimeout" mode="IN" type="Boolean" default-value="true"/>
		<attribute name="parameters" mode="IN" type="java.util.Map"/>
		<attribute name="listAllConditions" mode="IN" type="java.util.List" optional="true"/>
		<attribute name="listSortFields" mode="IN" type="java.util.List" optional="true"/>
		<attribute name="exportName" mode="IN" type="String" optional="true"/>
		<attribute name="exportTime" mode="IN" type="Timestamp" optional="true"/>
		<attribute name="fileName" mode="OUT" type="String" optional="true"/>
		<attribute name="resultCode" mode="OUT" type="String"/>
		<attribute name="workBook" mode="OUT" type="org.apache.poi.xssf.streaming.SXSSFWorkbook" optional="true"/>
   	</service>
	
	<service name="JQGetListFileExported" engine="java" invoke="jqGetListFileExported" location="com.olbius.common.export.ExportExcelServices">
		<implements service="jqxGetListInterface"/>
   	</service>
   	<service name="deleteFileExported" default-entity-name="FileExportedTemp" engine="java" invoke="deleteFileExported" 
   		location="com.olbius.common.export.ExportExcelServices" auth="true">
        <description>Delete FileExportedTemp</description>
        <auto-attributes include="pk" mode="IN" optional="false"/>
    </service>
    <service name="changeStatusFileExported" default-entity-name="FileExportedTemp" engine="java" invoke="changeStatusFileExported" 
   		location="com.olbius.common.export.ExportExcelServices" auth="true">
        <description>Change status FileExportedTemp</description>
        <auto-attributes include="pk" mode="IN" optional="false"/>
        <attribute name="statusId" mode="IN" type="String" optional="true"/>
    </service>
</services>