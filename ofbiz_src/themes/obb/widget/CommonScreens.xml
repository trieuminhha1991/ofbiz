	<screen name="content-view">
		<section>
			<actions>
				<script
					location="component://baseecommerce/webapp/baseecommerce/WEB-INF/actions/catalog/ProductCategories.groovy" />
				<entity-one value-field="checkContent" entity-name="Content">
					<field-map field-name="contentId" from-field="parameters.cId" />
				</entity-one>
			</actions>
			<widgets>
				<decorator-screen name="main-detail"
					location="${parameters.mainDecoratorLocation}">
					<decorator-section name="body">
						<section>
							<condition>
								<if-empty field="checkContent" />
							</condition>
							<widgets>
								<container style='container notfound'>
									<label>${uiLabelMap.BEContentNotFound}</label>
								</container>
							</widgets>
							<fail-widgets>
								<container style='container'>
									<include-screen name="viewcontent"
										location="component://baseecommerce/widget/ContentScreens.xml" />
								</container>
							</fail-widgets>
						</section>

					</decorator-section>
				</decorator-screen>
			</widgets>
		</section>
	</screen>

