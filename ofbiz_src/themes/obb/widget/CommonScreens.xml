<?xml version="1.0" encoding="UTF-8"?>

<screens xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/widget-screen.xsd">

	<screen name="root-decorator">
		<section>
			<actions>
				<!-- base/top/specific map first, then more common map added for shared
					labels -->
				<property-map resource="EcommerceUiLabels" map-name="uiLabelMap"
					global="true" />
				<property-map resource="BaseEcommerceUiLabels" map-name="uiLabelMap"
					global="true" />
				<property-map resource="EcommerceBackendUiLabels" map-name="uiLabelMap"
					global="true" />
				<property-map resource="ObbUiLabels" map-name="uiLabelMap"
					global="true" />
				<property-map resource="ContentUiLabels" map-name="uiLabelMap"
					global="true" />
				<property-map resource="CommonUiLabels" map-name="uiLabelMap"
					global="true" />
				<property-map resource="ProductUiLabels" map-name="uiLabelMap"
					global="true" />
				<property-map resource="AccountingUiLabels" map-name="uiLabelMap"
					global="true" />
				<property-map resource="PartyUiLabels" map-name="uiLabelMap"
					global="true" />
				<property-map resource="OrderUiLabels" map-name="uiLabelMap"
					global="true" />

				<set field="MainColumnStyle" value="nocolumns" global="true" />

				<!-- The default (global) shortcut icon -->
				<set field="layoutSettings.shortcutIcon" value="/images/favicon.ico"
					global="true" />

				<set field="initialLocaleComplete" type="String"
					value="${groovy:parameters?.userLogin?.lastLocale}" default-value="${groovy:locale.toString()}" />

				<!-- obb css -->
				<set field="layoutSettings.styleSheets[+0]" value="/obbresources/css/bootstrap.min.css" />
				<set field="layoutSettings.styleSheets[+1]" value="/obbresources/css/bootstrap-theme.min.css" />
				<set field="layoutSettings.styleSheets[+2]" value="/obbresources/css/facebook.css" />
				<set field="layoutSettings.styleSheets[+3]" value="/obbresources/css/font-awesome.min.css" />
				<set field="layoutSettings.styleSheets[+1]" value="/obbresources/asset/css/fontawesome-stars.css" />
				<set field="thumb" from-field="thumb" global="true" default-value=""/>
				<!-- Get the store VisualTheme -->
<!-- 				<set field="visualThemeId" from-field="productStore.visualThemeId" -->
<!-- 					default-value="EC_DEFAULT" /> -->
<!-- 				<service service-name="getVisualThemeResources"> -->
<!-- 					<field-map field-name="visualThemeId" /> -->
<!-- 					<field-map field-name="themeResources" from-field="layoutSettings" /> -->
<!-- 				</service> -->
				<script
					location="component://baseecommerce/webapp/baseecommerce/WEB-INF/actions/EcommerceSetup.groovy" />
				<script
					location="component://baseecommerce/webapp/baseecommerce/WEB-INF/actions/Main.groovy" />
				<set field="device" from-field="device" global="true"/>
				<set field="layoutSettings" from-field="themeResources"
					default-value="${layoutSettings}" global="true" />
				<set field="footerTemplateLocation" from-field="layoutSettings.VT_FTR_TMPLT_LOC[1]"
					default-value="component://obb/webapp/obb/includes/footer.ftl" />
				<entity-and list="webAnalyticsConfigs" entity-name="WebAnalyticsConfig">
					<field-map field-name="webSiteId" />
				</entity-and>
				<set field="layoutSettings.javaScripts[+0]" value="/obbresources/js/jquery-1.11.3.js"
					global="true" />
				<set field="layoutSettings.javaScripts[+0]" value="/obbresources/js/common/CommonUtils.js" global="true"/>
				<set field="layoutSettings.javaScripts[]" value="/obbresources/js/lib/bootstrap.min.js"
					global="true" />
				<set field="layoutSettings.javaScripts[]" value="/obbresources/js/jquery-ui-1.11.3.min.js"
					global="true" />
				<!-- <set field="layoutSettings.javaScripts[+1]" value="/obbresources/js/prototype/prototype.js"
					global="true"/> -->
				<set field="layoutSettings.javaScripts[+2]" value="/images/selectall.js"
					global="true" />
				<!-- <set field="layoutSettings.javaScripts[]" -->
				<!-- value="/images/jquery/plugins/datetimepicker/jquery-ui-timepicker-addon-1.0.5.js" -->
				<!-- global="true" /> -->
				<!-- <set field="layoutSettings.javaScripts[]" -->
				<!-- value="${groovy: org.ofbiz.common.JsLanguageFilesMapping.dateTime.getFilePath(initialLocaleComplete)}" -->
				<!-- global="true" /> -->

				<!-- <set field="layoutSettings.javaScripts[]" -->
				<!-- value="/images/jquery/ui/development-bundle/ui/jquery.ui.datepicker.js" -->
				<!-- global="true" /> -->
				<set field="layoutSettings.javaScripts[]"
					value="/images/jquery/plugins/validate/jquery.validate.min.js"
					global="true" />
				<set field="layoutSettings.javaScripts[]" value="/images/miscAjaxFunctions.js"
					global="true" />
				<set field="headerTemplateLocation" from-field="layoutSettings.VT_HDR_TMPLT_LOC[1]"
					default-value="component://obb/webapp/obb/includes/header.ftl"
					global="true" />
				<script
					location="component://baseecommerce/webapp/baseecommerce/WEB-INF/actions/customer/ContactList.groovy" />
			</actions>
			<widgets>
				<section>
					<condition>
						<not>
							<if-empty field="globalContext.productStore" />
						</not>
					</condition>
					<widgets>
						<platform-specific>
							<html>
								<html-template
									location="component://obb/webapp/obb/includes/headerHead.ftl" />
							</html>
						</platform-specific>
						<platform-specific>
							<html>
								<html-template
									location="component://obb/webapp/obb/includes/openbody.ftl" />
							</html>
						</platform-specific>
						<platform-specific>
							<html>
								<html-template location="component://obb/webapp/obb/includes/common/setLocation.ftl" />
							</html>
						</platform-specific>
						<include-screen name="facebookinit" />
						<container id="jm-wrapper">
							<container id="jm-header" style="wrap clearfix">
								<container style="main">
									<container style="inner clearfix">
										<decorator-section-include name="jmheader" />
									</container>
								</container>
							</container>
							<container id="jm-head" style="wrap clearfix">
								<platform-specific>
									<html>
										<html-template
											location="component://obb/webapp/obb/includes/headbar.ftl" />
									</html>
								</platform-specific>
							</container>
							<container id="off-canvas-nav">
								<container style="jm-mainnav">
									<platform-specific>
										<html>
											<html-template
												location="component://obb/webapp/obb/includes/common/categorymobile.ftl" />
										</html>
									</platform-specific>
								</container>
								<container id="off-canvas-container"></container>
							</container>
							<decorator-section-include name="menutop"></decorator-section-include>
							<!-- render messages -->
							<platform-specific>
								<html>
									<html-template
										location="component://obb/webapp/obb/includes/messages.ftl" />
								</html>
							</platform-specific>
							<!-- by default will render left-bar only if left/rightbarScreenName
								value not empty -->
							<decorator-section-include name="topspotlight" />
							<decorator-section-include name="body" />
						</container>
						<!-- render footer -->
						<container id="footer">
							<platform-specific>
								<html>
									<html-template location="${footerTemplateLocation}" />
								</html>
							</platform-specific>
						</container>
						<platform-specific>
							<html>
								<html-template
									location="component://common/webcommon/includes/closehtmlbody.ftl" />
							</html>
						</platform-specific>
						<include-screen name="zopim" />
					</widgets>
					<fail-widgets>
						<label
							text="A Product Store has not been defined for this ecommerce site.  A Product Store can be created using the ofbizsetup wizard." />
					</fail-widgets>
				</section>
			</widgets>
		</section>
	</screen>
	<screen name="main-decorator">
		<section>
			<actions>
				<set field="partyId" from-field="userLogin.partyId" />
				<set field="layoutSettings.styleSheets[+0]" value="/obbresources/css/flexslider.css" />
				<set field="layoutSettings.styleSheets[+1]" value="/obbresources/asset/css/fontawesome-stars.css" />
				<set field="layoutSettings.styleSheets[+1]" value="/obbresources/asset/css/bootstrap-editable.css" />
				<set field="layoutSettings.javaScripts[+0]" value="/obbresources/js/jquery.flexslider-min.js" />
				<set field="layoutSettings.javaScripts[+1]" value="/obbresources/js/common/mainslider.js" />
				<set field="layoutSettings.javaScripts[+2]" value="/obbresources/js/jquery.menu-aim.js" />
				<set field="layoutSettings.javaScripts[+2]" value="/obbresources/js/common/home.js" />
			</actions>
			<widgets>
				<decorator-screen name="root-decorator"
					location="${parameters.mainDecoratorLocation}">
					<decorator-section name="jmheader">
						<platform-specific>
							<html>
								<html-template location="${headerTemplateLocation}" />
							</html>
						</platform-specific>
						<platform-specific>
							<html>
								<html-template location="component://obb/webapp/obb/includes/menutop.ftl" />
							</html>
						</platform-specific>
					</decorator-section>
					<decorator-section name="topspotlight">
						<container style="container padding-bottom-15 top-content">
							<container style="inner clearfix">
								<container style="row">
									<container
										style='col-lg-3 col-md-3 hidden-sm hidden-xs product-category-container'>
										<include-screen name="leftcategory"/>
									</container>
									<container
										style='col-lg-6 col-md-6 no-paddingleft no-paddingright slidermobile'>
										<include-screen name="mainslider" />
										<platform-specific>
											<html>
												<html-template
													location="component://obb/webapp/obb/includes/common/bannermiddle.ftl" />
											</html>
										</platform-specific>
									</container>
									<container style='col-lg-3 col-md-3 hidden-sm hidden-xs'>
										<platform-specific>
											<html>
												<html-template
													location="component://obb/webapp/obb/includes/common/bannerhome.ftl" />
											</html>
										</platform-specific>
									</container>
								</container>
							</container>
						</container>
					</decorator-section>
					<decorator-section name="body">
						<decorator-section-include name="body" />
					</decorator-section>
				</decorator-screen>
			</widgets>
		</section>
	</screen>
