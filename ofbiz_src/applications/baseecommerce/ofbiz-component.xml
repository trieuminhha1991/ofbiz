<?xml version="1.0" encoding="UTF-8"?>
<ofbiz-component name="baseecommerce" enabled="true"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/ofbiz-component.xsd">
	<resource-loader name="main" type="component" />
	<classpath type="jar" location="build/lib/*" />
	<classpath type="dir" location="config" />
	<entity-resource type="model" reader-name="main" loader="main" location="entitydef/entitymodel.xml" />
	<entity-resource type="model" reader-name="main" loader="main" location="entitydef/entitymodel_view.xml" />

	<!-- <entity-resource type="eca" reader-name="main" loader="main" location="entitydef/eecas.xml" /> -->

	<entity-resource type="data" reader-name="coreseed" loader="main" location="data/EcommerceTypeData.xml" />
	<entity-resource type="data" reader-name="coreseed" loader="main" location="data/EcommerceSecurityData.xml" />
	<entity-resource type="data" reader-name="coreseed" loader="main" location="data/EcommerceSeedData.xml" />
	<entity-resource type="data" reader-name="coredemo" loader="main" location="data/WebSiteConfigDemoData.xml" />
	<!-- <entity-resource type="data" reader-name="coreseed" loader="main" location="data/WebsiteConfigData.xml"/> -->
	<!-- <entity-resource type="data" reader-name="coredemo" loader="main" location="data/ContentDemoData.xml"/> -->

	<!-- product & category demo data for medicine -->
	<!-- <entity-resource type="data" reader-name="coredemo" loader="main" location="data/medicine/ProdCatalogDemoData.xml"/> -->
	<!-- <entity-resource type="data" reader-name="coredemo" loader="main" location="data/medicine/ProductCategoryDemoData.xml"/> -->
	<!-- <entity-resource type="data" reader-name="coredemo" loader="main" location="data/medicine/ProdCatalogCategoryDemoData.xml"/> -->
	<!-- <entity-resource type="data" reader-name="coredemo" loader="main" location="data/medicine/ProductCategoryRollupDemoData.xml"/> -->
	<!-- <entity-resource type="data" reader-name="coredemo" loader="main" location="data/medicine/ProductDemoData.xml"/> -->
	<!-- <entity-resource type="data" reader-name="coredemo" loader="main" location="data/medicine/ProductCategoryMemberDemoData.xml"/> -->

	<service-resource type="model" loader="main" location="servicedef/services.xml" />
	<service-resource type="model" loader="main" location="servicedef/services_content.xml" />
	<service-resource type="model" loader="main" location="servicedef/services_configwebsite.xml" />
	<service-resource type="model" loader="main" location="servicedef/services_configproduct.xml" />
	<service-resource type="model" loader="main" location="servicedef/services_topic.xml" />
	<service-resource type="model" loader="main" location="servicedef/services_faq.xml" />
	
	<webapp name="baseecommerce"
		title="BaseEcommerce ecommerce"
		server="default-server"
		location="webapp/baseecommerce"
		base-permission="OFBTOOLS,BASEECOMMERCE"
		mount-point="/baseecommerce"
		app-bar-display="false" />
		
	<webapp name="baseecommerceresources"
		title="BaseEcommerce ecommerce"
		server="default-server"
		location="webapp/baseecommerce/resources"
		base-permission="BASEECOMMERCE"
		mount-point="/ecommerceresources"
		app-bar-display="false" />

	<!-- <container name="baseecommerce-cache-container" class="com.olbius.product.loader.CacheContainer">
		<property name="dispatcher-name" value="default" />
		<property name="delegator-name" value="default" />
		<property name="userLoginId" value="system" />
		<property name="defaultService" value="synchronizeListProduct" />
	</container> -->

</ofbiz-component>
