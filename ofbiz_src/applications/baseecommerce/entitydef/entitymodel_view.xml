		<member-entity entity-alias="PT" entity-name="Party" />
		<alias-all entity-alias="PRO" />
		<alias name="groupName" entity-alias="PTG" />
		<view-link rel-entity-alias="PTG" entity-alias="PRO">
			<key-map field-name="brandName" rel-field-name="partyId" />
		</view-link>
		<view-link rel-entity-alias="PT" entity-alias="PTG">
			<key-map field-name="partyId" />
			<entity-condition>
				<condition-expr field-name="statusId" entity-alias="PT" value="PARTY_DISABLED" operator="not-equals" />
			</entity-condition>
		</view-link>
	</view-entity>
	
