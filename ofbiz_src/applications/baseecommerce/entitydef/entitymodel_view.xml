		<alias name="fromDate" entity-alias="PCM" />
		<alias name="thruDate" entity-alias="PCM" />
		<alias name="availableFromDate" entity-alias="SP" />
		<alias name="availableThruDate" entity-alias="SP" />
		<alias name="partyId" entity-alias="SP" />
		<view-link rel-entity-alias="SP" entity-alias="PCM">
			<key-map field-name="productId" />
		</view-link>
		<view-link rel-entity-alias="PRD" entity-alias="PCM">
			<key-map field-name="productId" />
		</view-link>
	</view-entity>
	
	<view-entity entity-name="ProductBrand" package-name="com.olbius.ecommerce">
		<member-entity entity-alias="PRO" entity-name="Product" />
