<?xml version="1.0" encoding="UTF-8"?>
<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
	<title>Extend Entity for BaseEcommerce Component</title>
	<description>None</description>
	<version>1.0</version>
	
	<view-entity entity-name="StoreCatalogCategory" package-name="org.ofbiz.baseecommerce">
		<member-entity entity-alias="PSC" entity-name="ProductStoreCatalog"></member-entity>
		<member-entity entity-alias="PC" entity-name="ProdCatalog"></member-entity>
		<alias name="description" entity-alias="PC"></alias>
		<alias name="catalogName" entity-alias="PC"></alias>
		<alias name="headerLogo" entity-alias="PC"></alias>
		<alias name="productStoreId" entity-alias="PSC"></alias>
		<alias name="prodCatalogId" entity-alias="PSC"></alias>
		<alias name="sequenceNum" entity-alias="PSC"></alias>
		<alias name="fromDate" entity-alias="PSC"></alias>
		<alias name="thruDate" entity-alias="PSC"></alias>
		<view-link rel-entity-alias="PSC" entity-alias="PC">
			<key-map field-name="prodCatalogId" />
		</view-link>
	</view-entity>

	<view-entity entity-name="PersonAndPartyGroup" package-name="org.ofbiz.baseecommerce">
		<member-entity entity-alias="PT" entity-name="Party" />
		<member-entity entity-alias="PS" entity-name="Person" />
		<member-entity entity-alias="PG" entity-name="PartyGroup" />
		<alias-all entity-alias="PS"></alias-all>
		<alias-all entity-alias="PG"></alias-all>
		<alias name="partyFullName">
			<complex-alias operator="||' '||">
				<complex-alias-field entity-alias="PS" field="lastName" default-value="''" />
				<complex-alias-field entity-alias="PS" field="middleName" default-value="''" />
				<complex-alias-field entity-alias="PS" field="firstName" default-value="''" />
				<complex-alias-field entity-alias="PG" field="groupName" default-value="''" />
			</complex-alias>
		</alias>
		<alias name="partyCode" entity-alias="PT"></alias>
		<alias name="description" entity-alias="PT"></alias>
		<view-link rel-entity-alias="PS" entity-alias="PT" rel-optional="true">
			<key-map field-name="partyId" />
		</view-link>
		<view-link rel-entity-alias="PG" entity-alias="PT" rel-optional="true">
			<key-map field-name="partyId" />
		</view-link>
	</view-entity>

	<view-entity entity-name="ProductReviewDetail" package-name="org.ofbiz.baseecommerce">
		<member-entity entity-alias="PRR" entity-name="ProductReview"></member-entity>
		<member-entity entity-alias="PR" entity-name="Product"></member-entity>
		<alias-all entity-alias="PRR"></alias-all>
		<alias name="productName" entity-alias="PR"></alias>
		<view-link rel-entity-alias="PR" entity-alias="PRR">
			<key-map field-name="productId" />
		</view-link>
	</view-entity>

	<view-entity entity-name="ImageAttribute" package-name="com.olbius.ecommerce">
		<member-entity entity-alias="CTAN" entity-name="ContentAttribute" />
		<member-entity entity-alias="CTAW" entity-name="ContentAttribute" />
		<member-entity entity-alias="CTAH" entity-name="ContentAttribute" />
		<alias entity-alias="CTAN" name="contentId" />
		<alias entity-alias="CTAN" name="size" field="attrValue" />
		<alias entity-alias="CTAW" name="width" field="attrValue" />
		<alias entity-alias="CTAH" name="height" field="attrValue" />
		<view-link rel-entity-alias="CTAW" entity-alias="CTAN">
			<key-map field-name="contentId" />
			<entity-condition>
				<condition-list>
					<condition-expr field-name="attrName" entity-alias="CTAN" value="THUMBNAIL_SIZE" />
					<condition-expr field-name="attrName" entity-alias="CTAW" value="WIDTH" />
				</condition-list>
			</entity-condition>
		</view-link>
		<view-link rel-entity-alias="CTAH" entity-alias="CTAN">
			<key-map field-name="contentId" />
			<entity-condition>
				<condition-list>
					<condition-expr field-name="attrName" entity-alias="CTAN" value="THUMBNAIL_SIZE" />
					<condition-expr field-name="attrName" entity-alias="CTAH" value="HEIGHT" />
				</condition-list>
			</entity-condition>
		</view-link>
	</view-entity>
	
	<view-entity entity-name="ContentImageView" package-name="com.olbius.ecommerce">
		<member-entity entity-alias="CA" entity-name="ContentAssoc" />
		<member-entity entity-alias="CT" entity-name="Content" />
		<member-entity entity-alias="IMG" entity-name="Content" />
		<member-entity entity-alias="CTA" entity-name="ImageAttribute" />
		<alias entity-alias="CT" name="contentId" />
		<alias entity-alias="CT" name="contentName" />
		<alias entity-alias="CT" name="description" />
		<alias entity-alias="CT" name="createdDate" />
		<alias entity-alias="CT" name="contentTypeId" />
		<alias entity-alias="IMG" name="imgContentId" field="contentId" />
		<alias entity-alias="IMG" name="imgSrc" field="contentName" />
		<alias entity-alias="IMG" name="imgCaption" field="description" />
		<alias entity-alias="IMG" name="imgCreatedDate" field="createdDate" />
		<alias entity-alias="CTA" name="size" />
		<alias entity-alias="CTA" name="width" />
		<alias entity-alias="CTA" name="height" />
		<view-link rel-entity-alias="CT" entity-alias="CA">
			<key-map field-name="contentId" />
		</view-link>
		<view-link rel-entity-alias="IMG" entity-alias="CA">
			<key-map field-name="contentIdTo" rel-field-name="contentId" />
			<entity-condition>
				<condition-expr field-name="contentTypeId" entity-alias="IMG" value="IMAGE_FRAME" />
			</entity-condition>
		</view-link>
		<view-link rel-entity-alias="CTA" entity-alias="IMG">
			<key-map field-name="contentId" />
		</view-link>
	</view-entity>

	<view-entity entity-name="ProductAndContent" package-name="com.olbius.ecommerce">
		<member-entity entity-alias="PR" entity-name="Product" />
		<member-entity entity-alias="PRC" entity-name="ProductContent" />
		<member-entity entity-alias="C" entity-name="TopicAndComment" />
		<alias-all entity-alias="C"></alias-all>
		<alias name="productId" entity-alias="PR"></alias>
		<alias name="productName" entity-alias="PR"></alias>
		<alias name="productCode" entity-alias="PR"></alias>
		<alias name="productContentTypeId" entity-alias="PRC"></alias>
		<alias name="fromDate" entity-alias="PRC"></alias>
		<alias name="thruDate" entity-alias="PRC"></alias>
		<view-link rel-entity-alias="PR" entity-alias="PRC">
			<key-map field-name="productId" />
		</view-link>
		<view-link rel-entity-alias="C" entity-alias="PRC">
			<key-map field-name="contentId" />
		</view-link>
	</view-entity>

	<view-entity entity-name="TopicAndComment" package-name="com.olbius.ecommerce">
		<member-entity entity-alias="CONT" entity-name="WebSiteContentDetail"></member-entity>
		<member-entity entity-alias="CASS" entity-name="ContentAssoc"></member-entity>
		<alias name="webSiteId" entity-alias="CONT" group-by="true"></alias>
		<alias name="contentId" entity-alias="CONT" group-by="true"></alias>
		<alias name="contentTypeId" entity-alias="CONT" group-by="true"></alias>
		<alias name="dataResourceId" entity-alias="CONT" group-by="true"></alias>
		<alias name="dataSourceId" entity-alias="CONT" group-by="true"></alias>
		<alias name="statusId" entity-alias="CONT" group-by="true"></alias>
		<alias name="contentName" entity-alias="CONT" group-by="true"></alias>
		<alias name="description" entity-alias="CONT" group-by="true"></alias>
		<alias name="longDescription" entity-alias="CONT" group-by="true"></alias>
		<alias name="lastUpdatedStamp" entity-alias="CONT" group-by="true"></alias>
		<alias name="createdStamp" entity-alias="CONT" group-by="true"></alias>
		<alias name="infoString" entity-alias="CONT" group-by="true"></alias>
		<alias name="author" entity-alias="CONT" group-by="true"></alias>
		<alias name="detailImageUrl" entity-alias="CONT" group-by="true"></alias>
		<alias name="smallImageUrl" entity-alias="CONT" group-by="true"></alias>
		<alias name="mediumImageUrl" entity-alias="CONT" group-by="true"></alias>
		<alias name="largeImageUrl" entity-alias="CONT" group-by="true"></alias>
		<alias name="originalImageUrl" entity-alias="CONT" group-by="true"></alias>
		<alias name="numberOfComments" function="count" entity-alias="CASS" field="contentId"></alias>
		<view-link entity-alias="CONT" rel-entity-alias="CASS" rel-optional="true">
			<key-map field-name="contentId" rel-field-name="contentIdTo" />
		</view-link>
	</view-entity>

	<view-entity entity-name="ContentAndContentType" package-name="com.olbius.ecommerce">
		<member-entity entity-alias="CTYP" entity-name="ContentType"></member-entity>
		<member-entity entity-alias="TAC" entity-name="TopicAndComment"></member-entity>
		<alias-all entity-alias="TAC"></alias-all>
		<alias name="topicName" field="description" entity-alias="CTYP"></alias>
		<alias name="parentTypeId" entity-alias="CTYP"></alias>
		<view-link entity-alias="CTYP" rel-entity-alias="TAC">
			<key-map field-name="contentTypeId" />
		</view-link>
	</view-entity>

	<view-entity entity-name="FAQCategory" package-name="com.olbius.ecommerce">
		<member-entity entity-alias="CTYP" entity-name="ContentType"></member-entity>
		<member-entity entity-alias="CONT" entity-name="Content"></member-entity>
		<alias name="contentTypeId" entity-alias="CTYP" group-by="true"></alias>
		<alias name="topicName" field="description" entity-alias="CTYP" group-by="true"></alias>
		<alias name="parentTypeId" entity-alias="CTYP" group-by="true"></alias>
		<alias name="numberOfAsked" function="count" entity-alias="CONT" field="contentId"></alias>
		<view-link entity-alias="CTYP" rel-entity-alias="CONT" rel-optional="true">
			<key-map field-name="contentTypeId" />
		</view-link>
		<entity-condition>
			<condition-expr field-name="parentTypeId" entity-alias="CTYP" value="FAQ_ROOT" />
		</entity-condition>
	</view-entity>
	
	<view-entity entity-name="ProductCategoryMemberAndPriceFilter" package-name="com.olbius.ecommerce">
		<member-entity entity-alias="PCM" entity-name="ProductCategoryMember" />
		<member-entity entity-alias="PRD" entity-name="Product" />
		<member-entity entity-alias="PRP" entity-name="ProductPrice" />
		<alias-all entity-alias="PCM">
			<exclude field="comments" />
			<exclude field="fromDate" />
			<exclude field="thruDate" />
		</alias-all>
		<alias-all entity-alias="PRD" />
		<alias-all entity-alias="PRP">
			<exclude field="createdDate" />
		</alias-all>
		<alias name="categoryComments" field="comments" entity-alias="PCM" />
		<alias name="categoryFromDate" field="fromDate" entity-alias="PCM" />
		<alias name="categoryThruDate" field="thruDate" entity-alias="PCM" />
		<alias name="priceCreatedDate" field="createdDate" entity-alias="PRP" />
		<view-link rel-entity-alias="PRD" entity-alias="PCM">
			<key-map field-name="productId" />
		</view-link>
		<view-link rel-entity-alias="PRP" entity-alias="PRD" rel-optional="true">
			<key-map field-name="productId" />
			<entity-condition>
				<condition-expr field-name="productPriceTypeId" value="DEFAULT_PRICE" entity-alias="PRP" />
			</entity-condition>
		</view-link>
	</view-entity>
	
	<view-entity entity-name="ProductCategorySupplier" package-name="com.olbius.ecommerce">
