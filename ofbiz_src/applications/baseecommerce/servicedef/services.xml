	</service>
	
	<service name="createUserLoginSecurityGroup" engine="java" auth="false" location="com.olbius.baseecommerce.common.CommonServices"
		invoke="createUserLoginSecurityGroup" default-entity-name="UserLoginSecurityGroup">
		<description>Create a UserLogin</description>
		<auto-attributes mode="IN" include="pk" />
		<auto-attributes mode="IN" optional="true" include="nonpk" />
		<auto-attributes mode="OUT" optional="true" include="pk" />
	</service>

	<service name="createPartyEcommerceTelecomNumber" engine="simple" location="component://baseecommerce/script/com/baseecommerce/ecommerce/customer/PartyContactMechServices.xml"
		invoke="createPartyTelecomNumber" auth="true">
		<description>Create a Telecommunications Number</description>
		<permission-service service-name="partyContactMechPermissionCheck" main-action="CREATE" />
		<auto-attributes entity-name="PartyContactMech" include="all" mode="IN" optional="true" />
		<auto-attributes entity-name="TelecomNumber" include="nonpk" mode="IN" optional="true" />
		<attribute name="contactMechId" type="String" mode="OUT" />
	</service>
	
