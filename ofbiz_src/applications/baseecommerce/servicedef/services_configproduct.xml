<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
	<description>baseecommerce Backend Services</description>
	<vendor></vendor>
	<version>1.0</version>

	<service name="JQGetListProductContent" engine="java" invoke="listProductContent" location="com.olbius.baseecommerce.backend.ConfigProductServices">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="JQListCategoriesOfProduct" engine="java" invoke="listCategoriesOfProduct" location="com.olbius.baseecommerce.backend.ConfigProductServices">
		<implements service="jqxGetListInterface" />
		<override name="listIterator" type="List" optional="true"></override>
	</service>

	<service name="JQGetListProducts" engine="java" invoke="listProducts" location="com.olbius.baseecommerce.backend.ConfigProductServices" auth="true">
		<implements service="jqxGetListInterface" />
		<override name="listIterator" type="List"></override>
	</service>

	<service name="JQGetListProductReviews" engine="java" invoke="listProductReviews" location="com.olbius.baseecommerce.backend.ConfigProductServices"
		auth="true">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="loadProductIntroduction" engine="java" invoke="loadProductIntroduction" location="com.olbius.baseecommerce.backend.ConfigProductServices"
		auth="false">
		<attribute name="productId" mode="IN" type="String" />
		<attribute name="contentId" mode="IN" type="String" optional="true" />
		<attribute name="productIntroduction" mode="OUT" type="Map" optional="true" />
	</service>

	<service name="configCategory" engine="java" invoke="configCategory" location="com.olbius.baseecommerce.backend.ConfigProductServices">
		<attribute name="productId" mode="IN" type="String" />
		<attribute name="productCategoryId" mode="IN" type="String" />
		<attribute name="isBestSell" mode="IN" type="String" optional="true" />
		<attribute name="isPromos" mode="IN" type="String" optional="true" />
		<attribute name="isNew" mode="IN" type="String" optional="true" />
		<attribute name="isFeatured" mode="IN" type="String" optional="true" />
		<attribute name="bestSellFromDate" mode="IN" type="Long" optional="true" />
		<attribute name="promosFromDate" mode="IN" type="Long" optional="true" />
		<attribute name="newFromDate" mode="IN" type="Long" optional="true" />
		<attribute name="featuredFromDate" mode="IN" type="Long" optional="true" />
	</service>

	<service name="fixCategory" engine="java" invoke="fixCategory" location="com.olbius.baseecommerce.backend.ConfigProductServices">
	</service>

	<service name="JQGetSlideOfProduct" engine="java" invoke="slideOfProduct" location="com.olbius.baseecommerce.backend.ConfigProductServices">
		<attribute name="productId" mode="IN" type="String" />
		<attribute name="type" mode="IN" type="String" optional="true" />
		<attribute name="listSlide" mode="OUT" type="List" />
	</service>
	
	<service name="addImageToSlideOfProduct" engine="java" invoke="addImageToSlideOfProduct" location="com.olbius.baseecommerce.backend.ConfigProductServices">
		<attribute name="productId" mode="IN" type="String" />
		<attribute name="description" mode="IN" type="String" optional="true" />
		<attribute name="originalImageUrl" mode="IN" type="String" />
	</service>

	<service name="getCategoriesByProdCatalogId" engine="java" invoke="getCategoriesByProdCatalogId" location="com.olbius.baseecommerce.backend.ConfigProductServices">
		<attribute name="prodCatalogId" mode="IN" type="String" />
		<attribute name="categories" mode="OUT" type="List" />
	</service>

	<service name="getMainCategoriesByPrimaryParentCategoryId" engine="java" invoke="getMainCategoriesByPrimaryParentCategoryId"
		location="com.olbius.baseecommerce.backend.ConfigProductServices">
		<attribute name="productCategoryId" mode="IN" type="String" />
		<attribute name="categories" mode="OUT" type="List" />
	</service>

	<service name="removeProductCategoriesFromProdCatalog" engine="java" invoke="removeProductCategoriesFromProdCatalog" location="com.olbius.baseecommerce.backend.ConfigProductServices"
		default-entity-name="ProdCatalogCategory">
		<auto-attributes mode="IN" optional="true"></auto-attributes>
	</service>

	<service name="loadConfigOfProduct" engine="java" invoke="configOfProduct"
		location="com.olbius.baseecommerce.backend.ConfigProductServices">
		<attribute name="productId" mode="IN" type="String" />
		<attribute name="config" mode="OUT" type="Map" />
	</service>

	<service name="pharmaProduct" engine="interface">
		<attribute name="effects" mode="IN" type="String" optional="true" allow-html="any"></attribute>
		<attribute name="composition" mode="IN" type="String" optional="true" allow-html="any"></attribute>
		<attribute name="shelfLife" mode="IN" type="String" optional="true" allow-html="any"></attribute>
		<attribute name="users" mode="IN" type="String" optional="true" allow-html="any"></attribute>
		<attribute name="instructions" mode="IN" type="String" optional="true" allow-html="any"></attribute>
		<attribute name="license" mode="IN" type="String" optional="true" allow-html="any"></attribute>
		<attribute name="packing" mode="IN" type="String" optional="true" allow-html="any"></attribute>
		<attribute name="contraindications" mode="IN" type="String" optional="true" allow-html="any"></attribute>
		<attribute name="effectsId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="compositionId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="shelfLifeId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="usersId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="instructionsId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="licenseId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="packingId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="contraindicationsId" mode="IN" type="String" optional="true"></attribute>
	</service>

	<service name="additionalImage" engine="interface">
		<attribute name="ADDITIONAL_IMAGE_1" mode="IN" type="String" optional="true" allow-html="any"></attribute>
		<attribute name="ADDITIONAL_IMAGE_2" mode="IN" type="String" optional="true" allow-html="any"></attribute>
		<attribute name="ADDITIONAL_IMAGE_3" mode="IN" type="String" optional="true" allow-html="any"></attribute>
		<attribute name="ADDITIONAL_IMAGE_4" mode="IN" type="String" optional="true" allow-html="any"></attribute>
		<attribute name="ADDITIONAL_IMAGE_1Id" mode="IN" type="String" optional="true"></attribute>
		<attribute name="ADDITIONAL_IMAGE_2Id" mode="IN" type="String" optional="true"></attribute>
		<attribute name="ADDITIONAL_IMAGE_3Id" mode="IN" type="String" optional="true"></attribute>
		<attribute name="ADDITIONAL_IMAGE_4Id" mode="IN" type="String" optional="true"></attribute>
	</service>

	<service name="configProduct" engine="java" invoke="configProduct" location="com.olbius.baseecommerce.backend.ConfigProductServices">
		<implements service="pharmaProduct" />
		<implements service="additionalImage" />
		<attribute name="productId" mode="IN" type="String" />
		<attribute name="smallImageUrl" mode="IN" type="String" optional="true" />
		<attribute name="largeImageUrl" mode="IN" type="String" optional="true" />
	</service>
	<!-- TODOCHANGE deleted -->
	<service name="updateProductAdditionalImage" engine="java" invoke="updateProductAdditionalImage" location="com.olbius.baseecommerce.backend.ConfigProductServices">
		<attribute name="productId" mode="IN" type="String" />
		<attribute name="ADDITIONAL_IMAGE_1Id" mode="IN" type="String" optional="true" />
		<attribute name="ADDITIONAL_IMAGE_1" mode="IN" type="String" optional="true" />
		<attribute name="ADDITIONAL_IMAGE_2Id" mode="IN" type="String" optional="true" />
		<attribute name="ADDITIONAL_IMAGE_2" mode="IN" type="String" optional="true" />
		<attribute name="ADDITIONAL_IMAGE_3Id" mode="IN" type="String" optional="true" />
		<attribute name="ADDITIONAL_IMAGE_3" mode="IN" type="String" optional="true" />
		<attribute name="ADDITIONAL_IMAGE_4Id" mode="IN" type="String" optional="true" />
		<attribute name="ADDITIONAL_IMAGE_4" mode="IN" type="String" optional="true" />
	</service>

	<service name="loadCategoriesOfWebSite" engine="java" invoke="categoriesOfWebSite" location="com.olbius.baseecommerce.backend.ConfigProductServices">
		<attribute name="isEc" mode="IN" type="String"></attribute>
		<attribute name="prodCatalogId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="getAll" mode="IN" type="String" optional="true"></attribute>
		<attribute name="categories" mode="OUT" type="List" />
	</service>

	<service name="createProductCategoryWithWebSite" engine="java" invoke="createProductCategoryWithWebSite" location="com.olbius.baseecommerce.backend.ConfigProductServices">
		<auto-attributes mode="IN" entity-name="ProductCategory" optional="true"></auto-attributes>
	</service>

	<service name="JQGetConfigCategory" engine="java" invoke="getConfigCategory" location="com.olbius.baseecommerce.backend.ConfigProductServices">
		<attribute name="productCategoryId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="listBanners" mode="OUT" type="List" optional="true" />
	</service>
	<service name="addBannerCategory" engine="java" invoke="addBannerCategory" location="com.olbius.baseecommerce.backend.ConfigProductServices">
		<attribute name="productCategoryId" mode="IN" type="String" />
		<attribute name="url" mode="IN" type="String" optional="true" />
		<attribute name="originalImageUrl" mode="IN" type="String" />
	</service>

	<!-- TODOCHANGE deleted -->
	<service name="getProductByProductCategoryIdIncludeChild" engine="java" location="com.olbius.baseecommerce.backend.ConfigProductServices"
		invoke="getProductByProductCategoryIdIncludeChild" auth="true" default-entity-name="ProductCategory">
		<attribute name="productCategoryId" type="String" mode="IN" />
		<attribute name="listProductByProductCategoryId" mode="OUT" type="List" optional="true" />
	</service>

	<service name="configProductCategory" engine="java" invoke="configProductCategory" location="com.olbius.baseecommerce.backend.ConfigProductServices">
		<attribute name="productId" mode="IN" type="String" />
		<attribute name="productCategoryId" mode="IN" type="String" />
		<attribute name="mainCategoryId" mode="IN" type="String" />
		<attribute name="sequenceNum" mode="IN" type="Long" optional="true" />
		<attribute name="isBestSell" mode="IN" type="String" optional="true" />
		<attribute name="isPromos" mode="IN" type="String" optional="true" />
		<attribute name="isNew" mode="IN" type="String" optional="true" />
		<attribute name="isFeatured" mode="IN" type="String" optional="true" />
		<attribute name="bestSellFromDate" mode="IN" type="Long" optional="true" />
		<attribute name="promosFromDate" mode="IN" type="Long" optional="true" />
		<attribute name="newFromDate" mode="IN" type="Long" optional="true" />
		<attribute name="featuredFromDate" mode="IN" type="Long" optional="true" />
		<attribute name="fromDate" mode="IN" type="Long" optional="true" />
	</service>

	<service name="moveProductCategory" engine="java" invoke="moveProductCategory" location="com.olbius.baseecommerce.backend.ConfigProductServices"
		auth="true">
		<auto-attributes mode="IN" entity-name="ProductCategory" optional="true"></auto-attributes>
		<override name="primaryParentCategoryId" optional="false"></override>
	</service>

	<service name="loadCategoryImage" engine="java" invoke="loadCategoryImage" location="com.olbius.baseecommerce.backend.ConfigProductServices" auth="true">
		<attribute name="productCategoryId" mode="IN" type="String" />
		<attribute name="categoryConfig" mode="OUT" type="Map" optional="true" />
	</service>

	<!-- <service name="addRootCategory" default-entity-name="ProductCategory" engine="java" invoke="addRootCategory" location="com.olbius.basepo.product.ProductCategoryServices">
		<description>Create an Root ProductCategory</description>
		<auto-attributes include="pk" mode="INOUT" optional="true" />
		<auto-attributes include="nonpk" mode="IN" optional="true" />
		<override name="productCategoryTypeId" optional="false" />
		<override name="longDescription" allow-html="any"></override>
	</service> -->
	<service name="fixRootCategory" default-entity-name="ProductCategory" engine="java" invoke="fixRootCategory" location="com.olbius.baseecommerce.backend.ConfigProductServices">
	</service>
	<service name="checkProductErasable" engine="java" invoke="checkProductErasable" location="com.olbius.baseecommerce.backend.ConfigProductServices">
		<attribute name="productId" mode="IN" type="String"></attribute>
		<attribute name="productCategoryId" mode="IN" type="String"></attribute>
		<attribute name="erasable" mode="OUT" type="String"></attribute>
	</service>
	<service name="checkProductSaleable" engine="java" invoke="checkProductSaleable" location="com.olbius.baseecommerce.backend.ConfigProductServices">
		<attribute name="productId" mode="IN" type="String"></attribute>
		<attribute name="saleable" mode="OUT" type="Boolean"></attribute>
	</service>
	<service name="removeExuberancyProduct" engine="java" invoke="removeExuberancyProduct" location="com.olbius.baseecommerce.backend.ConfigProductServices">
	</service>
</services>