<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>BaseAccounting Services</description>
    <vendor></vendor>
    <version>1.0</version>

    <service name="noSyntaxError" engine="interface">
        <description>
            Dummy service to prevent empty files and syntax error - Remove when the 1st real service will be added here 
        </description>
    </service> 
    
   <service name="getLiabilityParty" engine="java" invoke="getLiabilityParty" location="com.olbius.acc.liability.LiabilityGeneralUtils">
        <attribute name="organizationPartyId" type="String" mode="IN" optional="true"/>
        <attribute name="partyId" type="String" mode="IN" optional="true"/>		
   		<attribute name="totalPayable" mode="OUT" type="BigDecimal" optional="false"></attribute>
   		<attribute name="totalReceivable" mode="OUT" type="BigDecimal" optional="false"></attribute>
   		<attribute name="totalLiability" mode="OUT" type="BigDecimal" optional="false"></attribute>
   </service>
   
    <service name="JQGetListCustLiabilities" engine="java" invoke="jqGetListCustLiabilities" location="com.olbius.acc.liability.LiabilityJQServices">
		<implements service="jqxGetListInterface"/>						
		<attribute name="TotalRows" type="String" mode="OUT" optional="false"/>														
   		<override name="listIterator" type="List" optional="false"></override>
   </service>
   
    <service name="jqGetLiabilityDetailDistributor" engine="java" invoke="jqGetLiabilityDetailDistributor" location="com.olbius.acc.liability.LiabilityJQServices">
		<implements service="jqxGetListInterface"/>						
		<attribute name="TotalRows" type="String" mode="OUT" optional="false"/>														
   		<override name="listIterator" type="List" optional="false"></override>
   </service>

    <service name="getTotalLiabilityDistributor" engine="java" invoke="getTotalLiabilityDistributor" location="com.olbius.acc.liability.LiabilityGeneralUtils">
        <attribute name="distributorId" type="String" mode="IN" optional="true"/>
        <attribute name="type" type="String" mode="IN" optional="true"/>
        <attribute name="TotalRows" mode="OUT" type="Integer" optional="false"></attribute>
        <attribute name="totalInvoice" mode="OUT" type="BigDecimal" optional="false"></attribute>
        <attribute name="totalAmountToApply" mode="OUT" type="BigDecimal" optional="false"></attribute>
    </service>
</services>