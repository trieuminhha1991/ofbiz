<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>BaseAccounting Services</description>
    <vendor></vendor>
    <version>1.0</version>

    <service name="createPrepaidExp" engine="java" invoke="createPrepaidExp" location="com.olbius.acc.prepaidexp.PrepaidExpServices">
    	<auto-attributes mode="IN" entity-name="PrepaidExp" include="nonpk" optional="true"></auto-attributes>
    	<auto-attributes mode="INOUT" entity-name="PrepaidExp" include="pk"></auto-attributes>
    </service>
    
    <service name="createPrepaidExpAlloc" engine="java" invoke="createPrepaidExpAlloc" location="com.olbius.acc.prepaidexp.PrepaidExpServices">
    	<auto-attributes mode="IN" entity-name="PrepaidExpAlloc" include="nonpk" optional="true"></auto-attributes>
    	<auto-attributes mode="INOUT" entity-name="PrepaidExpAlloc" include="pk"></auto-attributes>
    </service>
    
    <service name="JqxGetListPrepaidExp" engine="java" invoke="getListPrepaidExp" location="com.olbius.acc.prepaidexp.PrepaidExpServices">
   		<implements service="jqxGetListInterface"/>
   	</service>
   	
   	<service name="JqxGetListPEAllocPeriods" engine="java" invoke="getListPEAllocPeriods" location="com.olbius.acc.prepaidexp.PEAllocPeriodServices">
   		<implements service="jqxGetListInterface"/>
   	</service>
   	
   	<service name="JqxGetListAllocPrepaidExp" engine="java" invoke="getListAllocPrepaidExp" location="com.olbius.acc.prepaidexp.PEAllocPeriodServices">
   		<implements service="jqxGetListInterface"/>
   	</service>
   	
   	<service name="JqxGetListAvailabelPEs" engine="java" invoke="getListAvailabelPEs" location="com.olbius.acc.prepaidexp.PEAllocPeriodServices">
   		<implements service="jqxGetListInterface"/>
   		<override name="listIterator" type="List"></override>
   	</service>
   	
   	<service name="getPECostAlloc" engine="java" invoke="getPECostAlloc" location="com.olbius.acc.prepaidexp.PrepaidExpServices">
   		<attribute name="listCostAlloc" mode="OUT" type="List" optional="false"></attribute>
   	</service>
   	
  	<service name="postPEAllocTrans" engine="java" invoke="postPEAllocTrans" location="com.olbius.acc.prepaidexp.PEAllocPeriodServices">
   		<attribute name="customTimePeriodId" mode="IN" type="String" optional="false"></attribute>
   	</service>
</services>