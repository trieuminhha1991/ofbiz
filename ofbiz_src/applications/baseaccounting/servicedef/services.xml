    <service name="getProductAverageCostBaseSimple" engine="simple" 
            location="component://baseaccounting/script/com/olbius/acc/cost/CostServices.xml" invoke="getProductAverageCostBaseSimple" auth="true">
        <description>Get Average cost of a product with input : productId, facilityId, ownerPartyId</description>
        <attribute name="facilityId" type="String" mode="IN" optional="false"/>
        <attribute name="productId" type="String" mode="IN" optional="false"/>
        <attribute name="ownerPartyId" type="String" mode="IN" optional="false"/>
        <attribute name="unitCost" type="BigDecimal" mode="OUT" optional="false"/>
        <attribute name="purCost" type="BigDecimal" mode="OUT" optional="false"/>
        <attribute name="currencyUomId" type="String" mode="OUT" optional="true"/>
    </service>
    
    <service name="getProductAverageCostBaseOlbius" engine="java" invoke="getProductAverageCost" 
            location="com.olbius.acc.cost.CostServices" auth="true">
        <description>Get Average cost of a product with input : productId, facilityId, ownerPartyId</description>
        <attribute name="facilityId" type="String" mode="IN" optional="false"/>
        <attribute name="productId" type="String" mode="IN" optional="false"/>
        <attribute name="ownerPartyId" type="String" mode="IN" optional="false"/>
        <attribute name="unitCost" type="BigDecimal" mode="OUT" optional="false"/>
        <attribute name="purCost" type="BigDecimal" mode="OUT" optional="false"/>
        <attribute name="currencyUomId" type="String" mode="OUT" optional="true"/>
    </service>
    <service name="calcWeightProductAverageCost" engine="java" invoke="calcWeightProductAverageCost"
    	location="com.olbius.acc.cost.CostServices" auth="true">
    	<attribute name="productId" type="String" mode="IN"/>
        <attribute name="facilityId" type="String" mode="IN" optional="true"/>
        <attribute name="ownerPartyId" type="String" mode="IN" optional="true"/>
        <attribute name="totalAmountOnHandTotal" type="BigDecimal" mode="OUT"/>
        <attribute name="totalInventoryCost" type="BigDecimal" mode="OUT" optional="true"/>
        <attribute name="productAverageCost" type="BigDecimal" mode="OUT" optional="true"/>
        <attribute name="productAveragePurCost" type="BigDecimal" mode="OUT" optional="true"/>
        <attribute name="currencyUomId" type="String" mode="OUT" optional="true"/>	
   	</service>	
   	
