<?xml version="1.0" encoding="UTF-8"?>
<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
    <entity entity-name="Voucher" package-name="com.olbius.bacc.invoice">
	<field name="voucherId" type="id-ne"></field>
    	<field name="issuedDate" type="date-time"></field>
    	<field name="voucherForm" type="long-varchar"></field>
    	<field name="voucherSerial" type="long-varchar"></field>
    	<field name="voucherNumber" type="long-varchar"></field>
    	<field name="amount" type="currency-amount"></field>
    	<field name="taxAmount" type="currency-amount"></field>
    	<field name="totalAmountVoucher" type="currency-amount"></field>
    	<field name="dataResourceId" type="id-ne"></field>
    	<field name="description" type="description"></field>
    	<field name="taxProductCategoryId" type="id-ne"></field>
    	<field name="voucherCreatedDate" type="date-time">
    		<description>ngay nhan/ngay xuat hoa don</description>
    	</field>
    	<prim-key field="voucherId"/>
    	<relation rel-entity-name="DataResource" type="one" fk-name="DATA_RESOURCE_VOU">
    		<key-map field-name="dataResourceId"/>
    	</relation>
    </entity>
    <entity entity-name="VoucherInvoice" package-name="com.olbius.bacc.invoice">
    	<field name="invoiceId" type="id-ne"></field>
    	<field name="voucherId" type="id-ne"></field>
    	<prim-key field="invoiceId"/>
    	<prim-key field="voucherId"/>
    	<relation rel-entity-name="Voucher" type="one" fk-name="VOU_INV_VOU">
    		<key-map field-name="voucherId"/>
    	</relation>
    	<relation rel-entity-name="Invoice" type="one" fk-name="VOU_INV_INV">
    		<key-map field-name="invoiceId"/>
    	</relation>
    </entity>
    <entity entity-name="InvoiceTaxInfo" package-name="com.olbius.bacc.invoice">
    	<field name="invoiceId" type="id-ne"></field>
    	<field name="partyName" type="long-varchar"></field>
    	<field name="taxCode" type="long-varchar"></field>
    	<field name="address" type="long-varchar"></field>
    	<field name="countryGeoId" type="id-ne"></field>
    	<field name="stateGeoId" type="id-ne"></field>
    	<field name="phoneNbr" type="long-varchar"></field>
    	<prim-key field="invoiceId"/>
    	<relation rel-entity-name="Invoice" type="one" fk-name="INV_TAX_INV">
    		<key-map field-name="invoiceId"/>
    	</relation>
	<relation rel-entity-name="Geo" type="one" title="country" fk-name="INV_TAX_COUNTRY_GEO">
    		<key-map field-name="countryGeoId" rel-field-name="geoId"/>
    	</relation>
	<relation rel-entity-name="Geo" type="one" title="stateGeo" fk-name="INV_TAX_STATE_GEO">
    		<key-map field-name="stateGeoId" rel-field-name="geoId"/>
    	</relation>
    </entity>
    <entity entity-name="VoucherInvoiceSystemConfig" package-name="com.olbius.bacc.invoice">
    	<field name="systemConfigId" type="id-long"></field>
    	<field name="systemValue" type="long-varchar"></field>
    	<field name="description" type="description"></field>
    	<field name="dataType" type="name"></field>
    	<field name="fromDate" type="date-time"></field>
    	<field name="thruDate" type="date-time"></field>
    	<prim-key field="systemConfigId"/>
    	<prim-key field="fromDate"/>
    </entity>
    <!-- <entity entity-name="DeliveryItemChange" package-name="com.olbius.bacc.invoice">
    	<field name="deliveryId" type="id-ne"></field>
    	<field name="deliveryItemSeqId" type="id-ne"></field>
    	<field name="adjustDeliveredQty" type="fixed-point"></field>
    	<prim-key field="deliveryId"/>
    	<prim-key field="deliveryItemSeqId"/>
    	<relation rel-entity-name="DeliveryItem" type="one" fk-name="DELV_ITEM_CHANGE_DLV_IT">
    		<key-map field-name="deliveryId"/>
    		<key-map field-name="deliveryItemSeqId"/>
    	</relation>
    </entity> -->
    <entity entity-name="DeliveryItemChange" package-name="com.olbius.bacc.invoice">
    	<field name="deliveryItemChangeId" type="id-ne"></field>
    	<field name="deliveryId" type="id-ne"></field>
    	<field name="deliveryItemSeqId" type="id-ne"></field>
    	<field name="invoiceId" type="id-ne"></field>
    	<field name="invoiceItemSeqId" type="id-ne"></field>
    	<field name="shipmentReceiptId" type="id-ne"></field>
    	<field name="createdDate" type="date-time"></field>
    	<field name="modifiedUserLoginId" type="id-ne"></field>
    	<prim-key field="deliveryItemChangeId"/>
    	<relation rel-entity-name="DeliveryItem" type="one" fk-name="DLV_ITEM_CHANGE_DLV_ITEM">
    		<key-map field-name="deliveryId"/>
    		<key-map field-name="deliveryItemSeqId"/>
    	</relation>
    	<relation rel-entity-name="InvoiceItem" type="one" fk-name="DLV_ITEM_CHANGE_INV_ITEM">
    		<key-map field-name="invoiceId"/>
		<key-map field-name="invoiceItemSeqId"/>
    	</relation>
    	<relation rel-entity-name="UserLogin" type="one" fk-name="DLV_ITEM_CHANGE_UL">
    		<key-map field-name="modifiedUserLoginId" rel-field-name="userLoginId"/>
    	</relation>
    </entity>
</entitymodel>    