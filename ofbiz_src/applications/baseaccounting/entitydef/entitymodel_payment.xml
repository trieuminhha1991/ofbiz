<?xml version="1.0" encoding="UTF-8"?>
<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
    <!-- ========================================================= -->
    <!-- ======================== Defaults ======================= -->
    <!-- ========================================================= -->
    <title>Entity of BaseAccounting Component</title>
    <description>None</description>
    <copyright></copyright>
    <version></version>
	<entity entity-name="GlAccountReciprocal"
            package-name="org.ofbiz.accounting.ledger"
            title="GL Account Reciprocal Entity">
      <field name="invItemTypeContain" type="id"></field>
      <field name="organizationPartyId" type="id-ne"></field>
      <field name="glAccountReciprocalId" type="id-ne"></field>
      <prim-key field="invItemTypeContain"/>
      <prim-key field="organizationPartyId"/>      
      <relation type="one" fk-name="GLACRP_OPTY" title="Organization" rel-entity-name="Party">
        <key-map field-name="organizationPartyId" rel-field-name="partyId"/>
      </relation>
      <relation type="one" fk-name="GLACRP_AGR_IIT" rel-entity-name="InvoiceItemType">
        <key-map field-name="glAccountReciprocalId" rel-field-name="invoiceItemTypeId"/>
      </relation>      
    </entity>  	
    <entity entity-name="PaymentCodeSeqValue" package-name="com.olbius.bacc">
    	<field name="sequenceId" type="id-ne"></field>
    	<field name="paymentTypeId" type="id-ne"></field>
    	<field name="paymentMethodId" type="id-ne"></field>
    	<field name="year" type="int"></field>
    	<field name="sequenceValue" type="numeric"></field>
    	<prim-key field="sequenceId"/>
    	<relation rel-entity-name="PaymentType" type="one" fk-name="PMNT_CODE_SEQ_TYP">
    		<key-map field-name="paymentTypeId"/>
    	</relation>
    	<relation rel-entity-name="PaymentMethod" type="one" fk-name="PMNT_CODE_SEQ_METHOD">
    		<key-map field-name="paymentMethodId"/>
    	</relation>
    </entity>
    <entity entity-name="PaymentReceipt" package-name="com.olbius.bacc">
    	<field name="paymentReceiptId" type="id-ne"></field>
    	<field name="paymentId" type="id"></field>
      	<field name="receiptId" type="long-varchar"></field>
     	<field name="amountApplied" type="currency-amount"></field>
      	<prim-key field="paymentReceiptId"/>
      	<relation type="one" fk-name="PAYMENT_REC_PMT" rel-entity-name="Payment">
        	<key-map field-name="paymentId"/>
      	</relation>
    </entity>
    <entity entity-name="CustomerTimePayment" package-name="com.olbius.bacc">
        <field name="customerTimePaymentId" type="id-ne"/>
        <field name="partyId" type="id-ne"/>
        <field name="dueDay" type="numeric"/>
        <field name="fromDate" type="date-time"/>
        <field name="thruDate" type="date-time"/>
        <prim-key field="customerTimePaymentId"/>
        <relation type="one" rel-entity-name="Party" fk-name="CTP_PARTY">
            <key-map field-name="partyId"/>
        </relation>
    </entity>
</entitymodel>