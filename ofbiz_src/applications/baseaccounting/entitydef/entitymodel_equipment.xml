    <entity entity-name="EquipmentAllocation" package-name="com.olbius.bacc.equipment"
      		title="Equipment Allocation Entity">
      <field name="equipAllocationId" type="id-ne"></field>
      <field name="equipmentId" type="id-ne"></field>
	  <field name="alloTimes" type="fixed-point"></field>
	  <field name="preAllocationDate" type="date-time"></field>
	  <field name="amount" type="fixed-point"></field>
	  <field name="allocatedAmount" type="fixed-point"></field>
	  <field name="remainingValue" type="fixed-point"></field>
	  <field name="monthlyAllocAmount" type="fixed-point"></field>    
      <prim-key field="equipAllocationId"/>
      <relation type="one" fk-name="EQUIP_ALLO" rel-entity-name="Equipment">
        <key-map field-name="equipmentId"/>
      </relation>
    </entity>
    
    <entity entity-name="EquipmentPartyAlloc" package-name="com.olbius.bacc.equipment"
      		title="Equipment Party Allocation Entity">
      <field name="equipmentId" type="id-ne"></field>
      <field name="seqId" type="id-ne"></field>
      <field name="allocPartyId" type="id-ne"></field>
      <field name="allocRate" type="fixed-point"></field>
      <field name="allocGlAccountId" type="id-ne"></field>
      <prim-key field="equipmentId"/>
      <prim-key field="seqId"/>
      <relation rel-entity-name="GlAccount" type="one" fk-name="EPA_GLACCOUNT">
      	<key-map field-name="allocGlAccountId" rel-field-name="glAccountId"/>
      </relation>
      <relation rel-entity-name="Party" type="one" fk-name="EPA_PARTY">
      	<key-map field-name="allocPartyId" rel-field-name="partyId"/>
      </relation>
    </entity>
    
