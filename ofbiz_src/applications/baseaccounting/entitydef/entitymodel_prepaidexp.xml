<?xml version="1.0" encoding="UTF-8"?>
<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
    <!-- ========================================================= -->
    <!-- ======================== Defaults ======================= -->
    <!-- ========================================================= -->
    <title>Entity of BaseAccounting Component</title>
    <description>None</description>
    <copyright></copyright>
    <version></version>
	<entity entity-name="PrepaidExp"
            package-name="com.olbius.bacc.prepaidexp"
            title="Prepaid Expense Entity">
      <field name="prepaidExpId" type="id"></field>
      <field name="prepaidExpName" type="id-ne"></field>
      <field name="acquiredDate" type="date-time"></field>
      <field name="preAllocationDate" type="date-time"></field>
      <field name="amount" type="fixed-point"></field>
      <field name="allocPeriodNum" type="numeric"></field>
      <field name="remainingValue" type="fixed-point"></field>
      <field name="amountEachPeriod" type="fixed-point"></field>
      <field name="prepaidExpGlAccountId" type="id-ne"></field>
      <field name="description" type="description"></field>
      <prim-key field="prepaidExpId"/>
      <relation rel-entity-name="GlAccount" type="one" fk-name="PREPAIDEXP_GLACCOUNT">
      	<key-map field-name="prepaidExpGlAccountId" rel-field-name="glAccountId"/>
      </relation>
    </entity>
    
    <entity entity-name="PrepaidExpAlloc"
            package-name="com.olbius.bacc.prepaidexp"
            title="Prepaid Expense Allocation Entity">
      <field name="prepaidExpId" type="id-ne"></field>
      <field name="seqId" type="id-ne"></field>
      <field name="allocPartyId" type="id-ne"></field>
      <field name="allocRate" type="fixed-point"></field>
      <field name="allocGlAccountId" type="id-ne"></field>
      <prim-key field="prepaidExpId"/>
      <prim-key field="seqId"/>
      <relation rel-entity-name="GlAccount" type="one" fk-name="PREPAIDEXPALLOC_GLACCOUNT">
      	<key-map field-name="allocGlAccountId" rel-field-name="glAccountId"/>
      </relation>
      <relation rel-entity-name="Party" type="one" fk-name="PREPAIDEXPALLOC_PARTY">
      	<key-map field-name="allocPartyId" rel-field-name="partyId"/>
      </relation>
    </entity>
    
    <entity entity-name="PrepaidExpCustomTimePeriod"
            package-name="com.olbius.bacc.prepaidexp"
            title="Prepaid Expense Custom Time Period Entity">
      <field name="prepaidExpId" type="id-ne"></field>
      <field name="customTimePeriodId" type="id-ne"></field>
      <field name="amount" type="fixed-point"></field>
      <prim-key field="prepaidExpId"/>
      <prim-key field="customTimePeriodId"/>
      <relation rel-entity-name="PrepaidExp" type="one" fk-name="PREPAIDEXP_CUSTOMTIMEPERIOD">
      	<key-map field-name="prepaidExpId"/>
      </relation>
      <relation rel-entity-name="CustomTimePeriod" type="one" fk-name="CUSTOMTIMEPERIOD_PREPAIDEXP">
      	<key-map field-name="customTimePeriodId"/>
      </relation>
    </entity>
</entitymodel>