	<entity entity-name="FixedAssetDepreCalcItem" package-name="com.olbius.bacc.fixedasset">
		<field name="depreciationCalcId" type="id-ne"></field>
		<field name="fixedAssetId" type="id-ne"></field>
		<field name="debitGlAccountId" type="id-ne"></field>
		<field name="creditGlAccountId" type="id-ne"></field>
		<field name="depreciationAmount" type="currency-amount"></field>
		<prim-key field="depreciationCalcId"/>
		<prim-key field="fixedAssetId"/>
		<relation rel-entity-name="FixedAssetDepreciationCalc" type="one" fk-name="FA_DEPRE_CALC_ITEM_DEPRE_CALC">
			<key-map field-name="depreciationCalcId"/>		
