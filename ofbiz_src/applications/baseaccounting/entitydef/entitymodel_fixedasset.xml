    <entity entity-name="FixedAssetCustomTimePeriod" package-name="com.olbius.bacc.fixedasset"
            title="Fixed Asset Depreciation Entity">
      	<field name="fixedAssetId" type="id-ne"></field>
      	<field name="customTimePeriodId" type="id-ne"></field>
      	<field name="amount" type="fixed-point"></field>
      	<prim-key field="fixedAssetId"/>
      	<prim-key field="customTimePeriodId"/>
      	<relation type="one" fk-name="FIXED_ASSETDEP" rel-entity-name="FixedAsset">
        	<key-map field-name="fixedAssetId"/>
      	</relation>
      	<relation type="one" fk-name="FX_CUSTOM_TIME_PERIOD" rel-entity-name="CustomTimePeriod">
        	<key-map field-name="customTimePeriodId"/>
		</relation>
	</entity>
	
	<entity entity-name="FixedAssetIncrease" package-name="com.olbius.bacc.fixedasset">
		<field name="fixedAssetIncreaseId" type="id-ne"></field>
		<field name="paymentMethodTypeEnumId" type="id-ne"></field>
		<field name="dateArising" type="date-time">
			<description>ngay phat sinh</description>
		</field>
		<field name="isReceiveInvoice" type="boolean"></field>
		<field name="supplierId" type="id-ne"></field>
		<field name="address" type="long-varchar"></field>
		<field name="contactPerson" type="long-varchar"></field>
		<field name="description" type="long-varchar"></field>
		<field name="employeeBuyerId" type="id-ne">
			<description>nhan vien mua hang</description>
		</field>
		<field name="moneyReceiver" type="long-varchar"></field>
		<field name="accountPayer" type="id-ne"></field>
		<field name="accountReceiver" type="long-varchar"></field>
		<field name="currencyUomId" type="id-ne"></field>
		<field name="dueDate" type="date-time">
			<description>han thanh toan</description>
		</field>
		<field name="paymentVoucherNbr" type="short-varchar">
			<description>So PC/UNC</description>
		</field>
		<field name="isPosted" type="boolean"></field>
		<field name="acctgTransId" type="id-ne"></field>
        <prim-key field="fixedAssetIncreaseId"/>
	<relation rel-entity-name="AcctgTrans" type="one" fk-name="FA_INC_ACCTG">
    		<key-map field-name="acctgTransId"/>
    	</relation>
		<relation rel-entity-name="Enumeration" type="one" fk-name="FA_INC_ENUM">
			<key-map field-name="paymentMethodTypeEnumId" rel-field-name="enumId"/>
		</relation>
		<relation rel-entity-name="Party" type="one" title="supplier" fk-name="FA_INC_SUP">
			<key-map field-name="supplierId" rel-field-name="partyId"/>
		</relation>
		<relation rel-entity-name="Party" type="one" title="employee" fk-name="FA_INC_EMPL">
			<key-map field-name="employeeBuyerId" rel-field-name="partyId"/>
		</relation>
		<relation rel-entity-name="Uom" type="one" fk-name="FA_INC_UOM">
			<key-map field-name="currencyUomId" rel-field-name="uomId"/>
		</relation>
	</entity>
	
	<entity entity-name="FixedAssetIncreaseItem" package-name="com.olbius.bacc.fixedasset">
