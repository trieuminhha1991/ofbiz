      	<field name="partyId" type="id-ne"></field>
      	<field name="glAccountId" type="id-ne"></field>
      	<prim-key field="partyId"/>
      	<prim-key field="glAccountId"/>
      	<relation type="one" fk-name="FAPG_PARTY" rel-entity-name="Party">
      		<key-map field-name="partyId"/>
      	</relation>
      	<relation type="one" fk-name="FAPG_GlAccount" rel-entity-name="GlAccount">
      		<key-map field-name="glAccountId"/>
      	</relation>
    </entity>
    
