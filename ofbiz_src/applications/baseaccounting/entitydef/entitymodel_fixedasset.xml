    <copyright></copyright>
    <version></version>
	<entity entity-name="FixedAssetDepreciation" package-name="com.olbius.bacc.fixedasset"
            title="Fixed Asset Depreciation Entity">
		<field name="fixedAssetDepId" type="id-ne"></field>
		<field name="fixedAssetId" type="id-ne"></field>
		<field name="depGlAccountId" type="id-ne"></field>
		<field name="accDepGlAccountId" type="id-ne"></field>
		<field name="costGlAccountId" type="id-ne"></field>
		<field name="preDepDate" type="date"></field>
		<field name="usefulLives" type="numeric"></field>
		<field name="monthlyDepRate" type="fixed-point"></field>
		<field name="yearlyDepRate" type="fixed-point"></field>
		<field name="lifeDepAmount" type="fixed-point"></field>
		<field name="remainingValue" type="fixed-point"></field>
		<field name="monthlyDepAmount" type="fixed-point"></field>
		<field name="yearlyDepAmount" type="fixed-point"></field>
		<field name="accumulatedDep" type="fixed-point"></field>
		<prim-key field="fixedAssetDepId"/>
		<relation type="one" fk-name="FX_ASSETDEP" rel-entity-name="FixedAsset">
		  	<key-map field-name="fixedAssetId"/>
		</relation>
		<relation type="one" fk-name="FIXED_ASSETDEP_GLACC" title="dep" rel-entity-name="GlAccount">
			<key-map rel-field-name="glAccountId" field-name="depGlAccountId"/>
		</relation>
		<relation type="one" fk-name="FIXED_ASSETCOST_GLACC" title="cost" rel-entity-name="GlAccount">
			<key-map rel-field-name="glAccountId" field-name="costGlAccountId"/>
		</relation>
		<relation type="one" fk-name="FIXED_ASSETACCDEP_GLACC" title="accDep" rel-entity-name="GlAccount">
			<key-map rel-field-name="glAccountId" field-name="accDepGlAccountId"/>
		</relation>		
    </entity>
    <entity entity-name="FixedAssetAccompany" package-name="com.olbius.bacc.fixedasset">
    	<field name="fixedAssetId" type="id-ne"></field>
    	<field name="accompanySeqId" type="id-ne"></field>
    	<field name="componentName" type="long-varchar"></field>
    	<field name="unit" type="long-varchar"></field>
    	<field name="quantity" type="numeric"></field>
    	<field name="value" type="currency-amount"></field>
    	<prim-key field="fixedAssetId"/>
    	<prim-key field="accompanySeqId"/>
    	<relation rel-entity-name="FixedAsset" type="one" fk-name="FIX_ASST_COMPONENT">
    		<key-map field-name="fixedAssetId"/>
    	</relation>
    </entity>
    
    <entity entity-name="FixedAssetAlloc"
            package-name="com.olbius.bacc.fixedasset"
            title="Fixed Asset Allocation Entity">
      <field name="fixedAssetId" type="id-ne"></field>
      <field name="seqId" type="id-ne"></field>
      <field name="allocPartyId" type="id-ne"></field>
      <field name="allocRate" type="fixed-point"></field>
      <field name="allocGlAccountId" type="id-ne"></field>
      <prim-key field="fixedAssetId"/>
      <prim-key field="seqId"/>
      <relation rel-entity-name="GlAccount" type="one" fk-name="FIXEDASSETALLOC_GLACCOUNT">
      	<key-map field-name="allocGlAccountId" rel-field-name="glAccountId"/>
      </relation>
      <relation rel-entity-name="Party" type="one" fk-name="FIXEDASSETALLOC_PARTY">
      	<key-map field-name="allocPartyId" rel-field-name="partyId"/>
      </relation>
    </entity>
    
    <!-- maybe deleted -->
    <entity entity-name="Decrement" package-name="com.olbius.bacc.fixedasset"
      		title="Decrement Entity">
      <field name="decrementId" type="id-ne"></field>
      <field name="decreasedDate" type="date"></field>
      <field name="decrementTypeId" type="id-ne"></field>
      <field name="enumId" type="id-ne"></field>
      <field name="description" type="description"></field>
      <field name="isClosed" type="id-ne"></field>
      <prim-key field="decrementId"/>
      <relation type="one" fk-name="FIXED_ASSET_DEC_TYPE" rel-entity-name="DecrementType">
        <key-map field-name="decrementTypeId"/>
      </relation>
      <relation type="one" fk-name="DECREMENT_ENUM" rel-entity-name="Enumeration">
        <key-map field-name="enumId"/>
      </relation>
    </entity>
    
    <!-- maybe deleted -->
    <entity entity-name="DecrementType" package-name="com.olbius.bacc.fixedasset"
      		title="Decrement Type Entity">
      <field name="decrementTypeId" type="id-ne"></field>
      <field name="parentTypeId" type="id-ne"></field>
      <field name="hasTable" type="id-ne"></field>
      <field name="description" type="description"></field>
      <prim-key field="decrementTypeId"/>
      <relation type="one" fk-name="DEC_TYPE_PARENT" rel-entity-name="DecrementType">
        <key-map field-name="parentTypeId" rel-field-name="decrementTypeId"/>
      </relation>
    </entity>
    
    <!-- maybe deleted -->
    <entity entity-name="FixedAssetDecrementItem" package-name="com.olbius.bacc.fixedasset"
      		title="Fixed Asset Decrement Entity">
      	<field name="decrementId" type="id-ne"></field>
      	<field name="fixedAssetId" type="id-ne"></field>
      	<field name="amount" type="fixed-point"></field>
      	<prim-key field="decrementId"/>
      	<prim-key field="fixedAssetId"/>
      	<relation type="one" fk-name="FIXED_ASSET_ITEM" rel-entity-name="FixedAsset">
        	<key-map field-name="fixedAssetId"/>
      	</relation>
      	<relation type="one" fk-name="FX_ASSET_DEC_ITEM" rel-entity-name="Decrement">
        	<key-map field-name="decrementId"/>
      	</relation>
    </entity>
    
    <entity entity-name="FixedAssetPartyGlAccount" package-name="com.olbius.bacc.fixedasset"
