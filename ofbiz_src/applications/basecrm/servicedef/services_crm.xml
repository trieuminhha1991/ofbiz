<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
	<description>BaseCRM Services</description>
	<vendor></vendor>
	<version>1.0</version>

	<service name="getPartyInformation" engine="java" invoke="getPartyInformation" location="com.olbius.crm.CRMServices">
		<attribute name="partyId" mode="IN" type="String" />
		<attribute name="partyInfo" mode="OUT" type="Map" optional="true" />
	</service>

	<service name="JQGetListContactFamily" engine="java" invoke="listContactFamily" location="com.olbius.crm.CRMServices">
		<implements service="jqxGetListInterface" />
		<override name="listIterator" type="List"></override>
	</service>

	<service name="JQGetListContactFamilyByCampaign" engine="java" invoke="listContactFamilyByCampaign" location="com.olbius.crm.CRMServices">
		<implements service="jqxGetListInterface" />
		<override name="listIterator" type="List"></override>
	</service>

	<service name="JQGetListContactBusinessesByCampaign" engine="java" invoke="listContactBusinessesByCampaign" location="com.olbius.crm.CRMServices">
		<implements service="jqxGetListInterface" />
		<override name="listIterator" type="List"></override>
	</service>

	<service name="JQGetListContactBusinesses" engine="java" invoke="listContactBusinesses" location="com.olbius.crm.CRMServices">
		<implements service="jqxGetListInterface" />
		<override name="listIterator" type="List"></override>
	</service>

	<service name="JQGetListContactSchool" engine="java" invoke="listContactSchool" location="com.olbius.crm.CRMServices">
		<implements service="jqxGetListInterface" />
		<override name="listIterator" type="List"></override>
	</service>

	<service name="JQGetListCustomersFamily" engine="java" invoke="listCustomersFamily" location="com.olbius.crm.CRMServices">
		<implements service="jqxGetListInterface" />
		<override name="listIterator" type="List"></override>
	</service>

	<service name="JQGetListCustomersBusinesses" engine="java" invoke="listCustomersBusinesses" location="com.olbius.crm.CRMServices">
		<implements service="jqxGetListInterface" />
		<override name="listIterator" type="List"></override>
	</service>

	<service name="JQGetListCustomersSchool" engine="java" invoke="listCustomersSchool" location="com.olbius.crm.CRMServices">
		<implements service="jqxGetListInterface" />
		<override name="listIterator" type="List"></override>
	</service>

	<service name="autoCompleteGeoAjax" engine="java" invoke="autoCompleteGeoAjax" location="com.olbius.crm.CRMServices" auth="false">
		<attribute name="geoId" mode="IN" type="String" optional="true" />
		<attribute name="geoTypeId" mode="IN" type="String" />
		<attribute name="listGeo" mode="OUT" type="List" optional="true" />
	</service>

	<service name="getCustomerByPartyIdTo" engine="java" invoke="getCustomerByPartyIdTo" location="com.olbius.crm.CRMServices">
		<attribute name="partyIdTo" mode="IN" type="String" />
		<attribute name="listPartyIdFrom" mode="OUT" type="List" optional="true" />
	</service>

	<service name="getCustomerRole" engine="java" invoke="getCustomerRole" location="com.olbius.crm.CRMServices">
		<attribute name="partyIdFrom" mode="IN" type="String" />
		<attribute name="partyIdTo" mode="IN" type="String" />
		<attribute name="roleTypeIdTo" mode="IN" type="String" />
		<attribute name="partyRole" mode="OUT" type="String" optional="true" />
	</service>

	<service name="createOrStorePartyAttribute" engine="java" invoke="createOrStorePartyAttribute" location="com.olbius.crm.CRMServices" auth="true" default-entity-name="PartyAttribute">
		<description>create a party attribute record</description>
		<permission-service service-name="partyBasePermissionCheck" main-action="CREATE" />
		<auto-attributes include="pk" mode="IN" />
		<auto-attributes include="nonpk" mode="IN" optional="true" />
	</service>

	<service name="JQGetListStaffContract" engine="java" invoke="listStaffContract" location="com.olbius.crm.CRMServices">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="checkAndUpdateRelSalesExecutive" engine="java" invoke="checkAndUpdateRelSalesExecutive" location="com.olbius.crm.CRMServices">
		<attribute name="partyId" mode="IN" type="String"></attribute>
		<attribute name="salesExecutiveId" mode="IN" type="String"></attribute>
	</service>

	<service name="processName" engine="java" invoke="processName" location="com.olbius.crm.CallcenterServices">
		<attribute name="fullName" mode="IN" type="String" />
		<attribute name="firstName" mode="OUT" type="String" optional="true" />
		<attribute name="middleName" mode="OUT" type="String" optional="true" />
		<attribute name="lastName" mode="OUT" type="String" optional="true" />
	</service>

	<service name="updatePartyGroupIncludePartyCode" invoke="updatePartyGroup" engine="java" location="com.olbius.crm.CallcenterServices"
		default-entity-name="PartyGroup">
		<auto-attributes mode="IN" include="pk" optional="true" />
		<auto-attributes mode="IN" include="nonpk" optional="true" />
		<attribute name="description" type="String" mode="IN" optional="true" />
		<attribute name="partyTypeId" type="String" mode="IN" optional="true" />
		<attribute name="preferredCurrencyUomId" type="String" mode="IN" optional="true" />
		<attribute name="externalId" type="String" mode="IN" optional="true" />
		<attribute name="statusId" type="String" mode="IN" optional="true" />
		<attribute name="partyCode" type="String" mode="IN" />
		<override name="comments" allow-html="safe" />
	</service>

	<service name="createPartyGroupIncludePartyCode" invoke="createPartyGroup" engine="java" location="com.olbius.crm.CallcenterServices"
		default-entity-name="PartyGroup">
		<description>Create a PartyGroup</description>
		<auto-attributes mode="INOUT" include="pk" optional="true" />
		<auto-attributes mode="IN" include="nonpk" optional="true" />
		<attribute name="partyTypeId" type="String" mode="IN" optional="true" />
		<attribute name="description" type="String" mode="IN" optional="true" />
		<attribute name="preferredCurrencyUomId" type="String" mode="IN" optional="true" />
		<attribute name="externalId" type="String" mode="IN" optional="true" />
		<attribute name="statusId" type="String" mode="IN" optional="true" />
		<attribute name="partyCode" type="String" mode="IN" />
		<override name="groupName" optional="false" />
		<override name="comments" allow-html="safe" />
	</service>

</services>