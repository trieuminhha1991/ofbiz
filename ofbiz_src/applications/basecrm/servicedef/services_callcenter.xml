		<auto-attributes mode="IN" optional="true" include="pk" />
		<auto-attributes mode="OUT" optional="true" include="pk" />
	</service>

	<service name="checkPermissionWithCustomer" engine="java" invoke="checkPermissionWithCustomer" location="com.olbius.crm.CallcenterServices">
