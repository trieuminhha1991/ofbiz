<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
	<description>CRM Setting Services</description>
	<vendor></vendor>
	<version>1.0</version>

	<service name="JQGetListCommunicationSubject" engine="java" invoke="listCommunicationSubject" location="com.olbius.crm.CRMSettingServices">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="checkEnumerationCode" engine="java" invoke="checkEnumerationCode" location="com.olbius.crm.CRMSettingServices">
		<attribute name="enumCode" mode="IN" type="String"></attribute>
		<attribute name="enumId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="check" mode="OUT" type="String"></attribute>
	</service>

	<service name="JQGetListCommunicationResult" engine="java" invoke="listCommunicationResult" location="com.olbius.crm.CRMSettingServices">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="checkEnumerationTypeId" engine="java" invoke="checkEnumerationTypeId" location="com.olbius.crm.CRMSettingServices">
		<attribute name="enumTypeId" mode="IN" type="String"></attribute>
		<attribute name="check" mode="OUT" type="String"></attribute>
	</service>

	<service name="createEnumerationType" engine="java" invoke="createEnumerationType" location="com.olbius.crm.CRMSettingServices">
		<auto-attributes mode="IN" entity-name="EnumerationType" optional="true"></auto-attributes>
	</service>

	<service name="updateEnumerationType" engine="java" invoke="updateEnumerationType" location="com.olbius.crm.CRMSettingServices">
		<auto-attributes mode="IN" include="pk" entity-name="EnumerationType"></auto-attributes>
		<auto-attributes mode="IN" include="nonpk" entity-name="EnumerationType" optional="true"></auto-attributes>
	</service>

	<service name="deleteEnumerationType" engine="java" invoke="deleteEnumerationType" location="com.olbius.crm.CRMSettingServices">
		<auto-attributes mode="IN" include="pk" entity-name="EnumerationType"></auto-attributes>
	</service>

	<service name="JQGetListCommunicationReason" engine="java" invoke="listCommunicationReason" location="com.olbius.crm.CRMSettingServices">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="JQGetListRivals" engine="java" invoke="listRivals" location="com.olbius.crm.CRMSettingServices">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="createRival" engine="java" invoke="createRival" location="com.olbius.crm.CRMSettingServices">
		<auto-attributes mode="IN" entity-name="PartyGroup" optional="true"></auto-attributes>
		<attribute name="partyCode" mode="IN" type="String"></attribute>
	</service>

	<service name="JQGetListProductsOfRivals" engine="java" invoke="listProductsOfRivals" location="com.olbius.crm.CRMSettingServices">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="createRivalsProduct" engine="java" invoke="createRivalsProduct" location="com.olbius.crm.CRMSettingServices">
		<attribute name="partyId" mode="IN" type="String"></attribute>
		<attribute name="productName" mode="IN" type="String" optional="true"></attribute>
	</service>

	<service name="getProductsInSubject" engine="java" invoke="getProductsInSubject" location="com.olbius.crm.CRMSettingServices">
		<attribute name="enumId" mode="IN" type="String"></attribute>
		<attribute name="enumRelTypeId" mode="IN" type="String" default-value="RECOMMENDED"></attribute>
		<attribute name="products" mode="OUT" type="List" optional="true"></attribute>
	</service>

	<service name="createEnumerationRelProduct" engine="java" invoke="createEnumerationRelProduct" location="com.olbius.crm.CRMSettingServices"
		default-entity-name="EnumerationRelProduct">
		<auto-attributes mode="IN" include="all" optional="true"></auto-attributes>
	</service>

	<service name="updateEnumerationRelProduct" engine="java" invoke="updateEnumerationRelProduct" location="com.olbius.crm.CRMSettingServices"
		default-entity-name="EnumerationRelProduct">
		<auto-attributes mode="IN" include="pk"></auto-attributes>
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
	</service>

	<service name="deleteEnumerationRelProduct" engine="java" invoke="deleteEnumerationRelProduct" location="com.olbius.crm.CRMSettingServices"
		default-entity-name="EnumerationRelProduct">
		<auto-attributes mode="IN" include="pk"></auto-attributes>
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
	</service>

	<service name="JQGetListProductNotInSubject" engine="java" invoke="listProductNotInSubject" location="com.olbius.crm.CRMSettingServices">
		<implements service="jqxGetListInterface" />
	</service>

</services>