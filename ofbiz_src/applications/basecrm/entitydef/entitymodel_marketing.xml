		<member-entity entity-alias="PG" entity-name="PartyGroup" />
		<alias-all entity-alias="MP" />
		<alias name="groupName" entity-alias="PG" />
		<view-link entity-alias="MP" rel-entity-alias="PG" rel-optional="false">
			<key-map field-name="organizationId" rel-field-name="partyId" />
		</view-link>
	</view-entity>

	<view-entity entity-name="MarketingPlanContentDetail" package-name="com.olbius.marketing">
