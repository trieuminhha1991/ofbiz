	<entity entity-name="MarketingCampaignAttribute" package-name="com.olbius.marketing">
		<field name="marketingCampaignId" type="id-ne" />
		<field name="attrName" type="id-long-ne" />
		<field name="attrValue" type="value" />
		<prim-key field="marketingCampaignId" />
		<prim-key field="attrName" />
		<relation rel-entity-name="MarketingCampaign" fk-name="MK_ATCP" type="one">
			<key-map field-name="marketingCampaignId" />
		</relation>
	</entity>

	<entity entity-name="MarketingCampaignTypeAttr" package-name="org.ofbiz.party.party" title="Party Type Attribute Entity">
		<field name="marketingTypeId" type="id-ne"></field>
		<field name="attrName" type="id-long-ne"></field>
		<prim-key field="marketingTypeId" />
		<prim-key field="attrName" />
		<relation type="one" fk-name="MK_CPTYPE_ATTR" rel-entity-name="MarketingType">
			<key-map field-name="marketingTypeId" />
		</relation>
		<relation type="many" rel-entity-name="MarketingCampaignAttribute">
			<key-map field-name="attrName" />
		</relation>
	</entity>

	<view-entity entity-name="MarketingCampaignDetail" package-name="com.olbius.marketing">
		<member-entity entity-alias="MC" entity-name="MarketingCampaign" />
		<member-entity entity-alias="ULN" entity-name="UserLogin" />
		<member-entity entity-alias="PER" entity-name="Person" />
		<alias-all entity-alias="MC" />
		<alias name="partyId" entity-alias="PER" />
		<alias name="firstName" entity-alias="PER" />
		<alias name="middleName" entity-alias="PER" />
		<alias name="lastName" entity-alias="PER" />
		<view-link entity-alias="MC" rel-entity-alias="ULN" rel-optional="false">
			<key-map field-name="createdByUserLogin" rel-field-name="userLoginId" />
		</view-link>
		<view-link entity-alias="ULN" rel-entity-alias="PER" rel-optional="false">
			<key-map field-name="partyId" />
		</view-link>
	</view-entity>

	<view-entity entity-name="MarketingPlaceDetail" package-name="com.olbius.marketing">
