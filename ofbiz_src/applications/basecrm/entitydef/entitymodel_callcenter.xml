			<key-map field-name="partyIdFrom" />
			<entity-condition>
				<condition-list combine="and">
					<condition-list combine="or">
						<condition-expr entity-alias="TOTAL" field-name="roleTypeIdFrom" value="CONTACT" />
						<condition-expr entity-alias="TOTAL" field-name="roleTypeIdFrom" value="CUSTOMER" />
