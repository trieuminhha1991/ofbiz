					<condition-expr entity-alias="MOM" field-name="roleTypeIdTo" value="HOUSEHOLD" />
					<condition-expr entity-alias="MOM" field-name="roleTypeIdFrom" value="REPRESENTATIVE" />
				</condition-list>
			</entity-condition>
		</view-link>
		<view-link rel-entity-alias="CHILD" entity-alias="MOM">
			<key-map field-name="partyIdTo" />
			<entity-condition>
				<condition-list combine="and">
					<condition-expr entity-alias="CHILD" field-name="roleTypeIdTo" value="HOUSEHOLD" />
					<condition-expr entity-alias="CHILD" field-name="roleTypeIdFrom"
						value="HUSBAND,WIFE,PARENT,FATHER,MOTHER,BROTHER,SISTER,CHILDREN,SON,DAUGHTER,NGRNCHILD,GRANDPARENT,GRANDFATHER,GRANDMOTHER"
						operator="in" />
				</condition-list>
			</entity-condition>
		</view-link>
		<view-link rel-entity-alias="PER" entity-alias="CHILD">
			<key-map field-name="partyIdFrom" rel-field-name="partyId" />
		</view-link>
		<view-link rel-entity-alias="PERMOM" entity-alias="MOM">
			<key-map field-name="partyIdFrom" rel-field-name="partyId" />
		</view-link>
	</view-entity>

