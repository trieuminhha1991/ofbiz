	<entity entity-name="PartyCampaignRelationship" package-name="com.olbius.callcenter" title="Party Campaign Relationship Entity">
		<field name="marketingCampaignId" type="id-ne" />
		<field name="partyIdFrom" type="id-ne" />
		<field name="partyIdTo" type="id-ne" />
		<field name="roleTypeIdFrom" type="id-ne" />
		<field name="roleTypeIdTo" type="id-ne" />
		<field name="fromDate" type="date-time" />
		<field name="thruDate" type="date-time" />
		<field name="statusId" type="id" />
		<field name="relationshipName" type="name">
			<description>Official name of relationship, such as title in a company</description>
		</field>
		<field name="securityGroupId" type="id-ne" />
		<field name="priorityTypeId" type="id" />
		<field name="partyRelationshipTypeId" type="id" />
		<field name="permissionsEnumId" type="id-ne" />
		<field name="positionTitle" type="name">
			<description>The exact word used within the company</description>
		</field>
		<field name="comments" type="comment" />
		<field name="createdByUserLogin" type="id" />
		<field name="lastModifiedByUserLogin" type="id" />
		<prim-key field="marketingCampaignId" />
		<prim-key field="partyIdFrom" />
		<prim-key field="partyIdTo" />
		<prim-key field="roleTypeIdFrom" />
		<prim-key field="roleTypeIdTo" />
		<prim-key field="fromDate" />
		<relation type="one-nofk" title="Campaign" rel-entity-name="MarketingCampaign" fk-name="CampaignRelationshipMk">
			<key-map field-name="marketingCampaignId" rel-field-name="marketingCampaignId" />
		</relation>
		<relation type="one-nofk" title="From" rel-entity-name="Party" fk-name="FromPartyRelationshipMk">
			<key-map field-name="partyIdFrom" rel-field-name="partyId" />
		</relation>
		<relation type="one-nofk" title="To" rel-entity-name="Party" fk-name="ToPartyRelationshipMk">
			<key-map field-name="partyIdTo" rel-field-name="partyId" />
		</relation>
		<relation type="one-nofk" title="From" rel-entity-name="RoleType" fk-name="FromRoleTypeRelationshipMk">
			<key-map field-name="roleTypeIdFrom" rel-field-name="roleTypeId" />
		</relation>
		<relation type="one-nofk" title="To" rel-entity-name="RoleType" fk-name="ToRoleTypeRelationshipMk">
			<key-map field-name="roleTypeIdTo" rel-field-name="roleTypeId" />
		</relation>
		<relation type="one" fk-name="PRTCAMP_REL_FPROLE" title="From" rel-entity-name="PartyRole">
			<key-map field-name="partyIdFrom" rel-field-name="partyId" />
			<key-map field-name="roleTypeIdFrom" rel-field-name="roleTypeId" />
		</relation>
		<relation type="one" fk-name="PRTCAMP_REL_TPROLE" title="To" rel-entity-name="PartyRole">
			<key-map field-name="partyIdTo" rel-field-name="partyId" />
			<key-map field-name="roleTypeIdTo" rel-field-name="roleTypeId" />
		</relation>
		<relation type="one" fk-name="PRTCAMP_REL_STTS" rel-entity-name="StatusItem">
			<key-map field-name="statusId" />
		</relation>
		<relation type="one" fk-name="PRTCAMP_REL_PRTYP" rel-entity-name="PriorityType">
			<key-map field-name="priorityTypeId" />
		</relation>
		<relation type="one" fk-name="PRTCAMP_REL_TYPE" rel-entity-name="PartyRelationshipType">
			<key-map field-name="partyRelationshipTypeId" />
		</relation>
		<relation type="one" fk-name="PRTCAMP_REL_SECGRP" rel-entity-name="SecurityGroup">
			<key-map field-name="securityGroupId" rel-field-name="groupId" />
		</relation>
		<index name="THRU_DATE_PCR_IDX" unique="false">
			<index-field name="thruDate" />
		</index>
	</entity>

	<entity entity-name="PartyCampaignCommEvent" package-name="com.olbius.callcenter">
		<field name="marketingCampaignId" type="id-ne" />
		<field name="partyIdFrom" type="id-ne" />
		<field name="partyIdTo" type="id-ne" />
		<field name="roleTypeIdFrom" type="id-ne" />
		<field name="roleTypeIdTo" type="id-ne" />
		<field name="fromDate" type="date-time" />
		<field name="communicationEventId" type="id-ne" />
		<prim-key field="marketingCampaignId" />
		<prim-key field="partyIdFrom" />
		<prim-key field="partyIdTo" />
		<prim-key field="roleTypeIdFrom" />
		<prim-key field="roleTypeIdTo" />
		<prim-key field="fromDate" />
		<prim-key field="communicationEventId" />
		<relation type="one-nofk" title="Campaign" rel-entity-name="PartyCampaignRelationship">
			<key-map field-name="marketingCampaignId" rel-field-name="marketingCampaignId" />
            <key-map field-name="partyIdFrom" rel-field-name="partyIdFrom" />
            <key-map field-name="partyIdTo" rel-field-name="partyIdTo" />
            <key-map field-name="roleTypeIdFrom" rel-field-name="roleTypeIdFrom" />
            <key-map field-name="roleTypeIdTo" rel-field-name="roleTypeIdTo" />
            <key-map field-name="fromDate" rel-field-name="fromDate" />
        </relation>
	</entity>

