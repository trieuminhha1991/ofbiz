<?xml version="1.0" encoding="UTF-8"?>
<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
	<!-- ========================================================= -->
	<!-- ======================== Defaults ======================= -->
	<!-- ========================================================= -->
	<title>Entity of BaseCRM Component</title>
	<description>None</description>
	<copyright></copyright>
	<version></version>

	<view-entity entity-name="PostalAddressAndGeo" package-name="com.crm">
		<member-entity entity-alias="PAD" entity-name="PostalAddress" />
		<member-entity entity-alias="GEOCOUTRY" entity-name="Geo" />
		<member-entity entity-alias="GEOPROVINCE" entity-name="Geo" />
		<member-entity entity-alias="GEODISTRICT" entity-name="Geo" />
		<member-entity entity-alias="GEOWARD" entity-name="Geo" />
		<member-entity entity-alias="GEOP" entity-name="GeoPoint" />
		<alias-all entity-alias="PAD"></alias-all>
		<alias field="geoName" name="countryGeoName" entity-alias="GEOCOUTRY" />
		<alias field="geoName" name="stateProvinceGeoName" entity-alias="GEOPROVINCE" />
		<alias field="geoName" name="districtGeoName" entity-alias="GEODISTRICT" />
		<alias field="geoName" name="wardGeoName" entity-alias="GEOWARD" />
		<alias name="latitude" entity-alias="GEOP" />
		<alias name="longitude" entity-alias="GEOP" />
		<view-link rel-entity-alias="GEOCOUTRY" entity-alias="PAD" rel-optional="true">
			<key-map field-name="countryGeoId" rel-field-name="geoId" />
		</view-link>
		<view-link rel-entity-alias="GEOPROVINCE" entity-alias="PAD" rel-optional="true">
			<key-map field-name="stateProvinceGeoId" rel-field-name="geoId" />
		</view-link>
		<view-link rel-entity-alias="GEODISTRICT" entity-alias="PAD" rel-optional="true">
			<key-map field-name="districtGeoId" rel-field-name="geoId" />
		</view-link>
		<view-link rel-entity-alias="GEOWARD" entity-alias="PAD" rel-optional="true">
			<key-map field-name="wardGeoId" rel-field-name="geoId" />
		</view-link>
		<view-link entity-alias="PAD" rel-entity-alias="GEOP" rel-optional="true">
			<key-map field-name="geoPointId" />
		</view-link>
	</view-entity>

	<view-entity entity-name="PartyAndPostalAddressDetail" package-name="com.crm">
		<member-entity entity-alias="PCM" entity-name="PartyContactMech" />
		<member-entity entity-alias="PAG" entity-name="PostalAddressAndGeo" />
		<member-entity entity-alias="PCMP" entity-name="PartyContactMechPurpose" />
		<alias-all entity-alias="PAG"></alias-all>
		<alias-all entity-alias="PCM"></alias-all>
		<alias-all entity-alias="PCMP">
			<exclude field="fromDate" />
			<exclude field="thruDate" />
		</alias-all>
		<view-link entity-alias="PCM" rel-entity-alias="PAG">
			<key-map field-name="contactMechId" />
		</view-link>
		<view-link entity-alias="PAG" rel-entity-alias="PCMP">
			<key-map field-name="contactMechId" />
		</view-link>
	</view-entity>

	<view-entity entity-name="PartyAddressPurpose" package-name="com.crm">
		<member-entity entity-alias="PCM" entity-name="PartyContactMech" />
		<member-entity entity-alias="PAG" entity-name="PostalAddressAndGeo" />
		<member-entity entity-alias="PCMP" entity-name="PartyContactMechPurpose" />
		<alias-all entity-alias="PAG"></alias-all>
		<alias-all entity-alias="PCM"></alias-all>
		<alias-all entity-alias="PCMP">
			<exclude field="fromDate" />
			<exclude field="thruDate" />
		</alias-all>
		<view-link entity-alias="PCM" rel-entity-alias="PAG">
			<key-map field-name="contactMechId" />
		</view-link>
		<view-link entity-alias="PAG" rel-entity-alias="PCMP">
			<key-map field-name="contactMechId" />
		</view-link>
	</view-entity>

	<entity entity-name="TempCustomer" package-name="com.olbius.callcenter">
		<field name="customerId" type="id-ne"></field>
		<field name="partyFullName" type="name"></field>
		<field name="birthDate" type="date"></field>
		<field name="address1" type="description"></field>
		<field name="countryGeoId" type="name"></field>
		<field name="countryGeoName" type="name"></field>
		<field name="stateProvinceGeoId" type="name"></field>
		<field name="stateProvinceGeoName" type="name"></field>
		<field name="districtGeoId" type="name"></field>
		<field name="districtGeoName" type="name"></field>
		<field name="wardGeoId" type="name"></field>
		<field name="wardGeoName" type="name"></field>
		<field name="phoneHome" type="name"></field>
		<field name="phoneMobile" type="name"></field>
		<field name="phoneWork" type="name"></field>
		<field name="emailAddress" type="name"></field>
		<field name="childName1" type="name"></field>
		<field name="childName1birthDate" type="date"></field>
		<field name="childName2" type="name"></field>
		<field name="childName2birthDate" type="date"></field>
		<prim-key field="customerId" />
	</entity>

	<entity entity-name="EnumerationRelType" package-name="com.olbius.callcenter">
		<field name="enumerationRelTypeId" type="id-ne"></field>
		<field name="description" type="description"></field>
		<prim-key field="enumerationRelTypeId" />
	</entity>

	<entity entity-name="EnumerationRelProduct" package-name="com.olbius.callcenter">
		<field name="enumId" type="id-ne"></field>
		<field name="productId" type="id-ne"></field>
		<field name="enumRelTypeId" type="id"></field>
		<field name="sequenceNum" type="numeric"></field>
		<prim-key field="enumId"/>
		<prim-key field="productId"/>
		<relation rel-entity-name="Enumeration" type="one" fk-name="ENUM_REL_PR_ENUM">
			<key-map field-name="enumId"/>
		</relation>
		<relation rel-entity-name="Product" type="one" fk-name="ENUM_REL_PR_PR">
			<key-map field-name="productId" />
		</relation>
		<relation rel-entity-name="EnumerationRelType" type="one" fk-name="ENUM_REL_PR_ERT">
			<key-map field-name="enumRelTypeId" rel-field-name="enumerationRelTypeId" />
		</relation>
	</entity>

	<view-entity entity-name="EnumerationRelProductDetail" package-name="com.olbius.callcenter">
		<member-entity entity-alias="ERP" entity-name="EnumerationRelProduct"></member-entity>
		<member-entity entity-alias="PRD" entity-name="Product"></member-entity>
		<alias-all entity-alias="ERP"></alias-all>
		<alias-all entity-alias="PRD"></alias-all>
		<view-link rel-entity-alias="PRD" entity-alias="ERP">
			<key-map field-name="productId" />
		</view-link>
	</view-entity>

</entitymodel>