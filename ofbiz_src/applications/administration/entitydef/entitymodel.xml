<?xml version="1.0" encoding="UTF-8"?>
<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
	<!-- ========================================================= -->
	<!-- ======================== Defaults ======================= -->
	<!-- ========================================================= -->
	<title>Entity of Administration Component</title>
	<description>None</description>
	<copyright></copyright>
	<version></version>

	<entity entity-name="ApplicationSetting" package-name="com.olbius.administration">
		<field name="applicationSettingId" type="id-vlong"></field>
		<field name="applicationSettingTypeId" type="id"></field>
		<field name="partyId" type="id" not-null="false"></field>
		<field name="applicationSettingEnumId" type="id"></field>
		<field name="applicationSettingName" type="long-varchar"></field>
		<field name="value" type="very-long"></field>
		<field name="description" type="description"></field>
		<prim-key field="applicationSettingId" />
		<prim-key field="applicationSettingTypeId" />
		<prim-key field="applicationSettingEnumId" />
		<prim-key field="partyId" />
		<relation type="one" fk-name="APPSET_TYPE" rel-entity-name="ApplicationSettingType">
			<key-map field-name="applicationSettingTypeId" />
		</relation>
		<relation type="one" fk-name="APPSET_PARTY" rel-entity-name="Party">
			<key-map field-name="partyId" />
		</relation>
		<relation type="one" fk-name="APPSET_ENUM" title="Action"
			rel-entity-name="Enumeration">
			<key-map field-name="applicationSettingEnumId"
				rel-field-name="enumId" />
		</relation>
	</entity>

	<entity entity-name="ApplicationSettingType" package-name="com.olbius.administration">
		<field name="applicationSettingTypeId" type="id-ne"></field>
		<field name="parentTypeId" type="id"></field>
		<field name="description" type="description"></field>
		<prim-key field="applicationSettingTypeId" />
		<relation type="one" fk-name="APPSET_TYPE_PARENT" title="Parent"
			rel-entity-name="ApplicationSettingType">
			<key-map field-name="parentTypeId" rel-field-name="applicationSettingTypeId" />
		</relation>
	</entity>

	<entity entity-name="ImageDataTextResource" package-name="com.olbius.image"
            title="Contains All image encode base 64">
		<field name="visualThemeId" type="id-ne"></field>
		<field name="resourceTypeEnumId" type="id-ne"></field>
		<field name="sequenceId" type="id-ne"><description>Controls the loading order of duplicate resource types</description></field>
		<field name="resourceValue" type="very-long"><description>Contains the resource value</description></field>
		<prim-key field="visualThemeId"/>
		<prim-key field="resourceTypeEnumId"/>
		<prim-key field="sequenceId"/>
		<relation type="one" fk-name="VT_RES_THEME" rel-entity-name="VisualTheme">
			<key-map field-name="visualThemeId"/>
		</relation>
		<relation type="one" fk-name="VT_RES_TYPE_ENUM" rel-entity-name="Enumeration">
			<key-map field-name="resourceTypeEnumId" rel-field-name="enumId"/>
		</relation>
    </entity>
</entitymodel>