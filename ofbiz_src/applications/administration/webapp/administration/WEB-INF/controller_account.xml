<?xml version="1.0" encoding="UTF-8"?>
<site-conf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/site-conf.xsd">

	<request-map uri="PartyAccount">
		<security https="true" auth="true" />
		<response name="success" type="view" value="PartyAccount" />
	</request-map>

	<request-map uri="resetPassword">
		<security https="false" auth="true" />
		<event type="service" invoke="resetPassword" />
		<response name="success" type="request" value="json" />
		<response name="error" type="request" value="json" />
	</request-map>
	<!-- View Mappings -->
	<view-map name="PartyAccount" type="screen"
		page="component://administration/widget/AccountScreens.xml#PartyAccount" />
</site-conf>