<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../framework/service/dtd/services.xsd">
	<description>Administration Services</description>
	<vendor></vendor>
	<version>1.0</version>

	<service name="JQGetListRecurrenceRules" engine="java" invoke="listRecurrenceRules" location="com.olbius.administration.synchronization.SynchronizationServices">
		<implements service="jqxGetListInterface" />
	</service>
	<service name="JQGetListEntitySync" engine="java" invoke="listEntitySync" location="com.olbius.administration.synchronization.SynchronizationServices">
		<implements service="jqxGetListInterface" />
	</service>
	<service name="JQGetListEntitySyncHistory" engine="java" invoke="listEntitySyncHistory" location="com.olbius.administration.synchronization.SynchronizationServices">
		<implements service="jqxGetListInterface" />
	</service>
	<service name="JQGetListEntityGroupEntrySync" engine="java" invoke="listEntityGroupEntrySync" location="com.olbius.administration.synchronization.SynchronizationServices">
		<implements service="jqxGetListInterface" />
	</service>
	<service name="JQGetListPOSOffline" engine="java" invoke="listPOSOffline" location="com.olbius.administration.synchronization.SynchronizationServices">
		<implements service="jqxGetListInterface" />
	</service>
	
	<service name="updateRecurrenceRule" engine="java" default-entity-name="RecurrenceRule" invoke="updateRecurrenceRule" location="com.olbius.administration.synchronization.SynchronizationServices">
		<auto-attributes mode="IN" include="pk"></auto-attributes>
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
	</service>
	
	<service name="createEntityGroupEntries" engine="java" default-entity-name="EntityGroupEntry" invoke="createEntityGroupEntries" location="com.olbius.administration.synchronization.SynchronizationServices">
		<auto-attributes mode="IN" include="pk"></auto-attributes>
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
		<override name="entityOrPackage" type="java.util.List"></override>
	</service>
	<service name="createEntityGroupEntry" engine="java" default-entity-name="EntityGroupEntry" invoke="createEntityGroupEntry" location="com.olbius.administration.synchronization.SynchronizationServices">
		<auto-attributes mode="IN" include="pk"></auto-attributes>
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
	</service>
	<service name="deleteEntityGroupEntry" engine="java" default-entity-name="EntityGroupEntry" invoke="deleteEntityGroupEntry" location="com.olbius.administration.synchronization.SynchronizationServices">
		<auto-attributes mode="IN" include="pk"></auto-attributes>
	</service>
	
	<service name="loadEntityGroupId" engine="java" invoke="loadEntityGroupId" location="com.olbius.administration.synchronization.SynchronizationServices">
		<attribute name="values" mode="OUT" type="List"></attribute>
	</service>
	<service name="loadEntitiesNotInGroup" engine="java" invoke="loadEntitiesNotInGroup" location="com.olbius.administration.synchronization.SynchronizationServices">
		<attribute name="entityGroupId" mode="IN" type="String"></attribute>
		<attribute name="values" mode="OUT" type="List"></attribute>
	</service>
	
	<service name="createPOSOffline" engine="java" invoke="createPOSOffline" location="com.olbius.administration.synchronization.SynchronizationServices">
		<attribute name="productStoreId" mode="IN" type="String"></attribute>
	</service>
	
	<service name="reactiveSynchronization" engine="java" invoke="reactiveSynchronization" location="com.olbius.administration.synchronization.SynchronizationServices">
		<attribute name="entitySyncId" mode="IN" type="String"></attribute>
	</service>
	
</services>