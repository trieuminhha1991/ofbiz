<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../framework/service/dtd/services.xsd">
	<description>Administration Services</description>
	<vendor></vendor>
	<version>1.0</version>

	<service name="loadApplicationModuleTree" engine="java"
		invoke="loadApplicationModuleTree" location="com.olbius.administration.security.SecurityServices">
		<attribute name="moduleId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="value" mode="OUT" type="List" optional="true" />
	</service>

	<service name="updateOlbiusApplication" engine="java" invoke="updateOlbiusApplication" location="com.olbius.administration.security.SecurityServices"
		default-entity-name="OlbiusApplication">
		<auto-attributes mode="IN" include="pk"></auto-attributes>
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
	</service>

	<service name="createOlbiusPartyPermission" engine="java" invoke="createOlbiusPartyPermission" location="com.olbius.administration.security.SecurityServices"
		default-entity-name="OlbiusPartyPermission">
		<auto-attributes mode="IN" include="all" optional="true"></auto-attributes>
	</service>
	<service name="updateOlbiusPartyPermission" engine="java" invoke="updateOlbiusPartyPermission" location="com.olbius.administration.security.SecurityServices"
		default-entity-name="OlbiusPartyPermission">
		<auto-attributes mode="IN" include="pk"></auto-attributes>
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
	</service>

	<service name="JQGetListUserLoginInModule" engine="java"
		invoke="listUserLoginInModule" location="com.olbius.administration.security.SecurityServices">
		<implements service="jqxGetListInterface" />
	</service>
	<service name="JQGetListGroupInModule" engine="java"
		invoke="listGroupInModule" location="com.olbius.administration.security.SecurityServices">
		<implements service="jqxGetListInterface" />
	</service>
	<service name="JQGetListOlbiusPermissionOfParty" engine="java"
		invoke="listOlbiusPermissionOfParty" location="com.olbius.administration.security.SecurityServices">
		<implements service="jqxGetListInterface" />
	</service>
	<service name="JQGetListOlbiusPermissionOfAction" engine="java"
		invoke="listOlbiusPermissionOfAction" location="com.olbius.administration.security.SecurityServices">
		<implements service="jqxGetListInterface" />
	</service>
	<service name="JQGetListUserLogin" engine="java" invoke="listUserLogin"
		location="com.olbius.administration.security.SecurityServices">
		<implements service="jqxGetListInterface" />
	</service>
	<service name="JQGetListUserLoginNotInApplication" engine="java"
		invoke="listUserLoginNotInApplication" location="com.olbius.administration.security.SecurityServices">
		<implements service="jqxGetListInterface" />
	</service>
	<service name="JQGetListGroupNotInApplication" engine="java"
		invoke="listGroupNotInApplication" location="com.olbius.administration.security.SecurityServices">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="JQGetListActionInModule" engine="java"
		invoke="listActionInModule" location="com.olbius.administration.security.SecurityServices">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="JQGetListApplicationOfParty" engine="java"
		invoke="listApplicationOfParty" location="com.olbius.administration.security.SecurityServices">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="JQGetListApplicationOverride" engine="java"
		invoke="listApplicationOverride" location="com.olbius.administration.security.SecurityServices">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="JQGetListUserGroupSecurity" engine="java"
		invoke="listUserGroupSecurity" location="com.olbius.administration.security.SecurityServices">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="JQGetListUserInGroup" engine="java" invoke="listUserInGroup"
		location="com.olbius.administration.security.SecurityServices">
		<implements service="jqxGetListInterface" />
	</service>
	<service name="JQGetListUserNotInGroup" engine="java"
		invoke="listUserNotInGroup" location="com.olbius.administration.security.SecurityServices">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="loadPermissionOfPartyInApplication" engine="java"
		invoke="loadPermissionOfPartyInApplication" location="com.olbius.administration.security.SecurityServices">
		<attribute name="applicationId" mode="IN" type="String"></attribute>
		<attribute name="partyId" mode="IN" type="String"></attribute>
		<attribute name="permissions" mode="OUT" type="List" optional="true"></attribute>
	</service>

	<service name="addUsersToModule" engine="java" invoke="addUsersToModule" location="com.olbius.administration.security.SecurityServices">
		<attribute name="applicationId" mode="IN" type="String"></attribute>
		<attribute name="parties[]" type="List" mode="IN" />
		<attribute name="permissionIds[]" type="List" mode="IN" />
		<attribute name="fromDate" mode="IN" type="java.sql.Timestamp" optional="true"></attribute>
		<attribute name="thruDate" mode="IN" type="java.sql.Timestamp" optional="true"></attribute>
		<attribute name="allow" mode="IN" type="Boolean" optional="true" default-value="true"></attribute>
	</service>

	<service name="loadModuleDiagram" engine="java" invoke="loadModuleDiagram"
		location="com.olbius.administration.security.SecurityServices">
		<attribute name="applicationId" mode="IN" type="String"></attribute>
		<attribute name="diagram" mode="OUT" type="List" optional="true"></attribute>
	</service>

	<service name="addUserToGroup" engine="java" invoke="addUserToGroup"
		location="com.olbius.administration.security.SecurityServices">
		<attribute name="groupId" type="String" mode="IN" />
		<attribute name="parties[]" type="List" mode="IN" />
	</service>
	<service name="deleteUserInGroup" engine="java" invoke="deleteUserInGroup"
		location="com.olbius.administration.security.SecurityServices">
		<attribute name="partyId" type="String" mode="IN" />
		<attribute name="groupId" type="String" mode="IN" />
	</service>

	<service name="resetPassword" engine="java" invoke="resetPassword"
		location="com.olbius.administration.security.SecurityServices">
		<required-permissions join-type="AND">
			<check-permission permission="ADMINISTRATION" action="_ADMIN" />
		</required-permissions>
		<attribute name="userLoginId" type="String" mode="IN" />
	</service>

</services>