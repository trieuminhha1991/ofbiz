<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../framework/service/dtd/services.xsd">
	<description>Administration Services</description>
	<vendor></vendor>
	<version>1.0</version>

	<service name="createApplicationSetting" engine="java" invoke="createApplicationSetting" location="com.olbius.administration.AdministrationServices"
		default-entity-name="ApplicationSetting">
		<required-permissions join-type="AND">
			<check-permission permission="APPLICATIONSETTING" action="_CREATE" />
		</required-permissions>
		<auto-attributes mode="INOUT" include="pk" optional="true"></auto-attributes>
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
		<override name="partyId" default-value="_NA_"></override>
	</service>
	<service name="updateApplicationSetting" engine="java" invoke="updateApplicationSetting" location="com.olbius.administration.AdministrationServices"
		default-entity-name="ApplicationSetting">
		<required-permissions join-type="AND">
			<check-permission permission="APPLICATIONSETTING" action="_UPDATE" />
		</required-permissions>
		<auto-attributes mode="INOUT" include="pk"></auto-attributes>
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
		<override name="partyId" default-value="_NA_"></override>
	</service>
	<service name="createApplicationSettings" engine="java" invoke="createApplicationSettings" location="com.olbius.administration.AdministrationServices">
		<required-permissions join-type="AND">
			<check-permission permission="APPLICATIONSETTING" action="_CREATE" />
		</required-permissions>
		<attribute name="settings" mode="IN" type="String"></attribute>
		<override name="partyId" default-value="_NA_"></override>
	</service>
	<service name="updateApplicationSettings" engine="java" invoke="updateApplicationSettings" location="com.olbius.administration.AdministrationServices">
		<required-permissions join-type="AND">
			<check-permission permission="APPLICATIONSETTING" action="_UPDATE" />
		</required-permissions>
		<attribute name="settings" mode="IN" type="String"></attribute>
		<override name="partyId" default-value="_NA_"></override>
	</service>
	<service name="getApplicationSetting" engine="java" invoke="getApplicationSetting" location="com.olbius.administration.AdministrationServices"
		default-entity-name="ApplicationSetting">
		<auto-attributes mode="IN" include="pk"></auto-attributes>
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
		<attribute name="applicationSetting" mode="OUT" type="Map" optional="true"></attribute>
		<override name="applicationSettingId" optional="true"></override>
		<override name="partyId" default-value="_NA_"></override>
	</service>
	<service name="getValueApplicationSetting" engine="java" invoke="getValueApplicationSetting" location="com.olbius.administration.AdministrationServices"
		default-entity-name="ApplicationSetting">
		<auto-attributes mode="IN" include="pk"></auto-attributes>
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
		<attribute name="value" mode="OUT" type="String" optional="true"></attribute>
		<override name="applicationSettingId" optional="true"></override>
		<override name="partyId" default-value="_NA_"></override>
	</service>

	<service name="updateVisualThemeResources" engine="java" invoke="updateVisualThemeResources" location="com.olbius.administration.AdministrationServices">
		<attribute name="resources" mode="IN" type="String"></attribute>
	</service>
	<service name="getBrandLogo" engine="java" invoke="getBrandLogo" location="com.olbius.administration.AdministrationServices">
		<attribute name="partyId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="originalUrl" mode="OUT" type="String"></attribute>
	</service>
	<service name="getBrandLogoBase64" engine="java" invoke="getBrandLogoBase64" location="com.olbius.administration.AdministrationServices">
		<attribute name="partyId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="base64" mode="OUT" type="String"></attribute>
	</service>
	<service name="storeBrandLogo" engine="java" invoke="storeBrandLogo" location="com.olbius.administration.AdministrationServices">
		<attribute name="partyId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="logoImg" mode="IN" type="java.nio.ByteBuffer" />
	</service>
	
	<service name="updateSystemConfig" default-entity-name="SystemConfig" engine="java" auth="true" invoke="updateSystemConfig" location="com.olbius.administration.AdministrationServices">
		<description>Update SystemConfig</description>
		<auto-attributes include="pk" mode="IN" optional="false"/>
		<auto-attributes include="nonpk" mode="IN" optional="true"/>
	</service>
	<service name="JQGetListSystemConfig" engine="java" invoke="jqGetListSystemConfig" location="com.olbius.administration.AdministrationServices">
		<implements service="jqxGetListInterface"/>
	</service>
	<service name="updateSystemConfigWB" engine="java" auth="true" invoke="updateSystemConfigWB" location="com.olbius.administration.AdministrationServices">
		<description>Update SystemConfig Weight Barcode</description>
		<attribute name="weightBarcodeType" mode="IN" type="String" optional="true"/>
		<attribute name="prefixWeightBarcode" mode="IN" type="String" optional="true"/>
		<attribute name="patternWeightBarcode" mode="IN" type="String" optional="true"/>
		<attribute name="decimalsInWeight" mode="IN" type="Integer" optional="true"/>
	</service>
	<service name="changeDocumentLogo" engine="java" auth="true" invoke="changeDocumentLogo" location="com.olbius.administration.AdministrationServices">
		<description>Change logo of documents</description>
		<attribute name="uploadedFile" type="java.nio.ByteBuffer" mode="IN" optional="true"/>
		<attribute name="_uploadedFile_contentType" type="String" mode="IN" optional="true"/>
	</service>
	
</services>