<?xml version="1.0" encoding="UTF-8"?>

<service-eca xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/service-eca.xsd">

	<eca service="sendProduct" event="return">
		<set field-name="message" env-name="productXml" />
		<set field-name="destination" value="olbius:product" />
		<set field-name="destinationType" value="queue" />
		<action service="sendTextMessage" mode="async" />
	</eca>

</service-eca>