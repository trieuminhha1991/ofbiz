	    <response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="getSalesPolicyRuleAndCondAndAct">
		<security https="true" auth="true" />
		<event type="java" invoke="getSalesPolicyRuleAndCondAndAct" path="com.olbius.basehr.bonusPolicy.events.BonusPolicyEvents"></event>
		<response name="success" type="request" value="json"></response>
