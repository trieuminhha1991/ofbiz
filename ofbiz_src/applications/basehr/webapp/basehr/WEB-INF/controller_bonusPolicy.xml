	</request-map>
	<request-map uri="createDistributorBonusSummary">
		<security https="true" auth="true" />
		<event type="java" invoke="createDistributorBonusSummary" path="com.olbius.basehr.bonusPolicy.events.BonusPolicyEvents"></event>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="updateSalesBonusSummaryData">
		<security https="true" auth="true" />
		<event type="service" invoke="updateSalesBonusSummaryData"></event>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="deleteSalesBonusPolicyRule">
		<security https="true" auth="true" />
