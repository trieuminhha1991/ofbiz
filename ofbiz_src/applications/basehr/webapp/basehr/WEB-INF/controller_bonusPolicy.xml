		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="updateSalePolicyCondAndAct">
		<security https="true" auth="true" />
		<event type="java" invoke="updateSalePolicyCondAndAct" path="com.olbius.basehr.bonusPolicy.events.BonusPolicyEvents"></event>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	
	<view-map name="ViewDistributorBonusSummaryParty" type="screen"
