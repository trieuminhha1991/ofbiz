<?xml version="1.0" encoding="UTF-8"?>
<site-conf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/site-conf.xsd">
	<request-map uri="ViewDistributorBonusSummaryParty">
		<security https="true" auth="true"/>
		<response name="success" type="view" value="ViewDistributorBonusSummaryParty"></response>
		<response name="error" type="view" value="ViewDistributorBonusSummaryParty"></response>
	</request-map>
	<request-map uri="createDistributorBonusPolicy">
	    <security https="true" auth="true"/>
	    <event type="java" invoke="createDistributorBonusPolicy" path="com.olbius.basehr.bonusPolicy.events.BonusPolicyEvents"></event>
	    <response name="success" type="request" value="json"></response>
	</request-map>
	<request-map uri="addRuleToSalesBonusPolicy">
	    <security https="true" auth="true"/>
	    <event type="java" invoke="addRuleToSalesBonusPolicy" path="com.olbius.basehr.bonusPolicy.events.BonusPolicyEvents"></event>
