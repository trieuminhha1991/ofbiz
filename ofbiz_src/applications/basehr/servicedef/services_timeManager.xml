    <service name="createEmplLeaveRegulation" engine="java" invoke="createEmplLeaveRegulation" auth="true"
   		location="com.olbius.basehr.timeMgr.services.TimeManagerServices">
   		<required-permissions join-type="AND">
   			<check-permission permission="HR_TIMEMGREXT" action="_ADMIN"/>
   		</required-permissions>
   		<attribute name="year" mode="IN" type="String"></attribute>
   		<attribute name="isTransferredAnnualLastYear" mode="IN" type="String" optional="true"></attribute>
	</service>
	<service name="updateEmplLeaveRegulation" engine="java" invoke="updateEmplLeaveRegulation" auth="true"
		location="com.olbius.basehr.timeMgr.services.TimeManagerServices" default-entity-name="EmplLeaveRegulation">
		<required-permissions join-type="AND">
   			<check-permission permission="HR_TIMEMGREXT" action="_ADMIN"/>
   		</required-permissions>
   		<auto-attributes mode="IN" include="pk" optional="false"></auto-attributes>
   		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>	
	</service>
	<service name="approvalEmplLeave" engine="java" invoke="approvalEmplLeave" auth="true"
		location="com.olbius.basehr.timeMgr.services.TimeManagerServices">
		<required-permissions join-type="AND">
			<check-permission permission="HR_TIMEMGR" action="_UPDATE"/>
		</required-permissions>	
		<attribute name="emplLeaveId" mode="IN" type="String"></attribute>
		<attribute name="statusId" mode="IN" type="String"></attribute>
		<attribute name="commentApproval" mode="IN" type="String" optional="true" allow-html="any"></attribute>
	</service>
	<service name="updateEmplLeaveFromTimeTracker" engine="java" invoke="updateEmplLeaveFromTimeTracker" auth="true"
		location="com.olbius.basehr.timeMgr.services.TimeManagerServices">
		<required-permissions join-type="AND">
			<check-permission permission="HR_TIMEMGREXT" action="_UPDATE"/>
		</required-permissions>	
		<attribute name="fromDate" mode="IN" type="java.sql.Timestamp"></attribute>
		<attribute name="thruDate" mode="IN" type="java.sql.Timestamp"></attribute>
	</service>
