<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
	<service name="getPartyWorkingShift" engine="java" invoke="getPartyWorkingShift"
		location="com.olbius.basehr.timeMgr.services.TimeManagerServices" auth="true">
		<attribute name="partyId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="listReturn" mode="OUT" type="List"></attribute>		
	</service>    
	<service name="JQgetListAnnualLeaveSheet" engine="java" invoke="getListAnnualLeaveSheet" 
