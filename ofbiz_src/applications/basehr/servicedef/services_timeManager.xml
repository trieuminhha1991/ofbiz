		location="com.olbius.basehr.timeMgr.services.TimeManagerServices">
		<attribute name="listPartyId" mode="IN" type="List"></attribute>
		<attribute name="fromDate" mode="IN" type="java.sql.Timestamp" optional="true"></attribute>
		<attribute name="thruDate" mode="IN" type="java.sql.Timestamp" optional="true"></attribute>
		<attribute name="emplLeaveList" mode="OUT" type="List"></attribute>
	</service>
	<service name="createWorkOvertimeRegis" engine="java" invoke="createWorkOvertimeRegis" default-entity-name="WorkOvertimeRegistration"
		location="com.olbius.basehr.timeMgr.services.TimeManagerServices" auth="true">
		<required-permissions join-type="AND">
			<check-permission permission="HR_TIMEMGR" action="_CREATE"/>
		</required-permissions>
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
		<auto-attributes mode="OUT" include="pk" optional="false"></auto-attributes>
		<override name="reasonRegister" allow-html="any"></override>	
		<override name="fromDate" optional="false"></override>
		<override name="thruDate" optional="false"></override>
		<override name="dateRegistered" optional="false"></override>
	</service>
	<service name="createWorkOvertimeRegisEnum" engine="java" invoke="createWorkOvertimeRegisEnum" default-entity-name="WorkOvertimeRegisEnum"
		location="com.olbius.basehr.timeMgr.services.TimeManagerServices" auth="true">
		<required-permissions join-type="AND">
			<check-permission permission="HR_TIMEMGR" action="_CREATE"/>
		</required-permissions>
		<auto-attributes mode="IN" include="pk" optional="false"></auto-attributes>
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
	</service>
	<service name="createWorkingLateRegis" engine="java" invoke="createWorkingLateRegis" default-entity-name="WorkingLateRegister"
		location="com.olbius.basehr.timeMgr.services.TimeManagerServices" auth="true">
		<required-permissions join-type="AND">
			<check-permission permission="HR_TIMEMGR" action="_CREATE"/>
		</required-permissions>
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
		<auto-attributes mode="OUT" include="pk" optional="false"></auto-attributes>
		<override name="fromDate" optional="false"></override>
		<override name="thruDate" optional="false"></override>	
		<override name="reasonRegister" optional="true" allow-html="any"></override>
	</service>
	<service name="createWorkingLateRegisEnum" engine="java" invoke="createWorkingLateRegisEnum" default-entity-name="WorkingLateRegisEnum"
		location="com.olbius.basehr.timeMgr.services.TimeManagerServices" auth="true">
		<required-permissions join-type="AND">
			<check-permission permission="HR_TIMEMGR" action="_CREATE"/>
		</required-permissions>
		<auto-attributes mode="IN" include="pk" optional="false"></auto-attributes>
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
	</service>
	<service name="approvalWorkingOvertimeRegister" engine="java" invoke="approvalWorkingOvertimeRegister" default-entity-name="WorkOvertimeRegistration"
		location="com.olbius.basehr.timeMgr.services.TimeManagerServices" auth="true">
		<required-permissions join-type="AND">
			<check-permission permission="HR_TIMEMGR" action="_APPROVE"/>
		</required-permissions>
		<auto-attributes mode="IN" include="pk" optional="false"></auto-attributes>
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
		<override name="reasonApproval" allow-html="any"></override>
		<override name="statusId" optional="false"></override>
	</service>
	<service name="approvalWorkingLateRegister" engine="java" invoke="approvalWorkingLateRegister" default-entity-name="WorkingLateRegister"
		location="com.olbius.basehr.timeMgr.services.TimeManagerServices" auth="true">
		<required-permissions join-type="AND">
			<check-permission permission="HR_TIMEMGR" action="_APPROVE"/>
		</required-permissions>
		<auto-attributes mode="IN" include="pk" optional="false"></auto-attributes>
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
		<override name="reasonReject" allow-html="any"></override>
		<override name="statusId" optional="false"></override>
	</service>
	<service name="JQGetWorkingLateRegister" engine="java" invoke="getWorkingLateRegisterJQ" auth="true"
		location="com.olbius.basehr.timeMgr.services.TimeManagerServices">
		<required-permissions join-type="AND">
			<check-permission permission="HR_TIMEMGR" action="_VIEW"/>
		</required-permissions>
		<implements service="jqxGetListInterface"/>	
	</service>
</services>        