		<attribute name="listAllConditions" mode="IN" type="java.util.List" optional="false"></attribute>
		<attribute name="listSortFields" mode="IN" type="java.util.List" optional="false"></attribute>
		<attribute name="opts" mode="IN" type="org.ofbiz.entity.util.EntityFindOptions" optional="false"></attribute>
		<attribute name="parameters" mode="IN" type="java.util.Map" optional="false"></attribute>
		<attribute name="request" mode="IN" type="javax.servlet.http.HttpServletRequest" optional="false"></attribute>
		<attribute name="listIterator" mode="OUT" type="List" optional="false"></attribute>
		<attribute name="TotalRows" mode="OUT" type="String" optional="true"></attribute>
    </service>    
	<service name="JQgetListEmplLeave" engine="java" invoke="getListEmplLeaveJQ" 
   		location="com.olbius.basehr.timeMgr.services.TimeManagerServices" auth="true">
		<attribute name="listAllConditions" mode="IN" type="java.util.List" optional="false"></attribute>
		<attribute name="listSortFields" mode="IN" type="java.util.List" optional="false"></attribute>
		<attribute name="opts" mode="IN" type="org.ofbiz.entity.util.EntityFindOptions" optional="false"></attribute>
		<attribute name="parameters" mode="IN" type="java.util.Map" optional="false"></attribute>
		<attribute name="request" mode="IN" type="javax.servlet.http.HttpServletRequest" optional="true"></attribute>
		<attribute name="listIterator" mode="OUT" type="List" optional="false"></attribute>
		<attribute name="TotalRows" mode="OUT" type="String" optional="true"></attribute>
