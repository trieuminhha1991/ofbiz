<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>hrolbius Services</description>
    <vendor></vendor>
    <version>1.0</version>

	<service name="createOrgUnit" engine="java" invoke="createOrgUnit" auth="true"
		location="com.olbius.basehr.organization.services.OrganizationServices" default-entity-name="PartyGroup">
		<auto-attributes mode="INOUT" include="pk" optional="true"/>
        <auto-attributes mode="IN" include="nonpk" optional="true"/>
        <attribute name="partyCode" mode="IN" type="String"></attribute>
        <attribute name="partyTypeId" type="String" mode="IN" optional="true"/>
        <attribute name="description" type="String" mode="IN" optional="true"/>
        <attribute name="preferredCurrencyUomId" type="String" mode="IN" optional="true"/>
        <attribute name="externalId" type="String" mode="IN" optional="true"/>
        <attribute name="statusId" type="String" mode="IN" optional="true"/>
        <attribute name="address1" mode="IN" type="String" optional="true"></attribute>
        <attribute name="countryGeoId" mode="IN" type="String" optional="true"></attribute>
        <attribute name="stateProvinceGeoId" mode="IN" type="String" optional="true"></attribute>
        <attribute name="districtGeoId" mode="IN" type="String" optional="true"></attribute>
        <attribute name="wardGeoId" mode="IN" type="String" optional="true"></attribute>
        <attribute name="parentPartyGroupId" mode="IN" type="String"></attribute>
        <attribute name="partyRoleTypeId" mode="IN" type="String" optional="false"></attribute>
        <attribute name="uploadedFile" type="java.nio.ByteBuffer" mode="IN" optional="true"/>
		<attribute name="_uploadedFile_fileName" type="String" mode="IN" optional="true"/>
        <attribute name="_uploadedFile_contentType" type="String" mode="IN" optional="true"/>
        <override name="groupName" optional="false"/>
        <override name="comments" allow-html="any"/>	
	</service>
	<service name="assignSecGroupManager" engine="java" auth="true"
		invoke="assignSecGroupManager" location="com.olbius.basehr.organization.services.OrganizationServices">
		<attribute name="managerId" mode="IN" type="String"></attribute>
		<attribute name="title" mode="IN" type="String"></attribute>	
	</service>
	<service name="CreateMgrForOrg" engine="java" auth="true"
		invoke="CreateMgrForOrg" location="com.olbius.basehr.organization.services.OrganizationServices">
		<attribute name="managerId" mode="IN" type="String"></attribute>
		<attribute name="roleTypeId" mode="IN" type="String"></attribute>
		<attribute name="orgId" mode="IN" type="String"></attribute>	
		<attribute name="title" mode="IN" type="String"></attribute>
	</service>
	<service name="getOrganizationUnit" engine="java" invoke="getOrganizationUnit" auth="true"
		location="com.olbius.basehr.organization.services.OrganizationServices">
		<attribute name="partyId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="listReturn" mode="OUT" type="List"></attribute>	
	</service>
	
</services>