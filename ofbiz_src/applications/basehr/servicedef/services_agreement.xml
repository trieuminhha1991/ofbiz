<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
	<service name="JQListEmplAgreement" engine="java" invoke="getListEmplAgreement" auth="true"
		location="com.olbius.basehr.employment.services.AgreementServices">
		<implements service="jqxGetListInterface"/>	
   		<attribute name="request" mode="IN" type="javax.servlet.http.HttpServletRequest"></attribute>
	</service>    
	<service name="getAgreementEffectivePartyInPeriod" engine="java" invoke="getAgreementEffectivePartyInPeriod" auth="true"
		location="com.olbius.basehr.employment.services.AgreementServices">
		<required-permissions join-type="AND">
			<check-permission permission="HR_AGREEMENT" action="_VIEW"/>
		</required-permissions>
		<attribute name="partyIdTo" mode="IN" type="String"></attribute>
		<attribute name="fromDate" mode="IN" type="java.sql.Timestamp"></attribute>
		<attribute name="thruDate" mode="IN" type="java.sql.Timestamp" optional="true"></attribute>
		<attribute name="agreementTypeId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="agreementCode" mode="OUT" type="String" optional="true"></attribute>
		<attribute name="agreementId" mode="OUT" type="String" optional="true"></attribute>
	</service>
	
	<service name="uploadedFileAgreement" engine="java" invoke="uploadedFileAgreement" auth="true"
		location="com.olbius.basehr.employment.services.AgreementServices">
		<required-permissions join-type="AND">
			<check-permission permission="HR_AGREEMENT" action="_ADMIN"/>
		</required-permissions>
		<attribute name="uploadedFile" type="java.nio.ByteBuffer" mode="IN"/>
		<attribute name="_uploadedFile_fileName" type="String" mode="IN"/>
        <attribute name="_uploadedFile_contentType" type="String" mode="IN"/>
        <attribute name="contentId" mode="OUT" type="String"></attribute>
        <attribute name="path" mode="OUT" type="String"></attribute>
	</service>        
	      
	<service name="preCreateEmploymentAgreement" engine="java" invoke="preCreateEmploymentAgreement" auth="true"
		location="com.olbius.basehr.employment.services.AgreementServices">
		<required-permissions join-type="AND">
			<check-permission permission="HR_AGREEMENT" action="_ADMIN"/>
		</required-permissions>
        <attribute name="agreementTypeId" mode="IN" type="String"></attribute>
        <attribute name="agreementDate" mode="IN" type="String"></attribute>
        <attribute name="partyIdFrom" mode="IN" type="String"></attribute>
        <attribute name="partyIdTo" mode="IN" type="String"></attribute>
        <attribute name="roleTypeIdFrom" mode="IN" type="String" optional="true"></attribute>
        <attribute name="roleTypeIdTo" mode="IN" type="String" optional="true"></attribute>
        <attribute name="fromDate" mode="IN" type="String"></attribute>
        <attribute name="thruDate" mode="IN" type="String" optional="true"></attribute>
        <attribute name="description" mode="IN" type="String" optional="true"></attribute>
        <attribute name="agreementCode" mode="IN" type="String"></attribute>
        <attribute name="agreementDuration" mode="IN" type="String" optional="true"></attribute>
        <attribute name="payRate" mode="IN" type="java.math.BigDecimal" optional="true"></attribute>
        <attribute name="insuranceSalary" mode="IN" type="java.math.BigDecimal" optional="true"></attribute>
        <attribute name="basicSalary" mode="IN" type="java.math.BigDecimal" optional="true"></attribute>
        <attribute name="partyIdRep" mode="IN" type="String" optional="true"></attribute>
        <attribute name="allowance" mode="IN" type="String" optional="true"></attribute>
		<attribute name="listContentId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="emplPositionTypeId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="workPlace" mode="IN" type="String" optional="true"></attribute>
	</service>
	
	<service name="preUpdateEmploymentAgreement" engine="java" invoke="preUpdateEmploymentAgreement" auth="true"
		location="com.olbius.basehr.employment.services.AgreementServices">
		<required-permissions join-type="AND">
			<check-permission permission="HR_AGREEMENT" action="_ADMIN"/>
		</required-permissions>
        <attribute name="agreementTypeId" mode="IN" type="String"></attribute>
        <attribute name="agreementId" mode="IN" type="String"></attribute>
        <attribute name="agreementDate" mode="IN" type="String"></attribute>
        <attribute name="partyIdFrom" mode="IN" type="String"></attribute>
        <attribute name="partyIdTo" mode="IN" type="String"></attribute>
        <attribute name="roleTypeIdFrom" mode="IN" type="String" optional="true"></attribute>
        <attribute name="roleTypeIdTo" mode="IN" type="String" optional="true"></attribute>
        <attribute name="fromDate" mode="IN" type="String"></attribute>
        <attribute name="thruDate" mode="IN" type="String" optional="true"></attribute>
        <attribute name="description" mode="IN" type="String" optional="true"></attribute>
        <attribute name="agreementCode" mode="IN" type="String"></attribute>
        <attribute name="agreementDuration" mode="IN" type="String" optional="true"></attribute>
        <attribute name="payRate" mode="IN" type="java.math.BigDecimal" optional="true"></attribute>
        <attribute name="insuranceSalary" mode="IN" type="java.math.BigDecimal" optional="true"></attribute>
        <attribute name="basicSalary" mode="IN" type="java.math.BigDecimal" optional="true"></attribute>
        <attribute name="partyIdRep" mode="IN" type="String" optional="true"></attribute>
        <attribute name="allowance" mode="IN" type="String" optional="true"></attribute>
		<attribute name="listContentId" mode="IN" type="String" optional="true"></attribute>
		 <attribute name="emplPositionTypeId" mode="IN" type="String" optional="true"></attribute>
        <attribute name="workPlace" mode="IN" type="String" optional="true"></attribute>
	</service>
	
	<service name="createAgreementContent" engine="java" invoke="createAgreementContent" auth="true"
		location="com.olbius.basehr.employment.services.AgreementServices" default-entity-name="AgreementContent">
		<auto-attributes mode="IN" include="pk"></auto-attributes>	
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>	
	</service>
		      
	<service name="createEmploymentAgreement" engine="java" invoke="createEmploymentAgreement" auth="true"
		location="com.olbius.basehr.employment.services.AgreementServices" default-entity-name="Agreement">
		<required-permissions join-type="AND">
			<check-permission permission="HR_AGREEMENT" action="_ADMIN"/>
		</required-permissions>
		<auto-attributes include="nonpk" mode="IN" optional="true"/>
        <auto-attributes include="pk" mode="OUT" optional="true"/>
        <attribute name="allowance" mode="IN" type="String" optional="true"></attribute>
        <attribute name="insuranceSalary" mode="IN" type="java.math.BigDecimal" optional="true"></attribute>
        <attribute name="basicSalary" mode="IN" type="java.math.BigDecimal" optional="true"></attribute>
        <attribute name="agreementDuration" mode="IN" type="String" optional="true"></attribute>
        <attribute name="payRate" mode="IN" type="java.math.BigDecimal" optional="true"></attribute>
        <attribute name="emplPositionTypeId" mode="IN" type="String" optional="true"></attribute>
        <attribute name="workPlace" mode="IN" type="String" optional="true"></attribute>
        <override name="fromDate" optional="false"></override>
        <override name="agreementTypeId" optional="false"></override>	
        <override name="partyIdFrom" optional="false"></override>	
        <override name="partyIdTo" optional="false"></override>	
	</service>  
	
	<service name="updateEmploymentAgreement" engine="java" invoke="updateEmploymentAgreement" auth="true"
		location="com.olbius.basehr.employment.services.AgreementServices" default-entity-name="Agreement">
		<required-permissions join-type="AND">
			<check-permission permission="HR_AGREEMENT" action="_ADMIN"/>
		</required-permissions>
		<auto-attributes include="nonpk" mode="IN" optional="true"/>
        <auto-attributes include="pk" mode="IN" optional="false"/>
        <attribute name="allowance" mode="IN" type="String" optional="true"></attribute>
        <attribute name="insuranceSalary" mode="IN" type="java.math.BigDecimal" optional="true"></attribute>
        <attribute name="basicSalary" mode="IN" type="java.math.BigDecimal" optional="true"></attribute>
        <attribute name="agreementDuration" mode="IN" type="String" optional="true"></attribute>
        <attribute name="payRate" mode="IN" type="java.math.BigDecimal" optional="true"></attribute>
        <attribute name="emplPositionTypeId" mode="IN" type="String" optional="true"></attribute>
        <attribute name="workPlace" mode="IN" type="String" optional="true"></attribute>
        <override name="fromDate" optional="false"></override>
        <override name="agreementTypeId" optional="false"></override>	
        <override name="partyIdFrom" optional="false"></override>	
        <override name="partyIdTo" optional="false"></override>	
	</service>  
	<service name="deleteAgreementContent" engine="java" invoke="deleteAgreementContent" auth="true"
		location="com.olbius.basehr.employment.services.AgreementServices" default-entity-name="AgreementContent">
		<required-permissions join-type="AND">
			<check-permission permission="HR_AGREEMENT" action="_ADMIN"/>
		</required-permissions>
		<auto-attributes mode="IN" include="pk"></auto-attributes>	
	</service>
	<service name="getAgreementContent" engine="java" invoke="getAgreementContent" auth="true"
		location="com.olbius.basehr.employment.services.AgreementServices">
		<required-permissions join-type="AND">
			<check-permission permission="HR_AGREEMENT" action="_VIEW"/>
		</required-permissions>
		<attribute name="agreementId" mode="IN" type="String"></attribute>
		<attribute name="listReturn" mode="OUT" type="List" optional="true"></attribute>
	</service>
	<service name="getAgreementAllowance" engine="java" invoke="getAgreementAllowance" auth="true"
		location="com.olbius.basehr.employment.services.AgreementServices">
		<required-permissions join-type="AND">
			<check-permission permission="HR_AGREEMENT" action="_VIEW"/>
		</required-permissions>
		<attribute name="agreementId" mode="IN" type="String"></attribute>
		<attribute name="listReturn" mode="OUT" type="List" optional="true"></attribute>
	</service>
	<service name="getAgreementChangeStatus" engine="java" invoke="getAgreementChangeStatus" auth="true"
		location="com.olbius.basehr.employment.services.AgreementServices">
		<required-permissions join-type="AND">
			<check-permission permission="HR_AGREEMENT" action="_ADMIN"/>
		</required-permissions>
		<attribute name="partyIdTo" mode="IN" type="String"></attribute>
		<attribute name="agreementId" mode="IN" type="String"></attribute>
		<attribute name="fromDate" mode="IN" type="java.sql.Timestamp"></attribute>
		<attribute name="agreementIdChangeStt" mode="OUT" type="String" optional="true"></attribute>
		<attribute name="statusIdChange" mode="OUT" type="String" optional="true"></attribute>
		<attribute name="statusIdUpdate" mode="OUT" type="String" optional="true"></attribute>
	</service>
	<service name="createAgrDuration" engine="java" invoke="createAgrDuration" auth="true"
	    location="com.olbius.basehr.employment.services.AgreementServices" default-entity-name="PeriodType">
	    <required-permissions join-type="AND">
	        <check-permission permission="HR_AGREEMENT" action="_ADMIN"/>
	    </required-permissions>
	  <auto-attributes mode="OUT" include="pk" optional="false"></auto-attributes>
	    <auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
	    <override name="periodLength" optional="false"></override>
	    <override name="uomId" optional="false"></override>
	    <override name="description" optional="false"></override>
	</service>
	<service name="getAgrDurationList" engine="java" invoke="getAgrDurationList" auth="true"
	    location="com.olbius.basehr.employment.services.AgreementServices">
	    <attribute name="listReturn" mode="OUT" type="java.util.List"></attribute>
	</service>  
</services>        