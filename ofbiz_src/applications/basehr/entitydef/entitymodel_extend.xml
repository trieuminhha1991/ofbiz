<?xml version="1.0" encoding="UTF-8"?>
<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
	<!-- ========================================================= -->
	<!-- ======================== Defaults ======================= -->
	<!-- ========================================================= -->
	<title>Extend for existed entity</title>
	<description>None</description>
	<copyright></copyright>
	<version></version>
	
	<extend-entity entity-name="PartyBenefit">
		<field name="code" type="id-ne"></field>
		<field name="orgId" type="id-ne"></field>
		<relation rel-entity-name="PayrollEmplParameters" type="one" fk-name="PB_PEP">
			<key-map field-name="partyIdTo" rel-field-name="partyId"/>
			<key-map field-name="orgId"/>
			<key-map field-name="code"/>
			<key-map field-name="fromDate"/>
		</relation>
	</extend-entity>
	<extend-entity entity-name="PostalAddress">
		<!--<field name="districtGeoId" type="id"></field>-->
		<field name="wardGeoId" type="id"></field>
		<!--<relation type="one" fk-name="POST_ADDR_CGEO_NEW1" title="Country" rel-entity-name="Geo">-->
			<!--<key-map field-name="districtGeoId" rel-field-name="geoId"/>-->
		<!--</relation>-->
		<relation type="one" fk-name="POST_ADDR_SPGEO_NEW1" title="ward" rel-entity-name="Geo">
			<key-map field-name="wardGeoId" rel-field-name="geoId"/>
		</relation>
	</extend-entity>
	<extend-entity entity-name="EmplPositionType">
		<field name="dayLeaveRegulation" type="numeric"></field>
	</extend-entity>
	<extend-entity entity-name="OldEmplPositionTypeRate">
		<field name="rateAmount" type="currency-amount"></field>
		<field name="rateCurrencyUomId" type="id-ne"/>
		<relation type="one" fk-name="OLD_RATE_AMOUNT_RCT" rel-entity-name="Uom">
	        <key-map field-name="rateCurrencyUomId" rel-field-name="uomId"/>
	      </relation>
	</extend-entity>
	<extend-entity entity-name="Geo">
		<field name="codeNumber" type="short-varchar"></field> <!-- for country code and area code -->
	</extend-entity>
	<extend-entity entity-name="AgreementTerm">
		<field name="emplPositionId" type="id-ne"></field>
		<relation rel-entity-name="EmplPosition" type="one" fk-name="FK_AGREEMENT_TERM_POSID">
			<key-map field-name="emplPositionId"/>
		</relation>
	</extend-entity>
	<extend-entity entity-name="TermTypeAttr">
		<field name="attrValue" type="very-long"></field>
	</extend-entity>
	<extend-entity entity-name="TerminationReason">
		<field name="parentTypeId" type="id-ne"></field>
		<relation rel-entity-name="TerminationReason" type="one" fk-name="TERM_REASON_PARENT">
			<key-map field-name="parentTypeId" rel-field-name="terminationReasonId"/>
		</relation>
	</extend-entity>
	
	<extend-entity entity-name="Person" >
		<field name="idNumber" type="long-varchar"></field>
		<field name="idIssueDate" type="date"></field>
		<field name="idIssuePlace" type="id-ne"></field>
		<field name="maritalStatusId" type="id" ></field>
		<field name="ethnicOrigin" type="id-ne"></field>
		<field name="religion" type="id-ne"></field>
		<field name="nationality" type="id-ne"></field>
		<field name="numberChildren" type="numeric"></field>
		<field name="nativeLand" type="long-varchar"></field>
		<field name="birthPlace" type="long-varchar"></field>
		<field name="insuranceSocialNbr" type="long-varchar"></field>
		<field name="workingStatusId" type="id-ne"></field>
		<field name="dateParticipateIns" type="date"></field>
		<field name="educationSystemTypeId" type="id-ne"></field>
		<field name="majorId" type="id-ne"></field>
		<field name="classificationTypeId" type="id-ne"></field>
		<field name="graduationYear" type="numeric"></field>
		<field name="probationaryDeadline" type="date"><description>thoi han thu viec</description></field>
		<relation rel-entity-name="StatusItem" type="one" title="maritalStatus" fk-name="PER_MARITAL_STATUS_ITEM">
			<key-map field-name="maritalStatusId" rel-field-name="statusId"/>
		</relation>
		<relation rel-entity-name="EthnicOrigin" type="one" fk-name="FK_PER_ETHNIC">
			<key-map field-name="ethnicOrigin" rel-field-name="ethnicOriginId"/>
		</relation>
		<relation rel-entity-name="Religion" type="one" fk-name="FK_RELIGION_NEW2">
			<key-map field-name="religion" rel-field-name="religionId"/>
		</relation>
		<relation rel-entity-name="Nationality" type="one" fk-name="FK_NATIONALITY_NEW2">
			<key-map field-name="nationality" rel-field-name="nationalityId"/>
		</relation>
		<relation rel-entity-name="Geo" type="one" fk-name="PER_ID_NBR_GEO">
			<key-map field-name="idIssuePlace" rel-field-name="geoId"/>
		</relation>
		<relation rel-entity-name="StatusItem" type="one" title="workingStatus" fk-name="PER_WORK_STATUSITEM">
			<key-map field-name="workingStatusId" rel-field-name="statusId"/>
		</relation>
		<relation rel-entity-name="EducationSystemType" type="one" fk-name="PER_EDUSYSTYP">
			<key-map field-name="educationSystemTypeId"/>
		</relation>
		<relation rel-entity-name="Major" type="one" fk-name="PER_MAJOR">
			<key-map field-name="majorId"/>
		</relation>
		<relation rel-entity-name="DegreeClassificationType" type="one" fk-name="PER_DEG_CLASS_TYP">
			<key-map field-name="classificationTypeId"/>
		</relation>
	</extend-entity>
</entitymodel>