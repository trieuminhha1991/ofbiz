<?xml version="1.0" encoding="UTF-8"?>
<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
	<entity entity-name="TrainingCourse" package-name="com.olbius.hrolbius">
		<field name="trainingCourseId" type="id-ne"></field>
		<field name="trainingCourseCode" type="id-ne"></field>
		<field name="trainingCourseName" type="short-varchar"></field>
		<!-- <field name="estimatedFromDate" type="date-time"></field>
		<field name="estimatedThruDate" type="date-time"></field> -->
		<field name="actualFromDate" type="date-time"><description>ngay bat dau thuc te</description></field>
		<field name="actualThruDate" type="date-time"><description>ngay ket thuc thuc te</description></field>
		<field name="fromDate" type="date-time"><description>ngay bat dau du kien</description></field>
		<field name="thruDate" type="date-time"><description>ngay ket thuc du kien</description></field>
		<field name="registerFromDate" type="date-time"></field>
		<field name="registerThruDate" type="date-time"></field>
		<field name="description" type="description"></field>
		<field name="estimatedNumber" type="numeric"></field>
		<!-- <field name="actualNumber" type="numeric"></field> -->
		<field name="trainingTypeId" type="id-ne"></field>
		<field name="trainingFormTypeId" type="id-ne"></field>
		<field name="location" type="long-varchar"></field>
		<field name="statusId" type="id-ne"></field>
		<field name="estimatedEmplPaid" type="currency-amount"></field>
		<field name="actualEmplPaid" type="currency-amount"></field>
		<field name="amountCompanySupport" type="currency-amount"></field>
		<field name="actualAmountCompanySup" type="currency-amount"><description>So tien cong ty ho tro thuc te</description></field>
		<field name="uomId" type="id-ne"></field>
		<field name="certificate" type="long-varchar"></field>
		<field name="providerId" type="id-ne"><description>doi tac dao tao</description></field>
		<field name="isCompulsory" type="indicator"></field>
		<field name="isPublic" type="indicator"></field>
		<field name="isCancelRegister" type="indicator"></field>
		<field name="cancelBeforeDay" type="numeric"></field>
		<field name="createdDate" type="date-time"></field>
		<prim-key field="trainingCourseId"/>
		<!-- <relation rel-entity-name="Geo" type="one" fk-name="TRAINING_COUR_GEO">
			<key-map field-name="geoId"/>
		</relation> -->
		<relation rel-entity-name="TrainingFormType" type="one" fk-name="TRAINING_COUR_FORM">
			<key-map field-name="trainingFormTypeId"/>
		</relation>
		<relation rel-entity-name="StatusItem" type="one" fk-name="TRAINING_COUR_STTS">
			<key-map field-name="statusId"/>
		</relation>
		<relation rel-entity-name="Party" type="one" fk-name="TRAIN_PROVIDER_PTY">
			<key-map field-name="providerId" rel-field-name="partyId"/>
		</relation>
		<relation rel-entity-name="Uom" type="one" fk-name="TRAINING_COUR_UOM">
			<key-map field-name="uomId"/>
		</relation>
	</entity>
	
	<entity entity-name="TrainingCoursePartyAttendance" package-name="com.olbius.hrolbius">
		<field name="trainingCourseId" type="id-ne"></field>
		<field name="partyId" type="id-ne"></field>
		<field name="statusId" type="id-ne"></field>
		<field name="resultTypeId" type="id-ne"></field>
		<field name="statusIdRegister" type="id-ne">
			<description>trang thai dang ky khoa dao tao cua nhan vien</description>
		</field>
		<field name="employeeAmount" type="currency-amount"><description>so tien nhan vien phai dong</description></field>
		<field name="employerAmount" type="currency-amount"><description>so tien cong ty ho tro</description></field>
		<field name="employeePaid" type="currency-amount"><description>so tien nhan vien da dong</description></field>
		<field name="isExpectedAttend" type="boolean"><description>Nhan vien du kien tham gia</description></field>
		<field name="isRegister" type="boolean"><description>Nhan vien dang ky tham gia</description></field>
		<field name="dateRegisted" type="date-time"><description>ngay nhan vien dang ky khoa hoc</description></field>
		<field name="comment" type="comment"></field>
		<field name="createdDate" type="date-time"></field>
		<prim-key field="trainingCourseId"/>
		<prim-key field="partyId"/>
		<relation rel-entity-name="TrainingCourse" type="one" fk-name="TCOURPTYATT_TCOUR">
			<key-map field-name="trainingCourseId"/>
		</relation>
		<relation rel-entity-name="Party" type="one" fk-name="TCOURPTYATT_PTY">
			<key-map field-name="partyId"/>
		</relation>
		<relation rel-entity-name="TrainingResultType" type="one" fk-name="TCOURPTYATT_RTYP">
			<key-map field-name="resultTypeId"/>
		</relation>
		<relation rel-entity-name="StatusItem" title="status" type="one" fk-name="TCOURPTYATT_STT">
			<key-map field-name="statusId"/>
		</relation>
		<relation rel-entity-name="StatusItem" title="register" type="one" fk-name="TCPTYREGIS_STT">
			<key-map field-name="statusIdRegister" rel-field-name="statusId"/>
		</relation>
	</entity>
	
	<entity entity-name="TrainingCoursePurpose" package-name="com.olbius.hrolbius">
		<field name="trainingCourseId" type="id-ne"></field>
		<field name="trainingPurposeTypeId" type="id-ne"></field>
		<prim-key field="trainingCourseId"/>
		<prim-key field="trainingPurposeTypeId"/>
		<relation rel-entity-name="TrainingCourse" type="one" fk-name="TRAINING_COUR_PUR_COUR">
			<key-map field-name="trainingCourseId"/>
		</relation>	
		<relation rel-entity-name="TrainingPurposeType" type="one" fk-name="TRAINING_COUR_PUR_TYPR">
			<key-map field-name="trainingPurposeTypeId"/>
		</relation>
	</entity>
	
	<entity entity-name="TrainingFormType" package-name="org.ofbiz.hrolbius">
		<field name="trainingFormTypeId" type="id-ne"></field>
		<field name="parentTypeId" type="id-ne"></field>
		<field name="description" type="description"></field>
		<prim-key field="trainingFormTypeId"/>
		<relation rel-entity-name="TrainingFormType" type="one" fk-name="TRAINING_FORM_TYP_PARENT">
			<key-map field-name="parentTypeId" rel-field-name="trainingFormTypeId"/>
		</relation>
	</entity>
	<entity entity-name="TrainingPurposeType" package-name="com.olbius.hrolbius">
		<field name="trainingPurposeTypeId" type="id-ne"></field>
		<field name="parentTypeId" type="id-ne"></field>
		<field name="description" type="description"></field>
		<prim-key field="trainingPurposeTypeId"/>
		<relation rel-entity-name="TrainingPurposeType" type="one" fk-name="TRAINING_PURPOSE_PARENT_TYP">
			<key-map field-name="parentTypeId" rel-field-name="trainingPurposeTypeId"/>
		</relation>
	</entity>
	<entity entity-name="TrainingCourseSkillType" package-name="com.olbius.hrolbius">
		<field name="trainingCourseId" type="id-ne"></field>
		<field name="skillTypeId" type="id-ne"></field>
		<field name="resultTypeId" type="id-ne"></field>
		<prim-key field="trainingCourseId"/>
		<prim-key field="skillTypeId"/>
		<relation rel-entity-name="TrainingCourse" type="one" fk-name="TRAINSKILL_TRAINGCOURS">
			<key-map field-name="trainingCourseId"/>
		</relation>
		<relation rel-entity-name="SkillType" type="one" fk-name="TRAINCOURS_SKILLTYP">
			<key-map field-name="skillTypeId"/>
		</relation>
		<relation rel-entity-name="TrainingResultType" type="one" fk-name="TRAINCOURSSKILL_RS">
			<key-map field-name="resultTypeId"/>
		</relation>
	</entity>
	<entity entity-name="TrainingResultType" package-name="com.olbius.hrolbius">
		<field name="resultTypeId" type="id-ne"></field>
		<field name="parentTypeId" type="id-ne"></field>
		<field name="sequence" type="numeric"></field>
		<field name="description" type="id-ne"></field>
		<prim-key field="resultTypeId"/>
		<relation rel-entity-name="TrainingResultType" type="one" fk-name="TRAING_RES_TYP_PART">
			<key-map field-name="parentTypeId" rel-field-name="resultTypeId"/>
		</relation>
	</entity>
	<entity entity-name="TrainingCourseStatus" package-name="com.olbius.hrolbius">
		<field name="trainingCourseStatusId" type="id-ne"></field>
		<field name="trainingCourseId" type="id-ne"></field>
		<field name="statusId" type="id-ne"></field>
		<field name="statusDatetime" type="date-time"></field>
		<field name="statusUserLogin" type="id-ne"></field>
		<field name="changeReason" type="description"></field>
		<prim-key field="trainingCourseStatusId"/>
		<relation rel-entity-name="TrainingCourse" type="one" fk-name="TRAIN_COURS_STT_TC">
			<key-map field-name="trainingCourseId"/>
		</relation>
		<relation rel-entity-name="StatusItem" type="one" fk-name="TRAIN_COURS_STT_STT">
			<key-map field-name="statusId"/>
		</relation>
		<relation rel-entity-name="UserLogin" type="one" fk-name="TRAIN_COURS_STT_USL">
			<key-map field-name="statusUserLogin" rel-field-name="userLoginId"/>
		</relation>
	</entity>
</entitymodel>	