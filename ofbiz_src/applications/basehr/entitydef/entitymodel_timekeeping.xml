<?xml version="1.0" encoding="UTF-8"?>
<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
	<entity entity-name="TimekeepingDetail" package-name="com.olbius.hr">
		<field name="timekeepingDetailId" type="id-ne"></field>
		<field name="timekeepingDetailName" type="description"></field>
		<field name="partyId" type="id-ne"></field>
		<field name="timesheetDetailEnumId" type="id-ne"></field>
		<field name="month" type="numeric"></field>
		<field name="year" type="numeric"></field>
		<field name="fromDate" type="date-time"></field>
		<field name="thruDate" type="date-time"></field>
		<field name="statusId" type="id-ne"></field>
		<field name="createdDate" type="date-time"></field>
		<field name="workingShiftId" type="id-ne"></field>
		<prim-key field="timekeepingDetailId"/>
		<relation rel-entity-name="Party" type="one" fk-name="EMPL_TS_DETAIL_PTY">
			<key-map field-name="partyId"/>
		</relation>
		<relation rel-entity-name="StatusItem" type="one" fk-name="EMPL_TS_DETAIL_STT">
			<key-map field-name="statusId"/>
		</relation>
		<relation rel-entity-name="WorkingShift" type="one" fk-name="EMPL_TS_DETAIL_">
			<key-map field-name="workingShiftId"/>
		</relation>
	</entity>
	<entity entity-name="TimekeepingDetailParty" package-name="com.olbius.hr">
		<field name="timekeepingDetailId" type="id-ne"></field>
		<field name="partyId" type="id-ne"></field>
		<field name="dateTimekeeping" type="date"></field>
		<field name="workdayActual" type="floating-point">
			<description>ngay cong di lam</description>
		</field>
		<field name="workdayStandard" type="floating-point">
			<description>ngay cong chuan theo quy dinh</description>
		</field>
		<field name="workdayLeavePaid" type="floating-point">
			<description>ngay cong nghi phep duoc huong luong</description>
		</field>
		<field name="regulationHours" type="floating-point">
			<description>so gio lam duoc huong luong 100%</description>
		</field>
		<field name="overtimeHours" type="floating-point">
			<description>so gio lam them - duoc huong luong > 100%</description>
		</field>
		<field name="overtimeEnumId" type="id-ne">
			<description>enumId: lam them ngay thuong, lam them ngay le, lam them ngay nghi</description>
		</field>
		<field name="workingLateMinutes" type="numeric">
			<description>Tong so phut di muon</description>
		</field>
		<prim-key field="timekeepingDetailId"/>
		<prim-key field="partyId"/>
		<prim-key field="dateTimekeeping"/>
		<relation rel-entity-name="Party" type="one" fk-name="TIME_KEEPING_PTY_PTY">
			<key-map field-name="partyId"/>
		</relation>
		<relation rel-entity-name="TimekeepingDetail" type="one" fk-name="TIME_KEEPING_PTY_TKD">
			<key-map field-name="timekeepingDetailId"/>
		</relation>
		<relation rel-entity-name="Enumeration" type="one" fk-name="TIME_KEEPING_PTY_ENUM">
			<key-map field-name="overtimeEnumId" rel-field-name="enumId"/>
		</relation>
	</entity>
	<entity entity-name="TimekeepingSummary" package-name="com.olbius.hr">
		<field name="timekeepingSummaryId" type="id-ne"></field>
		<field name="timekeepingSummaryName" type="description"></field>
		<field name="partyId" type="id-ne"></field>
		<!-- <field name="timekeepingDetailId" type="id-ne"></field> -->
		<field name="month" type="numeric"></field>
		<field name="year" type="numeric"></field>
		<field name="fromDate" type="date-time"></field>
		<field name="thruDate" type="date-time"></field>
		<field name="statusId" type="id-ne"></field>
		<prim-key field="timekeepingSummaryId"/>
		<relation rel-entity-name="Party" type="one" fk-name="TK_SUM_PTY">
			<key-map field-name="partyId"/>
		</relation>
		<relation rel-entity-name="StatusItem" type="one" fk-name="TK_SUM_STT">
			<key-map field-name="statusId"/>
		</relation>
		<!-- <relation rel-entity-name="TimekeepingDetail" type="one" fk-name="TK_SUM_DETAIL">
			<key-map field-name="timekeepingDetailId"/>
		</relation> -->
	</entity>
	<entity entity-name="TimekeepingSummaryParty" package-name="com.olbius.hr">
		<field name="timekeepingSummaryId" type="id-ne"></field>
		<field name="partyId" type="id-ne"></field>
		<field name="workdayActual" type="floating-point">
			<description>ngay cong thuc te</description>
		</field>
		<field name="workdayLeavePaid" type="floating-point">
			<description>ngay cong nghi phep duoc huong luong</description>
		</field>
		<field name="workdayStandard" type="floating-point">
			<description>ngay cong tieu chuan</description>
		</field>
		<field name="overtimeHoursNormal" type="floating-point"></field>
		<field name="overtimeHoursWeekend" type="floating-point"></field>
		<field name="overtimeHoursHoliday" type="floating-point"></field>
		<field name="totalMinuteLate" type="numeric"></field>
		<field name="totalWorkLate" type="numeric"></field>
		<prim-key field="timekeepingSummaryId"/>
		<prim-key field="partyId"/>
		<relation rel-entity-name="TimekeepingSummary" type="one" fk-name="TK_SUM_PTY_TKSUM">
			<key-map field-name="timekeepingSummaryId"/>
		</relation>
		<relation rel-entity-name="Party" type="one" fk-name="TK_SUM_PTY_PTY">
			<key-map field-name="partyId"/>
		</relation>
	</entity>
	<entity entity-name="TimekeepingSummaryDetail" package-name="com.olbius.hr">
		<field name="timekeepingSummaryId" type="id-ne"></field>
		<field name="timekeepingDetailId" type="id-ne"></field>
		<prim-key field="timekeepingSummaryId"/>
		<prim-key field="timekeepingDetailId"/>
		<relation rel-entity-name="TimekeepingSummary" type="one" fk-name="TKSD_TKS">
			<key-map field-name="timekeepingSummaryId"/>
		</relation>
		<relation rel-entity-name="TimekeepingDetail" type="one" fk-name="TKSD_TKD">
			<key-map field-name="timekeepingDetailId"/>
		</relation>
	</entity>
</entitymodel>	