<?xml version="1.0" encoding="UTF-8"?>
<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
	<entity entity-name="WorkFlowProcess" package-name="com.olbius.hrolbius">
    	<field name="processId" type="id-ne"></field>
    	<field name="processName" type="long-varchar"></field>
    	<prim-key field="processId"/>
    </entity>
    
    <entity entity-name="WorkFlowProcessStatus" package-name="com.olbius.hrolbius">
    	<field name="processStatusId" type="id-long"></field>
    	<field name="processId" type="id-ne"></field>
    	<field name="statusId" type="id-ne"></field>
    	<!-- <field name="statusDateTime" type="date-time"></field>
    	<field name="changeReason" type="long-varchar"></field> -->
    	<field name="description" type="description"></field>
    	<prim-key field="processStatusId"/>
    	<relation rel-entity-name="WorkFlowProcess" type="one" fk-name="WORKFLOWPROC_PROC">
    		<key-map field-name="processId"/>
    	</relation>
    	<relation rel-entity-name="StatusItem" type="one" fk-name="WORKFLOWPROC_STT">
    		<key-map field-name="statusId"/>
    	</relation>
    </entity>
    
    <entity entity-name="WorkFlowGroup" package-name="com.olbius.hrolbius">
    	<field name="groupId" type="id-long"></field>
	<field name="processId" type="id"></field>
    	<prim-key field="groupId"/>
    	<relation rel-entity-name="WorkFlowProcess" type="one" fk-name="WORKFLOW_PROC">
    		<key-map field-name="processId"/>
    	</relation>
    </entity>
    <entity entity-name="WorkFlowGroupMember" package-name="com.olbius.hrolbius">
    	<field name="groupId" type="id-long"></field>
    	<field name="partyId" type="id-ne"></field>
    	<prim-key field="groupId"/>
    	<prim-key field="partyId"/>
    	<relation rel-entity-name="WorkFlowGroup" type="one" fk-name="WORKFLOWGRPMEM_GRP">
    		<key-map field-name="groupId"/>
    	</relation>
    	<relation rel-entity-name="Party" type="one" fk-name="WORKFLOWGRPMEM_PTY">
    		<key-map field-name="partyId"/>
    	</relation>
    </entity>
    <entity entity-name="WorkFlowRequest" package-name="com.olbius.hrolbius">
    	<field name="requestId" type="id-long"></field>
	<field name="processId" type="id"></field>
    	<field name="description" type="description"></field>
    	<field name="dateRequested" type="date-time"></field>
    	<field name="partyId" type="id-ne"></field>
    	<field name="processStatusId" type="id-long"></field>
    	<prim-key field="requestId"/>
    	<relation rel-entity-name="WorkFlowProcess" type="one" fk-name="WORKFLOWREQ_PROC">
    		<key-map field-name="processId"/>
    	</relation>
    	<relation rel-entity-name="Party" type="one" fk-name="WORKFLOW_REQ_PTY">
    		<key-map field-name="partyId"/>
    	</relation>
    	<relation rel-entity-name="WorkFlowProcessStatus" type="one" fk-name="WFREQPROC_STT">
    		<key-map field-name="processStatusId"/>
    	</relation>
    </entity>
    <entity entity-name="WorkFlowRequestStakeHolder" package-name="com.olbius.hrolbius">
    	<field name="requestId" type="id-long"></field>
    	<field name="partyId" type="id-ne"></field>
    	<prim-key field="requestId"/>
    	<prim-key field="partyId"/>
    	<relation rel-entity-name="WorkFlowRequest" type="one" fk-name="WFREQSH_WFREQ">
    		<key-map field-name="requestId"/>
    	</relation>
    	<relation rel-entity-name="Party" type="one" fk-name="WFREQSH_PTY">
    		<key-map field-name="partyId"/>
    	</relation>
    </entity>
    
    <entity entity-name="WorkFlowRequestAttr" package-name="com.olbius.hrolbius">
    	<field name="requestId" type="id-long"></field>
    	<field name="attrName" type="long-varchar"></field>
    	<field name="attrValue" type="long-varchar"></field>
    	<prim-key field="requestId"/>
    	<prim-key field="attrName"/>
    	<relation rel-entity-name="WorkFlowRequest" type="one" fk-name="WKFWREQATT_WFREQ">
    		<key-map field-name="requestId"/>
    	</relation>
    </entity>
    
    <entity entity-name="WorkFlowActionType" package-name="com.olbius.hrolbius">
	<field name="actionTypeId" type="id-ne"></field>
	<field name="parentTypeId" type="id"></field>
    	<field name="description" type="description"></field>
    	<prim-key field="actionTypeId"/>
    	<relation rel-entity-name="WorkFlowActionType" type="one" fk-name="WFACTTYP_PRT">
    		<key-map field-name="parentTypeId" rel-field-name="actionTypeId"/>
    	</relation>
    </entity>
    
    <entity entity-name="WorkFlowAction" package-name="com.olbius.hrolbius">
	<field name="actionId" type="id-ne"></field>
	<field name="actionTypeId" type="id"></field>
	<field name="processId" type="id"></field>
    	<field name="name" type="name"></field>
    	<field name="description" type="description"></field>
    	<prim-key field="actionId"/>
    	<relation rel-entity-name="WorkFlowProcess" type="one" fk-name="WORKFLOWACT_PROC">
    		<key-map field-name="processId"/>
    	</relation>
    	<relation rel-entity-name="WorkFlowActionType" type="one" fk-name="WORKFLOWACT_ACTTYP">
    		<key-map field-name="actionTypeId"/>
    	</relation>
    </entity>
    
    <entity entity-name="WorkFlowActionRoleType" package-name="com.olbius.hrolbius">
	<field name="actionRoleTypeId" type="id-ne"></field>
	<field name="actionId" type="id"></field>
    	<field name="groupId" type="id-long"></field>
    	<field name="roleTypeId" type="id-ne"></field>
    	<prim-key field="actionRoleTypeId"/>
    	<relation rel-entity-name="WorkFlowAction" type="one" fk-name="WORKFLOWACT_RTYACT">
    		<key-map field-name="actionId"/>
    	</relation>
    	<relation rel-entity-name="WorkFlowGroup" type="one" fk-name="WORKFLOWACT_RTYGRP">
    		<key-map field-name="groupId"/>
    	</relation>
    	<relation rel-entity-name="RoleType" type="one" fk-name="WORK_FLOW_ACT_RTY_RTY">
    		<key-map field-name="roleTypeId"/>
    	</relation>
    </entity>
    
    <entity entity-name="WorkFlowActivityType" package-name="com.olbius.hrolbius">
	<field name="activityTypeId" type="id-ne"></field>
	<field name="parentTypeId" type="id"></field>
    	<field name="description" type="description"></field>
    	<prim-key field="activityTypeId"/>
    	<relation rel-entity-name="WorkFlowActivityType" type="one" fk-name="WFACTY_TYPPRT">
    		<key-map field-name="parentTypeId" rel-field-name="activityTypeId"/>
    	</relation>
    </entity>
    
    <entity entity-name="WorkFlowActivity" package-name="com.olbius.hrolbius">
	<field name="activityId" type="id-ne"></field>
	<field name="activityTypeId" type="id-ne"></field>
	<field name="processId" type="id"></field>
    	<field name="name" type="name"></field>
    	<field name="description" type="description"></field>
    	<prim-key field="activityId"/>
    	<relation rel-entity-name="WorkFlowActivityType" type="one" fk-name="WF_ACTYTYP">
    		<key-map field-name="activityTypeId"/>
    	</relation>
    	<relation rel-entity-name="WorkFlowProcess" type="one" fk-name="WFACTY_WFPROC">
    		<key-map field-name="processId"/>
    	</relation>
    </entity>
    
    <entity entity-name="WorkFlowStatusActivity" package-name="com.olbius.hrolbius">
    	<field name="processStatusId" type="id-long"></field>
	<field name="activityId" type="id-ne"></field>
    	<prim-key field="processStatusId"/>
    	<prim-key field="activityId"/>
    	<relation rel-entity-name="WorkFlowProcessStatus" type="one" fk-name="WKFWSTTACTY_WFPSTT">
    		<key-map field-name="processStatusId"/>
    	</relation>
    	<relation rel-entity-name="WorkFlowActivity" type="one" fk-name="WKFWSTTACTY_WFACTY">
    		<key-map field-name="activityId"/>
    	</relation>
    </entity>
    
    <entity entity-name="WorkFlowTransition" package-name="com.olbius.hrolbius">
	<field name="transitionId" type="id-ne"></field>
	<field name="processId" type="id"></field>
    	<field name="processStatusId" type="id-long"></field>
    	<field name="nextProcessStatusId" type="id-long"></field>
    	<prim-key field="transitionId"/>
    	<relation rel-entity-name="WorkFlowProcess" type="one" fk-name="WFTRANS_WFPROC">
    		<key-map field-name="processId"/>
    	</relation>
	<relation rel-entity-name="WorkFlowProcessStatus" type="one" title="process" fk-name="WKFWTRANS_WFPROCSTT">
    		<key-map field-name="processStatusId"/>
    	</relation>
	<relation rel-entity-name="WorkFlowProcessStatus" type="one" title="nextProcess" fk-name="WKFWTRANS_WFPROCSTTNEXT">
    		<key-map field-name="nextProcessStatusId" rel-field-name="processStatusId"/>
    	</relation>
    </entity>
    
    <entity entity-name="WorkFlowTransitionAction" package-name="com.olbius.hrolbius">
	<field name="transitionId" type="id-ne"></field>
	<field name="actionId" type="id-ne"></field>
    	<prim-key field="transitionId"/>
    	<prim-key field="actionId"/>
    	<relation rel-entity-name="WorkFlowTransition" type="one" fk-name="WFTRANSACT_TRANS">
    		<key-map field-name="transitionId"/>
    	</relation>
    	<relation rel-entity-name="WorkFlowAction" type="one" fk-name="WFTRANSACT_ACT">
    		<key-map field-name="actionId"/>
    	</relation>
    </entity>
    
    <entity entity-name="WorkFlowActivityAction" package-name="com.olbius.hrolbius">
	<field name="actionId" type="id-ne"></field>
	<field name="activityId" type="id-ne"></field>
    	<prim-key field="actionId"/>
    	<prim-key field="activityId"/>
    	<relation rel-entity-name="WorkFlowAction" type="one" fk-name="WKFWTRANSACTY_ACT">
    		<key-map field-name="actionId"/>
    	</relation>
    	<relation rel-entity-name="WorkFlowActivity" type="one" fk-name="WKFWTRANSACTY_ACTY">
    		<key-map field-name="activityId"/>
    	</relation>
    </entity>
    
    <entity entity-name="WorkFlowActivityRoleType" package-name="com.olbius.hrolbius">
    	<field name="activityRoleTypeId" type="id-long"></field>
	<field name="activityId" type="id"></field>
    	<field name="roleTypeId" type="id-ne"></field>
    	<field name="groupId" type="id-long"></field>
    	<prim-key field="activityRoleTypeId"/>
    	<relation rel-entity-name="RoleType" type="one" fk-name="WKFWACTYRTY_RTY">
    		<key-map field-name="roleTypeId"/>
    	</relation>
    	<relation rel-entity-name="WorkFlowActivity" type="one" fk-name="WKFWACTYTAR_ACTY">
    		<key-map field-name="activityId"/>
    	</relation>
    	<relation rel-entity-name="WorkFlowGroup" type="one" fk-name="WKFWACTYTAR_GRP">
    		<key-map field-name="groupId"/>
    	</relation>
    </entity>
    
    <entity entity-name="WorkFlowRequestAction" package-name="com.olbius.hrolbius">
    	<field name="requestActionId" type="id-long"></field>
    	<field name="requestId" type="id-long"></field>
	<field name="actionId" type="id"></field>
	<field name="transitionId" type="id"></field>
    	<field name="isActive" type="indicator"></field>
    	<field name="isComplete" type="indicator"></field>
    	<prim-key field="requestActionId"/>
    	<relation rel-entity-name="WorkFlowRequest" type="one" fk-name="WFREQACT_WFREQ">
    		<key-map field-name="requestId"/>
    	</relation>
    	<relation rel-entity-name="WorkFlowAction" type="one" fk-name="WFREQACT_WFACT">
    		<key-map field-name="actionId"/>
    	</relation>
    	<!-- <relation rel-entity-name="WorkFlowGroup" type="one" fk-name="WFREQACT_WFGRP">
    		<key-map field-name="groupId"/>
    	</relation> -->
    	<relation rel-entity-name="WorkFlowTransition" type="one" fk-name="WFREQACT_WFRANS">
    		<key-map field-name="transitionId"/>
    	</relation>
    </entity>
</entitymodel>    