		<field name="emplPositionTypeRateId" type="id-ne"></field>	   
	    <field name="emplPositionTypeId" type="id-ne"/>
	    <field name="periodTypeId" type="id-ne"/>
	    <field name="payGradeId" type="id"/>
	    <field name="salaryStepSeqId" type="id"/>
