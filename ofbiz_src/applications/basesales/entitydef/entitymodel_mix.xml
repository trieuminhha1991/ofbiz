<?xml version="1.0" encoding="UTF-8"?>
<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
    <!-- ========================================================= -->
    <!-- ======================== Defaults ======================= -->
    <!-- ========================================================= -->
    <title>Entity of BaseSales Component</title>
    <description>None</description>
    <copyright></copyright>
    <version></version>

    <extend-entity entity-name="OrderItem">
    	<field name="expireDate" type="date-time"></field>
    </extend-entity>
    
    <extend-entity entity-name="UserLogin">
		<field name="lastOrg" type="id"></field>
		<field name="lastModule" type="id"></field>
	</extend-entity>
	<view-entity entity-name="AgreementAndAgreementAttribute"
        package-name="org.ofbiz.party.party"
        title="Relation between Agreement and AgreementAttribute">
        <member-entity entity-alias="AATR" entity-name="AgreementAttribute"/>
        <member-entity entity-alias="AG" entity-name="Agreement"/>
        <alias-all entity-alias="AATR"></alias-all>
        <alias-all entity-alias="AG"></alias-all>
        <view-link entity-alias="AATR" rel-entity-alias="AG">
            <key-map field-name="agreementId" rel-field-name="agreementId"/>
        </view-link>
    </view-entity>
    
    <extend-entity entity-name="TermTypeAttr">
		<field name="attrValue" type="very-long"></field>
	</extend-entity>
	
	<extend-entity entity-name="AgreementProductAppl">
		<field name="quantity" type="fixed-point"></field>
		<field name="quantityUomId" type="long-varchar"></field>
	</extend-entity>
	<extend-entity entity-name="Agreement">
		<field name="agreementCode" type="long-varchar"></field>
	</extend-entity>
	
	<extend-entity entity-name="Party">
		<field name="partyCode" type="long-varchar"></field>
	</extend-entity>
</entitymodel>