    <extend-entity entity-name="OrderItem">
    	<field name="quantityUomId" type="id"></field>
    	<field name="alternativeQuantity" type="fixed-point"></field>
    	<field name="alternativeUnitPrice" type="currency-precise"></field>
    	<relation type="one" fk-name="ORDER_ITEM_QTT_UOM" title="QuantityUom" rel-entity-name="Uom">
          	<key-map field-name="quantityUomId" rel-field-name="uomId"/>
      	</relation>
    </extend-entity>
    <extend-entity entity-name="ProductPriceRule">
    	<field name="productQuotationId" type="id"></field>
    	<field name="isExtra" type="id"><description>Generated when created by product price rule another, default is N</description></field>
    	<relation rel-entity-name="ProductQuotation" type="one" fk-name="PROD_PCRULE_QUOTATION">
    		<key-map field-name="productQuotationId"/>
    	</relation>
    </extend-entity>
    <extend-entity entity-name="Product">
    	<field name="productCode" type="id"></field>
    	<field name="numRival" type="indicator"><description>0 or null: company sale; 1: rival sale; 2: both company and rival sales</description></field>
    </extend-entity>
    <!-- <extend-entity entity-name="Product">
    	<field name="productPackingUomId" type="id"></field>
    	<relation rel-entity-name="Uom" type="one" fk-name="PROD_PACKING_UOM">
    		<key-map field-name="productPackingUomId" rel-field-name="uomId"/>
    	</relation>
    </extend-entity> -->
    <extend-entity entity-name="PostalAddress">
		<field name="roadGeoId" type="id"></field>
		<relation type="one" fk-name="POST_ADDR_ROGEO_NEW2" title="Road" rel-entity-name="Geo">
			<key-map field-name="roadGeoId" rel-field-name="geoId"/>
		</relation>
	</extend-entity>
	
	<!--Huyendt Extend Entity for ContentAdvertist -->
	<extend-entity entity-name="WebSiteContent">
    	<field name="timeShow" type="numeric"></field>
    	<field name="isAllStore" type="indicator">
			<description>
			    If set to Y or null then the Advertise Content is use by All of productStores. Else only ProductStore use 
			</description>
		</field>
		<field name="extContentId" type="id-ne"></field>
		<relation type="one" fk-name="EXT_CONTENTID_POS" title="Img2" rel-entity-name="Content">
			<key-map field-name="extContentId" rel-field-name="contentId"/>
		</relation>
    </extend-entity>
    
</entitymodel>