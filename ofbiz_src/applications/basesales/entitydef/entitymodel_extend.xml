<?xml version="1.0" encoding="UTF-8"?>
<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
    <!-- ========================================================= -->
    <!-- ======================== Defaults ======================= -->
    <!-- ========================================================= -->
    <title>View Entity of BaseSales Component</title>
    <description>None</description>
    <copyright></copyright>
    <version></version>
    
    <extend-entity entity-name="Agreement">
	    <field name="isAuto" type="indicator" not-null="false"/>
	</extend-entity>
    
	<extend-entity entity-name="PortalPage">
        <field name="roleTypeId" type="id"><description>Used to assign portal page to a roleTypeId</description></field>
        <field name="moduleId" type="id"/>
        <relation type="one" fk-name="PORTALPG_ROLETYPE" rel-entity-name="RoleType">
            <key-map field-name="roleTypeId"/>
        </relation>
    </extend-entity>
    
    <extend-entity entity-name="ProductStorePaymentSetting">
    	<field name="showToCustomer" type="indicator"></field>
    </extend-entity>
    <extend-entity entity-name="PortalPagePortlet">
        <field name="userLoginId" type="id-ne"/>
        <relation type="one" fk-name="PORTALPG_USRLOGIN" rel-entity-name="UserLogin">
            <key-map field-name="userLoginId"/>
        </relation>
    </extend-entity>
    
    <!-- ========================================================= -->
	<!-- extend entity -->
	<!-- ========================================================= -->
    
    <extend-entity entity-name="ProductPromo">
		<field name="fromDate" type="date-time"/>
		<field name="thruDate" type="date-time"/>
		<field name="productPromoTypeId" type="id"/>
		<field name="statusId" type="id"/>
		<field name="organizationPartyId" type="id"/>
		<field name="stateId" type="id"/>
		<relation type="one" fk-name="PROMO_PROMOTYPE" rel-entity-name="ProductPromoType">
			<key-map field-name="productPromoTypeId"/>
		</relation>
		<relation type="one" fk-name="PROMO_STATUS" title="status" rel-entity-name="StatusItem">
			<key-map field-name="statusId"/>
		</relation>
		<relation type="one" fk-name="PROMO_ORGPTY" title="organizationParty" rel-entity-name="Party">
			<key-map field-name="organizationPartyId" rel-field-name="partyId"/>
		</relation>
		<relation type="one" fk-name="PROMO_STATE" rel-entity-name="StatusItem">
			<key-map field-name="stateId" rel-field-name="statusId"/>
		</relation>
   	</extend-entity>
   	<extend-entity entity-name="ProductPromoRule">
   		<field name="ruleText" type="description"></field>
   	</extend-entity>
   	<extend-entity entity-name="ProductPromoCond">
   		<field name="usePriceWithTax" type="id"><description>Calculate promotion with price has tax if this value is Y</description></field>
   	</extend-entity>
   	<extend-entity entity-name="ProductPromoAction">
   		<field name="operatorEnumId" type="id"/>
   		<field name="isCheckInv" type="id"><description>Check inventory item is available when this value equal Y or null</description></field>
   		<relation type="one" fk-name="PROD_PRACT_OPENUM" title="Operator" rel-entity-name="Enumeration">
        	<key-map field-name="operatorEnumId" rel-field-name="enumId"/>
      	</relation>
   	</extend-entity>
	
	<extend-entity entity-name="ProductStore">
    	<field name="salesMethodChannelEnumId" type="id"></field>
    	<field name="includeOtherCustomer" type="indicator"></field>
    	<field name="statusId" type="id"></field>
    	<relation type="one" fk-name="PROD_STR_SMCENUM" title="SalesMethodChannel" rel-entity-name="Enumeration">
	        <key-map field-name="salesMethodChannelEnumId" rel-field-name="enumId"/>
      	</relation>
      	<relation type="one" fk-name="PROD_STR_STATUSITM" rel-entity-name="StatusItem">
        	<key-map field-name="statusId"/>
      	</relation>
    </extend-entity>
	<extend-entity entity-name="OrderHeader">
    	<field name="salesMethodChannelEnumId" type="id"></field>
    	<field name="agreementId" type="id"></field>
    	<field name="isFavorDelivery" type="indicator"></field>
    	<field name="contentId" type="id"/>
    	<field name="estimatedDeliveryDate" type="date-time"></field>
    	<field name="shipBeforeDate" type="date-time"></field>
      	<field name="shipAfterDate" type="date-time"></field>
      	<field name="isSupplierApproved" type="indicator"></field>
    	<relation type="one" fk-name="ORD_HDR_SMC_ENUM" title="SalesMethodChannel" rel-entity-name="Enumeration">
	        <key-map field-name="salesMethodChannelEnumId" rel-field-name="enumId"/>
      	</relation>
      	<relation type="one" fk-name="ORDER_HDR_AGREE" title="agreement" rel-entity-name="Agreement">
	        <key-map field-name="agreementId"/>
      	</relation>
      	<relation rel-entity-name="Content" type="one" fk-name="FK_ORD_SALES_CTN_UOM">
    		<key-map field-name="contentId" rel-field-name="contentId"/>
    	</relation>
    </extend-entity>
