        <response name="success" type="request" value="json"/>
        <response name="error" type="request" value="json"/>
    </request-map>
    <request-map uri="processPaymentSalesAjax">
        <security direct-request="false"/>
        <event type="java" path="com.olbius.basesales.order.CheckOutEvents" invoke="processPayment"/>
        <response name="success" type="request" value="clearCartSalesAjax"/>
        <response name="fail" type="request" value="json"/>
        <response name="error" type="request" value="json"/>
    </request-map>
    <request-map uri="clearCartSalesAjax">
        <security https="true" auth="true"/>
        <event type="java" path="org.ofbiz.order.shoppingcart.ShoppingCartEvents" invoke="destroyCart"/>
        <!-- <response name="success" type="request" value="emailOrderSalesAjax"/> -->
        <response name="success" type="request" value="json"/>
        <response name="error" type="request" value="json"/>
    </request-map>
    <request-map uri="emailOrderSalesAjax">
