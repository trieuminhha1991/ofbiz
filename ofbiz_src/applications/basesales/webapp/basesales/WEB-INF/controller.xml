	</request-map>
	<request-map uri="viewOrder">
		<security auth="true" https="true"/>
		<response name="success" type="view" value="ViewOrder"/>
	</request-map>
	<request-map uri="copySalesOrder">
        <security https="true" auth="true"/>
        <event type="java" path="org.ofbiz.order.shoppingcart.ShoppingCartEvents" invoke="loadCartFromOrder"/>
        <response name="success" type="request-redirect-noparam" value="newSalesOrder"/>
        <response name="error" type="request-redirect-noparam" value="newSalesOrder"/>
    </request-map>
    <request-map uri="newSalesOrder">
		<security https="true" auth="true"/>
		<response name="success" type="view" value="NewSalesOrder"/>
	</request-map>
	<request-map uri="initSalesOrderEntry">
		<security https="true" auth="true"/>
		<!-- <event type="java" path="com.olbius.basesales.order.OrderEvents" invoke="initializeSalesOrderEntry"/> -->
		<event type="java" path="com.olbius.basesales.shoppingcart.ShoppingCartEvents" invoke="initializeSalesOrderEntryService"/>
		<response name="success" type="view" value="NewSalesOrderConfirmAjax"/>
		<response name="error" type="request" value="json"/>
	</request-map>
	<request-map uri="processSalesOrder">
        <security https="true" auth="true"/>
        <event type="java" path="org.ofbiz.order.shoppingcart.CheckOutEvents" invoke="createOrder"/>
        <response name="sales_order" type="request" value="checkBlackListSales"/>
        <response name="error" type="view" value="NewSalesOrderConfirm"/>
    </request-map>
    <request-map uri="checkBlackListSales">
        <security direct-request="false"/>
        <event type="java" path="org.ofbiz.order.shoppingcart.CheckOutEvents" invoke="checkOrderBlacklist"/>
        <response name="success" type="request" value="processPaymentSales"/>
        <response name="failed" type="request" value="failedBlacklistSales"/>
        <response name="error" type="view" value="NewSalesOrderConfirm"/>
    </request-map>
    <request-map uri="failedBlacklistSales">
        <security direct-request="false"/>
        <event type="java" path="org.ofbiz.order.shoppingcart.CheckOutEvents" invoke="failedBlacklistCheck"/>
        <response name="success" type="view" value="main"/>
        <response name="error" type="view" value="main"/>
    </request-map>
    <request-map uri="processPaymentSales">
        <security direct-request="false"/>
        <event type="java" path="com.olbius.basesales.order.CheckOutEvents" invoke="processPayment"/>
        <response name="success" type="request" value="clearCartSales"/>
        <response name="fail" type="view" value="NewSalesOrderConfirm"/>
        <response name="error" type="view" value="NewSalesOrderConfirm"/>
    </request-map>
    <request-map uri="clearCartSales">
        <security https="true" auth="true"/>
        <event type="java" path="org.ofbiz.order.shoppingcart.ShoppingCartEvents" invoke="destroyCart"/>
        <!-- <response name="success" type="request" value="emailOrderSales"/> -->
        <response name="success" type="view" value="NewSalesOrderComplete"/>
        <response name="error" type="view" value="NewSalesOrderConfirm"/>
    </request-map>
    <request-map uri="emailOrderSales">
        <security https="true" direct-request="false"/>
        <event type="service" path="async" invoke="sendOrderConfirmation"/>
        <response name="success" type="view" value="NewSalesOrderComplete"/>
        <response name="error" type="view" value="NewSalesOrderComplete"/>
    </request-map>
    <!-- process order ajax -->
    <request-map uri="processSalesOrderAjax">
        <security https="true" auth="true"/>
        <event type="java" path="org.ofbiz.order.shoppingcart.CheckOutEvents" invoke="createOrder"/>
        <response name="sales_order" type="request" value="checkBlackListSalesAjax"/>
        <response name="error" type="request" value="json"/>
    </request-map>
    <request-map uri="checkBlackListSalesAjax">
        <security direct-request="false"/>
        <event type="java" path="org.ofbiz.order.shoppingcart.CheckOutEvents" invoke="checkOrderBlacklist"/>
        <response name="success" type="request" value="processPaymentSalesAjax"/>
        <response name="failed" type="request" value="failedBlacklistSalesAjax"/>
        <response name="error" type="request" value="json"/>
    </request-map>
    <request-map uri="failedBlacklistSalesAjax">
        <security direct-request="false"/>
