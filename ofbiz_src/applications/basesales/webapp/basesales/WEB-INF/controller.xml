    <!-- Request Mappings -->
    <request-map uri="main"><security https="true" auth="true"/><response name="success" type="view" value="main"/></request-map>
    <request-map uri="jqxGeneralServicer">
        <event type="java" invoke="jqxEventProcessor" path="com.olbius.events.JqxEvents" global-transaction="false"/>
        <response name="success" type="none" />
        <response name="error" type="none" />
    </request-map>
    
    <!-- sales order -->
    <request-map uri="listOrder">
		<security auth="true" https="true"/>
		<response name="success" type="view" value="ListOrder"/>
	</request-map>
    <request-map uri="listOrderTelesales">
		<security auth="true" https="true"/>
		<response name="success" type="request-redirect" value="listOrder">
			<redirect-parameter name="cn" value="ts"/>
		</response>
	</request-map>
	<request-map uri="listOrderPos">
		<security auth="true" https="true"/>
		<response name="success" type="request-redirect" value="listOrder">
			<redirect-parameter name="cn" value="ps"/>
		</response>
	</request-map>
	<request-map uri="listOrderEcommerce">
		<security auth="true" https="true"/>
