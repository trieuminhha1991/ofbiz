<?xml version="1.0" encoding="UTF-8"?>
<simple-methods xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/simple-methods-v2.xsd">
    
    <simple-method method-name="checkProcessPaymentDefault">
    	<set field="toProcess" value="true" type="Boolean"/>
    	<field-to-result field="toProcess" result-name="toProcess"/>
    	
    	<set field="orderId" from-field="parameters.orderId"/>
    	<return/>
    </simple-method>
    
    <simple-method method-name="processActionApproveOrderDefault">
		<set field="orderId" from-field="parameters.orderId"/>
    	<return/>
	</simple-method>
	
</simple-methods>