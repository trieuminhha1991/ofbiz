<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>API Sales Services</description>
    <vendor></vendor>
    <version>1.0</version>
    
    <service name="getListPromosRuleByProduct" engine="java" auth="true"
			location="com.olbius.basesales.product.ProductPromoServices" invoke="getListPromosRuleByProduct">
		<attribute name="productId" mode="IN" type="String"/>
		<attribute name="productStoreId" mode="IN" type="String" optional="true"/>
		<attribute name="productStoreIds" mode="IN" type="java.util.List" optional="true"/>
		<attribute name="timePoint" mode="IN" type="Timestamp" optional="true"/>
		<attribute name="isGetAll" mode="IN" type="String" optional="true" default-value="N"/>
		<attribute name="isGetFutureActive" mode="IN" type="String" optional="true" default-value="N"/>
		<attribute name="listPromoRules" mode="OUT" type="java.util.List" optional="true"/>
	</service>
	<service name="getListOrderItemsPromoReturn" engine="java" auth="true"
			location="com.olbius.basesales.order.OrderServices" invoke="getListOrderItemsPromoReturn">
		<attribute name="listOrderItems" mode="IN" type="java.util.List"/>
		<attribute name="listOrderItemsPromo" mode="OUT" type="java.util.List" optional="true"/>
		<attribute name="listOrderAdjustmentsPromo" mode="OUT" type="java.util.List" optional="true"/>
	</service>
	<!-- <service name="testListOrderItemsPromoReturn" engine="java" auth="true"
			location="com.olbius.basesales.order.OrderServices" invoke="testListOrderItemsPromoReturn">
		<attribute name="orderId" mode="IN" type="String"/>
		<attribute name="orderItemSeqId" mode="IN" type="String"/>
		<attribute name="listOrderItemsPromo" mode="OUT" type="java.util.List" optional="true"/>
		<attribute name="listOrderAdjustmentsPromo" mode="OUT" type="java.util.List" optional="true"/>
	</service> -->
</services>