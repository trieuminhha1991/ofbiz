<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../framework/service/dtd/services.xsd">
	<description>Marketing Services</description>
	<vendor></vendor>
	<version>1.0</version>

	<service name="JQGetListProductNonVirtual" engine="java" invoke="listProductNonVirtual" location="com.olbius.basepo.plan.PlanServicesv2">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="loadIndicesOfProduct" engine="java" invoke="loadIndicesOfProduct" location="com.olbius.basepo.plan.PlanServicesv2">
		<attribute name="productPlanId" mode="IN" type="String"></attribute>
		<attribute name="productId" mode="IN" type="String"></attribute>
		<attribute name="periodType" mode="IN" type="String"></attribute>
		<attribute name="previous" mode="IN" type="java.lang.Integer"></attribute>
		<attribute name="next" mode="IN" type="java.lang.Integer"></attribute>
		<attribute name="compressedPackage" mode="OUT" type="Map"></attribute>
	</service>

	<service name="loadProductPlanItem" engine="java" invoke="loadProductPlanItem" location="com.olbius.basepo.plan.PlanServicesv2">
		<attribute name="productPlanId" mode="IN" type="String"></attribute>
		<attribute name="periodType" mode="IN" type="String"></attribute>
		<attribute name="previous" mode="IN" type="java.lang.Integer"></attribute>
		<attribute name="next" mode="IN" type="java.lang.Integer"></attribute>
		<attribute name="compressedPackage" mode="OUT" type="Map"></attribute>
	</service>

	<service name="createOrStoreProductPlanItem" engine="java" invoke="createOrStoreProductPlanItem" location="com.olbius.basepo.plan.PlanServicesv2">
		<attribute name="productPlanId" mode="IN" type="String"></attribute>
		<attribute name="productId" mode="IN" type="String"></attribute>
		<attribute name="purchaseForecast" mode="IN" type="String"></attribute>
	</service>

	<service name="JQGetListProductPlanPO" engine="java" invoke="listProductPlan" location="com.olbius.basepo.plan.PlanServicesv2">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="createProductPlanHeader" engine="java" invoke="createProductPlanHeader" location="com.olbius.basepo.plan.PlanServicesv2"
		default-entity-name="ProductPlanHeader">
		<auto-attributes mode="IN" include="all" optional="true"></auto-attributes>
	</service>

	<service name="updateProductPlanHeader" engine="java" invoke="updateProductPlanHeader" location="com.olbius.basepo.plan.PlanServicesv2"
		default-entity-name="ProductPlanHeader">
		<auto-attributes mode="IN" include="pk"></auto-attributes>
		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
	</service>

</services>