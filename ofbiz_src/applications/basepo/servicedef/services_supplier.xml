<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../framework/service/dtd/services.xsd">
	<description>Marketing Services</description>
	<vendor></vendor>
	<version>1.0</version>

	<service name="jqGetListSupplierSimple" engine="java" invoke="jqGetListSupplierSimple" location="com.olbius.basepo.supplier.SupplierServices">
		<implements service="jqxGetListInterface" />
	</service>
	<service name="jqGetListPartySupplier" engine="java" invoke="listPartySupplier" location="com.olbius.basepo.supplier.SupplierServices">
		<implements service="jqxGetListInterface" />
		<override name="listIterator" type="List"></override>
	</service>

	<service name="loadPostalAddress" engine="java" invoke="loadPostalAddress" location="com.olbius.basepo.supplier.SupplierServices">
		<attribute name="contactMechId" mode="IN" type="String"></attribute>
		<attribute name="postalAddress" mode="OUT" type="org.ofbiz.entity.GenericValue" optional="true"></attribute>
	</service>
	<service name="createOrStorePostalAddress" engine="java" invoke="createOrStorePostalAddress" location="com.olbius.basepo.supplier.SupplierServices"
		default-entity-name="PostalAddress">
		<auto-attributes mode="INOUT" include="pk"></auto-attributes>
		<auto-attributes mode="INOUT" include="nonpk" optional="true"></auto-attributes>
	</service>

	<service name="updatePartySupplier" engine="java" invoke="updatePartySupplier" location="com.olbius.basepo.supplier.SupplierServices">
		<auto-attributes mode="IN" entity-name="Party" optional="true"></auto-attributes>
		<auto-attributes mode="IN" entity-name="PartyGroup" optional="true"></auto-attributes>
		<auto-attributes mode="IN" entity-name="PostalAddress" optional="true"></auto-attributes>
		<attribute name="infoString" mode="IN" type="String" optional="true"></attribute>
		<attribute name="emailContactMechId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="contactNumber" mode="IN" type="String" optional="true"></attribute>
		<attribute name="phoneContactMechId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="locationContactMechId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="taxAuth" mode="IN" type="String" optional="true"></attribute>
		<attribute name="taxCode" mode="IN" type="String" optional="true"></attribute>
	</service>
	<service name="createPartySupplier" engine="java" invoke="createPartySupplier" location="com.olbius.basepo.supplier.SupplierServices">
		<auto-attributes mode="IN" entity-name="Party" optional="true"></auto-attributes>
		<auto-attributes mode="IN" entity-name="PartyGroup" optional="true"></auto-attributes>
		<auto-attributes mode="IN" entity-name="PostalAddress" optional="true"></auto-attributes>
		<attribute name="infoString" mode="IN" type="String" optional="true"></attribute>
		<attribute name="emailContactMechId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="contactNumber" mode="IN" type="String" optional="true"></attribute>
		<attribute name="phoneContactMechId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="locationContactMechId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="taxAuth" mode="IN" type="String" optional="true"></attribute>
		<attribute name="taxCode" mode="IN" type="String" optional="true"></attribute>
		<!-- add finAcc by kieuanhvu -->
		<attribute name="finAccountName" mode="IN" type="String" optional="true"></attribute>
		<attribute name="finAccountCode" mode="IN" type="String" optional="true"></attribute>
		<attribute name="stateProvinceGeoId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="countryGeoId" mode="IN" type="String" optional="true"></attribute>
		<!-- ./ -->
	</service>
	<service name="createPartySupplierQuick" engine="java" invoke="createPartySupplierQuick" location="com.olbius.basepo.supplier.SupplierServices">
		<attribute name="partyId" mode="INOUT" type="String" optional="true"/>
		<attribute name="partyCode" mode="IN" type="String" optional="true"/>
		<attribute name="groupName" mode="IN" type="String" optional="true"/>
		<attribute name="preferredCurrencyUomId" mode="IN" type="String" optional="true"/>
		<attribute name="taxCode" mode="IN" type="String" optional="true"/>
		<attribute name="taxAuthGeoId" mode="IN" type="String" optional="true"/>
		<attribute name="taxAuthPartyId" mode="IN" type="String" optional="true"/>
		<attribute name="phoneNumber" mode="IN" type="String" optional="true"/>
		<attribute name="emailAddress" mode="IN" type="String" optional="true"/>
		<attribute name="countryGeoId" mode="IN" type="String" optional="true"/>
		<attribute name="stateProvinceGeoId" mode="IN" type="String" optional="true"/>
		<attribute name="districtGeoId" mode="IN" type="String" optional="true"/>
		<attribute name="wardGeoId" mode="IN" type="String" optional="true"/>
		<attribute name="address1" mode="IN" type="String" optional="true"/>
	</service>
	
	<service name="jqGetListSupplierProducts" engine="java" invoke="listSupplierProducts" location="com.olbius.basepo.supplier.SupplierServices">
		<implements service="jqxGetListInterface" />
	</service>
	
	<service name="JQGetPOListProductsWithoutSupplier" engine="java" invoke="listSupplierProductsWithoutSupplier" location="com.olbius.basepo.supplier.SupplierServices">
		<implements service="jqxGetListInterface" />
	</service>
	
	<service name="createReturnSupplierWithoutOrder" engine="java" invoke="createReturnSupplierWithoutOrder" location="com.olbius.basepo.returnSupplier.ReturnSupplierServices">
   		<attribute name="returnId" mode="INOUT" type="String" optional="true"/>
   		<attribute name="supplierId" mode="IN" type="String" optional="true"/>
   		<attribute name="originContactMechId" mode="IN" type="String" optional="true"/>
   		<attribute name="destinationFacilityId" mode="IN" type="String" optional="true"/>
   		<attribute name="currencyUomId" mode="IN" type="String" optional="true"/>
   		<attribute name="description" mode="IN" type="String" optional="true"/>
   		<attribute name="listProduct" mode="IN" type="java.util.List" optional="true"/>
   	</service>
   	<service name="getInfoProductAddToReturnSupplier" engine="java" invoke="getInfoProductAddToReturnSupplier" location="com.olbius.basepo.returnSupplier.ReturnSupplierServices">
		<attribute name="productId" mode="IN" type="String"/>
		<attribute name="quantityUomId" mode="IN" type="String" optional="true"/>
		<attribute name="facilityId" mode="IN" type="String" optional="true"/>
		<attribute name="productInfo" mode="OUT" type="Map"/>
	</service>
   	<service name="JQGetListProductToReturnSupplier" engine="java" invoke="jqGetListProductToReturnSupplier" location="com.olbius.basepo.returnSupplier.ReturnSupplierServices">
		<implements service="jqxGetListInterface"/>
		<override name="TotalRows" mode="OUT" type="String" optional="true"/>
		<override name="listIterator" mode="OUT" type="java.util.List" optional="true"/>
	</service>
	
	<service name="addProductsToSupplier" engine="java" invoke="addProductsToSupplier" location="com.olbius.basepo.supplier.SupplierServices">
		<attribute name="partyId" mode="IN" type="String" optional="false" />
		<attribute name="availableFromDate" mode="IN" type="String" optional="true" />
		<attribute name="availableThruDate" mode="IN" type="String" optional="true" />
		<attribute name="productItems" mode="IN" type="String" optional="false" />
		<attribute name="productIdsSuccess" mode="OUT" type="java.util.List" optional="true" />
		<attribute name="productIdsNotSuccess" mode="OUT" type="java.util.List" optional="true" />
	</service>
	
</services>