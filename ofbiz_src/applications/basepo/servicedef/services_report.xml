<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
	<description>Marketing Services</description>
	<vendor></vendor>
	<version>1.0</version>

	<service name="getTotalAmountBought" engine="java" invoke="getTotalAmountBought" location="com.olbius.basepo.report.ReportEvens">
		<attribute name="period" mode="IN" type="String" optional="true"></attribute>
		<attribute name="value" mode="OUT" type="Object" optional="true"></attribute>
	</service>

	<service name="evaluateTop5SupColumnChart" engine="java" location="com.olbius.basepo.report.TopSupplierChartServices" invoke="evaluateTop5SupColumnChart">
		<attribute name="fromDate" type="Timestamp" mode="IN" />
		<attribute name="thruDate" type="Timestamp" mode="IN" />
		<attribute name="topType" type="String" mode="IN" optional="true" />
		<attribute name="yAxis" mode="OUT" type="Map" />
		<attribute name="xAxis" mode="OUT" type="List" />
	</service>
	
	<service name="olapPurchaseOrderReport" engine="olbius" location="com.olbius.basepo.report.PurchaseOrderReport">
		<implements service="olapInterface"/>
		<attribute name="categories[]" mode="IN" type="List" optional="true"/>
	</service>
	
	<service name="olapPOImportedReport" engine="olbius" location="com.olbius.basepo.report.POImportedReport">
		<implements service="olapInterface"/>
		<attribute name="facility[]" mode="IN" type="List" optional="true"/>
		<attribute name="product[]" mode="IN" type="List" optional="true"/>
		<attribute name="categories[]" mode="IN" type="List" optional="true"/>
	</service>
	
	<service name="olapPOReturnReport" engine="olbius" location="com.olbius.basepo.report.POReturnReport">
		<implements service="olapInterface"/>
		<attribute name="facility[]" mode="IN" type="List" optional="true"/>
		<attribute name="product[]" mode="IN" type="List" optional="true"/>
		<attribute name="categories[]" mode="IN" type="List" optional="true"/>
	</service>
	
	<service name="olapChartColReceivedByPeriod" engine="olbius" location="com.olbius.basepo.report.ChartColReceivedByPeriod">
	    <implements service="olapInterface"/>
	    <attribute name="dateType" mode="IN" type="String" optional="true"/>
	    <attribute name="filterTop" mode="IN" type="Integer" optional="true"/>
	    <attribute name="filterSort" mode="IN" type="String" optional="true"/>
	</service>
	
	<service name="olapChartLineAverageCost" engine="olbius" location="com.olbius.basepo.report.AverageCostChart">
		<implements service="olapInterface"/>
		<attribute name="dateType" mode="IN" type="String"/>
		<attribute name="group[]" mode="IN" type="List" optional="true"/>
		<attribute name="facilityId" mode="IN" type="String" optional="true"/>
		<attribute name="productId" mode="IN" type="String" optional="true"/>
	</service>
</services>