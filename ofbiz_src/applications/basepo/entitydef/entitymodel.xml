	<view-entity entity-name="SupplierProductAndQuota"
      package-name="org.ofbiz.product.supplier">
      <member-entity entity-alias="SP" entity-name="SupplierProduct"/>
      <member-entity entity-alias="PR" entity-name="Product"/>
      <alias-all entity-alias="SP"/>
      <alias-all entity-alias="PR">
        <exclude field="productId"/>
        <exclude field="comments"/>
        <exclude field="quantityUomId"/>
      </alias-all>
      <view-link entity-alias="SP" rel-entity-alias="PR">
        <key-map field-name="productId"/>
      </view-link>
    </view-entity>
    
    <view-entity entity-name="SupplierProductAndQuotaAndTax"
      package-name="org.ofbiz.product.supplier">
      <member-entity entity-alias="SP" entity-name="SupplierProductAndQuota"/>
      <member-entity entity-alias="PTD" entity-name="ProductTempData"/>
      <alias-all entity-alias="SP"/>
      <alias entity-alias="PTD" field="taxPercentage" name="taxPercentage"></alias>
      <view-link entity-alias="SP" rel-entity-alias="PTD">
        <key-map field-name="productId"/>
      </view-link>
    </view-entity>
	
	<view-entity entity-name="ProductCategoryAndTax" package-name="org.ofbiz.product.facility">
		<member-entity entity-alias="PC" entity-name="ProductCategory"></member-entity>
		<member-entity entity-alias="PCM" entity-name="ProductCategoryMember"></member-entity>
		<member-entity entity-alias="TAX" entity-name="TaxAuthorityRateProduct"></member-entity>
		<alias-all entity-alias="PC"></alias-all>
		<alias-all entity-alias="PCM"></alias-all>
		<alias entity-alias="TAX" field="taxPercentage" name="taxPercentage"></alias>
		<alias entity-alias="PCM" field="fromDate" name="fromDate"></alias>
		<alias entity-alias="PCM" field="thruDate" name="thruDate"></alias>
		<alias entity-alias="TAX" field="description" name="description"></alias>
		<view-link rel-entity-alias="PCM" entity-alias="PC">
			<key-map field-name="productCategoryId" />
		</view-link>
		<view-link rel-entity-alias="TAX" entity-alias="PC">
			<key-map field-name="productCategoryId" />
		</view-link>
		<entity-condition>
			<condition-list combine="or">
				<condition-expr field-name="productCategoryTypeId" entity-alias="PC" value="TAX_IMP_CATEGORY"/>
				<condition-expr field-name="productCategoryTypeId" entity-alias="PC" value="TAX_EXCISE_CATEGORY"/>
			</condition-list>
		</entity-condition>
	</view-entity>

