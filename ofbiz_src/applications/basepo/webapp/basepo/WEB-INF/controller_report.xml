<?xml version="1.0" encoding="UTF-8"?>
<site-conf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/site-conf.xsd">

	<request-map uri="getTotalAmountBought">
		<security https="true" auth="true" />
		<event type="service" invoke="getTotalAmountBought"></event>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>

	<request-map uri="evaluateTop5SupColumnChart">
		<security https="true" auth="true" />
		<event type="service" invoke="evaluateTop5SupColumnChart"></event>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>

	<request-map uri="getNotShipOrder">
		<security https="true" auth="false" />
		<event type="service" invoke="getNotShipOrder"></event>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>

	<request-map uri="ImportedGoodsReport">
		<security https="true" auth="false" />
		<response name="success" type="view" value="ImportedGoodsReport"></response>
		<response name="error" type="view" value="ImportedGoodsReport"></response>
	</request-map>

	<request-map uri="getListImportedGoods">
		<security https="true" auth="false" />
		<event type="service" invoke="getListImportedGoods"></event>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>

	<!-- //View Map -->
	<view-map name="ImportedGoodsReport" type="screen"
		page="component://basepo/widget/ReportScreens.xml#ImportedGoodsReport" />
</site-conf>