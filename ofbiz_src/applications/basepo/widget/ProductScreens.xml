	<screen name="ViewProductCategoryTax">
		<section>
			<actions>
				<set field="titleHeaderValue" value="categoryTitle" global="true" />
				<set field="titleHeaderLocation" value="component://basepo/widget/ProductScreens.xml" global="true" />
				<set field="selectedMenuItem" value="product" />
				<set field="selectedSubMenuItem" value="listProductCategoryTax" />
				<set field="titleProperty" value="BSViewProductCategoryTax" />
				<set field="productCategoryId" from-field="parameters.productCategoryId" />
				<entity-one value-field="productCategory" entity-name="ProductCategory"></entity-one>
			</actions>
			<widgets>
				<decorator-screen name="main-decorator" location="${parameters.mainDecoratorLocation}">
					<decorator-section name="body">
						<section>
							<condition>
								<not>
									<if-empty field="productCategory" />
								</not>
							</condition>
							<widgets>
								<platform-specific>
									<html><html-template location="component://basepo/webapp/basepo/productcatalog/productCategoryTaxViewProduct.ftl" /></html>
								</platform-specific>
							</widgets>
							<fail-widgets>
								<container style="alert alert-info">
									<label text="${uiLabelMap.BSRecordIsNotFound}" />
								</container>
							</fail-widgets>
						</section>
					</decorator-section>
				</decorator-screen>
			</widgets>
		</section>
