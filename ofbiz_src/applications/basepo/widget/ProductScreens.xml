<?xml version="1.0" encoding="UTF-8"?>
<screens xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../framework/widget/dtd/widget-screen.xsd">

	<screen name="ListProducts">
		<section>
			<actions>
				<set field="selectedMenuItem" value="product" />
				<set field="selectedSubMenuItem" value="ListProducts" />
				<set field="titleProperty" value="DmsListProducts" />
			</actions>
			<widgets>
				<decorator-screen name="main-decorator" location="${parameters.mainDecoratorLocation}">
					<decorator-section name="body">
						<section>
							<condition>
								<if-olbius-has-permission />
							</condition>
							<widgets>
								<platform-specific>
									<html>
										<html-template location="component://basepo/webapp/basepo/productnew/productList.ftl" />
									</html>
								</platform-specific>
							</widgets>
							<fail-widgets>
								<container style="alert alert-info">
									<label text="${uiLabelMap.BSYouHavenotViewPermission}" />
								</container>
							</fail-widgets>
						</section>
					</decorator-section>
				</decorator-screen>
			</widgets>
		</section>
	</screen>

	<screen name="productTitle">
		<section>
			<condition>
				<not>
					<if-empty field="parameters.productId" />
				</not>
			</condition>
			<widgets>
				<label style="ibefore-icon-angle-right" text="${uiLabelMap.DAId}: "></label>
				<link url-mode="plain" target="${link}?productId=${parameters.productId}" text="${product.productCode}"></link>
			</widgets>
			<fail-widgets>
				<section>
					<condition>
						<not>
							<if-empty field="parameters.productIdOrg" />
						</not>
					</condition>
					<widgets>
						<label style="ibefore-icon-angle-right" text="${uiLabelMap.BSFrom}: "></label>
						<link url-mode="plain" target="${link}?productIdOrg=${parameters.productIdOrg}" text="${parameters.productIdOrg}"></link>
					</widgets>
				</section>
			</fail-widgets>
		</section>
	</screen>

	<screen name="AddNewProduct">
		<section>
			<actions>
				<entity-one value-field="product" entity-name="Product"></entity-one>
				<set field="link" value="AddNewProduct" />
				<set field="titleHeaderValue" value="productTitle" global="true" />
				<set field="titleHeaderLocation" value="component://basepo/widget/ProductScreens.xml" global="true" />
				<set field="selectedMenuItem" value="product" />
				<set field="selectedSubMenuItem" from-field="parameters.sub" default-value="AddNewProduct" />
				<set field="titleProperty" value="DmsAddNewProduct" />
				<set field="tabBarName" value="ProductTabBar" global="true" />
				<set field="tabBarLocation" value="component://appbase/widget/AppBaseMenus.xml" global="true" />
				<set field="tabButtonItem" value="AddNewProduct" global="true" />
				<set field="printTabBar" value="true" />
			</actions>
			<widgets>
				<section>
					<condition>
						<not>
							<if-empty field="product" />
						</not>
					</condition>
					<actions>
						<set field="selectedMenuItem" value="product" />
						<set field="selectedSubMenuItem" from-field="parameters.sub" default-value="ListProducts" />
						<set field="titleProperty" value="DmsEditProduct" />
					</actions>
					<widgets></widgets>
				</section>
				<decorator-screen name="main-decorator" location="${parameters.mainDecoratorLocation}">
					<decorator-section name="body">
						<section>
							<condition>
								<if-olbius-has-permission />
							</condition>
							<widgets>
								<screenlet title="${uiLabelMap[titleProperty]}"></screenlet>
								<platform-specific>
									<html>
										<html-template location="component://basepo/webapp/basepo/product/addNewProduct.ftl" />
									</html>
								</platform-specific>
							</widgets>
							<fail-widgets>
								<container style="alert alert-info">
									<label text="${uiLabelMap.BSYouHavenotViewPermission}" />
								</container>
							</fail-widgets>
						</section>
					</decorator-section>
				</decorator-screen>
			</widgets>
		</section>
	</screen>
	<screen name="NewProduct">
		<section>
			<actions>
				<entity-one value-field="product" entity-name="Product"></entity-one>
				<entity-one value-field="productCopy" entity-name="Product">
					<field-map field-name="productId" from-field="parameters.productIdOrg"/>
				</entity-one>
				<set field="link" value="viewProduct" />
				<set field="titleHeaderValue" value="productTitle" global="true" />
				<set field="titleHeaderLocation" value="component://basepo/widget/ProductScreens.xml" global="true" />
				<set field="selectedMenuItem" value="product" />
				<set field="selectedSubMenuItem" from-field="parameters.sub" default-value="AddNewProduct" />
				<set field="titleProperty" value="DmsAddNewProduct" />
				<set field="tabButtonItem" value="AddNewProduct" global="true"/>
				<set field="copyMode" type="Boolean" value="false"/>
				<set field="isCreateAction" type="Boolean" value="false"/>
			</actions>
			<widgets>
				<section>
					<condition>
						<and>
							<if-empty field="product"/>
							<not><if-empty field="productCopy"/></not>
						</and>
					</condition>
					<actions>
						<set field="product" from-field="productCopy"/>
						<set field="copyMode" type="Boolean" value="true"/>
						<set field="isCreateAction" type="Boolean" value="true"/>
					</actions>
					<widgets></widgets>
				</section>
				<section>
					<condition>
						<not>
							<if-empty field="product" />
						</not>
					</condition>
					<actions>
						<set field="selectedMenuItem" value="product" />
						<set field="selectedSubMenuItem" from-field="parameters.sub" default-value="ListProducts" />
						<set field="titleProperty" value="DmsEditProductInformation" />
						<set field="tabBarName" value="ProductTabBar" global="true" />
						<set field="tabBarLocation" value="component://appbase/widget/AppBaseMenus.xml" global="true" />
						<set field="printTabBar" value="true" />
						<script location="component://basepo/webapp/basepo/WEB-INF/actions/product/ProductNew.groovy" />
					</actions>
					<widgets></widgets>
				</section>
				<decorator-screen name="main-decorator" location="${parameters.mainDecoratorLocation}">
					<decorator-section name="body">
						<section>
							<condition>
								<if-compare operator="equals" value="true" field="isCreateAction" type="Boolean"/>
							</condition>
							<widgets>
								<include-screen name="NewProductInner"/>
							</widgets>
							<fail-widgets>
								<include-screen name="EditProductInner"/>
							</fail-widgets>
						</section>
					</decorator-section>
				</decorator-screen>
			</widgets>
		</section>
	</screen>
	
