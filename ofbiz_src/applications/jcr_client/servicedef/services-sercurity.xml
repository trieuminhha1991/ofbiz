<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
	<description>jcr Services</description>
	<vendor></vendor>
	<version>1.0</version>

	<service name="jackrabbitSetPermission" engine="java"
		location="com.olbius.jackrabbit.client.services.JackrabbitOlbiusSecurityServices" invoke="jackrabbitSetPermission">
		<implements service="jackrabbitRemoteLogin" optional="true" />
		<attribute name="curPath" type="String" mode="IN" optional="false" />
		<attribute name="userId" type="String" mode="IN" optional="true" />
		<attribute name="groupId" type="String" mode="IN" optional="true" />
		<attribute name="permission" type="String" mode="IN" optional="false" />
		<attribute name="allow" type="Boolean" mode="IN" optional="false" />
	</service>
	
	<service name="jackrabbitRemovePermission" engine="java"
		location="com.olbius.jackrabbit.client.services.JackrabbitOlbiusSecurityServices" invoke="jackrabbitRemovePermission">
		<implements service="jackrabbitRemoteLogin" optional="true" />
		<attribute name="curPath" type="String" mode="IN" optional="false" />
		<attribute name="userId" type="String" mode="IN" optional="true" />
		<attribute name="groupId" type="String" mode="IN" optional="true" />
		<attribute name="permission" type="String" mode="IN" optional="false" />
	</service>
	
	<service name="jackrabbitGetPermission" engine="java"
		location="com.olbius.jackrabbit.client.services.JackrabbitOlbiusSecurityServices" invoke="jackrabbitGetPermission">
		<implements service="jackrabbitRemoteLogin" optional="true" />
		<attribute name="curPath" type="String" mode="IN" optional="false" />
		<attribute name="userId" type="String" mode="IN" optional="true" />
		<attribute name="groupId" type="String" mode="IN" optional="true" />
		<attribute name="permission" type="List" mode="OUT" optional="false" />
	</service>

</services>