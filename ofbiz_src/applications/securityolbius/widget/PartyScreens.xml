<?xml version="1.0" encoding="UTF-8"?>
<screens xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../framework/widget/dtd/widget-screen.xsd">
	<screen name="party">
		<section>
			<actions>
				<script location="component://securityolbius/webapp/script/com/olbius/security/party/loadParty.groovy" />
			</actions>
			<widgets>
				<decorator-screen name="securityCommonDecorator" location="${parameters.mainDecoratorLocation}">
					<decorator-section name="body">
						<platform-specific>
							<html>
								<html-template location="component://securityolbius/webapp/ftl/party/partyMain.ftl" />
							</html>
						</platform-specific>
					</decorator-section>
				</decorator-screen>
			</widgets>
		</section>
	</screen>

	<screen name="partyList">
		<section>
			<widgets>
				<platform-specific>
					<html>
						<html-template location="component://securityolbius/webapp/ftl/party/partyList.ftl" />
					</html>
				</platform-specific>
			</widgets>
		</section>
	</screen>
	
	<screen name="partyPerm">
		<section>
			<widgets>
				<platform-specific>
					<html>
						<html-template location="component://securityolbius/webapp/ftl/party/partyPerm.ftl" />
					</html>
				</platform-specific>
			</widgets>
		</section>
	</screen>
</screens>