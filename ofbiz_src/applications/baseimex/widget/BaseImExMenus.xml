	<menu name="BaseImExAppBar" modelMenu="olbius" iconStyle="fa-cart-plus" style="dropdown-toggle" title="${uiLabelMap.BaseSalesApplication}" extends="CommonAppBarMenu" extends-resource="component://common/widget/CommonMenus.xml">

	</menu>

	<menu name="PurchaseAgreementNavMenu" modelMenu="olbiusScreenletMenu" extends="CommonTabBarMenu" extends-resource="component://common/widget/CommonMenus.xml">
		<menu-item name="backPendingAgreement" title="${uiLabelMap.Back}" widget-style="icon-back open-sans">
			<link target="PurchaseAgreements"></link>
		</menu-item>
		<menu-item name="Print" title="${uiLabelMap.CommonPrint}" widget-style="icon-print open-sans">
			<link target="printPurchaseAgreement">
				<parameter param-name="agreementId" from-field="agreementId" />
			</link>
		</menu-item>
		<menu-item name="Approved" title="${uiLabelMap.ApprovedAgreement}" widget-style="icon-check-square-o">
			<condition>
				<and>
					<if-compare operator="equals" value="AGREEMENT_CREATED" field="agreement.statusId" />
					<if-has-permission permission="AGREEMENT" action="_ADMIN" />
				</and>
			</condition>
			<link target="approvedPurchaseAgreement">
				<parameter param-name="agreementId" from-field="agreementId" />
				<parameter param-name="statusId" value="AGREEMENT_APPROVED" />
			</link>
		</menu-item>
		<menu-item name="Sent" title="${uiLabelMap.ConfirmSentAgreement}" widget-style="icon-arrow-right open-sans">
			<condition>
				<and>
					<if-compare operator="equals" value="AGREEMENT_APPROVED" field="agreement.statusId" />
					<if-has-permission permission="AGREEMENT" action="_ADMIN" />
				</and>
			</condition>
			<link target="sentPurchaseAgreement">
				<parameter param-name="agreementId" from-field="agreementId" />
				<parameter param-name="statusId" value="AGREEMENT_SENT" />
			</link>
		</menu-item>
	</menu>

