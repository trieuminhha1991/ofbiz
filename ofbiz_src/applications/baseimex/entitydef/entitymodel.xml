	<entity entity-name="BillOfLading" package-name="org.ofbiz.product.facility" title="Bill Of Lading Entity">
		<field name="billId" type="id-ne"></field>
		<field name="billNumber" type="name"></field>
		<field name="billTypeId" type="id"></field>
		<field name="partyIdFrom" type="id"></field>
		<field name="partyIdTo" type="id"></field>
		<field name="description" type="description"></field>
		<field name="departureDate" type="date-time"></field>
		<field name="arrivalDate" type="date-time"></field>
		<field name="fromDate" type="date-time" />
		<field name="thruDate" type="date-time" />
		<prim-key field="billId" />
		<relation type="one" fk-name="FK_BILL_PARTY_FROM" rel-entity-name="Party" title="partyFrom">
			<key-map field-name="partyIdFrom" rel-field-name="partyId" />
		</relation>
		<relation type="one" fk-name="FK_BILL_PARTY_TO" rel-entity-name="Party" title="partyTo">
			<key-map field-name="partyIdTo" rel-field-name="partyId" />
		</relation>
		<relation type="one" fk-name="FK_BILL_TYPE" rel-entity-name="BillOfLadingType">
			<key-map field-name="billTypeId" rel-field-name="billTypeId" />
		</relation>
	</entity>

	<entity entity-name="BillOfLadingType" package-name="com.olbius.po" title="Bill Of Lading Type Entity">
		<field name="billTypeId" type="id-ne"></field>
		<field name="fromDate" type="date-time" />
		<field name="thruDate" type="date-time" />
		<prim-key field="billTypeId" />
	</entity>

	<entity entity-name="OrderAndContainer" package-name="com.olbius.po">
		<field name="orderId" type="id-ne"></field>
		<field name="containerId" type="id-ne"></field>
		<field name="billId" type="id-ne"></field>
		<field name="fromDate" type="date-time"></field>
		<field name="thruDate" type="date-time"></field>
		<prim-key field="orderId" />
		<prim-key field="billId" />
		<relation rel-entity-name="OrderHeader" type="one" fk-name="FK_ORDER_CONTAINER">
			<key-map field-name="orderId" rel-field-name="orderId" />
		</relation>
		<relation rel-entity-name="BillOfLading" type="one" fk-name="FK_BILL_OF_LADING">
			<key-map field-name="billId" rel-field-name="billId" />
		</relation>
		<relation rel-entity-name="Container" type="one" fk-name="FK_CONTAINER_ORDER">
			<key-map field-name="containerId" rel-field-name="containerId" />
		</relation>
	</entity>

	<entity entity-name="ContainerItem" package-name="com.olbius.po" never-cache="true">
		<field name="containerId" type="id-ne"></field>
		<field name="containerItemSeqId" type="id-ne"></field>
		<field name="productId" type="id"></field>
		<field name="fromOrderId" type="id"></field>
		<field name="fromOrderItemSeqId" type="id"></field>
		<field name="expireDate" type="date-time"></field>
		<field name="quantity" type="fixed-point"></field>
		<prim-key field="containerId"></prim-key>
		<prim-key field="containerItemSeqId"></prim-key>
		<relation rel-entity-name="Container" type="one" fk-name="FK_CT_ITEM_CONTAINER">
			<key-map field-name="containerId" rel-field-name="containerId" />
		</relation>
		<relation rel-entity-name="Product" type="one" fk-name="FK_CT_ITEM_PRODUCT">
			<key-map field-name="productId" rel-field-name="productId" />
		</relation>
		<relation rel-entity-name="OrderItem" type="one" fk-name="FK_CT_ITEM_ORDER">
			<key-map field-name="fromOrderId" rel-field-name="orderId" />
			<key-map field-name="fromOrderItemSeqId" rel-field-name="orderItemSeqId" />
		</relation>
	</entity>

	<entity entity-name="AgreementAndOrder" package-name="org.ofbiz.order.order">
		<description>map between orders and Agreement</description>
		<field name="agreementId" type="id-ne"></field>
		<field name="orderId" type="id-ne"></field>
		<field name="fromDate" type="date-time"></field>
		<field name="thruDate" type="date-time"></field>
		<prim-key field="agreementId" />
		<prim-key field="orderId" />
		<relation rel-entity-name="Agreement" type="one" fk-name="FK_AGREEMENT_NEW">
			<key-map field-name="agreementId" />
		</relation>
		<relation rel-entity-name="OrderHeader" type="one" fk-name="FK_ORDER_HEADER_NEW">
			<key-map field-name="orderId" rel-field-name="orderId" />
		</relation>
		<index name="IND_AGREEMENT">
			<index-field name="agreementId" />
		</index>
		<index name="IND_ORDER">
			<index-field name="orderId" />
		</index>
	</entity>

	<entity entity-name="QualityPublication" package-name="org.ofbiz.po">
		<description>Product quality publication</description>
		<field name="productId" type="id-ne"></field>
		<field name="qualityPublicationName" type="id-long"></field>
		<field name="fromDate" type="date-time"></field>
		<field name="thruDate" type="date-time"></field>
		<field name="expireDate" type="numeric"></field>
		<field name="contentId" type="id-ne"></field>
		<prim-key field="productId" />
		<prim-key field="fromDate" />
		<prim-key field="expireDate" />
		<relation rel-entity-name="Product" type="one" fk-name="PRO_QUA_PUB">
			<key-map field-name="productId" />
		</relation>
		<relation rel-entity-name="Content" type="one" fk-name="QUA_PUB_CONTENT">
			<key-map field-name="contentId" />
		</relation>
	</entity>

