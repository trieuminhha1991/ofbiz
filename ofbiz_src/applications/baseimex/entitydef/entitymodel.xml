	<entity entity-name="QuotaType" package-name="org.ofbiz.order.order" default-resource-name="CommonEntityLabels">
    	<description>Managed type of quota</description>
    	<field name="quotaTypeId" type="id-ne"></field>
    	<field name="parentTypeId" type="id-ne"></field>
    	<field name="description" type="description"></field>
    	<prim-key field="quotaTypeId"/>
    	<relation rel-entity-name="QuotaType" type="one" fk-name="FK_QUO_TYPE_PARENT">
			<key-map field-name="parentTypeId" rel-field-name="quotaTypeId"/>
		</relation>
    </entity>
    
    <entity entity-name="QuotaHeader" package-name="org.ofbiz.order.order">
    	<description>Managed quota of national</description>
    	<field name="quotaId" type="id-ne"/>
    	<field name="quotaCode" type="id-ne"/>
    	<field name="statusId" type="id-ne"/>
    	<field name="quotaName" type="description"/>
    	<field name="quotaTypeId" type="id-ne"/>
    	<field name="supplierPartyId" type="id-ne"/>
    	<field name="currencyUomId" type="id-ne"/>
		<field name="organizationPartyId" type="id-ne"/>
    	<field name="description" type="description"/>
    	<field name="createdDate" type="date-time"/>
    	<field name="createdByUserLogin" type="id-ne"/>
    	<prim-key field="quotaId"/>
    	<relation rel-entity-name="QuotaType" type="one" fk-name="FK_QUOTA_TYPE">
    		<key-map field-name="quotaTypeId" rel-field-name="quotaTypeId"/>
    	</relation>
    	<relation rel-entity-name="Party" type="one" title="supplier" fk-name="FK_PARTY_SUP_IQ_HD">
			<key-map field-name="supplierPartyId" rel-field-name="partyId"/>
		</relation>
		<relation rel-entity-name="Party" type="one" title="organization" fk-name="FK_PARTY_INTERNAL_IQ_HD">
			<key-map field-name="organizationPartyId" rel-field-name="partyId"/>
		</relation>
		<relation rel-entity-name="StatusItem" type="one" fk-name="FK_STATUS_IQ_HD">
			<key-map field-name="statusId"/>
		</relation>
	 	<relation type="one" rel-entity-name="UserLogin" title="createdBy" fk-name="FK_QUOTA_CRUSER">
            <key-map field-name="createdByUserLogin" rel-field-name="userLoginId"/>
      	</relation>
	 	<relation type="one" rel-entity-name="Uom" title="currency" fk-name="FK_QUOTA_CURRENCY">
            <key-map field-name="currencyUomId" rel-field-name="uomId"/>
      	</relation>
    </entity>
    
    <entity entity-name="QuotaItem" package-name="org.ofbiz.order.order" >
    	<description>Managed quota of specific product</description>
    	<field name="quotaId" type="id-ne"/>
    	<field name="quotaItemSeqId" type="id-ne"/>
    	<field name="statusId" type="id-ne"/>
    	<field name="productId" type="id-ne"/>
    	<field name="quotaQuantity" type="fixed-point"/>
    	<field name="availableQuantity" type="fixed-point"/>
    	<field name="uomId" type="id-ne"/>
    	<field name="fromDate" type="date-time"/>
    	<field name="thruDate" type="date-time"/>
    	<prim-key field="quotaId"/>
    	<prim-key field="quotaItemSeqId"/>
    	<relation rel-entity-name="QuotaHeader" type="one" fk-name="FK_QUOTA_HEADER">
    		<key-map field-name="quotaId" rel-field-name="quotaId"/>
    	</relation>
    	<relation rel-entity-name="Product" type="one" fk-name="FK_QUOTA_PRODUCT">
    		<key-map field-name="productId" rel-field-name="productId"/>
    	</relation>
    	<relation rel-entity-name="Uom" type="one" fk-name="FK_QUOTA_UOM">
    		<key-map field-name="uomId" rel-field-name="uomId"/>
    	</relation>
    	<relation rel-entity-name="StatusItem" type="one" fk-name="FK_STATUS_IQ">
			<key-map field-name="statusId"/>
		</relation>
    </entity>
    
    <!-- test and quarantine -->
    
   	<entity entity-name="ProductEventType" package-name="org.ofbiz.order.order" default-resource-name="CommonEntityLabels">
    	<description>Managed type of quota</description>
    	<field name="eventTypeId" type="id-ne"></field>
    	<field name="parentTypeId" type="id-ne"></field>
    	<field name="description" type="description"></field>
    	<prim-key field="eventTypeId"/>
    	<relation rel-entity-name="ProductEventType" type="one" fk-name="FK_QALTEST_TYPE_PARENT">
			<key-map field-name="parentTypeId" rel-field-name="eventTypeId"/>
		</relation>
    </entity>
    
    <entity entity-name="ProductEvent" package-name="org.ofbiz.order.order">
    	<description>Managed event testing and quarantine product</description>
    	<field name="eventId" type="id-ne"/>
    	<field name="eventCode" type="id-ne"/>
    	<field name="statusId" type="id-ne"/>
    	<field name="eventName" type="description"/>
    	<field name="eventTypeId" type="id-ne"/>
		<field name="organizationPartyId" type="id-ne"/>
		<field name="agreementId" type="id-ne"/>
		<field name="packingListId" type="id-ne"/>
    	<field name="description" type="description"/>
    	<field name="createdDate" type="date-time"/>
    	<field name="executedDate" type="date-time"/>
    	<field name="completedDate" type="date-time"/>
    	<field name="createdByUserLogin" type="id-ne"/>
    	<prim-key field="eventId"/>
    	<relation rel-entity-name="ProductEventType" type="one" fk-name="FK_QLT_TEST_TYPE">
    		<key-map field-name="eventTypeId" rel-field-name="eventTypeId"/>
    	</relation>
		<relation rel-entity-name="Party" type="one" fk-name="FK_PARTY_INTERNAL_QTL_TEST">
			<key-map field-name="organizationPartyId" rel-field-name="partyId"/>
		</relation>
		<relation rel-entity-name="StatusItem" type="one" fk-name="FK_STATUS_QTL_TEST">
			<key-map field-name="statusId"/>
		</relation>
	 	<relation type="one" rel-entity-name="UserLogin" fk-name="FK_QTL_TEST_CRUSER">
            <key-map field-name="createdByUserLogin" rel-field-name="userLoginId"/>
      	</relation>
	 	<relation type="one" rel-entity-name="PackingListHeader" fk-name="FK_QLT_TEST_PKL">
            <key-map field-name="packingListId"/>
      	</relation>
	 	<relation type="one" rel-entity-name="Agreement" fk-name="FK_QLT_TEST_AGREE">
            <key-map field-name="agreementId"/>
      	</relation>
    </entity>
    
    <entity entity-name="ProductEventItem" package-name="org.ofbiz.order.order" >
    	<field name="eventId" type="id-ne"/>
    	<field name="eventItemSeqId" type="id-ne"/>
    	<field name="statusId" type="id-ne"/>
    	<field name="productId" type="id-ne"/>
    	<field name="registeredQuantity" type="fixed-point"/>
    	<field name="quantity" type="fixed-point"/>
    	<field name="uomId" type="id-ne"/>
    	<field name="fromDate" type="date-time"/>
    	<field name="thruDate" type="date-time"/>
    	<prim-key field="eventId"/>
    	<prim-key field="eventItemSeqId"/>
    	<relation rel-entity-name="ProductEvent" type="one" fk-name="FK_QLT_TEST_HEADER">
    		<key-map field-name="eventId" rel-field-name="eventId"/>
    	</relation>
    	<relation rel-entity-name="Product" type="one" fk-name="FK_QLT_TEST_PRODUCT">
    		<key-map field-name="productId" rel-field-name="productId"/>
    	</relation>
    	<relation rel-entity-name="Uom" type="one" fk-name="FK_QLT_TEST_UOM">
    		<key-map field-name="uomId" rel-field-name="uomId"/>
    	</relation>
    	<relation rel-entity-name="StatusItem" type="one" fk-name="FK_STATUS_QLT_TEST">
			<key-map field-name="statusId"/>
		</relation>
    </entity>
    	
	<extend-entity entity-name="Requirement">
		<field name="eventId" type="id"></field>
