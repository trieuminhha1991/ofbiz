    <sql>select sf.sales_forecast_id as eff_id, sf.parent_sales_forecast_id as parent_eff_id, sf.organization_party_id as organ_eff, sf.custom_time_period_id as time_eff,
	sfd.sales_forecast_detail_id as detail_eff, sfd.quantity as quantity_eff, null as value_eff, sfd.product_id as product_eff, sfd.product_category_id as category_eff,
	'SALES_FORECAST' as type, sf.internal_party_id as internal_party_id, 
	getDateDimensionId(DATE_FORMAT(ctp.from_date, '%Y-%m-%d')) as from_date_dim_id,
    getDateDimensionId(DATE_FORMAT(ctp.thru_date, '%Y-%m-%d')) as thru_date_dim_id
from sales_forecast_detail as sfd
inner join sales_forecast as sf on sf.sales_forecast_id = sfd.sales_forecast_id
inner join custom_time_period as ctp on ctp.custom_time_period_id = sf.custom_time_period_id
WHERE sf.last_updated_stamp >= '${lastupdate}' AND sfd.last_updated_stamp >= '${lastupdate}' AND ctp.last_updated_stamp >= '${lastupdate}'
UNION
select ss.sales_statement_id as eff_id, ss.parent_sales_statement_id as parent_eff_id, ss.organization_party_id as organ_eff, ss.custom_time_period_id as time_eff,
ssd.sales_statement_detail_seq_id as detail_eff, ssd.quantity as quantity_eff, ssd.amount as value_eff,ssd.product_id as product_eff, ssd.product_category_id as category_eff, 
ss.sales_statement_type_id as type, ss.internal_party_id as internal_party_id,
getDateDimensionId(DATE_FORMAT(ctp.from_date, '%Y-%m-%d')) as from_date_dim_id,
getDateDimensionId(DATE_FORMAT(ctp.thru_date, '%Y-%m-%d')) as thru_date_dim_id
from sales_statement_detail as ssd
inner join sales_statement as ss on ss.sales_statement_id = ssd.sales_statement_id
inner join custom_time_period as ctp on ctp.custom_time_period_id = ss.custom_time_period_id
where ssd.last_updated_stamp >= '${lastupdate}' AND ss.last_updated_stamp >= '${lastupdate}' AND ctp.last_updated_stamp >= '${lastupdate}'</sql>
