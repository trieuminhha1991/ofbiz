    <sql>SELECT null as address1, null as road_geo_id, null as ward_geo_id, null as district_geo_id, null as state_province_geo_id, null as country_geo_id, &#xd;&#xa;		null as road_name, null as ward_name, null as district_name, null as state_name, null as country_name, &#xd;&#xa;		null as sub_region_geo_id, &#x27;Kh&#xf4;ng x&#xe1;c &#x111;&#x1ecb;nh&#x27; as sub_region_name, null as region_geo_id, &#x27;Kh&#xf4;ng x&#xe1;c &#x111;&#x1ecb;nh&#x27; as region_name&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT PA.address1, PA.road_geo_id, PA.ward_geo_id, PA.district_geo_id, PA.state_province_geo_id, PA.country_geo_id,&#xd;&#xa;		RG.geo_name AS road_name, WG.geo_name as ward_name, DG.geo_name as district_name, SG.geo_name as state_name, CG.geo_name as country_name,&#xd;&#xa;		SUPREG.geo_id AS sub_region_geo_id, SUPREG.geo_name AS sub_region_name, REGION.geo_id AS region_geo_id, REGION.geo_name AS region_name&#xd;&#xa;FROM postal_address AS PA&#xd;&#xa;	LEFT JOIN geo AS RG on PA.road_geo_id &#x3d; RG.geo_id &#xd;&#xa;	LEFT JOIN geo AS WG on PA.ward_geo_id &#x3d; WG.geo_id &#xd;&#xa;	LEFT JOIN geo AS DG on PA.district_geo_id &#x3d; DG.geo_id &#xd;&#xa;	LEFT JOIN geo AS SG on PA.state_province_geo_id &#x3d; SG.geo_id &#xd;&#xa;	LEFT JOIN geo AS CG on PA.country_geo_id &#x3d; CG.geo_id &#xd;&#xa;	LEFT JOIN geo AS SUPREG on SUPREG.geo_id &#x3d; &#x28;&#xd;&#xa; 		SELECT G1.geo_id &#xd;&#xa; 		FROM geo_assoc as GA1, geo as G1&#xd;&#xa; 		WHERE GA1.geo_id_to &#x3d; SG.geo_id and GA1.geo_id &#x3d; G1.geo_id and G1.geo_type_id &#x3d; &#x27;SUBREGION&#x27; and GA1.geo_assoc_type_id &#x3d; &#x27;REGIONS&#x27; &#xd;&#xa; 		LIMIT 1&#xd;&#xa; 	&#x29; &#xd;&#xa; 	LEFT JOIN geo AS REGION on REGION.geo_id &#x3d; &#x28;&#xd;&#xa; 		SELECT G2.geo_id &#xd;&#xa; 		FROM geo_assoc as GA2, geo as G2&#xd;&#xa; 		WHERE GA2.geo_id_to &#x3d; SG.geo_id and GA2.geo_id &#x3d; G2.geo_id and G2.geo_type_id &#x3d; &#x27;REGION&#x27; and GA2.geo_assoc_type_id &#x3d; &#x27;REGIONS&#x27; &#xd;&#xa; 		LIMIT 1&#xd;&#xa; 	&#x29; &#xd;&#xa;WHERE PA.last_updated_stamp &#x3e;&#x3d; &#x27;&#x24;&#x7b;lastupdate&#x7d;&#x27;&#xd;&#xa;</sql>
    <limit>0</limit>
