           </partitioning>
    <connection>olbius</connection>
    <sql>SELECT&#xa;    invoice_amount.&#x2a;, &#xa;    case when invoice_tax.taxamount is not null then invoice_tax.taxamount else 0 end as taxamount, &#xa;    invoice_tax.tax_authority_rate_seq_id, &#xa;    inv.invoice_date, &#xa;    inv.invoice_type_id,&#xa;    inv_tax_info.party_name, &#xa;    inv_tax_info.tax_code, &#xa;    inv.currency_uom_id,&#xa;    getDateDimensionId&#x28;inv.invoice_date&#x3a;&#x3a;date&#x29; as invoice_date_dim_id&#xa;FROM invoice inv inner join invoice_tax_info as inv_tax_info on inv.invoice_id &#x3d; inv_tax_info.invoice_id&#xa;	inner join&#xa;    &#x28;&#xa;        SELECT&#xa;            ii.invoice_id ,&#xa;            SUM&#x28;ii.amount &#x2a; ii.quantity&#x29; AS totalAmount&#xa;        FROM invoice_item ii&#xa;        WHERE ii.tax_authority_rate_seq_id IS NULL&#xa;        GROUP BY ii.invoice_id&#xa;    &#x29; AS invoice_amount on invoice_amount.invoice_id &#x3d; inv.invoice_id&#xa;    LEFT JOIN&#x28;&#xa;        SELECT ii.invoice_id ,&#xa;            SUM&#x28;ii.amount &#x2a; ii.quantity&#x29; AS taxAmount, ii.tax_authority_rate_seq_id&#xa;        FROM invoice_item ii&#xa;        WHERE ii.tax_authority_rate_seq_id IS NOT NULL&#xa;        GROUP BY ii.invoice_id, ii.tax_authority_rate_seq_id&#xa;    &#x29; AS invoice_tax ON invoice_amount.invoice_id &#x3d; invoice_tax.invoice_id&#xa;	&#xa;WHERE inv.last_updated_stamp &#x3e;&#x3d; &#x27;&#x24;&#x7b;lastupdate&#x7d;&#x27;</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
