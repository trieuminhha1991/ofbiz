    <sql>SELECT SO.order_id, SO.order_item_seq_id,&#xd;&#xa;	SO.order_name, SO.order_date, CRCY.currency_id AS currency_uom,&#xd;&#xa;	PS.product_store_id, PS.store_name AS product_store_name,&#xd;&#xa;	SO.tax_amount, SO.discount_amount, SO.sub_total_amount, SO.total_amount, &#xd;&#xa;	PROD.product_code AS product_id, PROD.product_name, CATE.category_id AS primary_product_category_id,&#xd;&#xa;	SO.quantity, SO.cancel_quantity, SO.return_quantity, SO.total_quantity,&#xd;&#xa;	SO.total_selected_amount, SO.unit_price, &#xd;&#xa;	SO.quantity_uom_id, RETN.return_id, RETN.return_price, PACF.average_cost AS product_avg_cost, &#xd;&#xa;	ENUM_SM.enum_id AS sales_method_channel_enum_id, ENUM_SC.enum_id AS sales_channel_enum_id,&#xd;&#xa;	SO.order_status_id, SO.order_item_status_id, PTY.party_code AS creator_id, SO.sgc_customer_id &#xd;&#xa;FROM sales_order_new_fact AS SO&#xd;&#xa;LEFT JOIN currency_dimension AS CRCY ON SO.currency_dim_id &#x3d; CRCY.dimension_id&#xd;&#xa;LEFT JOIN product_store_dimension AS PS ON SO.product_store_dim_id &#x3d; PS.dimension_id&#xd;&#xa;LEFT JOIN product_dimension AS PROD ON SO.product_dim_id &#x3d; PROD.dimension_id&#xd;&#xa;LEFT JOIN category_dimension AS CATE ON PROD.primary_category_dim_id &#x3d; CATE.dimension_id&#xd;&#xa;LEFT JOIN return_sales_order_new_fact AS RETN ON SO.order_id &#x3d; RETN.order_id AND SO.order_item_seq_id &#x3d; RETN.order_item_seq_id&#xd;&#xa;LEFT JOIN enumeration_dimension AS ENUM_SM ON SO.sales_method_channel_dim_id &#x3d; ENUM_SM.dimension_id&#xd;&#xa;LEFT JOIN enumeration_dimension AS ENUM_SC ON SO.sales_channel_dim_id &#x3d; ENUM_SC.dimension_id&#xd;&#xa;LEFT JOIN party_dimension AS PTY ON SO.creator_dim_id &#x3d; PTY.dimension_id&#xd;&#xa;LEFT JOIN facility_dimension AS FACI ON PS.product_store_id &#x3d; FACI.facility_id&#xd;&#xa;LEFT JOIN LATERAL &#x28;&#xd;&#xa;	SELECT PACF_INNER.average_cost &#xd;&#xa;	FROM product_average_cost_fact as PACF_INNER &#xd;&#xa;	WHERE FACI.dimension_id &#x3d; PACF_INNER.facility_dim_id &#xd;&#xa;		AND SO.product_dim_id &#x3d; PACF_INNER.product_dim_id&#xd;&#xa;		AND &#x28;&#x28;SO.order_date BETWEEN PACF_INNER.from_date AND PACF_INNER.thru_date&#x29; OR &#x28;SO.order_date &#x3e; PACF_INNER.from_date AND PACF_INNER.thru_date is null&#x29;&#x29;&#xd;&#xa;	LIMIT 1&#xd;&#xa;&#x29; AS PACF on true&#xd;&#xa;WHERE SO.last_updated_stamp &#x3e;&#x3d; &#x27;&#x24;&#x7b;lastupdate&#x7d;&#x27;</sql>
    <limit>0</limit>
