    <sql>select  cast&#x28;&#x27;&#x24;&#x7b;cur_date&#x7d;&#x27; as datetime&#x29; AS _update,acctg_trans_id, acctg_trans_type_id, document_parent_type, document_type,&#xa;	case when document_parent_type &#x3d; &#x27;INVOICE_TYPE&#x27; then concat&#x28;COALESCE&#x28;document_id, &#x27;&#x27;&#x29;,document_number&#x29; else null end as invoice_id,&#xa;	null as payment_id, description, organization_party_id, organization_party_name, party_id, party_code, party_name,&#xa;	gl_account_id, account_code, account_name, currency_id, transaction_date, null as voucher_code, facility_id, facility_name,&#xa;	sum&#x28;dr_amount&#x29; as dr_amount, sum&#x28;cr_amount&#x29; as cr_amount&#xa;from acctg_document_list_fact&#xa;where document_parent_type &#x3d; &#x27;INVOICE_TYPE&#x27; and last_updated_stamp &#x3e;&#x3d; &#x27;&#x24;&#x7b;lastUpdateMin&#x7d;&#x27;&#xa;group by acctg_trans_id, acctg_trans_type_id, document_parent_type,document_id, document_number, document_type,&#xa;	description, organization_party_id, organization_party_name, party_id, party_code, party_name,&#xa;	gl_account_id, account_code, account_name, currency_id, transaction_date, voucher_code, facility_id, facility_name&#xa;union &#xa;select cast&#x28;&#x27;&#x24;&#x7b;cur_date&#x7d;&#x27; as datetime&#x29; AS _update, acctg_trans_id, acctg_trans_type_id, document_parent_type, document_type,&#xa;	null as invoice_id,&#xa;	case when document_parent_type &#x3d; &#x27;PAYMENT_TYPE&#x27; then document_number else null end as payment_id,&#xa;	description, organization_party_id, organization_party_name, party_id, party_code, party_name,&#xa;	gl_account_id, account_code, account_name, currency_id, transaction_date, voucher_code, facility_id, facility_name,&#xa;	sum&#x28;dr_amount&#x29; as dr_amount, sum&#x28;cr_amount&#x29; as cr_amount&#xa;from acctg_document_list_fact&#xa;where document_parent_type &#x3d; &#x27;PAYMENT_TYPE&#x27; and voucher_code is not null and last_updated_stamp &#x3e;&#x3d; &#x27;&#x24;&#x7b;lastUpdateMin&#x7d;&#x27;&#xa;group by acctg_trans_id, acctg_trans_type_id, document_parent_type, document_number, document_type,&#xa;	description, organization_party_id, organization_party_name, party_id, party_code, party_name,&#xa;	gl_account_id, account_code, account_name, currency_id, transaction_date, voucher_code, facility_id, facility_name&#xa;</sql>
