  <step>
    <name>Create view acctg detail</name>
    <type>ExecSQL</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>olbius</connection>
    <execute_each_row>N</execute_each_row>
    <single_statement>Y</single_statement>
    <replace_variables>Y</replace_variables>
    <quoteString>N</quoteString>
    <sql>-- STEP 1&#x3a; DROP VIEW&#xa;--DROP VIEW IF EXISTS view_acctg_trans_entry_detail CASCADE&#x3b;&#xa;&#xa;-- STEP 2&#x3a; CREATE VIEW&#xa;CREATE OR REPLACE VIEW view_acctg_trans_entry_detail AS&#xa;SELECT ats.acctg_trans_id,&#xa;       ate.acctg_trans_entry_seq_id,&#xa;       ats.acctg_trans_type_id,&#xa;       ate.organization_party_id,&#xa;       CASE&#xa;           WHEN ate.party_id NOTNULL THEN ate.party_id&#xa;           ELSE ats.party_id&#xa;       END AS party_id,&#xa;       ate.product_id,&#xa;       ate.gl_account_type_id,&#xa;       ate.gl_account_id,&#xa;       ate.amount,&#xa;       ate.orig_amount,&#xa;       ate.currency_uom_id,&#xa;       ate.orig_currency_uom_id,&#xa;       ate.debit_credit_flag,&#xa;       ats.transaction_date,&#xa;       ate.due_date,&#xa;       ats.invoice_id,&#xa;       ate.inventory_item_id,&#xa;       ate.tax_id,&#xa;       ats.payment_id,&#xa;       ats.shipment_id,&#xa;       ats.receipt_id,&#xa;       ats.work_effort_id&#xa;FROM acctg_trans AS ats&#xa;INNER JOIN acctg_trans_entry AS ate ON ate.acctg_trans_id &#x3d; ats.acctg_trans_id&#xa;WHERE ats.is_posted &#x3d; &#x27;Y&#x27;&#xa;--  AND ats.acctg_trans_type_id &#x3d; &#x27;PURCHASE_INVOICE&#x27;&#xa;--    AND &#x28;ate.last_updated_stamp &#x3e;&#x3d; &#x27;&#x24;&#x7b;lastupdate&#x7d;&#x27;&#xa;--        OR ats.last_updated_stamp &#x3e;&#x3d; &#x27;&#x24;&#x7b;lastupdate&#x7d;&#x27;&#x29;&#xa;ORDER BY ate.acctg_trans_id,&#xa;         ate.acctg_trans_entry_seq_id &#x3b;&#xa;&#x3b;</sql>
    <set_params>N</set_params>
    <insert_field/>
    <update_field/>
    <delete_field/>
    <read_field/>
    <arguments>
    </arguments>
