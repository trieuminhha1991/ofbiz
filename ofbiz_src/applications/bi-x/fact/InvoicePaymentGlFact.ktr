    <replace_variables>Y</replace_variables>
    <quoteString>N</quoteString>
    <sql>-- STEP 1&#x3a; DROP VIEW&#xa;--DROP VIEW IF EXISTS view_invoice_payment CASCADE&#x3b;&#xa;&#xa;-- STEP 2&#x3a; CREATE VIEW&#xa;CREATE OR REPLACE VIEW view_invoice_payment AS&#xa;SELECT pap.pa_payment_application_id AS pa_payment_application_id,&#xd;&#xa;    pap.pa_amount_applied AS pa_amount_applied,&#xd;&#xa;    pap.payment_app_status_id as payment_app_status_id,&#xd;&#xa;    --&#xd;&#xa;    pai.inv_invoice_id AS inv_invoice_id,&#xd;&#xa;    pai.inv_party_id_from AS inv_party_id_from,&#xd;&#xa;    pai.inv_party_id AS inv_party_id,&#xd;&#xa;    pai.inv_invoice_type_id AS inv_invoice_type_id,&#xd;&#xa;    pai.inv_parent_type_id AS inv_parent_type_id,&#xd;&#xa;    pai.inv_status_id AS inv_status_id,&#xd;&#xa;    pai.inv_due_date AS inv_due_date,&#xd;&#xa;    pai.inv_paid_date AS inv_paid_date,&#xd;&#xa;    pai.inv_invoice_date AS inv_invoice_date,&#xd;&#xa;    pai.inv_total_amount AS inv_total_amount,&#xd;&#xa;    --&#xd;&#xa;    pap.pt_payment_id AS pt_payment_id,&#xd;&#xa;    pap.pt_payment_type_id AS pt_payment_type_id,&#xd;&#xa;    pap.pt_parent_type_id AS pt_parent_type_id,&#xd;&#xa;    pap.pt_party_id_from AS pt_party_id_from,&#xd;&#xa;    pap.pt_party_id_to AS pt_party_id_to,&#xd;&#xa;    pap.pt_amount AS pt_amount,&#xd;&#xa;    pap.pt_status_id AS pt_status_id,&#xd;&#xa;    pap.pt_effective_date AS pt_effective_date,&#xd;&#xa;    pap.pt_payment_method_type_id AS pt_payment_method_type_id,&#xd;&#xa;    pap.pt_payment_method_id AS pt_payment_method_id FROM    &#xd;&#xa;&#x28;SELECT&#xd;&#xa;    pa.payment_application_id AS pa_payment_application_id,&#xd;&#xa;    pa.amount_applied AS pa_amount_applied,&#xd;&#xa;    pa.payment_app_status_id as payment_app_status_id,&#xd;&#xa;    --&#xd;&#xa;    i.invoice_id AS inv_invoice_id,&#xd;&#xa;    i.party_id_from AS inv_party_id_from,&#xd;&#xa;    i.party_id AS inv_party_id,&#xd;&#xa;    i.invoice_type_id AS inv_invoice_type_id,&#xd;&#xa;    it.parent_type_id AS inv_parent_type_id,&#xd;&#xa;    i.status_id AS inv_status_id,&#xd;&#xa;    i.due_date AS inv_due_date,&#xd;&#xa;    i.paid_date AS inv_paid_date,&#xd;&#xa;    i.invoice_date AS inv_invoice_date,&#xd;&#xa;    iis.total_amount AS inv_total_amount&#xd;&#xa;FROM payment_application_status AS pa&#xd;&#xa;---------------- JOIN&#x3a; payment ----------------&#xd;&#xa;RIGHT JOIN invoice AS i&#xd;&#xa;ON i.invoice_id &#x3d; pa.invoice_id&#xd;&#xa;INNER JOIN invoice_type AS it&#xd;&#xa;    ON it.invoice_type_id &#x3d; i.invoice_type_id&#xd;&#xa;INNER JOIN&#xd;&#xa;    &#x28;&#xd;&#xa;	 SELECT invoice_id, sum&#x28;total_amount&#x29; AS total_amount FROM&#xd;&#xa;     &#x28;SELECT ii.invoice_id, &#xd;&#xa;    	CASE &#xd;&#xa;    		WHEN &#x28;ii.invoice_item_type_id like &#x27;PAYROL_TUNS&#x25;&#x27;&#x29;  THEN 0 &#xd;&#xa;    		WHEN &#x28;ii.invoice_item_type_id like &#x27;PAYROL_SOCS&#x25;&#x27;&#x29;  THEN 0 &#xd;&#xa;    		WHEN &#x28;ii.invoice_item_type_id like &#x27;PAYROL_MDS&#x25;&#x27;&#x29;  THEN 0 &#xd;&#xa;    		WHEN &#x28;ii.invoice_item_type_id like &#x27;PAYROL_MEDS&#x25;&#x27;&#x29;  THEN 0 &#xd;&#xa;    		ELSE SUM&#x28;ii.amount &#x2a; ii.quantity&#x29;&#xd;&#xa;    	END AS total_amount&#xd;&#xa;    FROM invoice_item AS ii&#xd;&#xa;    GROUP BY ii.invoice_id , ii.invoice_item_type_id&#x29; AS iite&#xd;&#xa;    GROUP BY invoice_id&#xd;&#xa;    &#x29; AS iis&#xd;&#xa;    ON iis.invoice_id &#x3d; i.invoice_id&#xd;&#xa;WHERE&#xd;&#xa;	1 &#x3d; 1 AND&#xd;&#xa;    &#x28;i.status_id IN &#x28;&#x27;INVOICE_READY&#x27;, &#x27;INVOICE_PAID&#x27;, &#x27;INVOICE_RECEIVED&#x27;, &#x27;INVOICE_WRITEOFF&#x27;, &#x27;INVOICE_IN_PROCESS&#x27;&#x29;&#xd;&#xa;    OR i.status_id ISNULL&#x29;&#xd;&#xa;     AND &#x28;i.last_updated_stamp &#x3e;&#x3d; &#x27;&#x24;&#x7b;lastupdate&#x7d;&#x27;&#x29;      &#xd;&#xa;&#x29; AS pai&#xd;&#xa;FULL OUTER JOIN&#xd;&#xa;&#x28;SELECT &#xd;&#xa;    pa.payment_application_id AS pa_payment_application_id,&#xd;&#xa;    pa.amount_applied AS pa_amount_applied,&#xd;&#xa;    pa.payment_app_status_id as payment_app_status_id,   &#xd;&#xa;    --&#xd;&#xa;    p.payment_id AS pt_payment_id,&#xd;&#xa;    p.payment_type_id AS pt_payment_type_id,&#xd;&#xa;    pt.parent_type_id AS pt_parent_type_id,&#xd;&#xa;    p.party_id_from AS pt_party_id_from,&#xd;&#xa;    p.party_id_to AS pt_party_id_to,&#xd;&#xa;    p.amount AS pt_amount,&#xd;&#xa;    p.status_id AS pt_status_id,&#xd;&#xa;    p.effective_date AS pt_effective_date,&#xd;&#xa;    p.payment_method_type_id AS pt_payment_method_type_id,&#xd;&#xa;    p.payment_method_id AS pt_payment_method_id,&#xd;&#xa;    p.status_id&#xd;&#xa;    --&#xd;&#xa;FROM payment_application_status AS pa&#xd;&#xa;RIGHT JOIN payment AS p&#xd;&#xa;    ON &#x28;p.payment_id &#x3d; pa.payment_id&#x29;&#xd;&#xa;INNER JOIN payment_type AS pt&#xd;&#xa;    ON pt.payment_type_id &#x3d; p.payment_type_id&#xd;&#xa;WHERE&#xd;&#xa;	&#x28;1 &#x3d; 1&#x29;&#xd;&#xa;	AND &#x28;p.status_id IN &#x28;&#x27;PMNT_SENT&#x27;, &#x27;PMNT_RECEIVED&#x27;, &#x27;PMNT_CONFIRMED&#x27;, &#x27;PMNT_VOID&#x27;&#x29;&#xd;&#xa;        OR p.status_id ISNULL&#xd;&#xa;        &#x29;&#xd;&#xa;	AND &#x28;pt.last_updated_stamp &#x3e;&#x3d; &#x27;&#x24;&#x7b;lastupdate&#x7d;&#x27;&#xd;&#xa;		OR p.last_updated_stamp &#x3e;&#x3d; &#x27;&#x24;&#x7b;lastupdate&#x7d;&#x27;&#x29;        &#xd;&#xa;&#x29; AS pap    &#xd;&#xa;ON pap.pa_payment_application_id &#x3d; pai.pa_payment_application_id&#xd;&#xa;ORDER BY&#xd;&#xa;    pa_payment_application_id&#x3b;&#xd;&#xa; </sql>
    <set_params>N</set_params>
