    <quoteString>N</quoteString>
    <sql>-- STEP 1&#x3a; DROP VIEW&#xa;--DROP VIEW IF EXISTS view_invoice_payment_gl CASCADE&#x3b;&#xa;&#xa;-- STEP 2&#x3a; CREATE VIEW&#xa;CREATE OR REPLACE VIEW view_invoice_payment_gl AS&#xa;SELECT&#xa;    vip.pa_payment_application_id,&#xa;    vip.payment_app_status_id,&#xa;    vip.inv_invoice_id,&#xa;    vip.inv_total_amount,&#xa;    vip.pt_payment_id,&#xa;    vip.pa_amount_applied,&#xa;    -- inv&#xd;&#xa;    CASE&#xd;&#xa;       	WHEN vip.inv_party_id_from NOTNULL THEN vip.inv_party_id_from&#xd;&#xa;        	ELSE vip.pt_party_id_to&#xd;&#xa;       	END AS inv_party_id_from,&#xd;&#xa;    CASE&#xd;&#xa;       	WHEN vip.inv_party_id NOTNULL THEN vip.inv_party_id&#xd;&#xa;        	ELSE vip.pt_party_id_from&#xd;&#xa;       	END AS inv_party_id,&#xa;--    vip.inv_party_id_from,&#xa;--    vip.inv_party_id,&#xa;    vip.inv_invoice_type_id,&#xa;    vip.inv_parent_type_id,&#xa;    vip.inv_invoice_date,&#xa;     vip.inv_due_date,&#xa;    -- pt&#xa;    vip.pt_payment_type_id,&#xa;    vip.pt_parent_type_id,&#xa;    vip.pt_payment_method_type_id,&#xa;    vip.pt_payment_method_id,&#xa;    vip.pt_effective_date,&#xa;---------- acctg FROM invoice&#xa;    vated_inv.acctg_trans_id AS inv_acctg_trans_id,&#xa;    vated_inv.gl_account_type_id AS inv_gl_account_type_id,&#xa;    vated_inv.gl_account_id AS inv_gl_account_id,&#xa;    vated_inv.amount AS inv_amount,&#xa;    vated_inv.currency_uom_id AS inv_currency_uom_id,&#xa;    vated_inv.orig_amount AS inv_orig_amount,&#xa;    vated_inv.orig_currency_uom_id AS inv_orig_currency_uom_id,&#xa;    vated_inv.debit_credit_flag AS inv_debit_credit_flag,&#xa;---------- acctg FROM payment&#xa;    vated_pt.acctg_trans_id AS pt_acctg_trans_id,&#xa;    vated_pt.gl_account_type_id AS pt_gl_account_type_id,&#xa;    vated_pt.gl_account_id AS pt_gl_account_id,&#xa;    vated_pt.amount AS pt_amount,&#xa;    vated_pt.currency_uom_id AS pt_currency_uom_id,&#xa;    vated_pt.orig_amount AS pt_orig_amount,&#xa;    vated_pt.orig_currency_uom_id AS pt_orig_currency_uom_id,&#xa;    vated_pt.debit_credit_flag AS pt_debit_credit_flag&#xa;FROM view_invoice_payment AS vip&#xa;LEFT JOIN view_acctg_trans_entry_detail AS vated_inv&#xa;    ON vated_inv.invoice_id &#x3d; vip.inv_invoice_id&#xa;    AND vated_inv.amount &#x3d; vip.inv_total_amount&#xa;LEFT JOIN view_acctg_trans_entry_detail AS vated_pt&#xa;    ON vated_pt.payment_id &#x3d; vip.pt_payment_id&#xa;    AND vated_pt.amount &#x3d; vip.pt_amount&#xa;ORDER BY&#xa;    vip.inv_invoice_id,&#xa;    vip.pt_payment_id,&#xa;    vip.pa_payment_application_id&#xa;&#x3b;</sql>
