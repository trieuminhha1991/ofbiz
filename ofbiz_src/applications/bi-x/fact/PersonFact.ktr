    <sql>SELECT party_id, party_group, role_type_id_to,
	getDateDimensionId(DATE_FORMAT(cur_date, '%Y-%m-%d')) as cur_date,
	getDateDimensionId(DATE_FORMAT(from_date, '%Y-%m-%d')) as from_date,
	getDateDimensionId(DATE_FORMAT(thru_date, '%Y-%m-%d')) as thru_date,
	getDateDimensionId(DATE_FORMAT(birth_date, '%Y-%m-%d')) as birth_date
FROM (
	SELECT party_id, party_group, role_type_id_to,  cast('${cur_date}' as datetime) AS cur_date, from_date, thru_date, birth_date
	FROM (
		SELECT party.party_id, party_relationship.party_id_from as party_group, party_relationship.role_type_id_to,
			cast(party_relationship.thru_date as date) AS thru_date,
			CASE WHEN party_relationship.last_updated_stamp >= '${lastupdate}' THEN cast(party_relationship.from_date as date)
				 ELSE cast('${lastupdate}' as date) END AS from_date,
				 cast(person.BIRTH_DATE as date) as birth_date
		FROM party
		INNER JOIN party_relationship ON party.party_id = party_relationship.party_id_to
		left join person on party.party_id = person.party_id
		WHERE party_type_id = 'PERSON' AND (party_relationship.last_updated_stamp >= '${lastupdate}'
					OR cast('${cur_date}' as datetime) > cast('${lastupdate}' as datetime))
	) AS tmp
) AS tmp2
WHERE thru_date is NULL OR cur_date &lt; thru_date</sql>
