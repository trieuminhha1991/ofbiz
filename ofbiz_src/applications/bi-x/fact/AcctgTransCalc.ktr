    <sql>SELECT tmp6.&#x2a;,CASE WHEN tmp7.amount ISNULL THEN 0 ELSE tmp7.amount END AS amount&#xa;FROM &#x28;&#xa;	select &#x2a;, getDateDimensionId&#x28;_date&#x29; as date_dim_id from &#x28;&#xa;		SELECT gl_account_dim_id, currency_uom_dim_id, party_dim_id, organization, last_amount, &#xa;			generate_series&#x28;to_timestamp&#x28;transaction_dim_date&#x29;, &#x27;&#x24;&#x7b;now&#x7d;&#x27;&#x3a;&#x3a;date,&#x27;1 day&#x27;&#x29;&#x3a;&#x3a;date AS _date FROM &#x28;&#xa;				SELECT DISTINCT ON &#x28;gl_account_dim_id, currency_uom_dim_id, party_dim_id, organization, transaction_dim_date&#x29; &#xa;					gl_account_dim_id, currency_uom_dim_id, party_dim_id, organization, transaction_dim_date, &#xa;					CASE WHEN amount ISNULL THEN 0 ELSE amount END AS last_amount&#xa;				FROM &#x28;&#xa;					SELECT tmp2.gl_account_dim_id, tmp2.currency_uom_dim_id, tmp2.party_dim_id, tmp2.organization, tmp2.transaction_dim_date,&#xa;							tmp3.date_dim_id, tmp3.amount FROM &#x28;&#xa;						SELECT DISTINCT ON &#x28;gl_account_dim_id, currency_uom_dim_id, party_dim_id&#x29; transaction_dim_date,&#xa;								gl_account_dim_id, currency_uom_dim_id, party_dim_id, CAST&#x28;&#x27;&#x24;&#x7b;organization&#x7d;&#x27; AS text&#x29; AS organization FROM &#x28;&#xa;							SELECT gl_account_dim_id, &#x24;&#x7b;currency&#x7d; AS currency_uom_dim_id, &#x24;&#x7b;party&#x7d; AS party_dim_id, transaction_dim_date FROM acctg_trans_fact&#xa;							WHERE acctg_trans_fact.last_updated_stamp &#x3e;&#x3d; &#x27;&#x24;&#x7b;lastupdate&#x7d;&#x27;&#xa;						&#x29; AS tmp&#xa;						ORDER BY gl_account_dim_id, currency_uom_dim_id, party_dim_id, transaction_dim_date&#xa;					&#x29; AS tmp2&#xa;					LEFT JOIN &#x28;&#xa;						SELECT date_dim_id, gl_account_dim_id, currency_uom_dim_id, party_dim_id, organization, amount&#xa;						FROM acctg_trans_calc_fact					&#xa;					&#x29; AS tmp3 ON tmp2.gl_account_dim_id &#x3d; tmp3.gl_account_dim_id AND tmp2.currency_uom_dim_id &#x3d; tmp3.currency_uom_dim_id &#xa;					