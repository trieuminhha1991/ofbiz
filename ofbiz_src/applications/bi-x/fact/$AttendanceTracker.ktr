  <step>
    <name>empl_attendance_tracker</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>olbius</connection>
    <sql>SELECT empl_attendance_tracker.party_id, date_attendance, empl_attendance_tracker.start_time,&#xd;&#xa;	empl_attendance_tracker.end_time, tmp.start_time as _start, tmp.end_time as _end&#xd;&#xa;	FROM empl_attendance_tracker&#xd;&#xa;	INNER JOIN empl_position_fulfillment ON empl_position_fulfillment.party_id &#x3d; empl_attendance_tracker.party_id&#xd;&#xa;	INNER JOIN empl_position ON empl_position_fulfillment.empl_position_id &#x3d; empl_position.empl_position_id&#xd;&#xa;	INNER JOIN &#x28;SELECT empl_position_type_id, min&#x28;working_shift.start_time&#x29; as start_time,  max&#x28;working_shift.end_time&#x29; as end_time,&#xd;&#xa;		max&#x28;empl_position_type_work_week.from_date&#x29; as from_date, min&#x28;empl_position_type_work_week.thru_date&#x29; as thru_date,&#xd;&#xa;		CASE WHEN day_of_week&#x3d;&#x27;MONDAY&#x27; THEN 2&#xd;&#xa;			 WHEN day_of_week&#x3d;&#x27;TUESDAY&#x27; THEN 3&#xd;&#xa;			 WHEN day_of_week&#x3d;&#x27;WEDNESDAY&#x27; THEN 4&#xd;&#xa;			 WHEN day_of_week&#x3d;&#x27;THURSDAY&#x27; THEN 5&#xd;&#xa;			 WHEN day_of_week&#x3d;&#x27;FRIDAY&#x27; THEN 6&#xd;&#xa;			 WHEN day_of_week&#x3d;&#x27;SATURDAY&#x27; THEN 7&#xd;&#xa;			 WHEN day_of_week&#x3d;&#x27;SUNDAY&#x27; THEN 1&#xd;&#xa;		END AS _day&#xd;&#xa;		FROM empl_position_type_work_week&#xd;&#xa;		INNER JOIN working_shift ON empl_position_type_work_week.working_shift_id &#x3d; working_shift.working_shift_id&#xd;&#xa;		GROUP BY empl_position_type_id, day_of_week&#x29; AS tmp&#xd;&#xa;			ON tmp._day &#x3d; EXTRACT&#x28;DOW FROM date_attendance&#x29; &#x2b; 1&#xd;&#xa;				AND tmp.empl_position_type_id &#x3d;  empl_position.empl_position_type_id&#xd;&#xa;				AND date_attendance &#x3e;&#x3d; tmp.from_date AND &#x28;&#x28;tmp.thru_date NOTNULL AND date_attendance &#x3c; tmp.thru_date&#x29; OR  tmp.thru_date ISNULL&#x29;&#xd;&#xa;	WHERE empl_attendance_tracker.last_updated_stamp &#x3e;&#x3d; &#x27;&#x24;&#x7b;lastupdate&#x7d;&#x27;&#xd;&#xa;</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
