    <connection>olbiusolap</connection>
    <sql>select v2.custom_time_period_id, v2.from_date, v2.thru_date, group_concat(distinct concat('\'', v1.gl_account_id, '\'') separator ', ') as gl_account_ids,
group_concat(distinct concat('\'', v1.party_id, '\'') separator ', ') as party_ids, cast('${cur_date}' as datetime) as _update, v1.organization_party_id
from acctg_document_list_fact v1, custom_time_period_dimension v2
where v1.transaction_date >= v2.from_date and v1.transaction_date &lt;= v2.thru_date and v2.period_type_id = 'FISCAL_MONTH'
and v1.last_updated_stamp >= '${lastUpdateMin}'
group by v2.custom_time_period_id, v2.from_date, v2.thru_date, v1.ORGANIZATION_PARTY_ID</sql>
