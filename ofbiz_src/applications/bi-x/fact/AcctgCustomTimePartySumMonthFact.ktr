      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

var STATEMENT_AMOUNT;
STATEMENT_AMOUNT = "SELECT gl_account_id, party_id,"
+ " SUM(dr_amount) as dr_amount, SUM(cr_amount) as cr_amount"
+ " FROM ("
+ " SELECT gl_account_id, dr_amount, cr_amount, transaction_date,"
+ " organization_party_id, currency_id,"
+ " (CASE WHEN party_id IS NULL THEN 'company' ELSE party_id END) as party_id"
+ " FROM acctg_document_list_fact"
+ ") AS TMP"
+ " WHERE organization_party_id = '" + organization_party_id + "'"
+ " AND gl_account_id in (" + gl_account_ids + ")"
+ " AND party_id in (" + party_ids + ")"
+ " AND currency_id = '" + currency_id + "'"
+ " AND transaction_date >= '" + from_date + "'"
+ " AND transaction_date &lt;= '" + thru_date + "'"
+ " GROUP BY gl_account_id, party_id";
</jsScript_script>
