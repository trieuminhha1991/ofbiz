<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">

	<service name="getFacility" engine="java" location="com.olbius.bi.services.FacilityServices" invoke="getFacilityId">
		<attribute name="facility" mode="OUT" type="List" optional="false" />
	</service>

	<service name="getProduct" engine="java" location="com.olbius.bi.services.OlapServices" invoke="getProductId">
		<attribute name="product" mode="OUT" type="List" optional="false" />
	</service>

	<service name="getCategory" engine="java" location="com.olbius.bi.services.OlapServices" invoke="getCategory">
		<attribute name="category" mode="OUT" type="List" optional="false" />
	</service>

	<service name="getGeoType" engine="java" location="com.olbius.bi.services.FacilityServices" invoke="getGeoType">
		<attribute name="geoType" mode="OUT" type="List" optional="false" />
	</service>

	<service name="getGeo" engine="java" location="com.olbius.bi.services.FacilityServices" invoke="getGeoId">
		<attribute name="geoType" mode="IN" type="String" optional="false" />
		<attribute name="geo" mode="OUT" type="List" optional="false" />
	</service>


	<service name="getAllPartyParent" engine="java" location="com.olbius.bi.services.PartyServices" invoke="getAllPartyParent">
		<attribute name="parent" mode="OUT" type="List" />
	</service>

	<service name="getChildParty" engine="java" location="com.olbius.bi.services.PartyServices" invoke="getChildParty">
		<attribute name="parent" mode="IN" type="String" />
		<attribute name="child" mode="OUT" type="List" />
	</service>

	<service name="olapGridInterface" engine="interface">
		<attribute name="limit" type="Long" mode="IN" optional="true" />
		<attribute name="init" type="Boolean" mode="IN" optional="true" />
		<attribute name="offset" type="Long" mode="IN" optional="true" />
		<attribute name="sort" type="String" mode="IN" optional="true" />
		<attribute name="sorttype" type="String" mode="IN" optional="true" />
		<attribute name="filter[]" type="List" mode="IN" optional="true" />
		<attribute name="serviceTimestamp" type="Long" mode="IN" optional="true" />
		<attribute name="service" type="String" mode="IN" optional="true" />
		<attribute name="export" type="com.olbius.bi.olap.grid.export.OlapExport" mode="IN" optional="true"/>
		<attribute name="datafields" mode="OUT" type="List" />
		<attribute name="data" mode="OUT" type="List" />
		<attribute name="id" mode="INOUT" type="String" optional="true" />
		<attribute name="totalsize" mode="OUT" type="Integer" optional="true" />
		<attribute name="out" mode="OUT" type="java.io.ByteArrayOutputStream" optional="true" />
	</service>
	
	<service name="olapChartInterface" engine="interface">
		<attribute name="service" type="String" mode="IN" optional="true" />
		<attribute name="serviceTimestamp" type="Long" mode="IN" optional="true" />
		<attribute name="fromDate" type="Timestamp" mode="IN" optional="true"/>
        <attribute name="thruDate" type="Timestamp" mode="IN" optional="true"/>
        <attribute name="xAxis" mode="OUT" type="List"/>
        <attribute name="yAxis" mode="OUT" type="Map"/>
        <attribute name="dateType" mode="OUT" type="String" optional="true"/>
	</service>
	
	<service name="olapInterface" engine="interface">
		<implements service="olapGridInterface" optional="true"/>
		<implements service="olapChartInterface" optional="true"/>
		<attribute name="olapType" mode="IN" type="String"/>
	</service>

</services>