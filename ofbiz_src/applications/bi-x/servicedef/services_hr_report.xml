<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">

	<service name="payroll_salary_structure" engine="java" location="com.olbius.bi.services.HRServices" invoke="payrollSalaryStructure">
        <attribute name="party_person" mode="IN" type="String" optional="true"/>
        <attribute name="party_group" mode="IN" type="String" optional="true"/>
		<attribute name="fromDate" type="Timestamp" mode="IN" optional="false"/>
		<attribute name="thruDate" type="Timestamp" mode="IN" optional="false"/>
        <attribute name="xAxis" mode="OUT" type="List"/>
        <attribute name="yAxis" mode="OUT" type="Map"/>
	</service>

	<service name="payroll_salary_range" engine="java" location="com.olbius.bi.services.HRServices" invoke="payrollSalaryRange">
		<attribute name="group" mode="IN" type="String" optional="true"/>
		<attribute name="fromDate" type="Timestamp" mode="IN" optional="false"/>
        <attribute name="thruDate" type="Timestamp" mode="IN" optional="false"/>
       	<attribute name="xAxis" mode="OUT" type="List"/>
		<attribute name="yAxis" mode="OUT" type="Map"/>
	</service>

    <service name="payroll_salary_range_by_position" engine="java" location="com.olbius.bi.services.HRServices" invoke="payrollSalaryRangeByPosition">
        <attribute name="group" mode="IN" type="String" optional="true"/>
        <attribute name="dateType" mode="IN" type="String" optional="true"/>
        <attribute name="fromDate" type="Timestamp" mode="IN" optional="false"/>
        <attribute name="thruDate" type="Timestamp" mode="IN" optional="false"/>
        <attribute name="xAxis" mode="OUT" type="List"/>
        <attribute name="yAxis" mode="OUT" type="Map"/>
    </service>

</services>