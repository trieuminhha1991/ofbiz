<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
	
	<service name="evaluateAcc" engine="java" location="com.olbius.bi.services.AccountingServices" invoke="evaluateAccV2">
		<implements service="olapChartInterface"/>
		<attribute name="group" mode="IN" type="String" optional="true"/>
		<attribute name="group[]" mode="IN" type="List" optional="true"/>
		<attribute name="product" mode="IN" type="String" optional="true"/>
<!-- 		<attribute name="fromDate" type="Timestamp" mode="IN" optional="false"/> -->
<!--         <attribute name="thruDate" type="Timestamp" mode="IN" optional="false"/> -->
        <attribute name="dateType" type="String" mode="IN" optional="true"/>
        <attribute name="debitCreditFlag" type="String" mode="IN" optional="false"/>
        <attribute name="code" type="String" mode="IN" optional="false"/>
        <attribute name="currency" type="String" mode="IN" optional="false"/>
        <attribute name="orig" type="Boolean" mode="IN" optional="true"/>
        <attribute name="level" type="Boolean" mode="IN" optional="true"/>
        <attribute name="distrib" type="Boolean" mode="IN" optional="true"/>
        <attribute name="productFlag" type="Boolean" mode="IN" optional="true"/>
        <attribute name="categoryFlag" type="Boolean" mode="IN" optional="true"/>
        <attribute name="groupFlag" type="Boolean" mode="IN" optional="true"/>
        <attribute name="dateFlag" type="Boolean" mode="IN" optional="true"/>
        <attribute name="sort" type="Boolean" mode="IN" optional="true"/>
        <attribute name="limit" type="Long" mode="IN" optional="true"/>
<!-- 		<attribute name="yAxis" mode="OUT" type="Map"/> -->
<!--         <attribute name="xAxis" mode="OUT" type="List"/> -->
	</service>
	
	<service name="evaluateAccGrid" engine="java" location="com.olbius.bi.services.grid.AccountingGridServices" invoke="evaluateAcc">
		<implements service="olapGridInterface"/>
		<attribute name="fromDate" type="Timestamp" mode="IN" optional="false"/>
        <attribute name="thruDate" type="Timestamp" mode="IN" optional="false"/>
        <attribute name="dateType" type="String" mode="IN" optional="true"/>
        <attribute name="orig" type="Boolean" mode="IN" optional="true"/>
	</service>
	
</services>