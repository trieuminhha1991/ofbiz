<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
	
	<service name="salesOrderTotal" engine="java" location="com.olbius.basesales.report.SalesGridServices" invoke="salesOrderTotal">
		<implements service="olapChartInterface"/>
        <attribute name="dateType" type="String" mode="IN" optional="true"/>
        <attribute name="currency" type="String" mode="IN" optional="true"/>
        <attribute name="quantity" type="Boolean" mode="IN" optional="true"/>
        <attribute name="taxFlag" type="Boolean" mode="IN" optional="true"/>
        <attribute name="promoFlag" type="Boolean" mode="IN" optional="true"/>
        <attribute name="type[]" type="List" mode="IN" optional="false"/>
        <attribute name="show" type="String" mode="IN" optional="false"/>
        <attribute name="PRODUCT[]" type="List" mode="IN" optional="true"/>
        <attribute name="METHOD_CHANNEL" type="String" mode="IN" optional="true"/>
        <attribute name="CHANNEL" type="String" mode="IN" optional="true"/>
        <attribute name="PARTY_TO" type="String" mode="IN" optional="true"/>
        <attribute name="PARTY_FROM" type="String" mode="IN" optional="true"/>
        <attribute name="PRODUCT_STORE" type="String" mode="IN" optional="true"/>
        <override name="fromDate" optional="false"/>
        <override name="thruDate" optional="false"/>
	</service>
	
	<service name="salesOrderCount" engine="java" location="com.olbius.basesales.report.SalesGridServices" invoke="salesOrderCount">
		<attribute name="fromDate" type="Timestamp" mode="IN" optional="false"/>
        <attribute name="thruDate" type="Timestamp" mode="IN" optional="false"/>
        <attribute name="dateType" type="String" mode="IN" optional="true"/>
        <attribute name="type[]" type="List" mode="IN" optional="true"/>
        <attribute name="PRODUCT" type="String" mode="IN" optional="true"/>
        <attribute name="METHOD_CHANNEL" type="String" mode="IN" optional="true"/>
        <attribute name="CHANNEL" type="String" mode="IN" optional="true"/>
        <attribute name="PARTY_TO" type="String" mode="IN" optional="true"/>
        <attribute name="PARTY_FROM" type="String" mode="IN" optional="true"/>
        <attribute name="PRODUCT_STORE" type="String" mode="IN" optional="true"/>
        <attribute name="PARTY_TYPE" type="String" mode="IN" optional="true"/>
		<attribute name="yAxis" mode="OUT" type="Map"/>
        <attribute name="xAxis" mode="OUT" type="List"/>
	</service>
	
</services>