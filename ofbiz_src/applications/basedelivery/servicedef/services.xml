<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>BaseDelivery Services</description>
    <vendor></vendor>
    <version>1.0</version>

    <service name="noSyntaxError" engine="interface">
        <description>
            Dummy service to prevent empty files and syntax error - Remove when the 1st real service will be added here 
        </description>
    </service>

    <service name="getVehicles" engine="java" location="com.olbius.basedelivery.DeliveryServices"
             invoke="getVehicles" auth="true">
        <attribute name="ownerPartyId" mode="IN" type="String" optional="false"/>
        <attribute name="fromDate" mode="IN" type="Long" optional="false"/>
        <attribute name="thruDate" mode="IN" type="Long" optional="false"/>
        <attribute name="listVehicles" mode="OUT" type="java.util.List" optional="false"/>
    </service>


    <service name="getAllVehicleType" engine="java" location="com.olbius.basedelivery.DeliveryServices"
             invoke="getAllVehicleType" auth="true">
        <attribute name="listVehicleType" mode="OUT" type="java.util.List" optional="false"/>
    </service>

    <service name="JQGetParty" engine="java" invoke="jqGetParty"
             location="com.olbius.basedelivery.DeliveryServices">
        <implements service="jqxGetListInterface"/>
    </service>

    <service name="JQGetVehicle" engine="java" location="com.olbius.basedelivery.vehicle.VehicleServices"
             invoke="jqGetVehicle" auth="true">
        <implements service="jqxGetListInterface"/>
    </service>


    <service name="JQGetVehicleBySupplierId" engine="java" location="com.olbius.basedelivery.vehicle.VehicleServices"
             invoke="jqGetVehicleBySupplierId" auth="true">
        <implements service="jqxGetListInterface"/>
    </service>


    <service name="JQGetProductStoreGroup" engine="java" location="com.olbius.basedelivery.distributorgroup.DistributorGroupServices"
             invoke="jqGetProductStoreGroup" auth="true">
        <implements service="jqxGetListInterface"/>
        <override name="listIterator" mode="OUT" type="List" />
    </service>

    <service name="JQGetListSupplierVehicle" engine="java" location="com.olbius.basedelivery.suppliervehicle.SupplierVehicleServices"
             invoke="jqGetListSupplierVehicle" auth="true">
        <implements service="jqxGetListInterface"/>
    </service>

    <service name="JQGetListDelivery" engine="java" invoke="jqGetListDelivery" location="com.olbius.basedelivery.DeliveryServices">
        <implements service="jqxGetListInterface" />
        <override name="listIterator" mode="OUT" type="List" />
    </service>

    <service name="JQGetDeliveryByGroup" engine="java" invoke="jqGetListDeliveryByGroup" location="com.olbius.basedelivery.DeliveryServices">
        <implements service="jqxGetListInterface" />
        <override name="listIterator" mode="OUT" type="List" />
    </service>


    <service name="JQGetListVehicleType" engine="java" invoke="jqGetListVehicleType" location="com.olbius.basedelivery.DeliveryServices">
        <implements service="jqxGetListInterface" />
        <override name="listIterator" mode="OUT" type="List" />
    </service>

    <service name="JQGetListTrip" engine="java" invoke="jqGetListTrip" location="com.olbius.basedelivery.trip.TripServices">
        <implements service="jqxGetListInterface" />
        <attribute name="TotalRows" mode="OUT" type="String" optional="true"></attribute>
        <override name="listIterator" mode="OUT" type="List" />
    </service>


    <service name="JQGetListDeliveryByTrip" engine="java" invoke="jqGetListDeliveryByTrip" location="com.olbius.basedelivery.trip.TripServices">
        <implements service="jqxGetListInterface"/>
        <override name="listIterator" type="java.util.List" optional="true"></override>
    </service>

    <service name="JQGetListItemInTrip" engine="java" invoke="jqGetListItemInTrip" location="com.olbius.basedelivery.trip.TripServices">
        <implements service="jqxGetListInterface"/>
        <override name="listIterator" type="java.util.List" optional="true"></override>
    </service>


    <service name="JQGetListPartyGroup" engine="java" invoke="getListPartyGroups" location="com.olbius.basehr.common.services.CommonServices">
        <implements service="jqxGetListInterface"/>
        <!--<override name="listIterator" mode="OUT" type="java.util.List" optional="true"/>-->
    </service>

    <service name="updateVehicle" engine="java" auth="true"
             location="com.olbius.basedelivery.vehicle.VehicleServices"
             invoke="updateVehicle"
             transaction-timeout="7200">
        <description>Update the quantities/prices for an existing order</description>
        <attribute name="request" mode="IN" type="javax.servlet.http.HttpServletRequest" optional="false"></attribute>
        <attribute name="vehicleId" type="String" mode="OUT" optional="true"/>
    </service>

    <!--<service name="JQGetFixedAssetVehicles" engine="java"-->
             <!--location="com.olbius.basedelivery.DeliveryServices" invoke="jqGetFixedAssetVehicles"-->
             <!--auth="true">-->
        <!--<implements service="jqxGetListInterface"/>-->
    <!--</service>-->

    <!--     perfect_not_delete_important 	-->
    <service name="JQGetListDistributorAndGroup" engine="java" invoke="getListDistributorAndGroup" location="com.olbius.basedelivery.distributorgroup.DistributorGroupServices">
        <!-- <required-permissions join-type="AND">
            DEL<check-permission permission="PARTYDIST" action="_VIEW"/>
        </required-permissions> -->
        <implements service="jqxGetListInterface"/>
        <attribute name="TotalRows" mode="OUT" type="String" optional="true"></attribute>
        <override name="listIterator" type="java.util.List" optional="true"></override>
    </service>


    <!--     perfect_not_delete_important 	-->
    <service name="JQGetListDistributorsByDistributorGroupId" engine="java" invoke="getListDistributorsByDistributorGroupId" location="com.olbius.basedelivery.distributorgroup.DistributorGroupServices">
        <!-- <required-permissions join-type="AND">
            DEL<check-permission permission="PARTYDIST" action="_VIEW"/>
        </required-permissions> -->
        <implements service="jqxGetListInterface"/>
        <attribute name="TotalRows" mode="OUT" type="String" optional="true"></attribute>
        <override name="listIterator" type="java.util.List" optional="true"></override>
    </service>



<!--     <service name="JQGetFixedAssetVehicles" engine="java" -->
<!--              location="com.olbius.basedelivery.DeliveryServices" invoke="jqGetFixedAssetVehicles" -->
<!--              auth="true"> -->
