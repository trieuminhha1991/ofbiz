<!--     </service> -->

    <service name="testSOAPRequest" engine="soap" export="true"
             location="http://demo-trunk.ofbiz.apache.org:8080/webtools/control/SOAPService"
             invoke="testSoapSimple">
        <attribute name="defaultValue" type="Double" mode="IN" default-value="999.9999"/>
        <attribute name="message" type="String" mode="IN" optional="true"/>
        <attribute name="resp" type="String" mode="OUT"/>
    </service>

    <service name="editTrip" engine="java" invoke="editTrip" location="com.olbius.basedelivery.trip.TripServices">
        <!--<required-permissions join-type="AND">-->
            <!--<olbius-permission/>-->
        <!--</required-permissions>-->
        <attribute name="vehicleId" mode="IN" type="String" optional="true"/>
        <!--<attribute name="driverId" mode="IN" type="String" optional="true"/>-->
        <!--<attribute name="vehicleName" mode="IN" type="String" optional="true"/>-->
        <attribute name="statusId" mode="IN" type="String" optional="false"/>
        <attribute name="tripId" mode="INOUT" type="String" optional="false"/>
    </service>

    <service name="createTrip" engine="java" invoke="createTrip" location="com.olbius.basedelivery.trip.TripServices">
        <!--<required-permissions join-type="AND">-->
        <!--<olbius-permission/>-->
        <!--</required-permissions>-->
        <attribute name="description" mode="IN" type="String" optional="true" allow-html="safe"/>
        <attribute name="requirementStartDate" mode="IN" type="String" optional="false"/>
        <attribute name="contractorId" mode="IN" type="String" optional="false"/>
        <attribute name="vehicleId" mode="IN" type="String" optional="true"/>
        <!--<attribute name="vehicleName" mode="IN" type="String" optional="true"/>-->
        <attribute name="deliveryList" mode="IN" type="java.util.List" optional="false"/>
        <attribute name="totalWeight" mode="IN" type="String" optional="true"/>
        <attribute name="tripId" mode="OUT" type="String" optional="true"/>
    </service>

    <service name="createSupplierVehicle" engine="java" invoke="createSupplierVehicle" location="com.olbius.basedelivery.suppliervehicle.SupplierVehicleServices">
        <!--<required-permissions join-type="AND">-->
        <!--<olbius-permission/>-->
        <!--</required-permissions>-->
        <attribute name="partyId" mode="IN" type="String" optional="true" allow-html="safe"/>
        <attribute name="vehicleId" mode="IN" type="String" optional="false"/>
        <attribute name="supplierVehicleId" mode="OUT" type="String" optional="true"/>
    </service>
    <service name="createDistributorGroup" engine="java" invoke="createDistributorGroup" location="com.olbius.basedelivery.distributorgroup.DistributorGroupServices">
        <!--<required-permissions join-type="AND">-->
        <!--<olbius-permission/>-->
        <!--</required-permissions>-->
        <attribute name="description" mode="IN" type="String" optional="true" allow-html="safe"/>
        <attribute name="productStoreGroupName" mode="IN" type="String" optional="false"/>
        <attribute name="distributorList" mode="IN" type="java.util.List" optional="false"/>
        <attribute name="productStoreGroupId" mode="OUT" type="String" optional="true"/>
    </service>

    <service name="updateDistributorGroup" engine="java" invoke="updateDistributorGroup" location="com.olbius.basedelivery.distributorgroup.DistributorGroupServices">
        <!--<required-permissions join-type="AND">-->
        <!--<olbius-permission/>-->
        <!--</required-permissions>-->
        <attribute name="description" mode="IN" type="String" optional="true" allow-html="safe"/>
        <attribute name="productStoreGroupId" mode="IN" type="String" optional="false"/>
        <attribute name="distributorList" mode="IN" type="java.util.List" optional="false"/>
        <attribute name="productStoreGroupId" mode="OUT" type="String" optional="true"/>
    </service>



    <service name="getContactMechName" engine="java" invoke="getContactMechName" location="com.olbius.basedelivery.trip.TripServices">
        <attribute name="contactMechId" mode="IN" type="String" optional="false"></attribute>
        <attribute name="fullName" mode="OUT" type="String" optional="false"></attribute>
    </service>

    <service name="getVehicleTypeName" engine="java" invoke="getVehicleTypeName" location="com.olbius.basedelivery.DeliveryServices">
        <attribute name="vehicleTypeId" mode="IN" type="String" optional="false"></attribute>
        <attribute name="name" mode="OUT" type="String" optional="false"></attribute>
    </service>

    <service name="createVehicle" engine="java" invoke="createVehicle"
             location="com.olbius.basedelivery.vehicle.VehicleServices" auth="true"
             transaction-timeout="100000" max-retry="1">
        <attribute name="vehicleTypeId" type="String" mode="IN" optional="true"/>
        <attribute name="description" type="String" mode="IN" optional="true"/>
        <attribute name="licensePlate" type="String" mode="IN" optional="false"/>
        <attribute name="loading" type="String" mode="IN" optional="true"/>
        <attribute name="volume" type="String" mode="IN" optional="true"/>
        <attribute name="reqNo" type="String" mode="IN" optional="true"/>
        <attribute name="width" type="String" mode="IN" optional="true"/>
        <attribute name="height" type="String" mode="IN" optional="true"/>
        <attribute name="longitude" type="String" mode="IN" optional="true"/>
        <attribute name="length" type="String" mode="IN" optional="true"/>
    </service>
</services>