	<entity entity-name="PlanPurchaseOrder" package-name="org.ofbiz.webpos"
		never-cache="true" title="Plan Purchase Oreder Entity">
		<field name="planPOId" type="id-ne"></field>
		<field name="facilityId" type="id"></field>
		<field name="createdDate" type="date-time"></field>
		<field name="createdBy" type="id"></field>
		<field name="currencyUom" type="id"></field>
		<field name="supplierId" type="id"></field>
		<field name="grandTotal" type="currency-amount"></field>
		<prim-key field="planPOId" />

		<relation rel-entity-name="Party" type="one" fk-name="Plan_PO_Party">
			<key-map field-name="supplierId" rel-field-name="partyId" />
		</relation>
		<relation type="one" rel-entity-name="Uom" fk-name="Plan_PO_CUOM">
			<key-map field-name="currencyUom" rel-field-name="uomId" />
		</relation>
		<relation type="one" rel-entity-name="UserLogin" fk-name="Plan_PO_CBUL"
			title="CreatedBy">
			<key-map field-name="createdBy" rel-field-name="userLoginId" />
		</relation>
		<relation rel-entity-name="Facility" type="one" fk-name="Plan_PO_Facility">
			<key-map field-name="facilityId" />
		</relation>
	</entity>
	<entity entity-name="PlanPurchaseOrderItem" package-name="org.ofbiz.webpos"
		never-cache="true" title="Plan Purchase Order Item Entity">
		<field name="planPOId" type="id-ne"></field>
		<field name="planPOItemSeqId" type="id-ne"></field>
		<field name="productId" type="id"></field>
		<field name="productName" type="name"></field>
		<field name="qpd" type="fixed-point"></field> <!-- quantity per day -->
		<field name="qoh" type="fixed-point"></field> <!-- quantity on hand -->
		<field name="qoo" type="fixed-point"></field> <!-- quantity on order -->
		<field name="pickStandard" type="fixed-point"></field> <!-- qui cach dong goi cua product -->
		<field name="unitCost" type="fixed-point"></field>
		<field name="lastSold" type="date-time"></field>
		<field name="lastReceived" type="date-time"></field>
		<field name="qtyBox" type="fixed-point"></field> <!-- quantity box -->
		<field name="qtyPic" type="fixed-point"></field> <!-- quantity pic -->
		<field name="quantityUomId" type="id"></field> <!-- quantity uom id of product -->
		<field name="purchaseUomId" type="id"></field> <!-- default quantity uom id when purchase -->
		<prim-key field="planPOId" />
		<prim-key field="planPOItemSeqId" />
		<relation type="one" fk-name="PLANPO_ITEM_PLANPO"
			rel-entity-name="PlanPurchaseOrder">
			<key-map field-name="planPOId" />
		</relation>
		<relation type="one" fk-name="PLANPO_ITEM_UOM" title="Quantity"
			rel-entity-name="Uom">
			<key-map field-name="quantityUomId" rel-field-name="uomId" />
		</relation>
		<relation type="one" fk-name="PLANPO_ITEM_POUOM" title="purchase"
			rel-entity-name="Uom">
			<key-map field-name="purchaseUomId" rel-field-name="uomId" />
		</relation>
		<relation type="one" fk-name="PLANPO_ITEM_PRODUCT"
			rel-entity-name="Product">
			<key-map field-name="productId" />
		</relation>
	</entity>
