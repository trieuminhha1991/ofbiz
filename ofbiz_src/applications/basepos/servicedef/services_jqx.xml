<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../framework/service/dtd/services.xsd">
	<description>Services JQX</description>
	<vendor>OFBiz</vendor>
	<version>1.0</version>

	<service name="JQGetListWorkShift" engine="java" invoke="jqGetListWorkShift" location="com.olbius.basepos.jqservices.WorkShiftJQServices">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="JQGetListOtherIncome" engine="java" invoke="jqGetListOtherIncome" location="com.olbius.basepos.jqservices.WorkShiftJQServices">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="JQGetListOtherCost" engine="java" invoke="jqGetListOtherCost" location="com.olbius.basepos.jqservices.WorkShiftJQServices">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="jqGetProductAndProductFacility" engine="java" invoke="getProductAndProductFacility" location="com.olbius.basepos.jqservices.PurchaseOrder">
		<implements service="jqxGetListInterface" />
		<attribute name="request" mode="IN" type="javax.servlet.http.HttpServletRequest" optional="true"></attribute>
		<override name="listIterator" type="List"></override>
	</service>

	<service name="JQCalculatePODetail" engine="java" invoke="jQCalculatePODetail" location="com.olbius.basepos.jqservices.PurchaseOrder">
		<implements service="jqxGetListInterface" />
		<attribute name="request" mode="IN" type="javax.servlet.http.HttpServletRequest" optional="true"></attribute>
		<override name="listIterator" type="List"></override>
	</service>

	<service name="JQListSupplier" engine="java" invoke="jqListSupplier" location="com.olbius.basepos.jqservices.SupplierJQServices">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="JQViewPlanPO" engine="java" invoke="jqListPlanPOItem" location="com.olbius.basepos.jqservices.PlanPOJQServices">
		<implements service="jqxGetListInterface" />
		<override name="listIterator" type="List"></override>
	</service>

	<service name="JQViewPlanPOItemDetail" engine="java" invoke="JQViewPlanPOItemDetail" location="com.olbius.basepos.jqservices.PlanPOJQServices">
		<implements service="jqxGetListInterface" />
		<attribute name="request" mode="IN" type="javax.servlet.http.HttpServletRequest" optional="true"></attribute>
		<override name="listIterator" type="List"></override>
	</service>
	<service name="JQPurchaseOrderHistory" engine="java" invoke="jqPurchaseOrderHistory" location="com.olbius.basepos.order.OrderHistory">
		<implements service="jqxGetListInterface" />
	</service>
	<service name="JQPurchaseOrderHistoryDetail" engine="java" invoke="jqGetPurchaseOrderHistoryDetail" location="com.olbius.basepos.jqservices.PurchaseOrderHistory">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="JQSalesOrderHistory" engine="java" invoke="jqGetSalesOrderHistory" location="com.olbius.basepos.jqservices.SalesOrderHistory">
		<implements service="jqxGetListInterface" />
		<attribute name="request" mode="IN" type="Object" optional="false"/>
		<attribute name="TotalRows" mode="OUT" type="String" optional="true"/>
   		<override name="listIterator" mode="OUT" type="java.util.List" optional="true"/>
	</service>

	<service name="JQListPromotion" engine="java" invoke="jqGetListPromotion" location="com.olbius.basepos.jqservices.SalesOrderHistory">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="JQGetListProductStorePOS" engine="java" invoke="jqGetListProductStorePOS" location="com.olbius.basepos.jqservices.SupplierJQServices">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="JQGetListFacilityByStore" engine="java" invoke="getListFacilityByStore" location="com.olbius.basepos.jqservices.SupplierJQServices">
		<implements service="jqxGetListInterface" />
		<override name="listIterator" type="List"></override>
	</service>

	<service name="createTerminalPOS" engine="java" invoke="createTerminalPOS" location="com.olbius.basepos.settings.SettingServices">
		<required-permissions join-type="AND">
			<olbius-permission />
		</required-permissions>
		<auto-attributes mode="IN" include="all" entity-name="PosTerminal" optional="true"></auto-attributes>
	</service>

	<service name="updateTerminalPOS" engine="java" invoke="updateTerminalPOS" location="com.olbius.basepos.settings.SettingServices">
		<required-permissions join-type="AND">
			<olbius-permission />
		</required-permissions>
		<attribute name="posTerminalId" mode="IN" type="String"></attribute>
		<attribute name="terminalName" mode="IN" type="String"></attribute>
		<attribute name="facilityId" mode="IN" type="String"></attribute>
	</service>

	<service name="deleteTerminalPOS" engine="java" invoke="deleteTerminalPOS" location="com.olbius.basepos.settings.SettingServices">
		<required-permissions join-type="AND">
			<olbius-permission />
		</required-permissions>
		<attribute name="posTerminalId" mode="IN" type="String"></attribute>
	</service>

	<service name="JQGetListConfigPrintOrder" engine="java" invoke="listConfigPrintOrder" location="com.olbius.basepos.settings.SettingServices">
		<implements service="jqxGetListInterface" />
	</service>

	<service name="createConfigPrintOrderPOS" engine="java" invoke="createConfigPrintOrderPOS" location="com.olbius.basepos.settings.SettingServices"
		default-entity-name="ConfigPrintOrder">
		<required-permissions join-type="AND">
			<olbius-permission />
		</required-permissions>
		<auto-attributes mode="IN" include="all" optional="true"></auto-attributes>
	</service>

	<service name="updateConfigPrintOrderPOS" engine="java" invoke="updateConfigPrintOrderPOS" location="com.olbius.basepos.settings.SettingServices"
		default-entity-name="ConfigPrintOrder">
		<required-permissions join-type="AND">
			<olbius-permission />
		</required-permissions>
		<auto-attributes mode="IN" include="all" optional="true"></auto-attributes>
	</service>

	<service name="deleteConfigPrintOrderPOS" engine="java" invoke="deleteConfigPrintOrderPOS" location="com.olbius.basepos.settings.SettingServices">
		<required-permissions join-type="AND">
			<olbius-permission />
		</required-permissions>
		<attribute name="productStoreId" mode="IN" type="String"></attribute>
	</service>

</services>
