<?xml version="1.0" encoding="UTF-8"?>

<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>Pos Olap Services</description>
    <vendor>OFBiz</vendor>
    <version>1.0</version>
    <service name="salesOrderReport" engine="java" location="com.olbius.basepos.report.PosOlapGridServices" invoke="reportSalesOrder">
		<implements service="olapGridInterface"/>
  		<attribute name="fromDate" type="Timestamp" mode="IN" optional="true"/>
        <attribute name="thruDate" type="Timestamp" mode="IN" optional="true"/>
        <attribute name="reportType" type="String" mode="IN" optional="false"/>
        <attribute name="dateType" type="String" mode="IN" optional="true"/>
        <attribute name="productId" type="String" mode="IN" optional="true"/>
        <attribute name="categoryId" type="String" mode="IN" optional="true"/>
        <attribute name="productStoreId" type="String" mode="IN" optional="true"/>
        <attribute name="partyId" type="String" mode="IN" optional="true"/>
        <attribute name="sortField" type="String" mode="IN" optional="true"/>
        <attribute name="sortOption" type="String" mode="IN" optional="true"/>
        <attribute name="sumQuantity" type="BigDecimal" mode="OUT" optional="true"/>
        <attribute name="sumExtPrice" type="BigDecimal" mode="OUT" optional="true"/>
        <attribute name="customTime" type="String" mode="IN" optional="true"/>
	</service>
	
	<service name="returnReport" engine="java" location="com.olbius.basepos.report.PosOlapGridServices" invoke="reportReturn">
    	<implements service="olapGridInterface"/>
		<attribute name="fromDate" type="Timestamp" mode="IN" optional="false"/>
        <attribute name="thruDate" type="Timestamp" mode="IN" optional="false"/>
        <attribute name="sortField" type="String" mode="IN" optional="true"/>
        <attribute name="sortOption" type="String" mode="IN" optional="true"/>
        <attribute name="partyId" type="String" mode="IN" optional="true"/>
        <attribute name="facilityId" type="String" mode="IN" optional="true"/>
        <attribute name="sumQuantity" type="BigDecimal" mode="OUT" optional="true"/>
        <attribute name="sumExtPrice" type="BigDecimal" mode="OUT" optional="true"/>
	</service>
	
	<service name="bestSellerChartv2" engine="java" location="com.olbius.basepos.report.PosOlapChartServices" invoke="bestSellerChartv2">
		<implements service="olapChartInterface"/>
        <attribute name="limit" type="Long" mode="IN" optional="true"/>
        <attribute name="sort" type="Boolean" mode="IN" optional="true"/>
        <attribute name="typeChart" type="String" mode="IN" optional="true"/>
        <attribute name="productStoreId" type="String" mode="IN" optional="true"/>
        <attribute name="customTime" type="String" mode="IN" optional="true"/>
	</service>
	
	<service name="bestSellerChart" engine="java" location="com.olbius.basepos.report.PosOlapChartServices" invoke="bestSellerChart">
		<attribute name="fromDate" type="Timestamp" mode="IN" optional="false"/>
        <attribute name="thruDate" type="Timestamp" mode="IN" optional="false"/>
        <attribute name="limit" type="Integer" mode="IN" optional="true"/>
        <attribute name="sort" type="Boolean" mode="IN" optional="true"/>
        <attribute name="typeChart" type="String" mode="IN" optional="true"/>
        <attribute name="productStoreId" type="String" mode="IN" optional="true"/>
		<attribute name="yAxis" mode="OUT" type="Map"/>
        <attribute name="xAxis" mode="OUT" type="List"/>
	</service>
	
	<service name="storeChartv2" engine="java" location="com.olbius.basepos.report.PosOlapChartServices" invoke="storeChartv2">
		<implements service="olapChartInterface"/>
		<attribute name="customTime" type="String" mode="IN" optional="true"/>
	</service>
	<service name="storeChart" engine="java" location="com.olbius.basepos.report.PosOlapChartServices" invoke="storeChart">
		<attribute name="fromDate" type="Timestamp" mode="IN" optional="false"/>
        <attribute name="thruDate" type="Timestamp" mode="IN" optional="false"/>
		<attribute name="yAxis" mode="OUT" type="Map"/>
        <attribute name="xAxis" mode="OUT" type="List"/>
	</service>
	<service name="categoryChartv2" engine="java" location="com.olbius.basepos.report.PosOlapChartServices" invoke="categoryChartv2">
		<implements service="olapChartInterface"/>
        <attribute name="productStoreId" type="String" mode="IN" optional="true"/>
        <attribute name="customTime" type="String" mode="IN" optional="true"/>
	</service>
	<service name="categoryChart" engine="java" location="com.olbius.basepos.report.PosOlapChartServices" invoke="categoryChart">
		<attribute name="fromDate" type="Timestamp" mode="IN" optional="false"/>
        <attribute name="thruDate" type="Timestamp" mode="IN" optional="false"/>
        <attribute name="productStoreId" type="String" mode="IN" optional="true"/>
		<attribute name="yAxis" mode="OUT" type="Map"/>
        <attribute name="xAxis" mode="OUT" type="List"/>
	</service>
	
	<service name="returnChart" engine="java" location="com.olbius.basepos.report.PosOlapChartServices" invoke="returnChart">
		<attribute name="fromDate" type="Timestamp" mode="IN" optional="false"/>
        <attribute name="thruDate" type="Timestamp" mode="IN" optional="false"/>
        <attribute name="facilityId" type="String" mode="IN" optional="true"/>
        <attribute name="partyId" type="String" mode="IN" optional="true"/>
        <attribute name="limit" type="Integer" mode="IN" optional="true"/>
		<attribute name="yAxis" mode="OUT" type="Map"/>
        <attribute name="xAxis" mode="OUT" type="List"/>
	</service>
	
	<service name="returnChartv2" engine="java" location="com.olbius.basepos.report.PosOlapChartServices" invoke="returnChartv2">
        <implements service="olapChartInterface"/>
        <attribute name="facilityId" type="String" mode="IN" optional="true"/>
        <attribute name="partyId" type="String" mode="IN" optional="true"/>
        <attribute name="limit" type="Long" mode="IN" optional="true"/>
        <attribute name="customTime" type="String" mode="IN" optional="true"/>
	</service>
</services>   