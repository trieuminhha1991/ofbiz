<?xml version="1.0" encoding="UTF-8"?>

<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>Plan PO Services</description>
    <vendor>OFBiz</vendor>
    <version>1.0</version>
   <service name="createPlanHeader" engine="simple" location="component://basepos/script/com/olbius/webpos/plan/planServices.xml"
    	invoke="createPlanHeader"  default-entity-name="PlanHeader" auth="true">
    	<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
    	<attribute name="planId" mode="OUT" type="String" optional="false"></attribute>
    </service>
    <service name="createPlanItem" engine="simple" location="component://basepos/script/com/olbius/webpos/plan/planServices.xml"
    	invoke="createPlanItem" default-entity-name="PlanItem" auth="true">
    	<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
    	<auto-attributes mode="INOUT" include="pk"></auto-attributes>
    </service>
    <service name="JQGetPlanItemList" engine="java" invoke="jqGetPlanItem"
   		 location="com.olbius.basepos.product.ProductJQServices">
   		<attribute name="listAllConditions" mode="IN" type="java.util.List" optional="false"></attribute>
   		<attribute name="listSortFields" mode="IN" type="java.util.List" optional="false"></attribute>
   		<attribute name="opts" mode="IN" type="org.ofbiz.entity.util.EntityFindOptions" optional="false"></attribute>
   		<attribute name="parameters" mode="IN" type="java.util.Map" optional="false"></attribute>
   		<attribute name="listIterator" mode="OUT" type="org.ofbiz.entity.util.EntityListIterator" optional="false"></attribute>
    </service>
    <service name="JQGetRowDetailTransferPlanHistory" engine="java" invoke="jqGetRowDetailTransferPlanHistory"
   		 location="com.olbius.basepos.product.ProductJQServices">
   		<attribute name="listAllConditions" mode="IN" type="java.util.List" optional="false"></attribute>
   		<attribute name="listSortFields" mode="IN" type="java.util.List" optional="false"></attribute>
   		<attribute name="opts" mode="IN" type="org.ofbiz.entity.util.EntityFindOptions" optional="false"></attribute>
   		<attribute name="parameters" mode="IN" type="java.util.Map" optional="false"></attribute>
   		<attribute name="listIterator" mode="OUT" type="org.ofbiz.entity.util.EntityListIterator" optional="false"></attribute>
    </service>
     <service name="JQGetPlanPurchaseItemList" engine="java" invoke="jqGetPlanPurchaseItem"
   		 location="com.olbius.basepos.product.ProductJQServices">
   		<attribute name="listAllConditions" mode="IN" type="java.util.List" optional="false"></attribute>
   		<attribute name="listSortFields" mode="IN" type="java.util.List" optional="false"></attribute>
   		<attribute name="opts" mode="IN" type="org.ofbiz.entity.util.EntityFindOptions" optional="false"></attribute>
   		<attribute name="parameters" mode="IN" type="java.util.Map" optional="false"></attribute>
   		<attribute name="listIterator" mode="OUT" type="org.ofbiz.entity.util.EntityListIterator" optional="false"></attribute>
    </service>
</services>  