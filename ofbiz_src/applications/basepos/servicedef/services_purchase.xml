<?xml version="1.0" encoding="UTF-8"?>

<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
     <service name="createPlanPO" engine="simple" location="component://basepos/script/org/ofbiz/webpos/PlanPOServices.xml"
    	invoke="createPlanPO"  default-entity-name="PlanPurchaseOrder" auth="true">
    	<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
    	<attribute name="planPOId" mode="OUT" type="String" optional="false"></attribute>
    </service>
    
    <service name="createPlanPOItem" engine="simple" location="component://basepos/script/org/ofbiz/webpos/PlanPOServices.xml"
    	invoke="createPlanPOItem" default-entity-name="PlanPurchaseOrderItem" auth="true">
    	<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
    	<auto-attributes mode="INOUT" include="pk" optional="false"></auto-attributes>
    	<override name="productName" mode="IN" optional="true" type="String" allow-html="safe"></override>
    </service>
    
    <service name="createPlanPOItemFacility" engine="simple" location="component://basepos/script/org/ofbiz/webpos/PlanPOServices.xml"
    	invoke="createPlanPOItemFacility" default-entity-name="PlanPurchaseOrderItemFacility" auth="true">
    	<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
    	<auto-attributes mode="INOUT" include="pk" optional="false"></auto-attributes>
    </service>
</services>   