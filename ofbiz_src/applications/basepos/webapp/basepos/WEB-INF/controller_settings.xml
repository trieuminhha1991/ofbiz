<site-conf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/site-conf.xsd">
	<description>Web Pos Component Site Configuration File</description>

	<request-map uri="configTerminalPOS">
		<security https="true" auth="true" />
		<response name="success" type="view" value="ConfigTerminalPOS" />
	</request-map>

	<request-map uri="configPrintOrderPOS">
		<security https="true" auth="true" />
		<response name="success" type="view" value="ConfigPrintOrderPOS" />
	</request-map>

	<request-map uri="editTerminalPOS">
		<security https="true" auth="true" />
		<event type="service" invoke="updateTerminalPOS" />
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>

	<request-map uri="deleteTerminalPOS">
		<security https="true" auth="true" />
		<event type="service" invoke="deleteTerminalPOS" />
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>

	<request-map uri="uploadLogoStore">
		<security https="true" auth="true" />
