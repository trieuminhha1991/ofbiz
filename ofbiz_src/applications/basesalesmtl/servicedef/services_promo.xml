            <exclude field-name="createdDate"/>
            <exclude field-name="createdByUserLogin"/>
            <exclude field-name="lastModifiedDate"/>
            <exclude field-name="lastModifiedByUserLogin"/>
        </auto-attributes>
        <override name="productPromoId" mode="INOUT" optional="true"/>
        <override name="promoName" optional="true" allow-html="safe"/>
        <override name="promoText" optional="true" allow-html="safe"/>
        <override name="paymentMethod" optional="true" allow-html="safe"/>
