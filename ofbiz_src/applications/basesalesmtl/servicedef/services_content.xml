<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
	<description>BaseSalesMtl Services</description>
	<vendor></vendor>
	<version>1.0</version>
	
    <service name="uploadFileAttachment" engine="java" invoke="uploadFileAttachment" location="com.olbius.salesmtl.ContentServices">
		<description>Create new file in Jackrabbit </description>
		<implements service="uploadFileInterface" optional="false"/>
		<attribute name="entityName" mode="IN" type="String"/>
		<attribute name="fields" mode="IN" type="String"/>
	</service>
	<service name="loadFileAttachment" engine="java" invoke="loadFileAttachment" location="com.olbius.salesmtl.ContentServices">
		<attribute name="fields" mode="IN" type="String"/>
		<attribute name="entityName" mode="IN" type="String"/>
		<attribute name="fileAttachment" mode="OUT" type="List" optional="true"/>
	</service>
    
</services>