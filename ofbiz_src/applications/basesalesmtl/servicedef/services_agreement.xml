<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>BaseSalesMtl Services</description>
    <vendor></vendor>
    <version>1.0</version>

    <service name="noSyntaxError" engine="interface">
        <description>
            Dummy service to prevent empty files and syntax error - Remove when the 1st real service will be added here 
        </description>
    </service>
    
    <!-- <service name="JQGetListPartyTo" engine="java" invoke="listPartyTo" location="com.olbius.salesmtl.AgreementServices">
		<implements service="jqxGetListInterface"/>
	</service> -->
    <service name="JQGetListSalesRepresentative" engine="java" invoke="jqGetListSalesRepresentative" location="com.olbius.salesmtl.AgreementServices">
		<implements service="jqxGetListInterface"/>
	</service>
	<service name="JQGetListRepresentTo" engine="java" invoke="listRepresentTo" location="com.olbius.salesmtl.AgreementServices">
		<implements service="jqxGetListInterface"/>
	</service>
    
    <service name="createAgreementDistributor" engine="java" invoke="createAgreementDistributor" location="com.olbius.salesmtl.AgreementServices">
		<auto-attributes mode="INOUT" entity-name="Agreement" optional="true"></auto-attributes>
		<attribute name="representativeId" mode="IN" type="String"></attribute>
		<override name="agreementDate" type="Long"></override>
		<override name="fromDate" type="Long"></override>
		<override name="thruDate" type="Long"></override>
	</service>
    <service name="updateAgreementDistributor" engine="java" invoke="updateAgreementDistributor" location="com.olbius.salesmtl.AgreementServices">
		<auto-attributes mode="INOUT" entity-name="Agreement" optional="true"></auto-attributes>
		<override name="agreementId" optional="false"></override>
		<override name="agreementDate" type="Long"></override>
		<override name="fromDate" type="Long"></override>
		<override name="thruDate" type="Long"></override>
	</service>
    <service name="JQGetListAgreementDistributor" engine="java" invoke="listAgreementDistributor" location="com.olbius.salesmtl.AgreementServices">
		<implements service="jqxGetListInterface"/>
	</service>
    <service name="JQGetListAgreementAgent" engine="java" invoke="listAgreementAgent" location="com.olbius.salesmtl.AgreementServices">
		<implements service="jqxGetListInterface"/>
	</service>
	<service name="checkAgreementCode" engine="java" invoke="checkAgreementCode" location="com.olbius.salesmtl.AgreementServices">
		<attribute name="agreementCode" mode="IN" type="String"></attribute>
		<attribute name="agreementId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="check" mode="OUT" type="String"></attribute>
	</service>
	<service name="approveAgreement" engine="java" invoke="approveAgreement" location="com.olbius.salesmtl.AgreementServices" auth="true">
		<auto-attributes mode="IN" entity-name="Agreement" optional="true"></auto-attributes>
	</service>
	<service name="loadAgreementInfo" engine="java" invoke="loadAgreementInfo" location="com.olbius.salesmtl.AgreementServices" auth="true">
		<attribute name="agreementId" mode="IN" type="String"></attribute>
		<attribute name="agreementInfo" mode="OUT" type="Map"></attribute>
	</service>
	
	<service name="uploadFileScanAgreement" engine="java" invoke="uploadFileScanAgreement" location="com.olbius.salesmtl.AgreementServices">
		<description>Create new file in Jackrabbit </description>
		<implements service="uploadFileInterface" optional="false"/>
		<attribute name="folder" mode="IN" type="String" optional="true"/>
		<attribute name="pathType" mode="IN" type="String" optional="true"/>
		<attribute name="agreementId" mode="IN" type="String"></attribute>
		<attribute name="name" mode="OUT" type="String" optional="true"/>
		<attribute name="path" mode="OUT" type="String" optional="true"/>
		<attribute name="mimeType" mode="OUT" type="String" optional="true"/>
	</service>
	<service name="loadFileScanAgreement" engine="java" invoke="loadFileScanAgreement" location="com.olbius.salesmtl.AgreementServices">
		<attribute name="agreementId" mode="IN" type="String"></attribute>
		<attribute name="fileScanAgreement" mode="OUT" type="List" optional="true"/>
	</service>
	
	<service name="createAgreementFromPromoExtReg" engine="java" invoke="createAgreementFromPromoExtReg" location="com.olbius.salesmtl.AgreementServices">
		<attribute name="regProductPromoId" mode="IN" type="String"/>
		<attribute name="regProductPromoRuleId" mode="IN" type="String"/>
		<attribute name="regFromDate" mode="IN" type="String"/>
		<attribute name="customerId" mode="IN" type="String"/>
		<attribute name="agreementCode" mode="IN" type="String" optional="true"/>
		<attribute name="agreementDate" mode="IN" type="String"/>
		<attribute name="fromDate" mode="IN" type="String"/>
		<attribute name="thruDate" mode="IN" type="String" optional="true"/>
		<attribute name="agreementId" mode="OUT" type="String"/>
	</service>
	
</services>
