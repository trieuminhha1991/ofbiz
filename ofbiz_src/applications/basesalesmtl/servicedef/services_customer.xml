<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>BaseSalesMtl Services</description>
    <vendor></vendor>
    <version>1.0</version>

    <service name="JQGetListCustomerRelContact" engine="java" invoke="jqGetListCustomerRelContact" location="com.olbius.salesmtl.partycustomer.PartyCustomerServices" auth="true">
   		<implements service="jqxGetListInterface"/>
   	</service>
   	<service name="createCustomerRelContact" engine="java" invoke="createCustomerRelContact" location="com.olbius.salesmtl.partycustomer.PartyCustomerServices" auth="true">
		<attribute name="customerId" mode="IN" type="String"/>
		<attribute name="roleTypeId" mode="IN" type="String"/>
		<attribute name="firstName" mode="IN" type="String" optional="true"/>
		<attribute name="middleName" mode="IN" type="String" optional="true"/>
		<attribute name="lastName" mode="IN" type="String" optional="true"/>
		<attribute name="gender" mode="IN" type="String" optional="true"/>
		<attribute name="phoneNumber" mode="IN" type="String" optional="true"/>
	</service>
   	<service name="updateCustomerRelContact" engine="java" invoke="updateCustomerRelContact" location="com.olbius.salesmtl.partycustomer.PartyCustomerServices" auth="true">
		<attribute name="customerId" mode="IN" type="String"/>
		<attribute name="partyContactId" mode="IN" type="String"/>
		<attribute name="roleTypeId" mode="IN" type="String"/>
		<attribute name="firstName" mode="IN" type="String" optional="true"/>
		<attribute name="middleName" mode="IN" type="String" optional="true"/>
		<attribute name="lastName" mode="IN" type="String" optional="true"/>
		<attribute name="gender" mode="IN" type="String" optional="true"/>
		<attribute name="phoneNumber" mode="IN" type="String" optional="true"/>
	</service>
   	<service name="deleteCustomerRelContact" engine="java" invoke="deleteCustomerRelContact" location="com.olbius.salesmtl.partycustomer.PartyCustomerServices" auth="true">
		<attribute name="partyIdFrom" mode="IN" type="String"><description>Is customer id</description></attribute>
		<attribute name="partyId" mode="IN" type="String"><description>Is party id to</description></attribute>
	</service>
   	<service name="reopenCustomerRelContact" engine="java" invoke="reopenCustomerRelContact" location="com.olbius.salesmtl.partycustomer.PartyCustomerServices" auth="true">
		<attribute name="partyIdFrom" mode="IN" type="String"><description>Is customer id</description></attribute>
		<attribute name="partyId" mode="IN" type="String"><description>Is party id to</description></attribute>
		<attribute name="fromDate" mode="IN" type="String"></attribute>
	</service>
	
</services>