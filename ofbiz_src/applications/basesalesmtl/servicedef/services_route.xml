<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>BaseSalesMtl Services for route</description>
    <vendor></vendor>
    <version>1.0</version>

    <service name="JQGetListRouteMT" engine="java" invoke="getListRouteMT" location="com.olbius.salesmtl.route.MTRouteService">
        <implements service="jqxGetListInterface"/>
	</service>

    <service name="createRouteMT" engine="java" auth="true" invoke="createRouteMT" location="com.olbius.salesmtl.route.MTRouteService">
        <attribute name="routeName" mode="IN" type="String" optional="false"/>
        <attribute name="description" mode="IN" type="String" optional="true"/>
        <attribute name="scheduleRoute" mode="IN" type="String" optional="false"/>
        <attribute name="routeCode" mode="IN" type="String" optional="false"/>
        <attribute name="salesmanId" mode="IN" type="String" optional="true"/>
        <attribute name="weeks" mode="IN" type="String" optional="true"/>
        <attribute name="routeId" mode="OUT" type="String" optional="true"/>
    </service>

    <service name="deleteRouteMT" engine="java" auth="true" invoke="deleteRouteMT" location="com.olbius.salesmtl.route.MTRouteService">
        <attribute name="routeId" mode="IN" type="String" optional="false"></attribute>
    </service>


    <service name="JQGetListRouteCustomerChangeSaleman" engine="java" invoke="JQGetListRouteCustomerChangeSaleman" location="com.olbius.salesmtl.route.MTRouteService">
        <implements service="jqxGetListInterface"/>
    </service>


    <service name="updateRouteMT" engine="java" auth="true" invoke="updateRouteMT" location="com.olbius.salesmtl.route.MTRouteService">
        <attribute name="routeName" mode="IN" type="String" optional="false"/>
        <attribute name="description" mode="IN" type="String" optional="true"/>
        <attribute name="scheduleRoute" mode="IN" type="String" optional="false"/>
        <attribute name="salesmanId" mode="IN" type="String" optional="true"/>
        <attribute name="routeCode" mode="IN" type="String" optional="false"/>
        <attribute name="routeId" mode="IN" type="String" optional="false"/>
        <attribute name="weeks" mode="IN" type="String" optional="true"/>
        <attribute name="isPartyChange" mode="IN" type="String" optional="true"/>
    </service>

    <service name="JQGetListSalesmanManagementMT" engine="java" invoke="getListSalesmanManagementMT" location="com.olbius.salesmtl.route.MTRouteService">
        <implements service="jqxGetListInterface"/>
    </service>

    <service name="JQGetCustAssignedToRouteMT" engine="java" invoke="jqGetCustAssignedToRouteMT" location="com.olbius.salesmtl.route.MTRouteService">
        <implements service="jqxGetListInterface"/>
    </service>

    <service name="JQGetCustAvailableMT" engine="java" invoke="jqGetCustAvailableMT" location="com.olbius.salesmtl.route.MTRouteService">
        <implements service="jqxGetListInterface"/>
    </service>

    <service name="JQGetListRouteSimpleMT" engine="java" invoke="jqGetListRouteSimpleMT" location="com.olbius.salesmtl.route.MTRouteService">
        <attribute name="salesmanId" mode="IN" type="String" optional="true"></attribute>
        <implements service="jqxGetListInterface"/>
    </service>

    <service name="getCustAvailableBySalesmanMT" engine="java" invoke="getCustAvailableBySalesmanMT" location="com.olbius.salesmtl.route.MTRouteService">
        <attribute name="salesmanId" mode="IN" type="String" optional="false"/>
        <attribute name="routeId" mode="IN" type="String" optional="false"/>
        <attribute name="customers" mode="OUT" type="java.util.List"/>
    </service>

    <service name="JQGetListMobileDeviceLogMT" engine="java" invoke="jqGetListMobileDeviceLogMT" location="com.olbius.salesmtl.route.MTRouteService">
        <implements service="jqxGetListInterface"/>
    </service>
</services>