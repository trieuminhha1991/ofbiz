<?xml version="1.0" encoding="UTF-8"?>
<site-conf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/site-conf.xsd">

	<request-map uri="distributorFacilityReport">
		<security https="true" auth="true" />
		<response name="success" type="view" value="DistributorFacilityReport" />
		<response name="error" type="view" value="DistributorFacilityReport" />
	</request-map>

	<request-map uri="olapDistributorFacilityReport">
		<security https="true" auth="true" />
		<event type="service" invoke="olbiusReportFacility" />
		<response name="success" type="request" value="json" />
		<response name="error" type="request" value="json" />
	</request-map>
	
	<view-map name="DistributorFacilityReport" type="screen" page="component://basesalesmtl/widget/report/BaseSalesMtlReportScreens.xml#DistributorFacilityReport" />

</site-conf>