<?xml version="1.0" encoding="UTF-8"?>
<site-conf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/site-conf.xsd">
    
	<request-map uri="AgreementWithDistributor">
    	<security auth="true" https="true"/>
    	<response name="success" type="view" value="AgreementWithDistributor"/>
    </request-map>
	<request-map uri="AgreementWithAgent">
    	<security auth="true" https="true"/>
    	<response name="success" type="view" value="AgreementWithAgent"/>
    </request-map>
	<request-map uri="AddAgreement">
    	<security auth="true" https="true"/>
    	<response name="success" type="view" value="AddAgreement"/>
    </request-map>
	<request-map uri="AgreementDetail">
    	<security auth="true" https="true"/>
    	<response name="success" type="view" value="AgreementDetail"/>
    </request-map>
    
    <request-map uri="agreement.pdf">
		<security https="false" auth="true"/>
		<response name="success" type="view" value="AgreementPDF"/>
	</request-map>
    
    <request-map uri="getPartyInformationAjax">
		<security auth="true" https="true" />
		<event type="service" invoke="getPartyInformation"></event>
	 	<response name="success" type="request" value="json"></response>
	 	<response name="error" type="request" value="json"></response>
	</request-map>
    <request-map uri="createAgreementDistributor">
		<security auth="true" https="true" />
		<event type="service" invoke="createAgreementDistributor"></event>
	 	<response name="success" type="request" value="json"></response>
	 	<response name="error" type="request" value="json"></response>
	</request-map>
    <request-map uri="updateAgreementDistributor">
		<security auth="true" https="true" />
		<event type="service" invoke="updateAgreementDistributor"></event>
	 	<response name="success" type="request" value="json"></response>
	 	<response name="error" type="request" value="json"></response>
	</request-map>
    <request-map uri="checkAgreementCode">
		<security auth="true" https="true" />
		<event type="service" invoke="checkAgreementCode"></event>
	 	<response name="success" type="request" value="json"></response>
	 	<response name="error" type="request" value="json"></response>
	</request-map>
	
    <request-map uri="approveAgreement">
		<security auth="true" https="true" />
		<event type="service" invoke="approveAgreement"></event>
	 	<response name="success" type="request" value="json"></response>
	 	<response name="error" type="request" value="json"></response>
	</request-map>
	
    <request-map uri="loadAgreementInfo">
		<security auth="true" https="true" />
		<event type="service" invoke="loadAgreementInfo"></event>
	 	<response name="success" type="request" value="json"></response>
	 	<response name="error" type="request" value="json"></response>
	</request-map>
	
	<request-map uri="uploadFileScanAgreement">
        <security https="true" auth="true"/>
        <event type="service" invoke="uploadFileScanAgreement"/>
        <response name="success" type="request" value="json"></response>
        <response name="error" type="request" value="json"></response>
    </request-map>
	<request-map uri="loadFileScanAgreement">
        <security https="true" auth="true"/>
        <event type="service" invoke="loadFileScanAgreement"/>
        <response name="success" type="request" value="json"></response>
        <response name="error" type="request" value="json"></response>
    </request-map>
    
    <request-map uri="createAgreementFromPromoExtReg">
    	<security https="true" auth="true"/>
    	<event type="service" invoke="createAgreementFromPromoExtReg"/>
    	<response name="success" type="request" value="json"/>
    	<response name="error" type="request" value="json"/>
    </request-map>
    <request-map uri="exhibitedAgreement.pdf">
		<security https="true" auth="true" />
		<response name="success" type="view" value="ExhibitedAgreementPDF" />
	</request-map>
	
    <request-map uri="updateLocationAgent">
	   	<security https="true" auth="true"/>
	   	<event type="service" invoke="updateCoordinateCustomer"/>
	   	<response name="success" type="request" value="json"/>
	   	<response name="error" type="request" value="json"/>
	</request-map>
	
	<!-- PDFs -->
    <view-map name="ExhibitedAgreementPDF" type="screenfop" page="component://basesalesmtl/widget/PrintScreens.xml#ExhibitedAgreementPDF" content-type="application/pdf" encoding="none" />
	
    <view-map name="AgreementWithDistributor" type="screen" page="component://basesalesmtl/widget/agreement/AgreementScreens.xml#AgreementWithDistributor"/>
    <view-map name="AgreementWithAgent" type="screen" page="component://basesalesmtl/widget/agreement/AgreementScreens.xml#AgreementWithAgent"/>
    <view-map name="AddAgreement" type="screen" page="component://basesalesmtl/widget/agreement/AgreementScreens.xml#AddAgreement"/>
    <view-map name="AgreementDetail" type="screen" page="component://basesalesmtl/widget/agreement/AgreementScreens.xml#AgreementDetail"/>
    <view-map name="AgreementPDF" type="screenfop" page="component://basesalesmtl/widget/agreement/AgreementScreens.xml#AgreementPDF" content-type="application/pdf" />
</site-conf>