<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">

	<!-- SEND MESS -->
	<service name="productPublisher" engine="olbius" location="com.olbius.activemq.send.services.ProductPublisher">
		<attribute name="productId" mode="IN" type="String" />
	</service>
	<service name="registerSupplier" engine="olbius" location="com.olbius.activemq.send.services.RegisterSupplier">
		<attribute name="productStoreId" mode="IN" type="String" />
		<attribute name="products[]" mode="IN" type="List" optional="true" />
	</service>
	<service name="purchaseOrderSupplier" engine="olbius" location="com.olbius.activemq.send.services.PurchaseOrderSupplier">
		<attribute name="partyFromId" type="String" mode="IN" />
		<attribute name="partyToId" type="String" mode="IN" />
		<attribute name="orderId" type="String" mode="IN" />
		<attribute name="orderItems" type="List" mode="IN" />
		<attribute name="currencyUom" type="String" mode="IN" />
		<attribute name="orderItemShipGroupInfo" type="List" mode="IN" optional="true" />
	</service>

	<service name="salesOrderSupplier" engine="olbius" location="com.olbius.activemq.send.services.SalesOrderSupplier">
		<attribute name="partyToId" type="String" mode="IN" />
		<attribute name="orderId" type="String" mode="IN" />
		<attribute name="orderItems" type="List" mode="IN" />
		<attribute name="currencyUom" type="String" mode="IN" />
		<attribute name="orderItemShipGroupInfo" type="List" mode="IN" optional="true" />
	</service>

	<service name="updatePurchaseOrderSupplier" engine="java" location="com.olbius.activemq.send.PurchaseOrderSupplier" invoke="updatePurchaseOrderSupplier">
		<attribute name="orderId" mode="IN" type="String" />
	</service>

	<service name="orderCancelSupplier" engine="java" location="com.olbius.activemq.send.PurchaseOrderSupplier" invoke="orderCancelSupplier">
		<attribute name="orderId" mode="IN" type="String" />
	</service>

	<service name="orderItemCancelSupplier" engine="java" location="com.olbius.activemq.send.PurchaseOrderSupplier" invoke="orderItemCancelSupplier">
		<attribute name="orderId" mode="IN" type="String" />
		<attribute name="orderItemSeqId" mode="IN" type="String" />
	</service>

	<service name="confirmOlbiusOrder" engine="olbius" location="com.olbius.activemq.send.services.ConfirmOrder">
		<attribute name="orderId" mode="IN" type="String" />
	</service>

	<service name="testMessage" engine="olbius" location="com.olbius.test.TestMessage"></service>

	<service name="busGetListProductStore" engine="olbius" location="com.olbius.activemq.send.services.ListProductStore">
		<attribute name="data" mode="OUT" type="List"/>
	</service>

	<service name="busGetListProduct" engine="olbius" location="com.olbius.activemq.send.services.ListProduct">
		<attribute name="productStoreId" mode="IN" type="String" optional="true"/>
		<attribute name="data" mode="OUT" type="List" optional="true"/>
	</service>
	
</services>