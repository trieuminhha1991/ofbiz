<?xml version="1.0" encoding="UTF-8"?>
<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">

	<entity entity-name="BusConvert" package-name="org.ofbiz.bus">
		<field name="partyId" type="id-ne"/>
		<field name="type" type="id-ne" />
		<field name="busId" type="id-ne" />
		<field name="ownId" type="id-ne" />
		<field name="own" type="id-ne" />
		<prim-key field="partyId" />
		<prim-key field="busId" />
	</entity>
	
	<entity entity-name="BusEvent" package-name="org.ofbiz.bus">
		<field name="partyId" type="id-ne"/>
		<field name="event" type="id-ne" />
		<field name="update" type="date-time" />
		<prim-key field="partyId" />
		<prim-key field="event" />
	</entity>
	
	<entity entity-name="BusProductStore" package-name="org.ofbiz.bus">
		<field name="productStoreId" type="id-ne"/>
		<field name="productId" type="id-ne"/>
		<field name="partyId" type="id-ne" />
		<prim-key field="partyId" />
		<prim-key field="productId" />
	</entity>
	
	<entity entity-name="BusOrder" package-name="org.ofbiz.bus">
		<field name="orderId" type="id-ne"/>
		<field name="partyFromId" type="id-ne" />
		<field name="partyToId" type="id-ne"/>
		<field name="orderType" type="id-ne"/>
		<prim-key field="orderId" />
		<prim-key field="partyFromId" />
		<prim-key field="partyToId" />
	</entity>
	
</entitymodel>
