		<auto-attributes mode="IN" include="nonpk" optional="true"></auto-attributes>
		<override name="eventId" optional="true"></override>
		<override name="facilityId" optional="false"></override>
		<override name="isClosed" default-value="N"></override>
	</service>
	
	<service name="updateStockEvent" engine="java" invoke="updateStockEvent" location="com.olbius.baselogistics.inventory.StockingServices" auth="true" default-entity-name="StockEvent">
