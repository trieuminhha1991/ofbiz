<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>BaseLogistics Delivery Cluster Services</description>
    <vendor></vendor>
    <version>1.0</version>

    <service name="JQGetListCustomerCluster" engine="java" invoke="jqGetListCustomerCluster" location="com.olbius.baselogistics.util.DeliveryClusterUtil">
        <implements service="jqxGetListInterface"/>
    </service>
    <service name="JQGetListCustomerByCluster" engine="java" invoke="jqGetListCustomerByCluster" location="com.olbius.baselogistics.util.DeliveryClusterUtil">
        <implements service="jqxGetListInterface"/>
    </service>
    <service name="JQGetListCustomerShipperByCluster" engine="java" invoke="jqGetListCustomerShipperByCluster" location="com.olbius.baselogistics.util.DeliveryClusterUtil">
        <implements service="jqxGetListInterface"/>
    </service>
    <service name="JQGetCustAvailableClusterPurpose" engine="java" invoke="jqGetCustAvailableClusterPurpose" location="com.olbius.baselogistics.util.DeliveryClusterUtil">
        <implements service="jqxGetListInterface"/>
    </service>
    <service name="JQGetListDeliveryClusterSimple" engine="java" invoke="jqGetListDeliveryClusterSimple" location="com.olbius.baselogistics.util.DeliveryClusterUtil">
        <implements service="jqxGetListInterface"/>
    </service>
    <service name="JQGetListDeliveryCluster" engine="java" invoke="jqGetListDeliveryCluster" location="com.olbius.baselogistics.util.DeliveryClusterUtil">
        <implements service="jqxGetListInterface"/>
    </service>
    <service name="JQGetListShipperCluster" engine="java" invoke="jqGetListShipperCluster" location="com.olbius.baselogistics.util.DeliveryClusterUtil">
        <implements service="jqxGetListInterface"/>
    </service>
    <service name="createNewDeliveryCluster" engine="java" invoke="createNewDeliveryCluster" location="com.olbius.baselogistics.deliveryCluster.DeliveryClusterServices">
        <attribute name="deliveryClusterCode" mode="IN" type="String" optional="false"/>
        <attribute name="deliveryClusterName" mode="IN" type="String" optional="false"/>
        <attribute name="shipperId" mode="IN" type="String" optional="false"/>
        <attribute name="customerIds" mode="IN" type="String" optional="false"/>
        <attribute name="description" mode="IN" type="String" optional="true"/>
        <attribute name="deliveryClusterId" mode="OUT" type="String" optional="true"/>
    </service>

    <service name="deleteDeliveryCluster" engine="java" auth="true" invoke="deleteDeliveryCluster" location="com.olbius.baselogistics.deliveryCluster.DeliveryClusterServices">
        <attribute name="deliveryClusterId" mode="IN" type="String" optional="false"/>
    </service>

    <service name="updateDeliveryCluster" engine="java" auth="true" invoke="updateDeliveryCluster" location="com.olbius.baselogistics.deliveryCluster.DeliveryClusterServices">
        <attribute name="deliveryClusterId" mode="IN" type="String" optional="false"/>
        <attribute name="deliveryClusterCode" mode="IN" type="String" optional="false"/>
        <attribute name="deliveryClusterName" mode="IN" type="String" optional="false"/>
        <attribute name="executorId" mode="IN" type="String" optional="false"/>
        <attribute name="description" mode="IN" type="String" optional="true"/>
    </service>

    <service name="getListCustomerPerDeliveryCluster" engine="java" invoke="getListCustomerPerDeliveryCluster" location="com.olbius.baselogistics.util.DeliveryClusterUtil">
        <attribute name="deliveryClusterIds" mode="IN" type="String" optional="false"/>
        <attribute name="customers" mode="OUT" type="java.util.List" optional="true"/>
    </service>

    <service name="createDeliveryClusterCustomers" engine="java" auth="true" invoke="createDeliveryClusterCustomers" location="com.olbius.baselogistics.util.DeliveryClusterUtil">
        <attribute name="parties" mode="IN" type="String" optional="false"/>
        <attribute name="deliveryClusterId" mode="IN" type="String" optional="false"/>
    </service>

    <service name="removeDeliveryClusterCustomers" engine="java" auth="true" invoke="removeDeliveryClusterCustomers" location="com.olbius.baselogistics.util.DeliveryClusterUtil">
        <attribute name="parties" mode="IN" type="String" optional="false"/>
        <attribute name="deliveryClusterId" mode="IN" type="String" optional="false"/>
    </service>

    <service name="getCustomersInDeliveryCluster" engine="java" auth="true" invoke="getCustomersInDeliveryCluster" location="com.olbius.baselogistics.util.DeliveryClusterUtil">
        <attribute name="deliveryClusterId" mode="IN" type="String" optional="false"/>
        <attribute name="customers" mode="OUT" type="java.util.List" optional="true"/>
    </service>

    <service name="getCustomersAvailableInDeliveryCluster" engine="java" auth="true" invoke="getCustomersAvailableInDeliveryCluster" location="com.olbius.baselogistics.util.DeliveryClusterUtil">
        <attribute name="deliveryClusterId" mode="IN" type="String" optional="false"/>
        <attribute name="latLngBound" mode="IN" type="String" optional="true"/>
        <attribute name="customers" mode="OUT" type="java.util.List" optional="true"/>
    </service>

</services>