<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>BaseLogistics Services</description>
    <vendor></vendor>
    <version>1.0</version>
    
    <service name="getFacilities" engine="java" invoke="getFacilities" location="com.olbius.baselogistics.facility.FacilityServices">
		<implements service="jqxGetListInterface"/>
        <override name="listIterator" mode="OUT" type="java.util.List" optional="true"/>
	</service>
	
	<service name="jqGetFacilities" engine="java" invoke="jqGetFacilities" location="com.olbius.baselogistics.facility.FacilityServices">
		<implements service="jqxGetListInterface"/>
        <override name="listIterator" mode="OUT" type="java.util.List" optional="true"/>
	</service>

    <service name="jqGetFacilitiesForSup" engine="java" invoke="jqGetFacilitiesForSup" location="com.olbius.baselogistics.facility.FacilityServices">
        <implements service="jqxGetListInterface"/>
        <override name="listIterator" mode="OUT" type="java.util.List" optional="true"/>
    </service>
	<service name="jqGetFacilitiesDisByUserLogin" engine="java" invoke="jqGetFacilitiesDisByUserLogin" location="com.olbius.baselogistics.facility.FacilityServices">
		<implements service="jqxGetListInterface"/>
		<override name="listIterator" mode="OUT" type="java.util.List" optional="true"/>
	</service>

	<service name="updateFacility" engine="java" invoke="updateFacility" location="com.olbius.baselogistics.facility.FacilityServices">
		<attribute name="facilityName" mode="IN" type="String" optional="true" allow-html="any"/>
		<attribute name="facilityCode" mode="IN" type="String" optional="true" allow-html="any"/>
		<attribute name="primaryFacilityGroupId" mode="IN" type="String" optional="true"/>
		<attribute name="facilityTypeId" mode="IN" type="String" optional="true"/>
		<attribute name="listProductStoreId" mode="IN" type="java.util.List" optional="false"/>
		<attribute name="parentFacilityId" mode="IN" type="String" optional="true"/>
		<attribute name="ownerPartyId" mode="IN" type="String" optional="true"/>
		<attribute name="managerPartyId" mode="IN" type="String" optional="true"/>
		<attribute name="listStoreKeepers" mode="IN" type="String" optional="true"/>
		<attribute name="fromDate" mode="IN" type="String" optional="true"/>
		<attribute name="thruDate" mode="IN" type="String" optional="true"/>
		<attribute name="openedDate" mode="IN" type="String" optional="true"/>
		<attribute name="fromDateManager" mode="IN" type="String" optional="true"/>
		<attribute name="thruDateManager" mode="IN" type="String" optional="true"/>
		<attribute name="facilitySize" mode="IN" type="java.math.BigDecimal" optional="true"/>
		<attribute name="facilitySizeUomId" mode="IN" type="String" optional="true"/>
		<attribute name="facilityId" mode="IN" type="String" optional="true"/>
		<attribute name="facilityId" mode="OUT" type="String" optional="false"/>
		<attribute name="countryGeoId" mode="IN" type="String" optional="true"/>
		<attribute name="provinceGeoId" mode="IN" type="String" optional="true"/>
		<attribute name="districtGeoId" mode="IN" type="String" optional="true"/>
		<attribute name="wardGeoId" mode="IN" type="String" optional="true"/>
		<attribute name="requireLocation" mode="IN" type="String" optional="true"/>
		<attribute name="requireDate" mode="IN" type="String" optional="true"/>
		<attribute name="phoneNumber" mode="IN" type="String" optional="true"/>
		<attribute name="address" mode="IN" type="String" optional="true" allow-html="any"/>
		<attribute name="description" mode="IN" type="String" optional="true" allow-html="any"/>
		<attribute name="imagesPath" mode="IN" type="String" optional="true"/>
	</service>
	
	<service name="updateFacilityRequireDate" engine="java" invoke="updateFacilityRequireDate" location="com.olbius.baselogistics.facility.FacilityServices">
		<attribute name="facilityId" mode="IN" type="String" optional="false" />
		<attribute name="requireDate" mode="IN" type="String" optional="false" />
	</service>
	
	<service name="getFacilityDetail" engine="java" invoke="getFacilityDetail" location="com.olbius.baselogistics.facility.FacilityServices">
		<attribute name="facilityId" mode="IN" type="String" optional="false"/>
		<auto-attributes entity-name="Facility"  mode="OUT" optional="true"/>
		<attribute name="countryGeoId" mode="OUT" type="String" optional="true"/>
		<attribute name="provinceGeoId" mode="OUT" type="String" optional="true"/>
		<attribute name="geoPointId" mode="OUT" type="String" optional="true"/>
		<attribute name="faLat" mode="OUT" type="String" optional="true"/>
		<attribute name="faLng" mode="OUT" type="String" optional="true"/>
		<attribute name="postalAddressId" mode="OUT" type="String" optional="true"/>
		<attribute name="districtGeoId" mode="OUT" type="String" optional="true"/>
		<attribute name="wardGeoId" mode="OUT" type="String" optional="true"/>
		<attribute name="address" mode="OUT" type="String" optional="true"/>
		<attribute name="address1" mode="OUT" type="String" optional="true"/>
		<attribute name="phoneNumber" mode="OUT" type="String" optional="true"/>
		<attribute name="ownFromDate" mode="OUT" type="Timestamp" optional="true"/>
		<attribute name="ownThruDate" mode="OUT" type="Timestamp" optional="true"/>
		<attribute name="manageFromDate" mode="OUT" type="Timestamp" optional="true"/>
		<attribute name="manageThruDate" mode="OUT" type="Timestamp" optional="true"/>
		<attribute name="ownerName" mode="OUT" type="String" optional="true"/>
		<attribute name="managerName" mode="OUT" type="String" optional="true"/>
		<attribute name="managerPartyId" mode="OUT" type="String" optional="true"/>
		<attribute name="imagesPath" mode="OUT" type="String" optional="true"/>
		<attribute name="listProductStoreId" mode="OUT" type="java.util.List" optional="true"/>
		<attribute name="listFacilityParty" mode="OUT" type="java.util.List" optional="true"/>
	</service>
	
	<service name="JQGetListDepositFacilities" engine="java" invoke="listDepositFacilities" location="com.olbius.baselogistics.facility.FacilityServices">
		<implements service="jqxGetListInterface"/>
	</service>
	
	<service name="getFacilityOwnerables" engine="java" invoke="getFacilityOwnerables" location="com.olbius.baselogistics.facility.FacilityServices">
		<attribute name="searchKey" mode="IN" type="String" optional="true"/>
		<attribute name="listParties" mode="OUT" type="List" optional="false"/>
	</service>
	
	<service name="getFacilityManagerables" engine="java" invoke="getFacilityManagerables" location="com.olbius.baselogistics.facility.FacilityServices">
		<attribute name="searchKey" mode="IN" type="String" optional="true"/>
		<attribute name="listParties" mode="OUT" type="List" optional="false"/>
	</service>
	
	<service name="JQGetListContactMechByFacility" engine="java" invoke="jqGetListContactMechByFacility" location="com.olbius.basepo.contactMech.ContactMechServices">
		<implements service="jqxGetListInterface" />
		<override name="listIterator" mode="OUT" type="java.util.List" optional="true" />
	</service>

	<service name="updateCoordinateFacility" engine="java" auth="true"
			 location="com.olbius.baselogistics.facility.FacilityServices" invoke="updateCoordinateFacility">
		<description> </description>
		<attribute name="facilityId" type="String" mode="INOUT"/>
		<attribute name="postalAddressId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="geoPointId" mode="IN" type="String" optional="true"></attribute>
		<attribute name="lat" mode="IN" type="String" />
		<attribute name="lng" mode="IN" type="String" />
		<attribute name="dataSourceId" mode="IN" type="String" />

		<attribute name="message" mode="OUT" type="String"/>
	</service>
</services>