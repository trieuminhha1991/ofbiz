<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>BaseLogistics Services - Inventory</description>
    <vendor></vendor>
    <version>1.0</version>
    
	<service name="jqGetListInventoryGroupByDate" engine="java" invoke="jqGetListInventoryGroupByDate" location="com.olbius.baselogistics.inventory.InventoryServices">
		<implements service="jqxGetListInterface" />
	</service>
	
	<service name="getListInventoryGroupByDate" engine="java" invoke="getListInventoryGroupByDate" location="com.olbius.baselogistics.inventory.InventoryServices">
		<auto-attributes entity-name="InventoryItem" mode="IN" include="nonpk" optional="true"/>
		<attribute name="listInventory" mode="OUT" type="java.util.List" optional="true"/>
	</service>
	
	<service name="receiveProductFromPurchaseDelivery" engine="java" invoke="receiveProductFromPurchaseDelivery" location="com.olbius.baselogistics.inventory.ReceiveServices">
		<attribute name="deliveryId" mode="INOUT" type="String" optional="true"/>
		<attribute name="listDeliveryItems" mode="IN" type="String" optional="true"/>
		<attribute name="listProductAttributes" mode="IN" type="String" optional="true"/>
	</service>
	
	<service name="exportProductFromTransferDelivery" engine="java" invoke="exportProductFromTransferDelivery" location="com.olbius.baselogistics.inventory.ExportServices">
   		<attribute name="facilityId" type="String" mode="IN" optional="true"/>
   		<attribute name="contactMechId" type="String" mode="IN" optional="true"/>
   		<attribute name="listProducts" mode="IN" type="String" optional="false" allow-html="any"/>
   		<attribute name="listProductAttributes" mode="IN" type="String" optional="true" allow-html="any"/>
   		<attribute name="deliveryId" type="String" mode="INOUT" optional="false"/>
   	</service>
   	
	<service name="receiveProductFromTransferDelivery" engine="java" invoke="receiveProductFromTransferDelivery" location="com.olbius.baselogistics.inventory.ReceiveServices">
		<attribute name="deliveryId" mode="INOUT" type="String" optional="true"/>
		<attribute name="listDeliveryItems" mode="IN" type="String" optional="true"/>
		<attribute name="listProductAttributes" mode="IN" type="String" optional="true"/>
	</service>
   	
	<service name="createShipmentToReceiveForTransferDelivery" engine="java" invoke="createShipmentToReceiveForTransferDelivery" location="com.olbius.baselogistics.shipment.ShipmentServices">
		<attribute name="deliveryId" mode="IN" type="String" optional="false"/>
		<attribute name="shipmentId" mode="OUT" type="String" optional="false"/>
	</service>
	
	<service name="createShipmentForPurchaseDeliveryDistributor" engine="java" invoke="createShipmentForPurchaseDeliveryDistributor" location="com.olbius.baselogistics.shipment.ShipmentServices">
		<attribute name="deliveryId" mode="IN" type="String" optional="false"/>
		<attribute name="facilityId" mode="IN" type="String" optional="false"/>
		<attribute name="contactMechId" mode="IN" type="String" optional="false"/>
		<attribute name="listProducts" mode="IN" type="java.util.List" optional="false"/>
		<attribute name="shipmentId" mode="OUT" type="String" optional="false"/>
	</service>
   	
   	<service name="receiveProductFromPurchaseShipmentDis" engine="java" invoke="receiveProductFromPurchaseShipmentDis" location="com.olbius.baselogistics.inventory.ReceiveServices">
		<attribute name="deliveryId" mode="INOUT" type="String" optional="true"/>
		<attribute name="facilityId" mode="IN" type="String" optional="true"/>
		<attribute name="listDeliveryItems" mode="IN" type="String" optional="true"/>
		<attribute name="listProductAttributes" mode="IN" type="String" optional="true"/>
	</service>
</services>