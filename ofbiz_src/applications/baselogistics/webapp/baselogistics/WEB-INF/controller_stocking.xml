<?xml version="1.0" encoding="UTF-8"?>
<site-conf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/site-conf.xsd">
        
        
    <request-map uri="ListInventoryStocking">
		<security auth="true" https="true" />
		<response name="success" type="view" value="ListInventoryStocking" />
		<response name="error" type="view" value="ListInventoryStocking" />
	</request-map>
	<request-map uri="InventoryStocking">
		<security auth="true" https="true" />
		<response name="success" type="view" value="InventoryStocking" />
		<response name="error" type="view" value="InventoryStocking" />
	</request-map>
	<request-map uri="getEmployeeByPositionType">
		<security https="false" auth="true" />
		<event type="service" invoke="getEmployeeByPositionType"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="createStockEventAndRole">
		<security https="true" auth="true" />
		<event type="service" invoke="createStockEventAndRole"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="updateStockEventAndRole">
		<security https="true" auth="true" />
		<event type="service" invoke="updateStockEventAndRole"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="loadPartiesInEventByRole">
		<security https="false" auth="true" />
		<event type="service" invoke="loadPartiesInEventByRole"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="checkLocationExistsInStockEvent">
		<security https="true" auth="true" />
		<event type="service" invoke="checkLocationExistsInStockEvent"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="uploadStockingFiles">
		<security https="true" auth="true" />
		<event type="java" invoke="uploadStockingFiles" path="com.olbius.baselogistics.inventory.StockingServices"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="uploadStockingExcelFiles">
		<security https="true" auth="true" />
		<event type="service" invoke="uploadStockingExcelFiles" /> 
		<response name="success" type="request" value="json" />
		<response name="error" type="request" value="json" />
	</request-map>
	<request-map uri="deleteAllStockEventItemTempData">
		<security https="true" auth="true" />
		<event type="service" invoke="deleteAllStockEventItemTempData"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="transferToStockEventItem">
		<security https="true" auth="true" />
		<event type="service" invoke="transferToStockEventItem"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="loadLocationInEvent">
		<security https="false" auth="true" />
		<event type="service" invoke="loadLocationInEvent"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="getProductAverageCostBaseSimple">
		<security https="true" auth="true" />
		<event type="service" invoke="getProductAverageCostBaseSimple"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="checkStockEventFinishable">
		<security https="true" auth="true" />
		<event type="service" invoke="checkStockEventFinishable"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="deleteStockEventItem">
		<security https="true" auth="true" />
		<event type="service" invoke="deleteStockEventItem"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="InventoryStocking.pdf">
		<security https="true" auth="true" />
		<response name="success" type="view" value="InventoryStockingPDF" />
		<response name="error" type="view" value="InventoryStockingPDF" />
	</request-map>
	<request-map uri="updateQuantityRecheck">
		<security https="true" auth="true" />
		<event type="service" invoke="updateQuantityRecheck"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="finishStockEvent">
		<security https="true" auth="true" />
		<event type="service" invoke="finishStockEvent"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="checkStockEventSubmitable">
		<security https="true" auth="true" />
		<event type="service" invoke="checkStockEventSubmitable"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="submitQuantityAggregated">
		<security https="true" auth="true" />
		<event type="service" invoke="submitQuantityAggregated"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="exportListStockEventAggregatedExcel">
		<security https="true" auth="true" />
		<event type="java" invoke="export" path="com.olbius.baselogistics.inventory.StockingExcel"></event>
		<response name="success" type="none" />
	</request-map>
	
	<request-map uri="getQuantityNonVariance">
		<security https="true" auth="true" />
		<event type="service" invoke="getQuantityNonVariance"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	
	<request-map uri="deleteStockEvent">
		<security https="true" auth="true" />
		<event type="service" invoke="deleteStockEvent"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	
	
	<!-- view -->
	<view-map name="ListInventoryStocking" type="screen" page="component://baselogistics/widget/InventoryScreens.xml#ListInventoryStocking" />
    <view-map name="InventoryStocking" type="screen" page="component://baselogistics/widget/InventoryScreens.xml#InventoryStocking" />
    
    <!-- pdf -->
    <view-map name="InventoryStockingPDF" type="screenfop" page="component://baselogistics/widget/InventoryScreens.xml#InventoryStockingPDF" content-type="application/pdf" encoding="none" />
</site-conf>