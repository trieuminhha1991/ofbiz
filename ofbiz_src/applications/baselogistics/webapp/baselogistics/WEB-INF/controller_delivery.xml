<?xml version="1.0" encoding="UTF-8"?>
<site-conf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/site-conf.xsd">

	<!-- request map -->
	
	<request-map uri="exportPDFReceiptDocument">
		<security https="false" auth="true" />
		<response name="success" type="view" value="ReceiptNotePricePDF" />
		<response name="error" type="view" value="ReceiptNotePricePDF" />
	</request-map>
	
	<request-map uri="exportPDFStockInDocument">
		<security https="false" auth="true" />
		<response name="success" type="view" value="ReceiptPDF" />
		<response name="error" type="view" value="ReceiptPDF" />
	</request-map>
	
	<request-map uri="prepareReceivePurchaseDelivery">
		<security https="false" auth="true" />
		<response name="success" type="view" value="ReceivePurchaseDelivery" />
		<response name="error" type="view" value="ReceivePurchaseDelivery" />
	</request-map>
	
	<request-map uri="receiveProductFromPurchaseDelivery">
		<security auth="true" https="true"/>
		<event type="service" invoke="receiveProductFromPurchaseDelivery"/>
		<response name="success" type="request" value="json"/>
		<response name="error" type="request" value="json"/>
	</request-map>
	
	<request-map uri="getListTransferDeliveries">
		<security https="true" auth="true"/>
		<response name="success" type="view" value="ListTransferDeliveries"/>
		<response name="error" type="view" value="ListTransferDeliveries"/>
	</request-map>
	
	<request-map uri="deliveryTransferDeliveryDetail">
		<security https="true" auth="true"/>
		<response name="success" type="view" value="TransferDeliveryDetail"/>
		<response name="error" type="view" value="TransferDeliveryDetail"/>
	</request-map>
	
	<request-map uri="exportPDFTransferDeliveryDocument">
		<security https="false" auth="true" />
		<response name="success" type="view" value="PDFTransferDeliveryDocument" />
		<response name="error" type="view" value="PDFTransferDeliveryDocument" />
	</request-map>
	
	<request-map uri="prepareExportTransferDelivery">
		<security https="false" auth="true" />
		<response name="success" type="view" value="ExportProductFromTransferDelivery" />
		<response name="error" type="view" value="ExportProductFromTransferDelivery" />
	</request-map>
	
	<request-map uri="prepareReceiveTransferDelivery">
		<security https="false" auth="true" />
		<response name="success" type="view" value="ReceiveTransferDelivery" />
		<response name="error" type="view" value="ReceiveTransferDelivery" />
	</request-map>
	
	<request-map uri="exportProductFromTransferDelivery">
		<security auth="true" https="true"/>
		<event type="service" invoke="exportProductFromTransferDelivery"/>
		<response name="success" type="request" value="json"/>
		<response name="error" type="request" value="json"/>
	</request-map>
	
	<request-map uri="receiveProductFromTransferDelivery">
		<security auth="true" https="true"/>
		<event type="service" invoke="receiveProductFromTransferDelivery"/>
		<response name="success" type="request" value="json"/>
		<response name="error" type="request" value="json"/>
	</request-map>
	
	<!-- View Map -->
	<view-map name="ExportProductFromTransferDelivery" type="screen" page="component://baselogistics/widget/DeliveryScreens.xml#ExportProductFromTransferDelivery"/>
	<view-map name="ReceiveTransferDelivery" type="screen" page="component://baselogistics/widget/DeliveryScreens.xml#ReceiveTransferDelivery"/>
	<view-map name="TransferDeliveryDetail" type="screen" page="component://baselogistics/widget/DeliveryScreens.xml#TransferDeliveryDetail"/>
	<view-map name="ReceivePurchaseDelivery" type="screen" page="component://baselogistics/widget/DeliveryScreens.xml#ReceivePurchaseDelivery"/>
	<view-map name="ListTransferDeliveries" type="screen" page="component://baselogistics/widget/DeliveryScreens.xml#ListTransferDeliveries"/> 
	
	<!-- PDF -->
	<view-map name="ReceiptNotePricePDF" type="screenfop" page="component://baselogistics/widget/DeliveryScreens.xml#ReceiptNotePricePDF" content-type="application/pdf" />
	<view-map name="ReceiptPDF" type="screenfop" page="component://baselogistics/widget/DeliveryScreens.xml#DeliveryPDF" content-type="application/pdf" />
	<view-map name="PDFTransferDeliveryDocument" type="screenfop" page="component://baselogistics/widget/DeliveryScreens.xml#DeliveryTransferPDF" content-type="application/pdf" />
	
</site-conf>