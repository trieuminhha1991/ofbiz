<?xml version="1.0" encoding="UTF-8"?>
<site-conf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/site-conf.xsd">
    <include location="component://common/webcommon/WEB-INF/common-controller.xml"/>

    <description>CRM Report Component Site Configuration File</description>
    <!-- REQUEST -->
    <request-map uri="CreatePicklist">
		<security auth="true" https="true" />
		<response name="success" type="view" value="CreatePicklist" />
		<response name="error" type="view" value="CreatePicklist" />
	</request-map>
    <request-map uri="bufferIntoPickingItem">
		<security https="true" auth="true" />
		<event type="service" invoke="bufferIntoPickingItem"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="deleteAllPickingItemTempData">
		<security https="true" auth="true" />
		<event type="service" invoke="deleteAllPickingItemTempData"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="transferToPicklistItem">
		<security https="true" auth="true" />
		<event type="service" invoke="transferToPicklistItem"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="loadPicklistInfo">
		<security https="true" auth="true" />
		<event type="service" invoke="loadPicklistInfo"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="Picklist">
		<security auth="true" https="true" />
		<response name="success" type="view" value="Picklist" />
		<response name="error" type="view" value="Picklist" />
	</request-map>
	<request-map uri="exportPicklistExcelExcel">
		<security auth="true" https="true" />
		<event type="java" path="com.olbius.baselogistics.picklist.PicklistExcel" invoke="export"></event>
		<response name="success" type="view" value="DeliveryPlan" />
	</request-map>
	<request-map uri="DeliveryPlan">
		<security auth="true" https="true" />
		<response name="success" type="view" value="DeliveryPlan" />
		<response name="error" type="view" value="DeliveryPlan" />
	</request-map>
	<request-map uri="cancelPicklist">
		<security https="true" auth="true" />
		<event type="service" invoke="cancelPicklist"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="approvePicklist">
		<security https="true" auth="true" />
		<event type="service" invoke="approvePicklist"/>
		<response name="success" type="request" value="json"></response>
		<response name="error" type="request" value="json"></response>
	</request-map>
	<request-map uri="PicklistDetail">
		<security auth="true" https="true" />
		<response name="success" type="view" value="PicklistDetail" />
		<response name="error" type="view" value="PicklistDetail" />
	</request-map>
	<request-map uri="assignPartyToPicklistBin">
		<security https="true" auth="true"/>
		<event type="service" invoke="assignPartyToPicklistBin" /> 
		<response name="success" type="request" value="json" />
		<response name="error" type="request" value="json" />
	</request-map>
	<request-map uri="PicklistBin">
		<security auth="true" https="true" />
		<response name="success" type="view" value="PicklistBin" />
		<response name="error" type="view" value="PicklistBin" />
	</request-map>
	<request-map uri="approveListPicklistBinStatus">
		<security https="true" auth="true"/>
		<event type="service" invoke="approveListPicklistBinStatus" /> 
		<response name="success" type="request" value="json" />
		<response name="error" type="request" value="json" />
	</request-map>
	<request-map uri="changePicklistBinStatus">
		<security https="true" auth="true"/>
		<event type="service" invoke="changePicklistBinStatus" /> 
		<response name="success" type="request" value="json" />
		<response name="error" type="request" value="json" />
	</request-map>
	<request-map uri="deleteListPicklistBinStatus">
		<security https="true" auth="true"/>
		<event type="service" invoke="deleteListPicklistBinStatus" /> 
		<response name="success" type="request" value="json" />
		<response name="error" type="request" value="json" />
	</request-map>
	<request-map uri="getListPickistBins">
		<security https="true" auth="true"/>
		<response name="success" type="view" value="ListPickistBins"/>
		<response name="error" type="view" value="ListPickistBins"/>
	</request-map>
	<request-map uri="createDeliveryFromPicklistBin">
		<security https="true" auth="true"/>
		<event type="service" invoke="createDeliveryFromPicklistBin" /> 
		<response name="success" type="request" value="json" />
		<response name="error" type="request" value="json" />
	</request-map>
	<!--VIEW-MAP -->
	<view-map name="CreatePicklist" type="screen" page="component://baselogistics/widget/PicklistScreens.xml#CreatePicklist" />
	<view-map name="Picklist" type="screen" page="component://baselogistics/widget/PicklistScreens.xml#Picklist" />
	<view-map name="DeliveryPlan" type="screen" page="component://baselogistics/widget/PicklistScreens.xml#DeliveryPlan" />
	<view-map name="PicklistDetail" type="screen" page="component://baselogistics/widget/PicklistScreens.xml#PicklistDetail" />
	<view-map name="PicklistBin" type="screen" page="component://baselogistics/widget/PicklistScreens.xml#PicklistBin" />
	<view-map name="ListPickistBins" type="screen" page="component://baselogistics/widget/PicklistScreens.xml#ListPickistBins" />
   
</site-conf>