	<menu name="CustomerReturnDetailSubTabBar" modelMenu="olbiusScreenletMenu" extends="CommonTabBarMenu" extends-resource="component://common/widget/CommonMenus.xml">
		<menu-item name="AcceptReturn" tooltip="${uiLabelMap.OK}" widget-style="fa-check-circle-o fontStyleNormal">
			<condition>
				<and>
					<if-compare operator="equals" value="RETURN_REQUESTED" field="statusId"/>
					<if-has-permission permission="CUS_RETURN" action="_ADMIN"/>				
				</and>
			</condition>
			<link target="javascript:DetailReturnObj.changeReturnStatus('${returnId}', 'RETURN_ACCEPTED')" url-mode="plain"></link>
		</menu-item>
		<menu-item name="ReceiveReturn" tooltip="${uiLabelMap.ReceiveProduct}" widget-style="fa-download fontStyleNormal button-action button-size">
			<condition>
				<and>
					<if-compare operator="equals" value="RETURN_ACCEPTED" field="statusId"/>
					<if-has-permission permission="CUS_RETURN" action="_UPDATE"/>				
				</and>
			</condition>
			<link target="javascript:DetailReturnObj.changeReturnStatus('${returnId}', 'RETURN_RECEIVED')" url-mode="plain"></link>
		</menu-item>
	</menu>
	
