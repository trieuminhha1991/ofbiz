	<screen name="ViewContactMechInfo">
        <section>
            <actions>
            	<set field="selectedMenuItem" value="Facility"/>
                <set field="selectedSubMenuItem" value="ListFacility"/>
                <set field="facilityId" from-field="parameters.facilityId"/>
                <entity-one entity-name="Facility" value-field="facility"/>
                <set field="titleProperty" value="FacilityContactMechanism"/>
               	<entity-one value-field="facility" entity-name="Facility"/>
			    <set field="titleHeaderValue" value="FacilityTitle" global="true"/>
				<set field="titleHeaderLocation" value="component://baselogistics/widget/FacilityScreens.xml" global="true"/>
                <entity-condition list="listContactMechType" entity-name="ContactMechType">
                	<condition-list combine="or">
                		<condition-expr field-name="contactMechTypeId" value="POSTAL_ADDRESS" />
                		<condition-expr field-name="contactMechTypeId" value="EMAIL_ADDRESS" />
                		<condition-expr field-name="contactMechTypeId" value="TELECOM_NUMBER" />
                	</condition-list>
                </entity-condition>
            </actions>
            <widgets>
            	<section>
                	<actions>
					    <set field="tabBarName" value="FacilityTabBar" global="true" />
					    <set field="tabBarLocation" value="component://baselogistics/widget/BaseLogisticsMenus.xml" global="true"/>
				     	<set field="tabButtonItem" value="ContactMech" global="true"/>
					    <set field="printTabBar" value="true" />
				    </actions>
				    <widgets>
				    	<section>
				    		 <actions>
						      <set field="printTabBar" value="true" />
						     </actions>
						     <widgets>
						     	<container></container>
						     </widgets>
				    	</section>
				    </widgets>
                  </section>
                <decorator-screen name="FacilityCommonDecorator">
                    <decorator-section name="body">
                         <section>
                             <condition>
                                 <not><if-empty field="facilityId"/></not>
                             </condition>
                             <widgets>
                                 <section>
                                     <widgets>
				                            <platform-specific><html><html-template location="component://baselogistics/webapp/baselogistics/facility/contactMechDetails.ftl"/></html></platform-specific>
                                     </widgets>
                                 </section>
                             </widgets>
                         </section>
                    </decorator-section>
                </decorator-screen>
            </widgets>
        </section>
    </screen>
	
