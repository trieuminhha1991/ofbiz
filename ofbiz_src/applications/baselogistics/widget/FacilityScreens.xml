			</actions>
			<widgets>
				<label style="ibefore-icon-angle-right" text=" ${uiLabelMap.Facility}: "></label>
				<link url-mode="plain" target="detailFacility?facilityId=${parameters.facilityId}" text="${facility.facilityCode}"></link>
