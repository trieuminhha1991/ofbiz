import groovy.json.JsonSlurper

MIN_PRODUCT = vars.get(&apos;MIN_PRODUCT&apos;).toInteger()
MAX_PRODUCT = vars.get(&apos;MAX_PRODUCT&apos;).toInteger()
MIN_QUANTITY = vars.get(&apos;MIN_QUANTITY&apos;).toInteger()
MAX_QUANTITY = vars.get(&apos;MAX_QUANTITY&apos;).toInteger()

def randint(a, b) {
    Random random = new Random();
    randomNumber = random.nextInt(b + 1 - a) + a;
    return randomNumber
}

url_file = vars.get(&apos;url_file&apos;)

listProduct = new ArrayList&lt;Map&lt;String, String&gt;&gt;()
new File(url_file).splitEachLine(&quot;;&quot;) {fields -&gt;
    product = new HashMap&lt;String, String&gt;() 
    product.put(&quot;productId&quot;, fields[0])
    product.put(&quot;minimumOrderQuantity&quot;, fields[1])
    product.put(&quot;quantityUomId&quot;, fields[2])
    product.put(&quot;lastPrice&quot;, fields[3])
    listProduct.add(product);
}

listItem = new ArrayList&lt;Map&lt;String, String&gt;&gt;()
numProduct = randint(MIN_PRODUCT, MAX_PRODUCT)
for (int i = 0; i &lt; numProduct; ++i) {
    numQuantity = randint(MIN_QUANTITY, MAX_QUANTITY)
    product = listProduct[randint(0, listProduct.size() - 1)]
    item = new HashMap&lt;String, String&gt;()
    item.putAll(product)
    item.put(&apos;quantity&apos;, numQuantity)
    item.put(&apos;quantityChild&apos;, numQuantity)
    listItem.add(item)
}
listItem = new JsonBuilder(listItem).toString()
vars.put(&apos;orderItems&apos;, listItem)
</stringProp>
