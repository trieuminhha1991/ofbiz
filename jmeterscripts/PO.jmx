            <stringProp name="cacheKey"></stringProp>
            <stringProp name="script">import groovy.json.JsonSlurper
import groovy.json.JsonBuilder

def randint(a, b) {
    Random random = new Random();
    randomNumber = random.nextInt(b + 1 - a) + a;
    return randomNumber
}

def actualManufacturedDate = new Date() - 5
def actualExpireDate = new Date() + 90

raw_data = prev.getResponseDataAsString();
jsonSlurper = new JsonSlurper()
t = jsonSlurper.parseText(raw_data)

list = t.get(&apos;listDeliveryItems&apos;);
listItem = new ArrayList&lt;Map&lt;String, String&gt;&gt;()
for (int i = 0; i &lt; list.size(); i++){
	item = new HashMap&lt;String, String&gt;()
	item.put(&quot;fromOrderId&quot;, list[i].fromOrderId)
	item.put(&quot;fromOrderItemSeqId&quot;, list[i].fromOrderItemSeqId)
	item.put(&quot;actualExpireDate&quot;, actualExpireDate.time)
	item.put(&quot;actualManufacturedDate&quot;, actualManufacturedDate.time)
	item.put(&quot;deliveryId&quot;, list[i].deliveryId)
	item.put(&quot;deliveryItemSeqId&quot;, list[i].deliveryItemSeqId)
	item.put(&quot;actualExportedQuantity&quot;, list[i].quantity)
	item.put(&quot;actualDeliveredQuantity&quot;, list[i].quantity)
	item.put(&quot;quantity&quot;, list[i].quantity)
	item.put(&quot;statusId&quot;, list[i].statusId)
	item.put(&quot;deliveryStatusId&quot;, list[i].deliveryStatusId)
	item.put(&quot;batch&quot;, randint(0, 100))
	listItem.add(item)
}

listItem = new JsonBuilder(listItem).toString()
vars.put(&apos;listDeliveryItems&apos;, listItem)</stringProp>
