          <stringProp name="parameters"></stringProp>
          <stringProp name="script">def randint(a, b) {
    Random random = new Random();
    randomNumber = random.nextInt(b + 1 - a) + a;
    return randomNumber
}

listFacilityMB = new ArrayList&lt;Map&lt;String, String&gt;&gt;()
famb1 = new HashMap&lt;String, String&gt;()
famb1.put(&quot;facilityId&quot;, &quot;FAHN00001&quot;)
famb1.put(&quot;contactMechId&quot;, &quot;1004&quot;)
listFacilityMB.add(famb1);
famb2 = new HashMap&lt;String, String&gt;()
famb2.put(&quot;facilityId&quot;, &quot;FAPOS0001&quot;)
famb2.put(&quot;contactMechId&quot;, &quot;pos1000&quot;)
listFacilityMB.add(famb2);

listFacilityMN = new ArrayList&lt;Map&lt;String, String&gt;&gt;()
famn1 = new HashMap&lt;String, String&gt;()
famn1.put(&quot;facilityId&quot;, &quot;FAHCM00001&quot;)
famn1.put(&quot;contactMechId&quot;, &quot;1003&quot;)
listFacilityMN.add(famn1);
famn2 = new HashMap&lt;String, String&gt;()
famn2.put(&quot;facilityId&quot;, &quot;FAPOS0002&quot;)
famn2.put(&quot;contactMechId&quot;, &quot;pos2000&quot;)
listFacilityMN.add(famn2);

org = vars.get(&apos;org&apos;)

String facilityId
String contactMechId

if (org.equals(&apos;MB&apos;)){
	fa = listFacilityMB[randint(0, listFacilityMB.size() - 1)]	
	facilityId = fa.get(&apos;facilityId&apos;)
	contactMechId = fa.get(&apos;contactMechId&apos;)
}

if (org.equals(&apos;MN&apos;)){
	fa = listFacilityMN[randint(0, listFacilityMN.size() - 1)]	
	facilityId = fa.get(&apos;facilityId&apos;)
	contactMechId = fa.get(&apos;contactMechId&apos;)
}

vars.put(&apos;facilityId&apos;, facilityId)
vars.put(&apos;contactMechId&apos;, contactMechId)
</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="TestPlan.comments">Get Facility</stringProp>
