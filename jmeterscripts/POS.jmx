            <stringProp name="script">import groovy.json.JsonSlurper
import groovy.json.JsonBuilder

MIN_PRODUCT = vars.get(&apos;MIN_PRODUCT&apos;).toInteger()
MAX_PRODUCT = vars.get(&apos;MAX_PRODUCT&apos;).toInteger()
MIN_QUANTITY = vars.get(&apos;MIN_QUANTITY&apos;).toInteger()
MAX_QUANTITY = vars.get(&apos;MAX_QUANTITY&apos;).toInteger()

def randint(a, b) {
    Random random = new Random();
    randomNumber = random.nextInt(b + 1 - a) + a;
    return randomNumber
}

raw_data = prev.getResponseDataAsString();
jsonSlurper = new JsonSlurper()
t = jsonSlurper.parseText(raw_data)

listProduct = t.get(&apos;results&apos;);
listItem = new ArrayList&lt;Map&lt;String, String&gt;&gt;()
numProduct = randint(MIN_PRODUCT, MAX_PRODUCT)
for (int i = 0; i &lt; numProduct; ++i) {
    product = listProduct[randint(0, listProduct.size() - 1)]
    qoh = product.get(&apos;quantityOnHandTotal&apos;)
    maxQuantityTmp = new BigDecimal(MAX_QUANTITY)
    maxQuantity = MAX_QUANTITY
    if (maxQuantityTmp.compareTo(qoh) &gt; 0){
        maxQuantity = qoh.intValue()
    }
    numQuantity = randint(MIN_QUANTITY, maxQuantity)
