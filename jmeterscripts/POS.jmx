              <stringProp name="script">def randint(a, b) {
    Random random = new Random();
    randomNumber = random.nextInt(b + 1 - a) + a;
    return randomNumber
}

url_file = vars.get(&apos;url_file&apos;)

listCustomer = new ArrayList&lt;Map&lt;String, String&gt;&gt;()
new File(url_file).splitEachLine(&quot;;&quot;) {fields -&gt;
    customer = new HashMap&lt;String, String&gt;()
    customer.put(&quot;partyId&quot;, fields[0])
    customer.put(&quot;productStoreId&quot;, fields[1])
    customer.put(&quot;contactMechId&quot;, fields[2])
    customer.put(&quot;org&quot;, fields[3])
    listCustomer.add(customer);
}

listCustomerMB = new ArrayList&lt;Map&lt;String, String&gt;&gt;()
listCustomerMN = new ArrayList&lt;Map&lt;String, String&gt;&gt;()
for (int i = 0; i &lt; listCustomer.size(); i++){
	item = new HashMap&lt;String, String&gt;()
	if (listCustomer[i].get(&apos;org&apos;).equals(&apos;MB&apos;)){
		item.putAll(listCustomer[i])
		listCustomerMB.add(item)
	}
	if (listCustomer[i].get(&apos;org&apos;).equals(&apos;MN&apos;)){
		item.putAll(listCustomer[i])
		listCustomerMN.add(item)
	}
}

org = vars.get(&apos;org&apos;)

String partyId

if (org.equals(&apos;MB&apos;)){
	cus = listCustomerMB[randint(0, listCustomerMB.size() - 1)]
	partyId = cus.get(&apos;partyId&apos;)
}

if (org.equals(&apos;MN&apos;)){
	cus = listCustomerMN[randint(0, listCustomerMN.size() - 1)]
	partyId = cus.get(&apos;partyId&apos;)
}

vars.put(&apos;partyId&apos;, partyId)</stringProp>
