        BigDecimal quantity = list[i].get(&apos;quantity&apos;)
        if (qoh.compareTo(quantity) &gt;= 0){
            inventoryItemMap.put(list[i].get(&apos;productId&apos;), list[i].get(&apos;inventoryItemId&apos;))
        }
    }
}
